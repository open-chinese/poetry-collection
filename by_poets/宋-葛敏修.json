[
    {
        "uuid": "ebb573d6d81115d510e3ecb34b8475b4",
        "title": "题南华竹轩",
        "author": "葛敏修",
        "content": "不用山僧供帐迎，世间无此竹风清。\n独拳一手支颐卧，偷眼看云生未生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7e5188fc7e71f9de3fc7601fa886bdad",
        "title": "罗仙观",
        "author": "葛敏修",
        "content": "罗仙一去二百载，马迹飞桥今尚存。\n鹤驭不为缑氏岭，桃花空出武陵源。\n千年光景驰驹隙，三级波涛透禹门。\n欲见此翁论此事，夜深无梦入庄魂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "64b08d5d48543e722e0e13f4af7c5514",
        "title": "句  其一",
        "author": "葛敏修",
        "content": "一泻三百里，列为十八滩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dd3cfd56c4ade922601bd017274d3e72",
        "title": "句  其二",
        "author": "葛敏修",
        "content": "从今益勉为忠义，一噎如何便废餐。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "adecd78108dedd93b3ef99e358b38763",
        "title": "句  其三",
        "author": "葛敏修",
        "content": "章江来自西，贡水东其源。\n合流虎头下，共注鳖口村。",
        "dynasty": "宋",
        "collection_info": null
    }
]