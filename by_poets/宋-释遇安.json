[
    {
        "uuid": "cb508c6d6a5e7d3927dd03ee682d7b81",
        "title": "答僧",
        "author": "释遇安",
        "content": "欲识曹溪旨，云飞前面山。\n分明真实箇，不用别追攀。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cfafe41b681cf6922a131a2eac3bf58f",
        "title": "将卒示嗣子藴仁",
        "author": "释遇安",
        "content": "不是岭头携得事，岂从鸡足付将来。\n自古圣贤皆若此，非吾今日为君裁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cbaec437ac9b073b981aea211e3383a9",
        "title": "呈韶国师偈",
        "author": "释遇安",
        "content": "推真真无物，穷妄妄无形。\n返观真与妄，真妄亦虚名。",
        "dynasty": "宋",
        "collection_info": null
    }
]