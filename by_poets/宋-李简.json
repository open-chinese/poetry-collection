[
    {
        "uuid": "8eb04597a8a89ba29e2b765dfd86b410",
        "title": "伯时番王礼佛图卷",
        "author": "李简",
        "content": "佛身充满大千界，莲座玄谈海水翻。\n融摄群修证圆觉，灵通虚彻卓然存。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b429fbf9114cdc6648a5efd55a1190b1",
        "title": "句  其一",
        "author": "李简",
        "content": "挹香不盈怀，餐英淡无味。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bfd1caf00647786c9e51b39660d7acc2",
        "title": "句  其二",
        "author": "李简",
        "content": "向来红与紫，随流去如云。\n虽有故枝在，花落何纷纷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6077ebd3357b74c423076ba4f7661c39",
        "title": "送僧归护国寺",
        "author": "李简",
        "content": "来赴龙华约，归承帝泽新。\n道高传几祖，言下得何人。\n水想灵溪健，花饶法界春。\n自甘铅钝者，犹望指迷津。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4659d1c9e596807c17ce60d24e9832ee",
        "title": "句",
        "author": "李简",
        "content": "山遶一城藏古寺，江连二水送孤舟。",
        "dynasty": "宋",
        "collection_info": null
    }
]