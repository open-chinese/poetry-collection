[
    {
        "uuid": "1fd1221f99e388f2264a54abd8b98172",
        "title": "咏裕老庵前老松",
        "author": "王韶",
        "content": "绿皮皱剥玉嶙峋，高节分明似古人。\n解与干坤生气概，几因风雨长精神。\n装添景物年年换，摆捭穷愁日日新。\n惟有碧霄云裏月，共君孤影最相亲。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "89365009509458f8a2d5d4c49f08f1c8",
        "title": "南城",
        "author": "王韶",
        "content": "南城古要地，险隘接闽区。\n国家建昌垒，镇守东南隅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "063c415865296a41466f77ac454fcc64",
        "title": "东林泉",
        "author": "王韶",
        "content": "晋朝危乱有遗贤，陶谢宗雷总学禅。\n释殿夜深神运木，社堂秋后玉成莲。\n泉喷绣谷长时雨，云拥香炉白昼烟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c434641bfd5654993898a2701a0e67b5",
        "title": "投机颂",
        "author": "王韶",
        "content": "昼曾忘食夜忘眠，捧得骊珠欲上天。\n却向自身都放下，四棱塌地恰团圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f1b1d3cf38f8a498031e8016e25609ba",
        "title": "句  其一",
        "author": "王韶",
        "content": "草堂旧集三千首，莲社遗踪七百年。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9f214cdfd8fbb66f930c14cfd7ee75b9",
        "title": "句  其二",
        "author": "王韶",
        "content": "鄂陵事稀简。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d2a44f718837d7fe30e25c5f0337405f",
        "title": "句  其三",
        "author": "王韶",
        "content": "涂山广大纳万国，帝禹于此分江河。",
        "dynasty": "宋",
        "collection_info": null
    }
]