[
    {
        "uuid": "2f1fc9e9118c07e96306d85d64a4a9f5",
        "title": "赠萧韡",
        "author": "陈峤",
        "content": "一寸功名心已灰，翩然解组赋归来。\n中兴天子登耆旧，二十余年挽不回。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "def3034f039775b94a3c74d95085203a",
        "title": "题公署",
        "author": "陈峤",
        "content": "当默任言言地垢，当言任默默为尘。\n当默当言俱无任，尘埃何由得到身。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dda209a494b394cd8b933cd9f0c8d430",
        "title": "句  其一",
        "author": "陈峤",
        "content": "彭祖尚闻年八百，陈郎犹是小孩儿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8543a03ff179be66c1fa314e8b14116a",
        "title": "句  其二",
        "author": "陈峤",
        "content": "小桥风月年年事，争奈潘安老去何。",
        "dynasty": "宋",
        "collection_info": null
    }
]