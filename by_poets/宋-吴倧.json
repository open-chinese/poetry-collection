[
    {
        "uuid": "204fc4969c4e3cd5f9b7e10b1f896802",
        "title": "夏暑",
        "author": "吴倧",
        "content": "旧闻有西山，松风六月寒。\n明当整襟屦，舒笑青云端。\n招手尘外人，瀹泉采芳荪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "74d3dbba7a7dce69b9f95e4e020eb315",
        "title": "句  其一",
        "author": "吴倧",
        "content": "老去渐同嫫母丑，归来免效贾胡留。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ace2f4b588490cec3c062ec8d301eee3",
        "title": "句  其二",
        "author": "吴倧",
        "content": "两生更肯从绵蕞，高祖规模又不同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f9a86f1d23be2c448c54c98169380ad9",
        "title": "句  其三",
        "author": "吴倧",
        "content": "吴市终身避言耳，去家岂是去为仙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9a9f43d2274f987c7f542bbf60b3a22e",
        "title": "句  其四",
        "author": "吴倧",
        "content": "杨柳池深鱼乍出，桑麻土润燕初归。\n一年春草风前绿，万点桃花雨后红。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1cffef8be734552a34e480e29ec4dfaa",
        "title": "句  其五",
        "author": "吴倧",
        "content": "渔携网近鸥斜去，人过桥东影倒行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fbda5bcd0fea0ef8afab066a6da70be2",
        "title": "句  其六",
        "author": "吴倧",
        "content": "岸近田多损，潭深石迭平。\n村舂林外急，钓艇柳阴横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c0d96692d89c69374e84cdda1441cd45",
        "title": "句  其七",
        "author": "吴倧",
        "content": "竹牖迎风翻贝叶，铜炉温火养龙涎。",
        "dynasty": "宋",
        "collection_info": null
    }
]