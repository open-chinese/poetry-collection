[
    {
        "uuid": "efc6d3ca1d8933d3a8dfacf6bdac714e",
        "title": "夏暑",
        "author": "吴倧",
        "content": "旧闻有西山，松风六月寒。\n明当整襟屦，舒笑青云端。\n招手尘外人，瀹泉采芳荪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5870b5ac0f7888d8acab9ccf25a77941",
        "title": "句  其一",
        "author": "吴倧",
        "content": "老去渐同嫫母丑，归来免效贾胡留。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d7f19e6457c67ef9a635949013126871",
        "title": "句  其二",
        "author": "吴倧",
        "content": "两生更肯从绵蕞，高祖规模又不同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13715acb31ef66192edab37606c6e78e",
        "title": "句  其三",
        "author": "吴倧",
        "content": "吴市终身避言耳，去家岂是去为仙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "49b2d5c810e35ec648a8f96e54207efa",
        "title": "句  其四",
        "author": "吴倧",
        "content": "杨柳池深鱼乍出，桑麻土润燕初归。\n一年春草风前绿，万点桃花雨后红。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "949a9e571f4471c7898418c221e13fcb",
        "title": "句  其五",
        "author": "吴倧",
        "content": "渔携网近鸥斜去，人过桥东影倒行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "70cfc6dccefa65be540ac1313612536e",
        "title": "句  其六",
        "author": "吴倧",
        "content": "岸近田多损，潭深石迭平。\n村舂林外急，钓艇柳阴横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1ae0e5114492d35b7edae733eb3c2dda",
        "title": "句  其七",
        "author": "吴倧",
        "content": "竹牖迎风翻贝叶，铜炉温火养龙涎。",
        "dynasty": "宋",
        "collection_info": null
    }
]