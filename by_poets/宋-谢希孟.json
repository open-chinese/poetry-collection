[
    {
        "uuid": "6a3ce376dc08004ce17f76d04a00b6d4",
        "title": "樱桃",
        "author": "谢希孟",
        "content": "谷雨樱桃落，熏风柳带斜。\n舜弦新曲在，休唱后庭花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2c731818f02fe7cbc7eee2ecf9d34e0e",
        "title": "咏芍药",
        "author": "谢希孟",
        "content": "好是一时艷，本无千岁期。\n所以谑相赠，载之在声诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fedd11823406264f3bc9facf0392da23",
        "title": "句  其一",
        "author": "谢希孟",
        "content": "为花虽可期，论德亦终鲜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9db9b1f4a92ce06cd0556b4af800ba1",
        "title": "句  其二",
        "author": "谢希孟",
        "content": "句牵主人衣，一步行不得。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "482ba0c5bb6841cb41ca857815f63750",
        "title": "句  其三",
        "author": "谢希孟",
        "content": "熏熏麝脐烈，灼灼猩血殷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2973b2c1e5dfeedd4a3d6c5963358bad",
        "title": "句  其四",
        "author": "谢希孟",
        "content": "树既摧为薪，花亦落为尘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "35c425aa71db3721949f428bc6d2bf3c",
        "title": "句  其五",
        "author": "谢希孟",
        "content": "艷阳一何好，零落千载冤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "260ccbfe0c68123ea33550513c6e9e84",
        "title": "句  其六",
        "author": "谢希孟",
        "content": "盗者得其便，掉头笑且歌。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "55944089d16af943ec85cc26acbbdf1a",
        "title": "句  其七",
        "author": "谢希孟",
        "content": "卞子真赏季，庄生梦还怯。",
        "dynasty": "宋",
        "collection_info": null
    }
]