[
    {
        "uuid": "a5dc1bfaa406d2d699b51b7bcdad8603",
        "title": "樱桃",
        "author": "谢希孟",
        "content": "谷雨樱桃落，熏风柳带斜。\n舜弦新曲在，休唱后庭花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5cad07ce7b4748e19cf02b77b6dfb02a",
        "title": "咏芍药",
        "author": "谢希孟",
        "content": "好是一时艷，本无千岁期。\n所以谑相赠，载之在声诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "170feb8ca508dda9bfed280ef6417824",
        "title": "句  其一",
        "author": "谢希孟",
        "content": "为花虽可期，论德亦终鲜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d2a169103637f64544ad8ca78f14f522",
        "title": "句  其二",
        "author": "谢希孟",
        "content": "句牵主人衣，一步行不得。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "681a8b576c816f9b60ba3b8b2985dec1",
        "title": "句  其三",
        "author": "谢希孟",
        "content": "熏熏麝脐烈，灼灼猩血殷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8b013839753e25e789b86e155eb5d475",
        "title": "句  其四",
        "author": "谢希孟",
        "content": "树既摧为薪，花亦落为尘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "01e09cdd4cbb496dd1b500d72a321f04",
        "title": "句  其五",
        "author": "谢希孟",
        "content": "艷阳一何好，零落千载冤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0472613ab42516237c2821ba22683c2b",
        "title": "句  其六",
        "author": "谢希孟",
        "content": "盗者得其便，掉头笑且歌。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a48d88a63ad1e6ccdf742a27904a7e4",
        "title": "句  其七",
        "author": "谢希孟",
        "content": "卞子真赏季，庄生梦还怯。",
        "dynasty": "宋",
        "collection_info": null
    }
]