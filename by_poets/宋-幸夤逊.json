[
    {
        "uuid": "1cf653a80bc4bf6ef8111a34d0dc5a25",
        "title": "登戎州江楼闲望",
        "author": "幸夤逊",
        "content": "满目江山四望幽，白云高卷嶂烟收。\n日回禽影穿疏木，风递猿声入小楼。\n远岫似屏横碧落，断帆如叶截中流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "03b64d02d3c124a7f356532835078f92",
        "title": "雪",
        "author": "幸夤逊",
        "content": "片片飞来静又闲，楼头江上復山前。\n飘零尽日不归去，帖破清光万里天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ba9a58a3b9132b848d201741b4f5a1e1",
        "title": "云",
        "author": "幸夤逊",
        "content": "因登巨石知来处，勃勃元生绿藓痕。\n静即等闲藏草木，动时顷刻徧干坤。\n横天未必朋元恶，捧日还曾瑞至尊。\n不独朝朝在巫峡，楚王何事谩劳魂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "25b23f17d098ed87f2ea2767190b8163",
        "title": "句  其一",
        "author": "幸夤逊",
        "content": "若教作镇居中国，争得泥金在泰山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c0578b8ff591138afca0153f7c7c3be2",
        "title": "句  其二",
        "author": "幸夤逊",
        "content": "纔闻暖律先偷眼，既待和风始展眉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "22b9d9940216f66cb4ee78ebec0bfd0a",
        "title": "句  其三",
        "author": "幸夤逊",
        "content": "嚼处春冰敲齿冷，嚥时雪液沃心寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "807ed002aac529989ec0e6f27b444745",
        "title": "句  其四",
        "author": "幸夤逊",
        "content": "蒙君知重惠琼实，薄起金刀钉玉深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fe28ac67d524bef6c612f76c45d36831",
        "title": "句  其五",
        "author": "幸夤逊",
        "content": "深妆玉瓦平无壠，乱拂芦花细有声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3dc5d671818d8659848c826cc2d76777",
        "title": "句  其六",
        "author": "幸夤逊",
        "content": "片逐银蟾落醉觥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c832486b0d3545db19841d8aa272b13",
        "title": "句  其七",
        "author": "幸夤逊",
        "content": "巧剪银花乱，轻飞玉叶狂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "85c8c59f5339d80596e34ef3736de86b",
        "title": "句  其八",
        "author": "幸夤逊",
        "content": "寒艷芳姿色尽明。",
        "dynasty": "宋",
        "collection_info": null
    }
]