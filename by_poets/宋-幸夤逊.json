[
    {
        "uuid": "dc17a2bacd7d95d02ea1fb1c4d88214b",
        "title": "登戎州江楼闲望",
        "author": "幸夤逊",
        "content": "满目江山四望幽，白云高卷嶂烟收。\n日回禽影穿疏木，风递猿声入小楼。\n远岫似屏横碧落，断帆如叶截中流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "07cc453f8306f0416ddf3bbf9b52f635",
        "title": "雪",
        "author": "幸夤逊",
        "content": "片片飞来静又闲，楼头江上復山前。\n飘零尽日不归去，帖破清光万里天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7609a823d30b3ee537bb0a4ad5394bb8",
        "title": "云",
        "author": "幸夤逊",
        "content": "因登巨石知来处，勃勃元生绿藓痕。\n静即等闲藏草木，动时顷刻徧干坤。\n横天未必朋元恶，捧日还曾瑞至尊。\n不独朝朝在巫峡，楚王何事谩劳魂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "350cee6ad191784d25b6f9fcbc8d4cf1",
        "title": "句  其一",
        "author": "幸夤逊",
        "content": "若教作镇居中国，争得泥金在泰山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0ce7c0116e09fb8527f22887f5e93ff5",
        "title": "句  其二",
        "author": "幸夤逊",
        "content": "纔闻暖律先偷眼，既待和风始展眉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b05b61ba52320b3ff6672b05bf9bd609",
        "title": "句  其三",
        "author": "幸夤逊",
        "content": "嚼处春冰敲齿冷，嚥时雪液沃心寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "58a0767eeaa95f3cf2f34183427d302b",
        "title": "句  其四",
        "author": "幸夤逊",
        "content": "蒙君知重惠琼实，薄起金刀钉玉深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "054aa71c033242b9038db9484cd52d19",
        "title": "句  其五",
        "author": "幸夤逊",
        "content": "深妆玉瓦平无壠，乱拂芦花细有声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "97aa66ada5c2589c9400dede4499f546",
        "title": "句  其六",
        "author": "幸夤逊",
        "content": "片逐银蟾落醉觥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "42e4fa513cf03577a869ffc50bc5408c",
        "title": "句  其七",
        "author": "幸夤逊",
        "content": "巧剪银花乱，轻飞玉叶狂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e4c0399f595631b424ad9d3ec41d8431",
        "title": "句  其八",
        "author": "幸夤逊",
        "content": "寒艷芳姿色尽明。",
        "dynasty": "宋",
        "collection_info": null
    }
]