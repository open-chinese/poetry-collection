[
    {
        "uuid": "72bffa2cdc444cc568192173e13a9081",
        "title": "偈",
        "author": "释本",
        "content": "五峰家风，南北西东。\n要用便用，以橛钉空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6b7b903aa1232c7e458aaf0c9e56828c",
        "title": "偈二首  其一",
        "author": "释本",
        "content": "清明已过十余日，华雨阑珊方寸深。\n春色恼人眠不得，黄鹂飞过绿杨阴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9786b02ac8e50b7b941d18b42b8b7695",
        "title": "偈二首  其二",
        "author": "释本",
        "content": "鲍老当年笑郭郎，郭郎舞袖太郎当。\n及乎鲍老当场舞，鲍老郎当胜郭郎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "953a70912ce1958b5e1057574ddf4774",
        "title": "偈",
        "author": "释本",
        "content": "半在河南半在北，一片虚凝似墨黑。\n冷地思量愁杀人，叵耐云门这老贼。",
        "dynasty": "宋",
        "collection_info": null
    }
]