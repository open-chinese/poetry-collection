[
    {
        "uuid": "395e9c6364a8ae84046c870e68374122",
        "title": "题灵隐寺",
        "author": "王予可",
        "content": "游山无处浣尘埃，出郭寻幽入翠苔。\n众水尽从双涧去，一峰元自五天来。\n行春人散题名在，坐夏僧閒听讲回。\n清磬一声猿鸟寂，石楠花落满经台。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9043d81968188e951eb1065808ce9b8d",
        "title": "宫词",
        "author": "王予可",
        "content": "水曲朱门漪漾漫，一帘花雨月波寒。\n金闺背衬鸳鸯冷，春困秋千立画干。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0b84a7d6482c1f5dcdc435d8f57cb719",
        "title": "南园湖石",
        "author": "王予可",
        "content": "翠雀衔云堕翠芜，砥峰倒影卧平湖。\n飞花不到穿帘月，高倚晴天一剑孤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7080d321500872bd0114e10531bcab7f",
        "title": "驯鹤图",
        "author": "王予可",
        "content": "寝处粧铅未捲钗，孤云花带月边来。\n六宫帘幕金鸾冷，露湿晨烟啄翠苔。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9cded27e2180acbc474457ef64cb73c",
        "title": "杂诗二首  其一",
        "author": "王予可",
        "content": "白露沙滩浸绿湄，小舟舣岸尚依稀。\n山回屏曲江连树，春锁人家深处归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f963a8907abafe2e074aa2ff98451cf8",
        "title": "杂诗二首  其二",
        "author": "王予可",
        "content": "暗悲秋色素团团，一雨飘零飚霁寒。\n天浄长空烟敛处，彩虹金挂树头山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c7c6b82717c3ea993a889d4314d55f8f",
        "title": "宫体  其一",
        "author": "王予可",
        "content": "红叶铺霜撼御阶，绣莲尘蹴衬罗鞋。\n袖沾莺翅调簧语，堕却衔翘入鬓钗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a89f83510e2b12f4e0f01fc71735cbbc",
        "title": "宫体  其二",
        "author": "王予可",
        "content": "骄马金笼藉草归，翠鸾屏曲染红霏。\n凭栏山色春风裏，唤得莺儿燕子飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3e00c49d722670d46d6d6faecb0fc7ba",
        "title": "句  其一",
        "author": "王予可",
        "content": "石裂雯华渍月秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c36a6037e679a4a4ee2d0599308c148",
        "title": "句  其二",
        "author": "王予可",
        "content": "壶树苔波月渍皴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b470b7856970930571ed455327606150",
        "title": "句  其三",
        "author": "王予可",
        "content": "一壶天地醒眠小。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ba30f6838c7a2386530785f0b5202cb9",
        "title": "句  其四",
        "author": "王予可",
        "content": "万迭云山飞小雁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fec7b047c8227abf2d9cfaa9050c13b8",
        "title": "句  其五",
        "author": "王予可",
        "content": "风色偃貂裘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "81122793785894b67abda81d83a9404b",
        "title": "句  其六",
        "author": "王予可",
        "content": "唾尖绒舌淡红甜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3a04c3a9f45432b69cb288757bb7c23d",
        "title": "句  其七",
        "author": "王予可",
        "content": "金盆水不暖，翠雀啄晴苔。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "29792d01b889a6970956a32e850a395a",
        "title": "句  其八",
        "author": "王予可",
        "content": "凤蹴瑶华散，龙衔桂子香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5e1fa5fe22040d088d2e93964b21a036",
        "title": "句  其九",
        "author": "王予可",
        "content": "一片冷截潭底月，六弯斜卷陇头云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6988beb9238b893fb0a17339a45334a8",
        "title": "句  其一○",
        "author": "王予可",
        "content": "帘捲绿阴花外月，玉山冰雪醉扶翁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "596247ec4aedb6726d5de1592fa4f871",
        "title": "句  其一一",
        "author": "王予可",
        "content": "啼鸟倒衔金羽舞，惊蛇斜傍玉帘飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "04786fd47fccf9ab4fe703b516bcbb7b",
        "title": "句  其一二",
        "author": "王予可",
        "content": "凤环捧席带香屏，鲸杯倚伎和云捲。",
        "dynasty": "宋",
        "collection_info": null
    }
]