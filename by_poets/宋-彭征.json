[
    {
        "uuid": "7bb50dbd76649355d27e928ab05dc06c",
        "title": "真人  其一",
        "author": "彭征",
        "content": "白云深处是仙家，清壁飞来瀑布斜。\n采药不知归路晚，洞门开遍碧桃花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "08ee95a41e17aa98f404d3d2121fe990",
        "title": "真人  其二",
        "author": "彭征",
        "content": "散髪披襟万仞冈，天池风度碧莲香。\n洞云去作人间雨，留下瑶坛月影凉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3d95e647994c0aac68827129db2936a1",
        "title": "真人  其三",
        "author": "彭征",
        "content": "老桂吹香入古瓢，月华如水碧天寥。\n酒醒夜半谁惊觉，鹤背仙人紫玉箫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "92abfcc198220f01bc44d832e86e25fe",
        "title": "真人  其四",
        "author": "彭征",
        "content": "山中无历任光阴，梅正开时雪意深。\n弹罢蕊珠香满座，天花几个落瑶琴。",
        "dynasty": "宋",
        "collection_info": null
    }
]