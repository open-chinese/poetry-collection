[
    {
        "uuid": "2c95efe0baedc0769df4212cf8a92cfe",
        "title": "和孙公索酒",
        "author": "刘延世",
        "content": "紫貂寒拥鼻，绿螘细侵唇。\n莲烛当时事，壶头此日春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2b5f6794dada2dbc4d971b9c96804fa1",
        "title": "种竹",
        "author": "刘延世",
        "content": "梅蒸方过有余润，竹醉由来自古云。\n掘地聊栽数竿竹，开帘还当一溪云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "708cbec3466a8119b54a00a973819747",
        "title": "自题墨竹",
        "author": "刘延世",
        "content": "酷爱此君心，常将墨点真。\n毫端虽在手，难写淡精神。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d94d74031035026fc67d61553eadf67e",
        "title": "鹧鸪",
        "author": "刘延世",
        "content": "冥冥花朵隔遥山，脉脉风烟暗百蛮。\n曾向黄陵嘲竹裏，羽毛犹带泪痕斑。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fadbe672ab32fd4f325752d79ec8db04",
        "title": "翡翠",
        "author": "刘延世",
        "content": "避人忽起鸣衣桁，掠水飞来立钓矶。\n静处欲留看不足，翠光点破夕阳归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d513153fbc403fab170044d229761268",
        "title": "子规",
        "author": "刘延世",
        "content": "一声迸裂青山竹，半夜沾残病客衣。\n还道离于天上去，不知何苦只思归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c4798e0d49653e528d34d2116cbb23d5",
        "title": "水帘",
        "author": "刘延世",
        "content": "冷筛千嶂雨，寒□一鈎□。\n不放香烟出，长教隔画檐。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1413fcbce50c0ebd0d5fed940b7bca7c",
        "title": "句  其一",
        "author": "刘延世",
        "content": "静室焚香盘膝坐，长廊看画散衣行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9c0c2f807c81338912c3bc60efa72acd",
        "title": "句  其二",
        "author": "刘延世",
        "content": "舟行明鑑裏。",
        "dynasty": "宋",
        "collection_info": null
    }
]