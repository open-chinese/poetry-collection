[
    {
        "uuid": "c1c66ea28ba2df8f18a1979e53045b44",
        "title": "拟梅轩三首  其一",
        "author": "吴幵",
        "content": "阴阴绿叶不胜垂，着子全多欲压枝。\n自得君王一留顾，故应雨露亦饶滋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e35315b500dec201910ffbe307e0a2c2",
        "title": "拟梅轩三首  其二",
        "author": "吴幵",
        "content": "五月霏霏雨不开，若耶溪畔摘楞梅。\n朱丸忽向云窗见，疑是灵根越岭来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6dc404054d16f608b26e48d4d2425539",
        "title": "拟梅轩三首  其三",
        "author": "吴幵",
        "content": "谁将蜜渍借微酸，小摘曾闻修玉盘。\n争似江南风致在，瓶红初向绿阴看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5d634fe59748f907a6491b56ed7ffee",
        "title": "句  其一",
        "author": "吴幵",
        "content": "李衡千树橘，张翰一渔舟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "170c3075d26c59da179b668b9a4950ea",
        "title": "句  其二",
        "author": "吴幵",
        "content": "木落千山瘦，天高一雁横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d5173bfb60c16427f61c8075c3ed1d5e",
        "title": "句  其三",
        "author": "吴幵",
        "content": "鸟声催暮急，山气欲晴寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e934ba3c62deca713dd2170468efe4df",
        "title": "句  其四",
        "author": "吴幵",
        "content": "鸣玉不妨风细细，琐窗时借月娟娟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "efde2165084c147b1cbd56a56eb123ce",
        "title": "句  其五",
        "author": "吴幵",
        "content": "星斗垂光烛紫坛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7318af3bf21f6e4caed06f729755242a",
        "title": "句  其六",
        "author": "吴幵",
        "content": "欲识懽心从四海，钧天一曲引回銮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e0db63bbd5d03d029fc73b255b825ccf",
        "title": "句  其七",
        "author": "吴幵",
        "content": "纷纶委贶知旁达，渗漉流恩亦下蟠。",
        "dynasty": "宋",
        "collection_info": null
    }
]