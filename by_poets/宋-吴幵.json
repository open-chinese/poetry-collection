[
    {
        "uuid": "89ed226600aa485e9088a8f136f34d4f",
        "title": "拟梅轩三首  其一",
        "author": "吴幵",
        "content": "阴阴绿叶不胜垂，着子全多欲压枝。\n自得君王一留顾，故应雨露亦饶滋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b12f19eb49509f07e8af38d9103db699",
        "title": "拟梅轩三首  其二",
        "author": "吴幵",
        "content": "五月霏霏雨不开，若耶溪畔摘楞梅。\n朱丸忽向云窗见，疑是灵根越岭来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ce63b19e2dc7279d44cfaa8d4f0ecd15",
        "title": "拟梅轩三首  其三",
        "author": "吴幵",
        "content": "谁将蜜渍借微酸，小摘曾闻修玉盘。\n争似江南风致在，瓶红初向绿阴看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d7da1a35dc05b6cca6400f9d0637224c",
        "title": "句  其一",
        "author": "吴幵",
        "content": "李衡千树橘，张翰一渔舟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11369ab44c8d179c528f10466491df96",
        "title": "句  其二",
        "author": "吴幵",
        "content": "木落千山瘦，天高一雁横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b2bf08d2136974b16752ca139872118e",
        "title": "句  其三",
        "author": "吴幵",
        "content": "鸟声催暮急，山气欲晴寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed43bab419ee149dc4d8dd16aba0a7f0",
        "title": "句  其四",
        "author": "吴幵",
        "content": "鸣玉不妨风细细，琐窗时借月娟娟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f4627be5ec9209d45f1c2232322c3a1e",
        "title": "句  其五",
        "author": "吴幵",
        "content": "星斗垂光烛紫坛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "acc8184164c03582bacef7ada70db71b",
        "title": "句  其六",
        "author": "吴幵",
        "content": "欲识懽心从四海，钧天一曲引回銮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9f2738cac083e5f80c2bf74e6bcfe7b7",
        "title": "句  其七",
        "author": "吴幵",
        "content": "纷纶委贶知旁达，渗漉流恩亦下蟠。",
        "dynasty": "宋",
        "collection_info": null
    }
]