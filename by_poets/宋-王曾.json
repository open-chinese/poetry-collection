[
    {
        "uuid": "77e58b766f39fe6d719ac7dbee6f5a2d",
        "title": "送李文定移镇兖海诗",
        "author": "王曾",
        "content": "锦标夺得曾相继，金鼎调时亦荐更。\n并上儿童君再见，会稽章绂我偏荣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "21dd40f71fbc675526d7196a6ebc255f",
        "title": "早梅",
        "author": "王曾",
        "content": "雪压乔林冻欲摧，始知天意欲春回。\n雪中未问和羹事，且向百花头上开。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3f3ce8d3fe0b0cee7b9090a188e931e8",
        "title": "送钱易",
        "author": "王曾",
        "content": "籍其声名喧魏阙，幽奇风物指吴乡。\n上饶此去逾千里，莫惜临岐酒满觞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "75f6001a8ae2c5fae3c07d627cacd75a",
        "title": "献金陵牧薛大夫白马诗",
        "author": "王曾",
        "content": "白马披丝练一团，今朝被绊欲行难。\n雪中放去唯留迹，月下牵来只见鞍。\n向北长鸣天外远，临风斜坠耳边寒。\n自知毛骨还应异，更请王良子细看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c8c6533d41030fef380087c1c5d1a7e4",
        "title": "皇帝阁立春帖子",
        "author": "王曾",
        "content": "北陆凝阴尽，千门淑气新。\n年年金殿裏，宝字贴宜春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "545cba3c36ff5c44d3840a4429858127",
        "title": "送何水部蒙出牧袁州",
        "author": "王曾",
        "content": "雉省荣迁拜命新，鱼符重佩出关频。\n停骖却访枌榆社，恋阙迴瞻析木津。\n水薤有谁裨政术，江山终日助吟神。\n一麾岭外遥相望，应合趋庭问狷人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c77b648977c0002aa95d92bf5fac9a47",
        "title": "送李寺丞归临江",
        "author": "王曾",
        "content": "辞荣知退出尘埃，泽国皆推隐逸才。\n清世不为王事累，白云重向帝乡来。\n僧因好古留棋诀，鸥为忘机恋钓台。\n应见酒旗回马首，远烟沈澹百花开。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2b3d05cf9f6340b7185e6ae234101a0a",
        "title": "句  其一",
        "author": "王曾",
        "content": "綵燕迎春入宾飞，轻寒未放缕金衣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2b698ee846beabef05e907bb81d15e58",
        "title": "句  其二",
        "author": "王曾",
        "content": "争传九子糉，皇祚续千春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "25bae758392a4366101f0ef47b5510ff",
        "title": "句  其三",
        "author": "王曾",
        "content": "夕燃辟恶仙香度，朝结延年帝缕成。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "54d8280c53e71a8dd992ec139f16fdb9",
        "title": "句  其四",
        "author": "王曾",
        "content": "绕臂双条达，红纱昼梦警。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "59705f4ff4826736f31aaba49477e467",
        "title": "句  其五",
        "author": "王曾",
        "content": "欲谢君恩却无语，心前笑指赤灵符。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e92913a90219c58f3ac8bba3f9bf8bae",
        "title": "句  其六",
        "author": "王曾",
        "content": "明朝知是天中节，旋刻菖蒲要辟邪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "21ac980d5be1994ba39dbd5591a57f54",
        "title": "句  其七",
        "author": "王曾",
        "content": "愿上菖花酒，年年圣子心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "91adfd0a4a760366b8940f252f512b00",
        "title": "句  其八",
        "author": "王曾",
        "content": "如何金殿裏，犹献辟兵符。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e706377b86873c82b1b74f56e79ed9cb",
        "title": "句  其九",
        "author": "王曾",
        "content": "仙艾垂门绿，魂丝绕户长。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f0750261090704b0f218cda09bbe5c9f",
        "title": "句  其一○",
        "author": "王曾",
        "content": "百灵扶绣户，不假艾为人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6bef29d7625a1ee027b84d2606785fd4",
        "title": "句  其一一",
        "author": "王曾",
        "content": "钗头艾虎辟羣邪，晓驾祥云七宝车。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a86fda98b2bc114763335d4f2a328df2",
        "title": "句  其一二",
        "author": "王曾",
        "content": "更闻天子孝，荐术助长生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4d0b1468cad1823f8b4bb56ff9247015",
        "title": "句  其一三",
        "author": "王曾",
        "content": "人畏轩台久，春归雨泽多。",
        "dynasty": "宋",
        "collection_info": null
    }
]