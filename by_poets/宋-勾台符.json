[
    {
        "uuid": "76bbe039d5981f75d27fa3084ec1dba8",
        "title": "宿上清宫",
        "author": "勾台符",
        "content": "寄宿翠微颠，身疑入半天。\n晓钟鸣物外，残月落岩前。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7c553fab1cf692aa0b5f7482553858e5",
        "title": "延庆宫",
        "author": "勾台符",
        "content": "一簇楼台地，嵯峨四面函。\n路盘七折岭，坐对六时岩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "827f7096cb44bae9c7d57a111304ea3f",
        "title": "句  其一",
        "author": "勾台符",
        "content": "松门开碧嶂，五甃锁清泉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d36d529c7212c303e47e107cac2b7c52",
        "title": "句  其二",
        "author": "勾台符",
        "content": "无叶滋春色，有花开晚红。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9bde71956457641e1e61f6cde4d5be7c",
        "title": "句  其三",
        "author": "勾台符",
        "content": "料得桓温登剑阁，便抛李势入岷山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8ace2e9c2fbaf7718776e2c9c003e42f",
        "title": "句  其四",
        "author": "勾台符",
        "content": "胚浑凿开元精结，三十六峰排截嶭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1ced83d86cd7557b0d1eb09514891a1a",
        "title": "句  其五",
        "author": "勾台符",
        "content": "酷爱青城好山色，终年不出白云门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4d0e3751dab846b6065be64ba03de7a6",
        "title": "句  其六",
        "author": "勾台符",
        "content": "右执范贤袂，左拍薛昌肩。\n举头傲白日，长啸揭青天。",
        "dynasty": "宋",
        "collection_info": null
    }
]