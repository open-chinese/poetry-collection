[
    {
        "uuid": "3c15e07b22eca6b10b1e37e3b3eff1d7",
        "title": "宿上清宫",
        "author": "勾台符",
        "content": "寄宿翠微颠，身疑入半天。\n晓钟鸣物外，残月落岩前。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1ad87a1954094a54f2b19e15fb760e09",
        "title": "延庆宫",
        "author": "勾台符",
        "content": "一簇楼台地，嵯峨四面函。\n路盘七折岭，坐对六时岩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7e045753d308cbc60ddce2c2bce65ad5",
        "title": "句  其一",
        "author": "勾台符",
        "content": "松门开碧嶂，五甃锁清泉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e4db332eadbe8747ecf20127b5347b45",
        "title": "句  其二",
        "author": "勾台符",
        "content": "无叶滋春色，有花开晚红。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aaba13e98e0934c1c883808f716cbbea",
        "title": "句  其三",
        "author": "勾台符",
        "content": "料得桓温登剑阁，便抛李势入岷山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9d904f49a320e0e766066fe2444b7f77",
        "title": "句  其四",
        "author": "勾台符",
        "content": "胚浑凿开元精结，三十六峰排截嶭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "55bc9f65e951df7dbc37a5612b3e6f43",
        "title": "句  其五",
        "author": "勾台符",
        "content": "酷爱青城好山色，终年不出白云门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e00e8990b9670ce0e5d3ef01e5bc7f40",
        "title": "句  其六",
        "author": "勾台符",
        "content": "右执范贤袂，左拍薛昌肩。\n举头傲白日，长啸揭青天。",
        "dynasty": "宋",
        "collection_info": null
    }
]