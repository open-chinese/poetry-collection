[
    {
        "uuid": "92feca1c24852461e1136f220675d195",
        "title": "中秋雨下偈",
        "author": "释慧兰",
        "content": "家家启户待婵娟，岂谓风云翳碧天。\n赖得箇中消息在，团团不动照三千。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8998d63110594d59890677063ebe1205",
        "title": "颂古 五位 其一",
        "author": "释慧兰",
        "content": "正中偏拈转，虚空过那边。\n夜半金鸡生铁卵，明明谁辨暗中圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f6605901eb3a36e907f8afbad4738acb",
        "title": "颂古 五位 其二",
        "author": "释慧兰",
        "content": "偏中正晓月，梳云华弄影。\n石人惊起梦魂消，带雪乌鸡回凤岭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aad341b2cf4a3624a73ff4f43cdf4370",
        "title": "颂古 五位 其三",
        "author": "释慧兰",
        "content": "正中来玉兔，团天耀九垓。\n万古碧潭无影像，红炉焰裏雪华开。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "49368666bfe04f4f8a7ea2ddb3baa6c0",
        "title": "颂古 五位 其四",
        "author": "释慧兰",
        "content": "兼中至锦缝，金针双锁备。\n靴头线绽足知音，有耳不临流水流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "10fd436fe1f4924f0a70a41a7d7a458e",
        "title": "颂古 五位 其五",
        "author": "释慧兰",
        "content": "兼中到眼裏，双瞳齐喝道。\n少室山前铁马驹，三便吃尽无根艹。",
        "dynasty": "宋",
        "collection_info": null
    }
]