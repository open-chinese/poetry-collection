[
    {
        "uuid": "6c3d75294b6ef25c38651b7d269056f7",
        "title": "中秋雨下偈",
        "author": "释慧兰",
        "content": "家家启户待婵娟，岂谓风云翳碧天。\n赖得箇中消息在，团团不动照三千。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e33a66f3ce15309190445ed2dd12a2d6",
        "title": "颂古 五位 其一",
        "author": "释慧兰",
        "content": "正中偏拈转，虚空过那边。\n夜半金鸡生铁卵，明明谁辨暗中圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a35d4b2723b0bb88e8c82da46c7e033e",
        "title": "颂古 五位 其二",
        "author": "释慧兰",
        "content": "偏中正晓月，梳云华弄影。\n石人惊起梦魂消，带雪乌鸡回凤岭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "073abfb8b31221a91de201631db3db47",
        "title": "颂古 五位 其三",
        "author": "释慧兰",
        "content": "正中来玉兔，团天耀九垓。\n万古碧潭无影像，红炉焰裏雪华开。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8dce807ed8e17375ef3f916b35405b12",
        "title": "颂古 五位 其四",
        "author": "释慧兰",
        "content": "兼中至锦缝，金针双锁备。\n靴头线绽足知音，有耳不临流水流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "80e68aa0dd6756a86be2b417352a3fcd",
        "title": "颂古 五位 其五",
        "author": "释慧兰",
        "content": "兼中到眼裏，双瞳齐喝道。\n少室山前铁马驹，三便吃尽无根艹。",
        "dynasty": "宋",
        "collection_info": null
    }
]