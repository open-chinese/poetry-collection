[
    {
        "uuid": "e3b18b7bda4869af9b485b23c75e737f",
        "title": "三姑石",
        "author": "李左史",
        "content": "风舞芳林鬓脚垂，朝云行雨湿仙衣。\n不知当日缘何事，化石山头更不归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c94429833f2411073e82e67c5463fb9e",
        "title": "大隠屏",
        "author": "李左史",
        "content": "高峰崛起翠云屏，何事呼为大隠名。\n自是市朝人不到，非关洞户锁峥嵘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8d490c894a9199fa76159889a01cfd3d",
        "title": "幔亭峰",
        "author": "李左史",
        "content": "更无裀幕空中举，时有笙竽静处闻。\n猿鸟哀啼千嶂月，松篁寒锁一溪云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e331bafb4126e808efbcb8a12e41b06",
        "title": "天柱峰",
        "author": "李左史",
        "content": "巉巉千丈插烟空，始见天南一柱峰。\n绝顶雾开擎日月，半岩云暝噫雷风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9a6d3a02733efbe79d17fe4c4eed6126",
        "title": "仙迹岩",
        "author": "李左史",
        "content": "当时天姥立溪边，石上遗踪见宛然。\n笑杀茂陵缑氏迹，只凭方士口中传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "19df720a3a11c64212b028817ce970af",
        "title": "句  其一",
        "author": "李左史",
        "content": "溪流玉雪三三曲，山锁烟霞六六峰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a4d6954ed5be68ba8878e2d0be8f97aa",
        "title": "句  其二",
        "author": "李左史",
        "content": "刘公隠后今谁继，张湛仙来不记年。",
        "dynasty": "宋",
        "collection_info": null
    }
]