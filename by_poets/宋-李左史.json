[
    {
        "uuid": "72b2b300766599a7594f4b39e2443ee1",
        "title": "三姑石",
        "author": "李左史",
        "content": "风舞芳林鬓脚垂，朝云行雨湿仙衣。\n不知当日缘何事，化石山头更不归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ace3464c3ac22a80adf659e6215e2936",
        "title": "大隠屏",
        "author": "李左史",
        "content": "高峰崛起翠云屏，何事呼为大隠名。\n自是市朝人不到，非关洞户锁峥嵘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1323101922efa1c7e2a76450231c1ffd",
        "title": "幔亭峰",
        "author": "李左史",
        "content": "更无裀幕空中举，时有笙竽静处闻。\n猿鸟哀啼千嶂月，松篁寒锁一溪云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0271f802a7d29ee3b4bcd068f17eb29a",
        "title": "天柱峰",
        "author": "李左史",
        "content": "巉巉千丈插烟空，始见天南一柱峰。\n绝顶雾开擎日月，半岩云暝噫雷风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f0d1a5ced1c52432507f63a9c655a230",
        "title": "仙迹岩",
        "author": "李左史",
        "content": "当时天姥立溪边，石上遗踪见宛然。\n笑杀茂陵缑氏迹，只凭方士口中传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3e51d5ce115ea50fbc68fdaeb2c2c3fa",
        "title": "句  其一",
        "author": "李左史",
        "content": "溪流玉雪三三曲，山锁烟霞六六峰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5f54fb6a2ee7133453ef16d1a4c5a198",
        "title": "句  其二",
        "author": "李左史",
        "content": "刘公隠后今谁继，张湛仙来不记年。",
        "dynasty": "宋",
        "collection_info": null
    }
]