[
    {
        "uuid": "a0d259605aa10c7c0e1f3705c9489ce8",
        "title": "渔父",
        "author": "吕炎",
        "content": "万顷江天一叶舟，桃花春水蓼花秋。\n翛然白鸟苍烟外，只有渔翁得自由。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "47ed33d0eac84b1e18a6163634ec1356",
        "title": "田园词",
        "author": "吕炎",
        "content": "阴阴径底忽抽叶，莫莫篱边豆结花。\n唳鹤清愁牵犬恨，料应不到菜园家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cb0ca084982fa357b4c8907a2263e71a",
        "title": "甘园",
        "author": "吕炎",
        "content": "曾入当年富贵家，东风几度换年华。\n满园桃李不知主，独自开花自落花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9c8da271f1e453189f2c4ebdbfad7da",
        "title": "闺思",
        "author": "吕炎",
        "content": "夜久香销翡翠奁，十分春恨上眉尖。\n荼䕷谢了樱桃过，人影参差月半帘。",
        "dynasty": "宋",
        "collection_info": null
    }
]