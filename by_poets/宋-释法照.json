[
    {
        "uuid": "46bc637a64844ed9a02e0c7a893b06c1",
        "title": "游洞霄",
        "author": "释法照",
        "content": "天柱华心结蕊宫，翠蛟飞玉滚溪风。\n岩生洞穴非人世，不到那知造化功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dbe71110a2beb2b42a68553e50bf09d5",
        "title": "垂虹晚望",
        "author": "释法照",
        "content": "老眼青青望洞庭，虹垂断岸月初生。\n滔滔桥下无情水，不为吴王洗甲兵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "96fbef65a66d9b95b24981c658fe5b8e",
        "title": "表忠观",
        "author": "释法照",
        "content": "钱王古庙锁莓苔，华表秋深鹤不来。\n昨夜石坛风露重，凌霄花落凤仙开。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "261e909b12dc1134c7dbf9a22f821959",
        "title": "辞世偈",
        "author": "释法照",
        "content": "佛寿八十，我多九年。\n虚空掇转，大用现前。",
        "dynasty": "宋",
        "collection_info": null
    }
]