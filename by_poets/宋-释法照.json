[
    {
        "uuid": "9b0f859786d96fda8fc89ac58f043dee",
        "title": "游洞霄",
        "author": "释法照",
        "content": "天柱华心结蕊宫，翠蛟飞玉滚溪风。\n岩生洞穴非人世，不到那知造化功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d1a1886f4e3768eb0d9be86de207f918",
        "title": "垂虹晚望",
        "author": "释法照",
        "content": "老眼青青望洞庭，虹垂断岸月初生。\n滔滔桥下无情水，不为吴王洗甲兵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2e300c96347747be2aa51ea8b67926ac",
        "title": "表忠观",
        "author": "释法照",
        "content": "钱王古庙锁莓苔，华表秋深鹤不来。\n昨夜石坛风露重，凌霄花落凤仙开。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "61737335ed48270b5d412e7a0d83f688",
        "title": "辞世偈",
        "author": "释法照",
        "content": "佛寿八十，我多九年。\n虚空掇转，大用现前。",
        "dynasty": "宋",
        "collection_info": null
    }
]