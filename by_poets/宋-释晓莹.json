        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c7db359d18eab2ef200323a1b96a9d2",
        "title": "句  其三九",
        "author": "释晓莹",
        "content": "清泉槛此流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5a36b2d2e3186ebbe29cbf1438987613",
        "title": "句  其四○",
        "author": "释晓莹",
        "content": "是身多落寞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bbdce8252fb75cd899c710d2834a3494",
        "title": "句  其四一",
        "author": "释晓莹",
        "content": "放怀便所便。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7f86d5ad8908506086165977cfab2cf6",
        "title": "句  其四二",
