[
    {
        "uuid": "dc55d9b0ba56b43682c502ee730f3fa8",
        "title": "南昌道中",
        "author": "释晓莹",
        "content": "东游心渺渺，趼足步迟迟。\n马驿黄茅路，人家白竹篱。\n归云横断岭，落日映荒陂。\n何地可容足，长哦招隐诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "713baff6f1c6c3fe39853547e1d4a3fa",
        "title": "句  其一",
        "author": "释晓莹",
        "content": "隐泛一船东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1e629216890866b0510bcb8abf5fc890",
        "title": "句  其二",
        "author": "释晓莹",
        "content": "回看苍木杪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "359e1325ed036fffb8ab22f22c08f4d3",
        "title": "句  其三",
        "author": "释晓莹",
        "content": "蝉噪日将落。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b19d0e14db70162185c7a11d9ffd9c6a",
        "title": "句  其四",
        "author": "释晓莹",
        "content": "不到兹亭久。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ff19413791957b83f43806be653ab1d9",
        "title": "句  其五",
        "author": "释晓莹",
        "content": "晚来供望眼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d320522b1fad7461a55ccf7822bb40e1",
        "title": "句  其六",
        "author": "释晓莹",
        "content": "携朋上故基。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1236205d65ee88cf5527effdd25ad00b",
        "title": "句  其七",
        "author": "释晓莹",
        "content": "隆寒正北风，飘飘任此身。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e16e895c32a4be4e21fc76e1a828fafd",
