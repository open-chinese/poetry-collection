        "collection_info": null
    },
    {
        "uuid": "9a8ca6389d3aeaf8a1dfe4b469a5a48a",
        "title": "句  其四九",
        "author": "释晓莹",
        "content": "颓然只任真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7b628bc449752583ceecfa95397a6e79",
        "title": "句  其五○",
        "author": "释晓莹",
        "content": "二月春方好。",
