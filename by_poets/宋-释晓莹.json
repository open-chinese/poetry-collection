    },
    {
        "uuid": "09d26741d018a9f772af647981a4a5ec",
        "title": "句  其七八",
        "author": "释晓莹",
        "content": "故人携客作幽寻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2066837078153e446eca9d1b3724a45d",
        "title": "句  其七九",
        "author": "释晓莹",
        "content": "薄饭粗缯老此生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f68ad2a3e92f0eed05548d97556c5207",
        "title": "句  其八○",
        "author": "释晓莹",
        "content": "及瓜一笑上浮艎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4e7fbf6edff8d51245ec45fd3208a95e",
        "title": "句  其八一",
        "author": "释晓莹",
        "content": "放怀端与世情疏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d7763097a7a595327563536cbe1bf6c0",
        "title": "句  其八二",
        "author": "释晓莹",
        "content": "如公自是銮坡具。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fb0b53d5ea674ed8253603d1d98189b5",
        "title": "句  其八三",
        "author": "释晓莹",
        "content": "门外黄尘扑面飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b35bdd6ac0364cd0c6fa189b4cba0e8",
        "title": "句  其八四",
        "author": "释晓莹",
        "content": "谁教併向此中见。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c20aab2600e6c40b3ba07a5f2df8f7df",
        "title": "句  其八五",
        "author": "释晓莹",
        "content": "不妨谈笑又诗成。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "93053b98559716e85d2f98bc6303cc15",
        "title": "句  其八六",
        "author": "释晓莹",
        "content": "胸中历历古兼今。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3bb2b7534a5ef37fd03b84a2c54c5b48",
        "title": "句  其八七",
        "author": "释晓莹",
        "content": "偶来清赏曲江头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ac4892f23471219ff73779c5e6e620d0",
        "title": "句  其八八",
        "author": "释晓莹",
        "content": "寄目悠然上杳冥，好是园林争秀发。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "19e40708ac095bcce56959d046fd0f0c",
        "title": "句  其八九",
        "author": "释晓莹",
        "content": "负暄孤坐松根石。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c27999092cc1ded89b4fcc6cad0ee0cc",
        "title": "句  其九○",
        "author": "释晓莹",
        "content": "由来八座尚书贵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bdb6562892d9655c13de71e0151f03cc",
        "title": "句  其九一",
        "author": "释晓莹",
        "content": "地去秉钧余咫尺。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "69e61609d85968bf7758d50f9b3ed495",
        "title": "句  其九二",
        "author": "释晓莹",
        "content": "贪看春山如许青。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b18a63b597deddd4ba6ce5a04639d7ab",
        "title": "句  其九三",
        "author": "释晓莹",
        "content": "春光和暖恰相宜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1523288eda587aa17c1d84d17177308d",
        "title": "句  其九四",
        "author": "释晓莹",
        "content": "石壁巉然不可攀。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71808e2e812620cb538f424a124427fc",
        "title": "句  其九五",
        "author": "释晓莹",
        "content": "闲来湖上立移时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fc240e4698cb9a7fa72a6f1e03c69428",
        "title": "句  其九六",
        "author": "释晓莹",
        "content": "而今总入老夫诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1ef9026249e1ea0aaa691e148448cd0e",
        "title": "句  其九七",
        "author": "释晓莹",
        "content": "北山迤逦自苍然。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6b3fb0b47356882beb84d819008c9b30",
        "title": "句  其九八",
        "author": "释晓莹",
        "content": "南屏寄傲且今年，况復相羊得自便。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b7782d68d84b6126163f876117ca754e",
        "title": "句  其九九",
        "author": "释晓莹",
        "content": "自知潦倒无余事。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ae6afa2dc9965377d910e8f291955f8b",
        "title": "句  其一○○",
        "author": "释晓莹",
        "content": "漏鼓乍听连岸响。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4ac971d4f5dcab6cf8a49a92e7bab4c2",
        "title": "句  其一○一",
        "author": "释晓莹",
        "content": "秋色偏宜晚望中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd47cf110eb97d77175efa285cad1840",
        "title": "句  其一○二",
        "author": "释晓莹",
        "content": "不记登临日几迴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1f6844336ea7b6485b256dbcd0f727e5",
        "title": "句  其一○三",
        "author": "释晓莹",
        "content": "即看青女夜飞霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c71e4eebabb98edff4c8e60fb3894fde",
        "title": "句  其一○四",
        "author": "释晓莹",
        "content": "寒窗安着清相向。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "642db3d7de883e2b5b3505e5975ff092",
        "title": "句  其一○五",
        "author": "释晓莹",
        "content": "倒拄乌藤乘逸兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "179be3de4f5092dceee1649e9cc6da50",
        "title": "句  其一○六",
        "author": "释晓莹",
        "content": "谁知老子懒成癖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "98e8ee370928411c0d39eaf6a0e92d8c",
        "title": "句  其一○七",
        "author": "释晓莹",
        "content": "随宜领览到斜晖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9ec914a9a0db6d83154298b8db1b734d",
        "title": "句  其一○八",
        "author": "释晓莹",
        "content": "天教春信到寒乡。\n波中船舫来还去。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0a8d79fe1de997125afe51e357ce4c59",
        "title": "句  其一○九",
        "author": "释晓莹",
        "content": "几迴相送復相迎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e19b223914093e74afc120eb9e1055e4",
        "title": "句  其一一○",
        "author": "释晓莹",
        "content": "对景由来诗句丽。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e0a9141936ef50af9ebbaabdb6ee474b",
        "title": "句  其一一一",
        "author": "释晓莹",
        "content": "出门纵步细哦诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4de7abe8b5b548b50c76bbdc2a59fbe9",
        "title": "句  其一一二",
        "author": "释晓莹",
        "content": "顾我于兹诗有兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2ca43f1493b7e42045994e4707251618",
        "title": "句  其一一三",
        "author": "释晓莹",
        "content": "江云黯黯作愁阴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2642fe615dc4d1b9ade34622004a6318",
        "title": "句  其一一四",
        "author": "释晓莹",
        "content": "欲罢不能聊一吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eab6f7cef2b70d984c294929ba54147e",
        "title": "句  其一一五",
        "author": "释晓莹",
        "content": "甘棠不翦向来荣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ea53e25068cd6dc2c0ee11a6ff72cd9f",
        "title": "句  其一一六",
        "author": "释晓莹",
        "content": "素王妙道况亲传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a26cc8260cefac6719c6d6b4f7c39c1d",
        "title": "句  其一一七",
        "author": "释晓莹",
        "content": "雅淡天姿固可知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6d0d4f25d3eb343932832a61f575d17a",
        "title": "句  其一一八",
        "author": "释晓莹",
        "content": "江表人材独数公。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3ae3300e3ac942a00cc62f796ac4c0d4",
        "title": "句  其一一九",
        "author": "释晓莹",
        "content": "平章风物诗无敌。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e99539154b02d312d22912c6e5c17c1e",
        "title": "句  其一二○",
        "author": "释晓莹",
        "content": "苍生从此得伸眉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "747aac3a7a146bb7323efdd0d4dc0ae1",
        "title": "句  其一二一",
        "author": "释晓莹",
        "content": "江上闲吟立又行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ab6a52bcc1d8a2a2edc996a9e7ca5ee6",
        "title": "句  其一二二",
        "author": "释晓莹",
        "content": "可堪时序苦侵寻，留滞谁知此日心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c5b657e974f08841f76e6ef139cd5c67",
        "title": "句  其一二三",
        "author": "释晓莹",
        "content": "莫笑吾侬躭此癖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4e2b3b698c7a3fefaf91575837be9aed",
        "title": "句  其一二四",
        "author": "释晓莹",
        "content": "绊身何用尚浮名。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fa841e18ff0c7957f75dacee933be5e1",
        "title": "句  其一二五",
        "author": "释晓莹",
        "content": "可怜赢得鬓成霜。",
        "dynasty": "宋",
        "collection_info": null
    }
]