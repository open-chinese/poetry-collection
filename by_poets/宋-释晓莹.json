        "collection_info": null
    },
    {
        "uuid": "e06844412341ee0bd4f226f78a8f5316",
        "title": "句  其一五",
        "author": "释晓莹",
        "content": "寂寂饶孤兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6a21e8ef4e5dd8f16487d55d54502eff",
        "title": "句  其一六",
        "author": "释晓莹",
        "content": "那敢谓长城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "266e984fb9a0d8021b5c6a873491c6ee",
        "title": "句  其一七",
        "author": "释晓莹",
        "content": "徘徊幽兴熟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "546ea437f0667552d8f0c7c79e90ac04",
