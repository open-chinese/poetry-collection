        "collection_info": null
    },
    {
        "uuid": "a765039c1fb2996d85ed26ffc22b70b5",
        "title": "句  其九",
        "author": "释晓莹",
        "content": "舟横渔唱晚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9d8b329cb5f78e9f8aba9ef05b90a853",
        "title": "句  其一○",
        "author": "释晓莹",
