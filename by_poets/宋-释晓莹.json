[
    {
        "uuid": "e118d5c85b3008ce680ef07c746ca02e",
        "title": "南昌道中",
        "author": "释晓莹",
        "content": "东游心渺渺，趼足步迟迟。\n马驿黄茅路，人家白竹篱。\n归云横断岭，落日映荒陂。\n何地可容足，长哦招隐诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bb5ce77c463cd4fcf2640fa3048aa6b0",
        "title": "句  其一",
        "author": "释晓莹",
        "content": "隐泛一船东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2ef38ddb34b5e77939e4fed5e242c054",
        "title": "句  其二",
        "author": "释晓莹",
        "content": "回看苍木杪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d9d6c8362da8454b4dde1fc382fc1ee0",
        "title": "句  其三",
        "author": "释晓莹",
        "content": "蝉噪日将落。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "75eb00fb517477c220e106956a593cf2",
        "title": "句  其四",
        "author": "释晓莹",
        "content": "不到兹亭久。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11337c2944babce082a43c75c9fba3d9",
        "title": "句  其五",
        "author": "释晓莹",
        "content": "晚来供望眼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cd505a971ddb0f8fda66b2be33598b49",
        "title": "句  其六",
        "author": "释晓莹",
        "content": "携朋上故基。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f03fba204eef6141306fdaa7eaa94e58",
        "title": "句  其七",
        "author": "释晓莹",
        "content": "隆寒正北风，飘飘任此身。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "51082fc7c1fdff1bfc87b7e222a1944c",
        "title": "句  其八",
        "author": "释晓莹",
        "content": "脉脉受风斜，野望迷青草。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "555e3c709201bd2bdd6eaaea0f38a67f",
        "title": "句  其九",
        "author": "释晓莹",
        "content": "舟横渔唱晚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "620c5ded499b35917c60f89366de5be3",
        "title": "句  其一○",
        "author": "释晓莹",
        "content": "落涧水淙淙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "be0b56076700891e03cb491dc0d11759",
        "title": "句  其一一",
        "author": "释晓莹",
        "content": "未厌长为客。",
