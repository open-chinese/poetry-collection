        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e845608a5c1433af2cafc8fef8622db9",
        "title": "句  其二四",
        "author": "释晓莹",
        "content": "悠悠落日悬。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0a5f3b245b02d56de8b46938e833634f",
        "title": "句  其二五",
        "author": "释晓莹",
        "content": "挥犀闻妙论。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a15c31a87519eef3e9361ae0b5f002b3",
        "title": "句  其二六",
        "author": "释晓莹",
        "content": "倚栏哦五字。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0c9d43550a331eaf3e408d3707acfeec",
        "title": "句  其二七",
        "author": "释晓莹",
        "content": "细柳乱摇风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d57691e4bc58868c22c8f98170d99862",
        "title": "句  其二八",
        "author": "释晓莹",
        "content": "久客欣无事。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c63f3627db6fce1ec4478df8bb0bdc79",
        "title": "句  其二九",
        "author": "释晓莹",
        "content": "谁与共平章。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "16de5a9e68a6d0e52c64e114994255d3",
        "title": "句  其三○",
        "author": "释晓莹",
        "content": "泉石惬幽情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aeaa91e84bd7d24ec1f96237f28a38f0",
        "title": "句  其三一",
        "author": "释晓莹",
        "content": "前村鸦噪断。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b23c726720ce9b35529cbeac4205f332",
        "title": "句  其三二",
        "author": "释晓莹",
        "content": "栏干聊小凭，取次缀诗篇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b3f6d057f65ad979c2c04dd945893288",
        "title": "句  其三三",
        "author": "释晓莹",
        "content": "丰城千里外。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9a5c5f0abb317082f045a99f7cf48014",
        "title": "句  其三四",
        "author": "释晓莹",
        "content": "谈笑会当同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8254ab52dd196ee6a993e70fa69b5867",
        "title": "句  其三五",
        "author": "释晓莹",
        "content": "有恨春将晚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "29b97044ac3d4ee3698e326e0d71c9e0",
        "title": "句  其三六",
        "author": "释晓莹",
        "content": "在路日已久。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2e5d7b8a301677164923aa88d4d7f05f",
        "title": "句  其三七",
        "author": "释晓莹",
        "content": "一言知具眼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "292ec6d5d99b83af76e5aa3b7e03a0ba",
        "title": "句  其三八",
