    },
    {
        "title": "句  其一○五",
        "author": "释晓莹",
        "content": "倒拄乌藤乘逸兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一○六",
        "author": "释晓莹",
        "content": "谁知老子懒成癖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一○七",
        "author": "释晓莹",
        "content": "随宜领览到斜晖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一○八",
        "author": "释晓莹",
        "content": "天教春信到寒乡。\n波中船舫来还去。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一○九",
        "author": "释晓莹",
        "content": "几迴相送復相迎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一○",
        "author": "释晓莹",
        "content": "对景由来诗句丽。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一一",
        "author": "释晓莹",
        "content": "出门纵步细哦诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一二",
        "author": "释晓莹",
        "content": "顾我于兹诗有兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一三",
        "author": "释晓莹",
        "content": "江云黯黯作愁阴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一四",
        "author": "释晓莹",
        "content": "欲罢不能聊一吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一五",
        "author": "释晓莹",
        "content": "甘棠不翦向来荣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一六",
        "author": "释晓莹",
        "content": "素王妙道况亲传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一七",
        "author": "释晓莹",
        "content": "雅淡天姿固可知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一八",
        "author": "释晓莹",
        "content": "江表人材独数公。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一九",
        "author": "释晓莹",
        "content": "平章风物诗无敌。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二○",
        "author": "释晓莹",
        "content": "苍生从此得伸眉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二一",
        "author": "释晓莹",
        "content": "江上闲吟立又行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二二",
        "author": "释晓莹",
        "content": "可堪时序苦侵寻，留滞谁知此日心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二三",
        "author": "释晓莹",
        "content": "莫笑吾侬躭此癖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二四",
        "author": "释晓莹",
        "content": "绊身何用尚浮名。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二五",
        "author": "释晓莹",
        "content": "可怜赢得鬓成霜。",
        "dynasty": "宋",
        "collection_info": null
    }
