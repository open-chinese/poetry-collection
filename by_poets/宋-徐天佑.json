[
    {
        "uuid": "d9c8a47f0deebcb7c0cb66cda17b2716",
        "title": "洛思山",
        "author": "徐天佑",
        "content": "归去蹉跎岁月深，羁愁无示故乡心。\n人生毕竟俱怀土，庄舄当时自越吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c279f7d987fbc942a36b580b4f4438a9",
        "title": "许询园",
        "author": "徐天佑",
        "content": "高栖不受鹤书招，北干家园久寂寥。\n明月空怀人姓许，故山犹自岫名萧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c935fa7e4e97390bb52e35d3ee5cb133",
        "title": "箪醪河",
        "author": "徐天佑",
        "content": "往事悠悠逝水知，临流尚想报吴时。\n一壶能遣三军醉，不比商家酒作池。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6dca61d7f6c474ac6732da74995af7f5",
        "title": "钱王祠",
        "author": "徐天佑",
        "content": "石槛山前结驷游，故乡霞锦遍林丘。\n祇今东府空遗庙，露立唐碑老树秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "816ccde179d878c7ed89b124bc6352d1",
        "title": "方干岛",
        "author": "徐天佑",
        "content": "平生心事白欧知，一卷云庵处士诗。\n占得镜中奇绝处，祇缘身值广明时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "989280134fe6cdbbe651e0295ae57003",
        "title": "白楼亭",
        "author": "徐天佑",
        "content": "江左名流共往还，白楼何许只青山。\n不须商略闲今古，物换星移俯仰间。",
        "dynasty": "宋",
        "collection_info": null
    }
]