[
    {
        "uuid": "f1c0e3cdce15e45bf2c86c737e11a922",
        "title": "洛思山",
        "author": "徐天佑",
        "content": "归去蹉跎岁月深，羁愁无示故乡心。\n人生毕竟俱怀土，庄舄当时自越吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "898e7c5a80d27a18ebbc5e8128c0d724",
        "title": "许询园",
        "author": "徐天佑",
        "content": "高栖不受鹤书招，北干家园久寂寥。\n明月空怀人姓许，故山犹自岫名萧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "85a5ad066a2f7c2f8278d0db5045a10e",
        "title": "箪醪河",
        "author": "徐天佑",
        "content": "往事悠悠逝水知，临流尚想报吴时。\n一壶能遣三军醉，不比商家酒作池。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "590546482f66ca045f24bc99b3f4b6da",
        "title": "钱王祠",
        "author": "徐天佑",
        "content": "石槛山前结驷游，故乡霞锦遍林丘。\n祇今东府空遗庙，露立唐碑老树秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d6cca6dc6efea4843ac353ae15465d49",
        "title": "方干岛",
        "author": "徐天佑",
        "content": "平生心事白欧知，一卷云庵处士诗。\n占得镜中奇绝处，祇缘身值广明时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "699905ac4fecb7beaa4090e2cbd5b99f",
        "title": "白楼亭",
        "author": "徐天佑",
        "content": "江左名流共往还，白楼何许只青山。\n不须商略闲今古，物换星移俯仰间。",
        "dynasty": "宋",
        "collection_info": null
    }
]