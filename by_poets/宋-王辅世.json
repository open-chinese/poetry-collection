[
    {
        "uuid": "8faaca2bc6e23d3097f0f489f280d379",
        "title": "洪岩山六题 石门",
        "author": "王辅世",
        "content": "岩穴神仙宅，山门向岭开。\n时横云作钥，怕有俗人来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "101293b123b875d3a9f41696f24b0b08",
        "title": "洪岩山六题 石金刚",
        "author": "王辅世",
        "content": "天地应怜我，遗留鉅力神。\n定须凭国手，神妙与传真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "356423281fd024a7bdc44f50a4f4cb72",
        "title": "洪岩山六题 仙人台",
        "author": "王辅世",
        "content": "任峻我须上，更高人亦行。\n神仙虽已往，咫尺是蓬瀛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a2b38f67b0de328ba7c478ea9094d641",
        "title": "洪岩山六题 石猴",
        "author": "王辅世",
        "content": "猿猴尝学禅，立德固坚久。\n能乘狮子游，不为狮子后。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "de8db36159284005e42bd1a7374199cf",
        "title": "洪岩山六题 山风",
        "author": "王辅世",
        "content": "山露湿吾履，山风吹我衣。\n白云褰举处，依约认崖扉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5284ff860962dae7baac80690193204f",
        "title": "洪岩山六题 石山鸡",
        "author": "王辅世",
        "content": "山鸡化为石，锦翼已成藏。\n俗类休争食，吾心不为粱。",
        "dynasty": "宋",
        "collection_info": null
    }
]