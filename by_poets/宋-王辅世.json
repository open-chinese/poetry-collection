[
    {
        "uuid": "a4c3a1af2a3ce040172f7dc9e65464de",
        "title": "洪岩山六题 石门",
        "author": "王辅世",
        "content": "岩穴神仙宅，山门向岭开。\n时横云作钥，怕有俗人来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f1fa51b73704ac13f4484304aec93c17",
        "title": "洪岩山六题 石金刚",
        "author": "王辅世",
        "content": "天地应怜我，遗留鉅力神。\n定须凭国手，神妙与传真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "98af5f7d74fb12da9172c96e392fc40f",
        "title": "洪岩山六题 仙人台",
        "author": "王辅世",
        "content": "任峻我须上，更高人亦行。\n神仙虽已往，咫尺是蓬瀛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "89ae5ced47d271bda5ef690a6c98bd83",
        "title": "洪岩山六题 石猴",
        "author": "王辅世",
        "content": "猿猴尝学禅，立德固坚久。\n能乘狮子游，不为狮子后。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "96caf063335902f683e9a5ae2516d45f",
        "title": "洪岩山六题 山风",
        "author": "王辅世",
        "content": "山露湿吾履，山风吹我衣。\n白云褰举处，依约认崖扉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f143c538dd445f06b8da44420f169bbb",
        "title": "洪岩山六题 石山鸡",
        "author": "王辅世",
        "content": "山鸡化为石，锦翼已成藏。\n俗类休争食，吾心不为粱。",
        "dynasty": "宋",
        "collection_info": null
    }
]