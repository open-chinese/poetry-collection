[
    {
        "uuid": "5202c9d483a064580514d874008b3e04",
        "title": "送郑待制闳中归闽",
        "author": "孙升",
        "content": "清晓都门祖帐开，路人相与叹贤哉。\n流尘几翳看山眼，落日休停别酒杯。\n何待诸生留北阙，自存遗直在东台。\n连江四老嗟谁在，白首今朝只独来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5a83cfc24143edbd3c5617265db576b2",
        "title": "索刘述之新酿",
        "author": "孙升",
        "content": "稍觉香熏鼻，还思酒入唇。\n盈缸止三斗，可拨瓮头春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "169d79e0d8deed77a105475c583fe901",
        "title": "句  其一",
        "author": "孙升",
        "content": "漏残余一榻，曾不为黄金。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18b66f9f8cbab0b45194bb19a69b4b9f",
        "title": "句  其二",
        "author": "孙升",
        "content": "北扉西掖青云士，千载飘零只两人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b76bc90f9b8a374c95efa8227d885e88",
        "title": "句  其三",
        "author": "孙升",
        "content": "玉笋千官散，珠帘一夜空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1ad338c2a54db2d02ffc86ac474f3b63",
        "title": "句  其四",
        "author": "孙升",
        "content": "来书占喜鹊，落日听鸣蛩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b006af670f5cb4dcc1ec941af4061b9",
        "title": "句  其五",
        "author": "孙升",
        "content": "若与蛟龙争角黍，应同渔父啜糟醨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2bc49f5335b54ec8fea4e21d94507a1a",
        "title": "句  其六",
        "author": "孙升",
        "content": "睡须山鸟唤，酒听竹枝斟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6ec8a4b36ca8573bfe2998bf1f3488d1",
        "title": "句  其七",
        "author": "孙升",
        "content": "穷搜诗句熟，老练世情通。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "63a3e0ebbfaa186d60eb5d6ad1b97aeb",
        "title": "句  其八",
        "author": "孙升",
        "content": "白云每逐晨光出，红鹤长随暮霭还。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3ab42853c520785f4851d8f9c1f67c73",
        "title": "句  其九",
        "author": "孙升",
        "content": "梁台歌吹余衰草，湓浦琵琶怅晚风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ca26b6879179c8d0a7e416e7ab7fd5d6",
        "title": "句  其一○",
        "author": "孙升",
        "content": "慈竹笋抽疑夏箨，木犀花发认春香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a8c8b2871ab0e6d504d3bc2cfd9dbff",
        "title": "句  其一一",
        "author": "孙升",
        "content": "更约与君三十载，北陵原上望残霞。",
        "dynasty": "宋",
        "collection_info": null
    }
]