[
    {
        "uuid": "67e8c9c0035ca3a93b3e21051de56114",
        "title": "送郑待制闳中归闽",
        "author": "孙升",
        "content": "清晓都门祖帐开，路人相与叹贤哉。\n流尘几翳看山眼，落日休停别酒杯。\n何待诸生留北阙，自存遗直在东台。\n连江四老嗟谁在，白首今朝只独来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6833d3a42fb0f4aab4e59ef01e652bac",
        "title": "索刘述之新酿",
        "author": "孙升",
        "content": "稍觉香熏鼻，还思酒入唇。\n盈缸止三斗，可拨瓮头春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13f8a2e0e92b32a7e0339250959fda4a",
        "title": "句  其一",
        "author": "孙升",
        "content": "漏残余一榻，曾不为黄金。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aa9dd73de4380ece3770a24997acf567",
        "title": "句  其二",
        "author": "孙升",
        "content": "北扉西掖青云士，千载飘零只两人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dc71c4e2129d8785e66c04da978f238c",
        "title": "句  其三",
        "author": "孙升",
        "content": "玉笋千官散，珠帘一夜空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a7fd98bedd4ab71b97058283ce32f6b",
        "title": "句  其四",
        "author": "孙升",
        "content": "来书占喜鹊，落日听鸣蛩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c42cdbe84f9686b91aaf6f7fd70d2fa8",
        "title": "句  其五",
        "author": "孙升",
        "content": "若与蛟龙争角黍，应同渔父啜糟醨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "156b948b40de1e7e4d06e0326cbe9200",
        "title": "句  其六",
        "author": "孙升",
        "content": "睡须山鸟唤，酒听竹枝斟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ac03a0d509fef928847e1b4f5bf56ad6",
        "title": "句  其七",
        "author": "孙升",
        "content": "穷搜诗句熟，老练世情通。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6ecd70fd977df1b9570012416b89452f",
        "title": "句  其八",
        "author": "孙升",
        "content": "白云每逐晨光出，红鹤长随暮霭还。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c6b19a76ce37dc8415764923601c570",
        "title": "句  其九",
        "author": "孙升",
        "content": "梁台歌吹余衰草，湓浦琵琶怅晚风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cbf452e1063c0e9984431a5bbcecf4bf",
        "title": "句  其一○",
        "author": "孙升",
        "content": "慈竹笋抽疑夏箨，木犀花发认春香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "50d4c0998a61ec80efa8e07d59cda8cd",
        "title": "句  其一一",
        "author": "孙升",
        "content": "更约与君三十载，北陵原上望残霞。",
        "dynasty": "宋",
        "collection_info": null
    }
]