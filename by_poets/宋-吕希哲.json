[
    {
        "uuid": "b94fb9ea8356b5eebe7f0d6817e10e84",
        "title": "绝句",
        "author": "吕希哲",
        "content": "礼仪三百復三千，酬酢天机理必然。\n寒即加衣饥即食，孰为末节孰为先。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "259057c1a119f498353164a071517149",
        "title": "绝句二首  其一",
        "author": "吕希哲",
        "content": "水晶宫殿玉花零，点缀空槐卧素屏。\n特敕下帘延墨客，不因风雪废谈经。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "60dc3d0f03ed43a5a9984426d4d7b355",
        "title": "绝句二首  其二",
        "author": "吕希哲",
        "content": "强记师承道古先，无穷新意出陈编。\n一言有补天颜动，全胜三军贺凯还。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d04a62ab6171176372f81fb044847781",
        "title": "和尧夫打乖吟",
        "author": "吕希哲",
        "content": "先生不是闭关人，高趣逍遥混世尘。\n得志须为天下雨，放怀聊占洛阳春。\n家无甔石宾常满，论极锱铢意始新。\n任使终身卧安乐，一毫何费养天真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c0f65b9c841ca57707f7bfec7b38853",
        "title": "绝句",
        "author": "吕希哲",
        "content": "老读文书兴易阑，须知养病不如闲。\n行林瓦枕虚堂上，卧看江南雨后山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b9972430259f49615219c78abdcf2f80",
        "title": "登单州城楼",
        "author": "吕希哲",
        "content": "断霞孤鹜欲寒天，无復青山碍目前。\n世路崎岖饱经历，始知平地是神仙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c87168e07f05ad1b50c69630759ba7f0",
        "title": "句  其一",
        "author": "吕希哲",
        "content": "将谓清风全扫地，世间今復有卢鸿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "84399484aa8b3172302a8d355de05c9d",
        "title": "句  其二",
        "author": "吕希哲",
        "content": "他日稍成毛义志，再求师友究渊源。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3d9da5e1c6c2637ed6f8d1521f6ac802",
        "title": "句  其三",
        "author": "吕希哲",
        "content": "独抱遗经唐处士，差强人意汉将军。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "32daa9065897af456ad0071b1e110c9b",
        "title": "句  其四",
        "author": "吕希哲",
        "content": "除却借书沽酒外，更无一事扰公私。",
        "dynasty": "宋",
        "collection_info": null
    }
]