[
    {
        "uuid": "a29b9dc0a4e4d8baf8459149e1c8c68f",
        "title": "南楼",
        "author": "廖德明",
        "content": "三里之城渺大荒，两江横截壮金汤。\n唐家设险基犹在，圣代增陴虑更长。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4158d5a342634aa22efd28f2eb9590a3",
        "title": "鬰林郡城",
        "author": "廖德明",
        "content": "荒烟漠漠双江上，往事悠悠古戍孤。\n春到偏临青草渡，梦中犹记白鸥湖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "761eedc98ebf942bca46e5f39dc18ebb",
        "title": "四穿楼  其一",
        "author": "廖德明",
        "content": "四壁无遮碍，凭阑豁远胸。\n何须高百尺，亦可卧元龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "715105af86a85352b6a3a552695a0f69",
        "title": "四穿楼  其二",
        "author": "廖德明",
        "content": "南北东西四望开，熙熙人似上春台。\n时平不用筹边策，閒看双江抱郭来。",
        "dynasty": "宋",
        "collection_info": null
    }
]