[
    {
        "uuid": "d87466418e5457c8c7a4c5b0ae0c8f1d",
        "title": "临终偈",
        "author": "朱真静",
        "content": "去来如一，真性湛然。\n风收云散，月在青天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e50c13a2f05d0191cea78079f2d24ef",
        "title": "石室庵夜坐",
        "author": "朱真静",
        "content": "青灯寒焰弄微明，灯与幽人一样清。\n案上黄庭浑懒看，时闻败叶打窗声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "af73c91c5177ada41d0abd9b44264a64",
        "title": "入山采药遇异人",
        "author": "朱真静",
        "content": "紧着芒鞋高繫裙，野猿相狎许同羣。\n绿萝林裏采茶去，踏碎青山一片云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f9c1645553a58621c5f6b1eb90ebcd62",
        "title": "又题座右窗间",
        "author": "朱真静",
        "content": "经书不是语虚无，荆棘丛中有坦涂。\n不遇至人真口诀，误人错下死工夫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "00ca4eae9b5b7687f251b7d2a3b18b1a",
        "title": "题石室庵前竹上",
        "author": "朱真静",
        "content": "十年身与鬼为邻，曾见岩花几度春。\n莫怪柴门终日闭，怕将尘事恼閒人。",
        "dynasty": "宋",
        "collection_info": null
    }
]