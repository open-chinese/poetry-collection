[
    {
        "uuid": "913b3af6fbaed976abc3cdd393a2dee9",
        "title": "子猷访戴",
        "author": "来梓",
        "content": "四山摇玉夜光浮，一舸玻璃凝不流。\n若使过门相见了，千年风致一时休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e33d0a986cdac10ffc9d544b1df72a85",
        "title": "杜甫游春",
        "author": "来梓",
        "content": "典尽春衣不肯归，熊儿扶过瀼溪西。\n伤时怀抱深于海，掠眼风光醉似泥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "698680a67b3080e59a501750320d8569",
        "title": "六言二首  其一",
        "author": "来梓",
        "content": "叶淅历而争堕，菊徘徊而尚开。\n流年逼人太甚，佳客折简不来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1c3d96c5869c81f84e4e1d2b3a5f8781",
        "title": "六言二首  其二",
        "author": "来梓",
        "content": "行空月不着相，出岫云无恋心。\n胸中一丘一壑，不矶城市山林。",
        "dynasty": "宋",
        "collection_info": null
    }
]