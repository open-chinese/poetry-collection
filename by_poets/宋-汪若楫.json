[
    {
        "uuid": "645b12b8d5b0ff0f5612371c2ffed9eb",
        "title": "白际岭下即目",
        "author": "汪若楫",
        "content": "白际摩天秀，秋光满蓼汀。\n山呈金字面，田画井文形。\n夜出萤相照，更阑鷄可听。\n杉松柯不改，点染四时青。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "560cbb935f1bace5eaefb7bbea739cbc",
        "title": "开窗",
        "author": "汪若楫",
        "content": "草色侵书绿，依山多得阴。\n月高猿挂树，惊起夜栖禽。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7d6147ed89bba2634604461ef0fb75ca",
        "title": "夜坐",
        "author": "汪若楫",
        "content": "鑪煨榾柮烟容冷，酒泻茅柴春色轻。\n夜坐不眠人悄悄，梅梢随月绕窗行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0e1ba2b69bdffe22c81d2ba77f70f4ad",
        "title": "久雨",
        "author": "汪若楫",
        "content": "连连春雨几黄昏，溪岸人家水近门。\n借问人家水曾落，竹林枝上看风痕。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71f5e9f20e9d37a21027dd8a5870d59e",
        "title": "门前松棚",
        "author": "汪若楫",
        "content": "突兀山边屋数间，每嫌斜照上阑干。\n当门只得添松盖，未到秋来夜已寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "55a88df9e0ddc7a61d6c65d24c15ec2b",
        "title": "青山",
        "author": "汪若楫",
        "content": "万木惊秋各自残，蛩声扶砌诉新寒。\n西风不是吹黄落，要放青山与客看。",
        "dynasty": "宋",
        "collection_info": null
    }
]