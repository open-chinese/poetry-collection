[
    {
        "uuid": "79122cc080447f8f37d709ce518bd7d1",
        "title": "句  其一",
        "author": "谌祜",
        "content": "两表蜀天开日月，三军汉地出旌旗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "166f838f120d3d90b6c671e2675d64fd",
        "title": "句  其二",
        "author": "谌祜",
        "content": "王侯螘穴青青草，鼓吹蛙庐焱焱镫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a26f0ce69c748fcd6120237ace0d250",
        "title": "句  其三",
        "author": "谌祜",
        "content": "动地百年无桀跖，后天一寿有颜曾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cee0fabe635f692109d7deca44949a77",
        "title": "句  其四",
        "author": "谌祜",
        "content": "百年窗下棋千着，万事灯前酒一斟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c8a6b0328043482fed6ae6f33e8832e9",
        "title": "句  其五",
        "author": "谌祜",
        "content": "白髮古今天北极，黄华宇宙客西风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "82bcd8561f7cec3dbbfd663db9c00ec8",
        "title": "句  其六",
        "author": "谌祜",
        "content": "封浚九州尧舜禹，典刑万世契夔龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0014fd15eb64a7ad4ca94949048b8363",
        "title": "句  其七",
        "author": "谌祜",
        "content": "陶写有时欣律熟，去来无着证鑪芬。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4a62997451eb4073183914ca570d6143",
        "title": "句  其八",
        "author": "谌祜",
        "content": "西风木叶吹秦晋，春雨桃华送古今。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aaf4f3e654c3cc9191ccbfaa2629ff24",
        "title": "句  其九",
        "author": "谌祜",
        "content": "两水夹城飘近远，万山拥翠绿参差。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a3f3a7e59fd1492e25317833f02a956a",
        "title": "句  其一○",
        "author": "谌祜",
        "content": "不知元佑今何日，慨想儋翁过此时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c5183492c3fc01acf175404b3e3e132d",
        "title": "句  其一一",
        "author": "谌祜",
        "content": "为惜旧时慈母线，宁甘今夜隔帘霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "26bb4cd9acb0e03c1c666ecf633a8b5b",
        "title": "句  其一二",
        "author": "谌祜",
        "content": "梦犹平昔醒来别，忆到儿童事转长。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "85ccdb9fd261d894f2cb0b43d7b85993",
        "title": "句  其一三",
        "author": "谌祜",
        "content": "江山落日前朝梦，风雨黄华太古心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "318e1856e3da6b82b7f7196c087f18ce",
        "title": "句  其一四",
        "author": "谌祜",
        "content": "观空观化此凭栏，会合人间景一团。\n棋罢客随流水散，酒阑花带夕阳看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bdd73ff6698dd6b73b04da32bdbd590d",
        "title": "句  其一五",
        "author": "谌祜",
        "content": "飞雨隔愁横笛后，残镫照梦落花前。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "406fcba230e9ff91258f8c25ea21a6f6",
        "title": "句  其一六",
        "author": "谌祜",
        "content": "诗到二南风化在，句流三谢语言工。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b21e3b3dc89d2ad966a21ef60b631af7",
        "title": "句  其一七",
        "author": "谌祜",
        "content": "中原一髮天涯梦，元佑孤臣海外归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "357ddda2c36c9c8e98d4b702ad9ab32b",
        "title": "句  其一八",
        "author": "谌祜",
        "content": "造物未尝私化外，英雄何事老隆中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "16a6a4dc7b68cb91f354b2bb833252ad",
        "title": "句  其一九",
        "author": "谌祜",
        "content": "看翻后夜月回照，梦忆前生地不同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "66ed9bf50e30391b716bc7311b6ae4a8",
        "title": "句  其二○",
        "author": "谌祜",
        "content": "天地浮萍云聚散，江山罨画月西东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c453a12e21c859a7b64412405acb4576",
        "title": "句  其二一",
        "author": "谌祜",
        "content": "地上断无秦世草，山中犹读汉时书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "38b3eeb78c8dd021d753d64ee5659776",
        "title": "句  其二二",
        "author": "谌祜",
        "content": "梦临水月心先觉，看到霜空幻亦稀。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4fa7e1add6f21727e25539a551a29e98",
        "title": "句  其二三",
        "author": "谌祜",
        "content": "天开暝色云千里，雨洗秋空月十分。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5531a5550159c5d8f6f931e1a219bafa",
        "title": "句  其二四",
        "author": "谌祜",
        "content": "止水不留前世景，夕阳空照古人心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "37191bc3021a9ab9bafa6d2ee5e110a9",
        "title": "句  其二五",
        "author": "谌祜",
        "content": "花閒笑语从云散，天下英雄岂陆沈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7d5594eb14ac4f6fbaec511ae9b97738",
        "title": "句  其二六",
        "author": "谌祜",
        "content": "隔云烽火山河旧，列土旌旗雨露深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ffa3d4e220a1adf8b8a82e946f1f7a22",
        "title": "句  其二七",
        "author": "谌祜",
        "content": "层城楼橹看如画，三镇军民梦解围。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ca6ddaaa4a498a440a6441f918d6565d",
        "title": "句  其二八",
        "author": "谌祜",
        "content": "树带夕阳雅半集，藋寒秋浦雁初来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ab3e5e27ce6b8a5da8ccb138384aa331",
        "title": "句  其二九",
        "author": "谌祜",
        "content": "交游雨过三生梦，今古风前酒一杯。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "33bcfee04166ecebb35f924b315974c1",
        "title": "句  其三○",
        "author": "谌祜",
        "content": "千帆过眼人何在，一笛穿云水自横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "30789a74ef167a0dd168da7f7a069b4c",
        "title": "句  其三一",
        "author": "谌祜",
        "content": "宫烛骊山酒未醒，妖星䁭䁭弄青荧。\n三朝伐叛知谋略，万国归心服典刑。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d92e175b4f566f223ac186a31fe9696e",
        "title": "句  其三二",
        "author": "谌祜",
        "content": "梦游碧落风偏急，望断金精月正中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "30080b580859cc96e235fc277257fef8",
        "title": "句  其三三",
        "author": "谌祜",
        "content": "万壑松风天鼓吹，四围花月锦帘栊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0881777fe065baec2fc9c95e83e4aecf",
        "title": "句  其三四",
        "author": "谌祜",
        "content": "风雨昼酣秦地阔，关河春梦楚云低。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "096fe2c7397218aa692138077106910d",
        "title": "句  其三五",
        "author": "谌祜",
        "content": "蕃汉盛衰霜叶梦，陈隋兴废露华秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a45b6c8482ecb5224f9cecd6315c0ff4",
        "title": "句  其三六",
        "author": "谌祜",
        "content": "聚火有莲知佛性，衡门无雁见交情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f4fd0a3ce109accfae0c1ae9258ce3f1",
        "title": "句  其三七",
        "author": "谌祜",
        "content": "一帆秋水渔歌远，半塔斜阳雁影高。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71f5e86393214d0c29348c2987cad233",
        "title": "句  其三八",
        "author": "谌祜",
        "content": "邑自乱来新鼓角，屋从兵后尚蓬蒿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3ae7f7607eb5aa61fb750cdd6293c46e",
        "title": "句  其三九",
        "author": "谌祜",
        "content": "落雁景翻秋宇迥，乱雅啼带夕阳飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "48f99fe09b2553cdd1d7f0bc0aec9ab9",
        "title": "句  其四○",
        "author": "谌祜",
        "content": "蜀天过雨浮岚翠，汉地连云小麦黄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c5a0f6fc6260f6260851c8402608b8ac",
        "title": "句  其四一",
        "author": "谌祜",
        "content": "城上鸟乌知息战，陈前笳鼓缓归装。\n将军斗大黄金印，犹待先登缚鬼章。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8f145c52a9b38eae15abfb38f693d5a6",
        "title": "句  其四二",
        "author": "谌祜",
        "content": "凤城色映红云见，龙衮香飘紫气浮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9061608e9627b80c5e6c282608850184",
        "title": "句  其四三",
        "author": "谌祜",
        "content": "地势尊严今上国，天王明圣此中兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6f78428b6d72283a61fbe74b261b9514",
        "title": "句  其四四",
        "author": "谌祜",
        "content": "弦管入春波面醉，云天开镜画中行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1c907077837aca19243c59d64a7f1ee5",
        "title": "句  其四五",
        "author": "谌祜",
        "content": "潮回海北风偏壮，秋满江南客未归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c883c4307e700ab757d692a8987eb9ed",
        "title": "句  其四六",
        "author": "谌祜",
        "content": "宫大阿房行日月，关通函谷混车书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a33515b3ade8ec8cb5614c991748bbe",
        "title": "句  其四七",
        "author": "谌祜",
        "content": "雁寒景落三山月，骑俊神通万里城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f82a877a4a33c6d520780ed972973715",
        "title": "句  其四八",
        "author": "谌祜",
        "content": "帝城烟冷江沱在，郿坞金多社稷危。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "84d44d90e4a1de53974951c0b8f43071",
        "title": "句  其四九",
        "author": "谌祜",
        "content": "吴天北斗行宫在，汉节西风閒道归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "772be4e0d825bf3d46008eabfbd2bd26",
        "title": "句  其五○",
        "author": "谌祜",
        "content": "雅翻汉月催黄髮，雁断胡天想翠华。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "37e1e2c0aeac2fb903a21248102d27d2",
        "title": "句  其五一",
        "author": "谌祜",
        "content": "燕云海阔归鸿杳，蜀汉城荒戍马閒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bb68fe879c96627d5488b7c7857ad1b6",
        "title": "句  其五二",
        "author": "谌祜",
        "content": "汉殿落花龙衮弃，周原荒棘虎营新。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "797921e1894de4d661c55a1fb3ed14be",
        "title": "句  其五三",
        "author": "谌祜",
        "content": "短褐尚时吟杜曲，采芝何处问秦峯。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8df2920febd6a0d051a2998bc1b95404",
        "title": "句  其五四",
        "author": "谌祜",
        "content": "晋墅风流棋局散，江东柱石酒壶倾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "19d07782d626bdd1eb7562caa306db09",
        "title": "句  其五五",
        "author": "谌祜",
        "content": "雁过大江风帖帖，雅啼荒树日悠悠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e0a8d20dfc3efde77558d687162e501a",
        "title": "句  其五六",
        "author": "谌祜",
        "content": "赤帜露光王汉地，锦帆波荡帝隋舟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "144bba83eb6423dcde729b28e7502d18",
        "title": "句  其五七",
        "author": "谌祜",
        "content": "楚草泽深陈胜在，秦瓜地旷邵平閒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "832ff7dc273f7d00bd4f2d5fcfa2b207",
        "title": "句  其五八",
        "author": "谌祜",
        "content": "天开汉统王巴蜀，云从秦繇起沛丰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4daf5b6e05cbb196fcc25985f820d9fe",
        "title": "句  其五九",
        "author": "谌祜",
        "content": "向日不知今解老，他时应忆此何年。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b7470cc7596d45d6afba3c509647b0c1",
        "title": "句  其六○",
        "author": "谌祜",
        "content": "万国梦回秦月在，千年事过蜀云飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ebebeb6e0db75131762f97d3df6c37f5",
        "title": "句  其六一",
        "author": "谌祜",
        "content": "六鳌倾侧动干坤，国母巡荒护帝孙。\n北极星辰临草地，西风观阙卧花门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "23d3591c92cc8a405ad34ee24da8c95b",
        "title": "句  其六二",
        "author": "谌祜",
        "content": "南极大明秋气迥，西风吹梦泰阶平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2c3936cc170100256a248fd3b464ec91",
        "title": "句  其六三",
        "author": "谌祜",
        "content": "西风客老雄心在，泰古人稀侠骨香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f0921ef73ebcdc4989eb045f1b741c02",
        "title": "句  其六四",
        "author": "谌祜",
        "content": "梁有字题何岁月，藏无经在亦香镫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "972b374c3699cd19b098c6c309a179e5",
        "title": "句  其六五",
        "author": "谌祜",
        "content": "千山落木秋来客，两袖西风旦过僧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "55c12498e8babc473ca277625ddf4efc",
        "title": "句  其六六",
        "author": "谌祜",
        "content": "昨日共吟今是梦，少时轻别老翻愁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0163c3f88e0000ddd43d7bef74d17c25",
        "title": "句  其六七",
        "author": "谌祜",
        "content": "天教向老逢春在，诗且忘忧此命通。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "47e8d9f0ee5a26286a8f66530a87e691",
        "title": "句  其六八",
        "author": "谌祜",
        "content": "何事重来看明月，不堪垂老向西风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7d1d5f7cfc7a033f2ec3df4d32ee480e",
        "title": "句  其六九",
        "author": "谌祜",
        "content": "风雨黄花秋兴熟，江湖归路客愁轻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "658a25626b3d67916d7a43ddb2eb76a1",
        "title": "句  其七○",
        "author": "谌祜",
        "content": "昨朝骨肉关情大，今日平安过望多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9eb2992ef88fcace8e4272bd66593ea6",
        "title": "句  其七一",
        "author": "谌祜",
        "content": "归来今夕岁云徂，且共平安酒一壶。\n浩劫定中逃得过，旧愁空外看来无。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5e594d05cfc853ef4fc6c65972c397b",
        "title": "句  其七二",
        "author": "谌祜",
        "content": "五更历转花红在，万里晴光草绿敷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b4f51a309ff95566399d51ee72e6d450",
        "title": "句  其七三",
        "author": "谌祜",
        "content": "一日又从头上过，百年多在梦中行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6e6c3ea5a378f8c695dc5b11dd07c09d",
        "title": "句  其七四",
        "author": "谌祜",
        "content": "月明老树吟风鹤，露湿荒郊徧士枭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2933cd0dce0749ecc4bc8c91bc06d290",
        "title": "句  其七五",
        "author": "谌祜",
        "content": "事去尚时寻败局，老来犹日读何书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "674b5bcae816aeae9174a637278e25cd",
        "title": "句  其七六",
        "author": "谌祜",
        "content": "术岂有灵能起死，梦从何处忍忘归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ffc0bb01bbdbe9420b0c8db24438064d",
        "title": "句  其七七",
        "author": "谌祜",
        "content": "燕尚有情迎社到，蜨空无梦过时休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "726bc94548b30a5bd636fb7e7a919d5a",
        "title": "句  其七八",
        "author": "谌祜",
        "content": "壮日尚曾云出岫，暮年偏怪海扬尘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "faf7b2cc632987f87804872c47a3f22e",
        "title": "句  其七九",
        "author": "谌祜",
        "content": "紫微星奠三阶正，黄道天开万象新。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6bf7ff2b09d62592e5a35474e0ffc2a3",
        "title": "句  其八○",
        "author": "谌祜",
        "content": "梁燕未归花尚在，杜鹃有恨月常孤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cd36d2351e8c3437c13e17eab22f30fe",
        "title": "句  其八一",
        "author": "谌祜",
        "content": "此情消遣终难去，无路追寻岂是愚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b470b0451ca8122ae5f2299bb6e534cc",
        "title": "句  其八二",
        "author": "谌祜",
        "content": "天空日照阴霾伏，八表天开泰象同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9933f6a08725f787240dfdc71f16c0a9",
        "title": "句  其八三",
        "author": "谌祜",
        "content": "禹甸草荒昏雾徧，汉宫花梦昼云酣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d5b1d26be1cf740c401dfc1cddf17975",
        "title": "句  其八四",
        "author": "谌祜",
        "content": "花柳晴光新态度，江湖春梦老英雄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "54302c7613632cfda688a9fbe035074f",
        "title": "句  其八五",
        "author": "谌祜",
        "content": "日月任从愁裏过，客尘常向静中参。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3237b86b2a8b274e4d2c33a4b80a31f1",
        "title": "句  其八六",
        "author": "谌祜",
        "content": "归棹舞风鸥不下，愁笳吹月雁斜行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d1575de9abf42577005cc737969fa746",
        "title": "句  其八七",
        "author": "谌祜",
        "content": "风雨低黄鹄，江湖老白鸥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8132a83e2fc4774810959a3232aca8ec",
        "title": "句  其八八",
        "author": "谌祜",
        "content": "歌舞从儿女，艰难忆父兄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b28f644011cd2e73cd29b7f05aa60010",
        "title": "句  其八九",
        "author": "谌祜",
        "content": "塞云横蜀栈，胡马出巴陵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3e822a249e51d1c09c82cea1edc50d4d",
        "title": "句  其九○",
        "author": "谌祜",
        "content": "白髮风前酒，黄花雨后镫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9abed75f59981401a9408ed90d922643",
        "title": "句  其九一",
        "author": "谌祜",
        "content": "月满商山夜，风高汉殿秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7a54011c1101095b7f72e1a092dcb066",
        "title": "句  其九二",
        "author": "谌祜",
        "content": "世情江水淡，心事海鸥知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3cd97bff83707d0028dff7b978cc4700",
        "title": "句  其九三",
        "author": "谌祜",
        "content": "巴云秋景薄，汉月夜光寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e7a957ba8b6e6754837a712adce9e317",
        "title": "句  其九四",
        "author": "谌祜",
        "content": "克復归神主，多才自不难。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d5b05f2fed3588d095eccb77510a5823",
        "title": "句  其九五",
        "author": "谌祜",
        "content": "玉花嘶徼外，翠辇过云中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2f640f6e830892bbfe26d84561523c20",
        "title": "句  其九六",
        "author": "谌祜",
        "content": "社稷新亭涕，山河禹贡书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7fa83a7d682a3d1135754e1ab31ebfb8",
        "title": "句  其九七",
        "author": "谌祜",
        "content": "星辰天北户，汉沔国西门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5c64b081e5647c1b45f8691771a63b7d",
        "title": "句  其九八",
        "author": "谌祜",
        "content": "马悲仙仗过，雁入海门斜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9890a2ae6bf32b4c160d5bb56badedac",
        "title": "句  其九九",
        "author": "谌祜",
        "content": "国被儒冠误，如今尚忍言。\n雪霜昏日月，盗贼满干坤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "336fba7e56aecfd066f5fff36e981295",
        "title": "句  其一○○",
        "author": "谌祜",
        "content": "昔谁醒中梦，今独梦中醒。\n万缘水上沤，百岁花前景。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8b1c586576f38067bf14062f1eece64e",
        "title": "句  其一○一",
        "author": "谌祜",
        "content": "中年叹忧患，不及儿童岁。\n今儿机械多，反不如人意。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "128d1b48944cc9c7013e49e3b20d0998",
        "title": "句  其一○二",
        "author": "谌祜",
        "content": "月行虚空閒，月影印在水。\n月落影还无，一笑人閒世。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b6f4bcd703ef23d0c21c608e036e3724",
        "title": "句  其一○三",
        "author": "谌祜",
        "content": "地上有钱流，冥閒无半字。\n自作槁木看，静阅沧江流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "868e93706405822a62e9578af7b61752",
        "title": "句  其一○四",
        "author": "谌祜",
        "content": "江头春水多，汤汤今復古。\n不载落花回，只载斜阳去。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "57fdfdee45d79e1f291df3a6e80edd19",
        "title": "句  其一○五",
        "author": "谌祜",
        "content": "胡笳明月夜，汉节秋风前。\n此士已骨朽，此名至今传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a6c1ec8675337974e03946b43cec322f",
        "title": "句  其一○六",
        "author": "谌祜",
        "content": "旧者今不见，今者旧不知。\n万古明月在，千山黄云飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9411412f089c60c6db624264cc3a1038",
        "title": "句  其一○七",
        "author": "谌祜",
        "content": "东郊独行冰玉时，江左诸贤王谢起。\n若以比花花或宜，自不矜持花亦耻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "375cb798b41be1f241976c2bcaa92934",
        "title": "句  其一○八",
        "author": "谌祜",
        "content": "屈原离骚芳草徧，召南治世梅先见。\n皎如佩玉上清来，不敢班渠国风变。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7ac7a0d35248f931d2d0d4fa3c9bb514",
        "title": "句  其一○九",
        "author": "谌祜",
        "content": "花今国在横浦南，看谁冰雪来同参。\n入南出南此时节，元佑名卿花底雪。\n骨香直到桑海翻，花尚永存香岂灭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d583cafab06aec46d28422da75b2a60a",
        "title": "句  其一一○",
        "author": "谌祜",
        "content": "茂陵秋草不如钱，老尽日光行宇宙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7c2784091f6ec19fe8110043005c4193",
        "title": "句  其一一一",
        "author": "谌祜",
        "content": "世言如花人薄命，谁识如花入卫霍。\n只今保塞愿称臣，鬓蝉夺却貂蝉勋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71d3fffb07190313d6814ade36bdd19a",
        "title": "句  其一一二",
        "author": "谌祜",
        "content": "一声金殿玉阑干，一曲马嵬坡下土。\n夕阳空照古今愁，年年醉醒桃花雨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "70f0d9f76548fdb4c139bbcea1fa8b10",
        "title": "句  其一一三",
        "author": "谌祜",
        "content": "山花笑人人似醉，劝农文似天花坠。\n农今一杯回劝官，吏瘠民肥官有利。\n官休休，民休休，劝农文在墙壁头。\n官此日，民此日，官酒三行官事毕。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e1dc7b96b279d4408ffe5973bbd6cfab",
        "title": "句  其一一四",
        "author": "谌祜",
        "content": "黄叶枝头又早春，红花景裏多明日。\n任翻河汉风北来，终是干坤日东出。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "670a36354911d9817cd3c04a4cb3546f",
        "title": "句  其一一五",
        "author": "谌祜",
        "content": "笳鼓宫商塞外归，似此殊勋古难匹。\n后来渭上朝正月，尚忆祁连冢中骨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "317578cf166c2ca096273ff72198b493",
        "title": "句  其一一六",
        "author": "谌祜",
        "content": "后无关险无中士，大夜宜春狐昼舞。\n万户千门草绿深，芦叶捲吹红日暮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3ca2a345370c8b90f33f098348fdc9df",
        "title": "句  其一一七",
        "author": "谌祜",
        "content": "莫将黩武议前朝，今日人思此奇绝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6278d8599428516c17f529220a7c07a6",
        "title": "句  其一一八",
        "author": "谌祜",
        "content": "失道原不在阴陵，放逐义帝知亡形。\n西风所过皆残灭，不据关险都彭城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d6dd72de47b011171222efda4bdbc2f9",
        "title": "句  其一一九",
        "author": "谌祜",
        "content": "霓裳风秋舞天半，舞到玉花飞石栈。\n三台四辅绕星垣，只一曲中知后患。\n风流天子悟转圜，不见蜀山横翠面。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ba3f02867f2b6f6e12313d49584ee5f5",
        "title": "句  其一二○",
        "author": "谌祜",
        "content": "梨园曲谱韶音秘，响入马嵬花委地。\n开元不再宫不花，天街草绿西风起。",
        "dynasty": "宋",
        "collection_info": null
    }
]