[
    {
        "uuid": "352b835719791852ebc387949e7c3189",
        "title": "句  其一",
        "author": "谌祜",
        "content": "两表蜀天开日月，三军汉地出旌旗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ae91bc6ac0f6624712de98e317b962bb",
        "title": "句  其二",
        "author": "谌祜",
        "content": "王侯螘穴青青草，鼓吹蛙庐焱焱镫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed6201458ecdbc6d871f4b16943b5c0f",
        "title": "句  其三",
        "author": "谌祜",
        "content": "动地百年无桀跖，后天一寿有颜曾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "086e1feb10fe656ce45e17fe8f9c33f9",
        "title": "句  其四",
        "author": "谌祜",
        "content": "百年窗下棋千着，万事灯前酒一斟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0d3b87140feb161c914df516a63ebbef",
        "title": "句  其五",
        "author": "谌祜",
        "content": "白髮古今天北极，黄华宇宙客西风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aed18e1b68e533eb7131eb6d9b0531e8",
        "title": "句  其六",
        "author": "谌祜",
        "content": "封浚九州尧舜禹，典刑万世契夔龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "90d910243e0c2c09ccba76ed4c5a5b93",
        "title": "句  其七",
        "author": "谌祜",
        "content": "陶写有时欣律熟，去来无着证鑪芬。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4a0f549570eab930e79da2024c60ee2b",
        "title": "句  其八",
        "author": "谌祜",
        "content": "西风木叶吹秦晋，春雨桃华送古今。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ca5b7b081c774012319447ced81b378f",
        "title": "句  其九",
        "author": "谌祜",
        "content": "两水夹城飘近远，万山拥翠绿参差。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d82bea5cf66f6a9f72f985afe22743b7",
        "title": "句  其一○",
        "author": "谌祜",
        "content": "不知元佑今何日，慨想儋翁过此时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0a7e086bd683a5f1dd84f3365f683ec1",
        "title": "句  其一一",
        "author": "谌祜",
        "content": "为惜旧时慈母线，宁甘今夜隔帘霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "53e2fde041675f3766452d64056222b0",
        "title": "句  其一二",
        "author": "谌祜",
        "content": "梦犹平昔醒来别，忆到儿童事转长。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cedbc26e8cd346c5b10292378a393853",
        "title": "句  其一三",
        "author": "谌祜",
        "content": "江山落日前朝梦，风雨黄华太古心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3f6bae0fa6f7cd23e67ff06776364e0c",
        "title": "句  其一四",
        "author": "谌祜",
        "content": "观空观化此凭栏，会合人间景一团。\n棋罢客随流水散，酒阑花带夕阳看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2eccba59b92f0268a0d36c254e68dbff",
        "title": "句  其一五",
        "author": "谌祜",
        "content": "飞雨隔愁横笛后，残镫照梦落花前。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d3886b8263b9a763c6605822973c0e5e",
        "title": "句  其一六",
        "author": "谌祜",
        "content": "诗到二南风化在，句流三谢语言工。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "597276ae94c41e5b3d693d9ccd620fcd",
        "title": "句  其一七",
        "author": "谌祜",
        "content": "中原一髮天涯梦，元佑孤臣海外归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "847468b1618a5a62d0009e73d07b68c5",
        "title": "句  其一八",
        "author": "谌祜",
        "content": "造物未尝私化外，英雄何事老隆中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "39d89036f17dbe230fb04005eaf8466d",
        "title": "句  其一九",
        "author": "谌祜",
        "content": "看翻后夜月回照，梦忆前生地不同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ba9de4f8c97b7a97d245031ed4e2d9f9",
        "title": "句  其二○",
        "author": "谌祜",
        "content": "天地浮萍云聚散，江山罨画月西东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3116931fa1012e040914244b03bd995c",
        "title": "句  其二一",
        "author": "谌祜",
        "content": "地上断无秦世草，山中犹读汉时书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fac8041ebf592271e6c9a1c1e7fbdba6",
        "title": "句  其二二",
        "author": "谌祜",
        "content": "梦临水月心先觉，看到霜空幻亦稀。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7077221dc5dbcf31b70dfd4033f6da52",
        "title": "句  其二三",
        "author": "谌祜",
        "content": "天开暝色云千里，雨洗秋空月十分。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7b5647dc0f1b81068345e982a55106cd",
        "title": "句  其二四",
        "author": "谌祜",
        "content": "止水不留前世景，夕阳空照古人心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cfcfb5d9995579067deada1774332992",
        "title": "句  其二五",
        "author": "谌祜",
        "content": "花閒笑语从云散，天下英雄岂陆沈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "780e06a506f2a3f9aec84fae9a6eab98",
        "title": "句  其二六",
        "author": "谌祜",
        "content": "隔云烽火山河旧，列土旌旗雨露深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a04ff775f64745c58d158d5243462fb7",
        "title": "句  其二七",
        "author": "谌祜",
        "content": "层城楼橹看如画，三镇军民梦解围。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "07978eb3a3fdf5775a90d964511dc509",
        "title": "句  其二八",
        "author": "谌祜",
        "content": "树带夕阳雅半集，藋寒秋浦雁初来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d8f2160d74408b3c4fac2d4f9a9f36cf",
        "title": "句  其二九",
        "author": "谌祜",
        "content": "交游雨过三生梦，今古风前酒一杯。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c3b9db7963bad724e98de6e12f65dfae",
        "title": "句  其三○",
        "author": "谌祜",
        "content": "千帆过眼人何在，一笛穿云水自横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ac59ae6d76696a8fab993665291418bb",
        "title": "句  其三一",
        "author": "谌祜",
        "content": "宫烛骊山酒未醒，妖星䁭䁭弄青荧。\n三朝伐叛知谋略，万国归心服典刑。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e1f7705121eb0a2bc404112c861b4008",
        "title": "句  其三二",
        "author": "谌祜",
        "content": "梦游碧落风偏急，望断金精月正中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "abade7bc70039ecd5fc112922cd0ad26",
        "title": "句  其三三",
        "author": "谌祜",
        "content": "万壑松风天鼓吹，四围花月锦帘栊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "79c484fe3d82ac05494761640f30b2a9",
        "title": "句  其三四",
        "author": "谌祜",
        "content": "风雨昼酣秦地阔，关河春梦楚云低。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11fa10336df5ea5d897061b51565ea17",
        "title": "句  其三五",
        "author": "谌祜",
        "content": "蕃汉盛衰霜叶梦，陈隋兴废露华秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eb98ea3601b0de19b04339b079783516",
        "title": "句  其三六",
        "author": "谌祜",
        "content": "聚火有莲知佛性，衡门无雁见交情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b47d0ebd769f09b9ef1c493cf1ecfd58",
        "title": "句  其三七",
        "author": "谌祜",
        "content": "一帆秋水渔歌远，半塔斜阳雁影高。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11791b491a6454536de9c33c37f34b7a",
        "title": "句  其三八",
        "author": "谌祜",
        "content": "邑自乱来新鼓角，屋从兵后尚蓬蒿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f5476d7ed48fce4ed751fcc69c7fe879",
        "title": "句  其三九",
        "author": "谌祜",
        "content": "落雁景翻秋宇迥，乱雅啼带夕阳飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "265e2728a03c51f3028ec6da5047cf60",
        "title": "句  其四○",
        "author": "谌祜",
        "content": "蜀天过雨浮岚翠，汉地连云小麦黄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c7592ab360433a5d4ed096b21d30b5d3",
        "title": "句  其四一",
        "author": "谌祜",
        "content": "城上鸟乌知息战，陈前笳鼓缓归装。\n将军斗大黄金印，犹待先登缚鬼章。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "588c2de5e5494d7b27586ed03f4b5418",
        "title": "句  其四二",
        "author": "谌祜",
        "content": "凤城色映红云见，龙衮香飘紫气浮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "838c1042a4be42a74b086cdbbe6da6f1",
        "title": "句  其四三",
        "author": "谌祜",
        "content": "地势尊严今上国，天王明圣此中兴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4dbf6449ecefcde3f39410ace75de5f5",
        "title": "句  其四四",
        "author": "谌祜",
        "content": "弦管入春波面醉，云天开镜画中行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cc75c60e758f2adf361dda14db40f0b8",
        "title": "句  其四五",
        "author": "谌祜",
        "content": "潮回海北风偏壮，秋满江南客未归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "37b185f0df15ddd8ea1095aaa11a1478",
        "title": "句  其四六",
        "author": "谌祜",
        "content": "宫大阿房行日月，关通函谷混车书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "afee0ee4a1757437a36b55827014d758",
        "title": "句  其四七",
        "author": "谌祜",
        "content": "雁寒景落三山月，骑俊神通万里城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "25d944416d763fbd24fe32211c77f96f",
        "title": "句  其四八",
        "author": "谌祜",
        "content": "帝城烟冷江沱在，郿坞金多社稷危。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "176a77fe3913f163059a97ab83fa2551",
        "title": "句  其四九",
        "author": "谌祜",
        "content": "吴天北斗行宫在，汉节西风閒道归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c83107acbca6917aa583bec318e54db4",
        "title": "句  其五○",
        "author": "谌祜",
        "content": "雅翻汉月催黄髮，雁断胡天想翠华。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "992b7c5cc1543791fe11d7a65d521901",
        "title": "句  其五一",
        "author": "谌祜",
        "content": "燕云海阔归鸿杳，蜀汉城荒戍马閒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "759f1e2519ea8374db0173b366d5b322",
        "title": "句  其五二",
        "author": "谌祜",
        "content": "汉殿落花龙衮弃，周原荒棘虎营新。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "889959fd967d2cc517842e76f3b3b691",
        "title": "句  其五三",
        "author": "谌祜",
        "content": "短褐尚时吟杜曲，采芝何处问秦峯。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "60db9e8d97aefbd233e839f33759f91d",
        "title": "句  其五四",
        "author": "谌祜",
        "content": "晋墅风流棋局散，江东柱石酒壶倾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "db9da91a8c6b3a1c4a74eace1c97f702",
        "title": "句  其五五",
        "author": "谌祜",
        "content": "雁过大江风帖帖，雅啼荒树日悠悠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8c3e6911e1624d02a9d21e11cdc16847",
        "title": "句  其五六",
        "author": "谌祜",
        "content": "赤帜露光王汉地，锦帆波荡帝隋舟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd721da3a63252d225226bd057c0a640",
        "title": "句  其五七",
        "author": "谌祜",
        "content": "楚草泽深陈胜在，秦瓜地旷邵平閒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1b15b3db0f38fca9f91dcce554fc0f8f",
        "title": "句  其五八",
        "author": "谌祜",
        "content": "天开汉统王巴蜀，云从秦繇起沛丰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b59dd3d56e090dc86637cbc2b4ffeeb",
        "title": "句  其五九",
        "author": "谌祜",
        "content": "向日不知今解老，他时应忆此何年。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a303a1efc7af2368c9e9888da74d1563",
        "title": "句  其六○",
        "author": "谌祜",
        "content": "万国梦回秦月在，千年事过蜀云飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3f2946c654ee62213ce82441b9fccda9",
        "title": "句  其六一",
        "author": "谌祜",
        "content": "六鳌倾侧动干坤，国母巡荒护帝孙。\n北极星辰临草地，西风观阙卧花门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "caf50cd6462390e3f46385b6e8b74a08",
        "title": "句  其六二",
        "author": "谌祜",
        "content": "南极大明秋气迥，西风吹梦泰阶平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8bcb3f15c5fcf67efe17b432ab4a7e75",
        "title": "句  其六三",
        "author": "谌祜",
        "content": "西风客老雄心在，泰古人稀侠骨香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "361f33139bf03892ab5c0b4b57ad7116",
        "title": "句  其六四",
        "author": "谌祜",
        "content": "梁有字题何岁月，藏无经在亦香镫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "336189d084c7fc2f44265b18fc3640d3",
        "title": "句  其六五",
        "author": "谌祜",
        "content": "千山落木秋来客，两袖西风旦过僧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5cc08547da0cdad8968d6e27deeeb6d4",
        "title": "句  其六六",
        "author": "谌祜",
        "content": "昨日共吟今是梦，少时轻别老翻愁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2687f1e335e14899611b6f1b4ec18aec",
        "title": "句  其六七",
        "author": "谌祜",
        "content": "天教向老逢春在，诗且忘忧此命通。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1a346d1b426ccb5984c9c803d626306c",
        "title": "句  其六八",
        "author": "谌祜",
        "content": "何事重来看明月，不堪垂老向西风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5157873e59f2bdfb94bcf0415527751b",
        "title": "句  其六九",
        "author": "谌祜",
        "content": "风雨黄花秋兴熟，江湖归路客愁轻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6923a68bf5fa838577ff2883e9d8489f",
        "title": "句  其七○",
        "author": "谌祜",
        "content": "昨朝骨肉关情大，今日平安过望多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3ef41ca4cca74761304aa6a7fe2e3dbd",
        "title": "句  其七一",
        "author": "谌祜",
        "content": "归来今夕岁云徂，且共平安酒一壶。\n浩劫定中逃得过，旧愁空外看来无。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f77c10f9ff5b9fd6be07ea14e23d61d9",
        "title": "句  其七二",
        "author": "谌祜",
        "content": "五更历转花红在，万里晴光草绿敷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "54a71d379685c9dd5a80b71791846263",
        "title": "句  其七三",
        "author": "谌祜",
        "content": "一日又从头上过，百年多在梦中行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c1c4df9e9d17b72898af320e63e95356",
        "title": "句  其七四",
        "author": "谌祜",
        "content": "月明老树吟风鹤，露湿荒郊徧士枭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "81b6c24bbeeb575ddec3770f9e4d7dd8",
        "title": "句  其七五",
        "author": "谌祜",
        "content": "事去尚时寻败局，老来犹日读何书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9beb2938b888a8088acf5d051f01e196",
        "title": "句  其七六",
        "author": "谌祜",
        "content": "术岂有灵能起死，梦从何处忍忘归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "48575a72fcecf5d736a3f8c8245db1e5",
        "title": "句  其七七",
        "author": "谌祜",
        "content": "燕尚有情迎社到，蜨空无梦过时休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bf05a445ef1f26dff8ef485eb7ac8c38",
        "title": "句  其七八",
        "author": "谌祜",
        "content": "壮日尚曾云出岫，暮年偏怪海扬尘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f6d9eb93be56386cd13af493b4c3a953",
        "title": "句  其七九",
        "author": "谌祜",
        "content": "紫微星奠三阶正，黄道天开万象新。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9879ab9aa57b844c19dba31b380dc1ed",
        "title": "句  其八○",
        "author": "谌祜",
        "content": "梁燕未归花尚在，杜鹃有恨月常孤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6dade129e40bf4caedce5220d9e4b6db",
        "title": "句  其八一",
        "author": "谌祜",
        "content": "此情消遣终难去，无路追寻岂是愚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "22552737a00f3da203ca471377d5daed",
        "title": "句  其八二",
        "author": "谌祜",
        "content": "天空日照阴霾伏，八表天开泰象同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "93717950de97e8c90b48fe5a067ea76f",
        "title": "句  其八三",
        "author": "谌祜",
        "content": "禹甸草荒昏雾徧，汉宫花梦昼云酣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5275379d508365f456200b818b66c4b6",
        "title": "句  其八四",
        "author": "谌祜",
        "content": "花柳晴光新态度，江湖春梦老英雄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "243ff9dc385d7c923463e3c1a86b531b",
        "title": "句  其八五",
        "author": "谌祜",
        "content": "日月任从愁裏过，客尘常向静中参。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b7f3fa0e5ba01ed4054912118d47a856",
        "title": "句  其八六",
        "author": "谌祜",
        "content": "归棹舞风鸥不下，愁笳吹月雁斜行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e471a0679e251147de543587042eb720",
        "title": "句  其八七",
        "author": "谌祜",
        "content": "风雨低黄鹄，江湖老白鸥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "619df90881efe2143b7164b08b2d7281",
        "title": "句  其八八",
        "author": "谌祜",
        "content": "歌舞从儿女，艰难忆父兄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "78b850474101254b21294e59c82de453",
        "title": "句  其八九",
        "author": "谌祜",
        "content": "塞云横蜀栈，胡马出巴陵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e593157d5e569df420ca6331a6e93bcb",
        "title": "句  其九○",
        "author": "谌祜",
        "content": "白髮风前酒，黄花雨后镫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "896450f28b6623da864699acb066a025",
        "title": "句  其九一",
        "author": "谌祜",
        "content": "月满商山夜，风高汉殿秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6769d6f0415bf53b62e0449603350a90",
        "title": "句  其九二",
        "author": "谌祜",
        "content": "世情江水淡，心事海鸥知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c9370b794e0b07fa5a0d08736ef1df1",
        "title": "句  其九三",
        "author": "谌祜",
        "content": "巴云秋景薄，汉月夜光寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "01ce71b2dbef3eb85f1e05065337d703",
        "title": "句  其九四",
        "author": "谌祜",
        "content": "克復归神主，多才自不难。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c1fe15987a7e7963567e9684301390a1",
        "title": "句  其九五",
        "author": "谌祜",
        "content": "玉花嘶徼外，翠辇过云中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5c67a534f5d5a649be23d6ed2ecf4543",
        "title": "句  其九六",
        "author": "谌祜",
        "content": "社稷新亭涕，山河禹贡书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ec5701141ef3b53d38a6983817702525",
        "title": "句  其九七",
        "author": "谌祜",
        "content": "星辰天北户，汉沔国西门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2fe5205bf0122b034999b45e042cfbab",
        "title": "句  其九八",
        "author": "谌祜",
        "content": "马悲仙仗过，雁入海门斜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "99e9c7b7a8dcbaa56945e20435063f41",
        "title": "句  其九九",
        "author": "谌祜",
        "content": "国被儒冠误，如今尚忍言。\n雪霜昏日月，盗贼满干坤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4f32b24692b36c29e2c3f89f5ab54553",
        "title": "句  其一○○",
        "author": "谌祜",
        "content": "昔谁醒中梦，今独梦中醒。\n万缘水上沤，百岁花前景。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d10fe3f1194d492b0c39337abb91d827",
        "title": "句  其一○一",
        "author": "谌祜",
        "content": "中年叹忧患，不及儿童岁。\n今儿机械多，反不如人意。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "016d1d15b6ef4e079909c2955204a0aa",
        "title": "句  其一○二",
        "author": "谌祜",
        "content": "月行虚空閒，月影印在水。\n月落影还无，一笑人閒世。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ff0de914b510677835b0c6006ddea02f",
        "title": "句  其一○三",
        "author": "谌祜",
        "content": "地上有钱流，冥閒无半字。\n自作槁木看，静阅沧江流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "54708c6f9ff220256066360ea86287ee",
        "title": "句  其一○四",
        "author": "谌祜",
        "content": "江头春水多，汤汤今復古。\n不载落花回，只载斜阳去。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "70079fd729236a15a71b2dcac40612da",
        "title": "句  其一○五",
        "author": "谌祜",
        "content": "胡笳明月夜，汉节秋风前。\n此士已骨朽，此名至今传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ccec6715aaa6c6e9436b5bc7524f2b7c",
        "title": "句  其一○六",
        "author": "谌祜",
        "content": "旧者今不见，今者旧不知。\n万古明月在，千山黄云飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e0baf8335354d72553912732c8d0f531",
        "title": "句  其一○七",
        "author": "谌祜",
        "content": "东郊独行冰玉时，江左诸贤王谢起。\n若以比花花或宜，自不矜持花亦耻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3c9cab476b33e9cf70415a38513adf4a",
        "title": "句  其一○八",
        "author": "谌祜",
        "content": "屈原离骚芳草徧，召南治世梅先见。\n皎如佩玉上清来，不敢班渠国风变。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4da465f655398bc8a5e2172f6a291ff3",
        "title": "句  其一○九",
        "author": "谌祜",
        "content": "花今国在横浦南，看谁冰雪来同参。\n入南出南此时节，元佑名卿花底雪。\n骨香直到桑海翻，花尚永存香岂灭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "880b9c029995312456500f00a86ecb43",
        "title": "句  其一一○",
        "author": "谌祜",
        "content": "茂陵秋草不如钱，老尽日光行宇宙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f1e3775a72f196e0c618a55966516416",
        "title": "句  其一一一",
        "author": "谌祜",
        "content": "世言如花人薄命，谁识如花入卫霍。\n只今保塞愿称臣，鬓蝉夺却貂蝉勋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fdc9673c13870451463c0390e2112c7f",
        "title": "句  其一一二",
        "author": "谌祜",
        "content": "一声金殿玉阑干，一曲马嵬坡下土。\n夕阳空照古今愁，年年醉醒桃花雨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f601c6eca38c534a4b584f3c058473bb",
        "title": "句  其一一三",
        "author": "谌祜",
        "content": "山花笑人人似醉，劝农文似天花坠。\n农今一杯回劝官，吏瘠民肥官有利。\n官休休，民休休，劝农文在墙壁头。\n官此日，民此日，官酒三行官事毕。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "96f66ba4fdf8488d472e0c7f01acbb33",
        "title": "句  其一一四",
        "author": "谌祜",
        "content": "黄叶枝头又早春，红花景裏多明日。\n任翻河汉风北来，终是干坤日东出。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "59bf6854ab13d62b19c81ea6300aaf15",
        "title": "句  其一一五",
        "author": "谌祜",
        "content": "笳鼓宫商塞外归，似此殊勋古难匹。\n后来渭上朝正月，尚忆祁连冢中骨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cb74e299d99eb55fdb2a7f83e5625fa1",
        "title": "句  其一一六",
        "author": "谌祜",
        "content": "后无关险无中士，大夜宜春狐昼舞。\n万户千门草绿深，芦叶捲吹红日暮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f9380d6e5afbe153349855b9bd70a408",
        "title": "句  其一一七",
        "author": "谌祜",
        "content": "莫将黩武议前朝，今日人思此奇绝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "26f1b2454f897bbc0b987f90e912f558",
        "title": "句  其一一八",
        "author": "谌祜",
        "content": "失道原不在阴陵，放逐义帝知亡形。\n西风所过皆残灭，不据关险都彭城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b8f90027568b04b47b7d01dd5a741cbd",
        "title": "句  其一一九",
        "author": "谌祜",
        "content": "霓裳风秋舞天半，舞到玉花飞石栈。\n三台四辅绕星垣，只一曲中知后患。\n风流天子悟转圜，不见蜀山横翠面。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e8ae531c316b79503f3f093e73c1ae45",
        "title": "句  其一二○",
        "author": "谌祜",
        "content": "梨园曲谱韶音秘，响入马嵬花委地。\n开元不再宫不花，天街草绿西风起。",
        "dynasty": "宋",
        "collection_info": null
    }
]