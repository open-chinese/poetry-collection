[
    {
        "uuid": "a3be368fdb395e0ad3b04fc0d30c1279",
        "title": "答外兄王子常问修行之术",
        "author": "刘卞功",
        "content": "非道亦非律，又非虚空禅。\n独守一亩宅，惟耕己心田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7088d2ca47e013c34a8d2f695bfa8f40",
        "title": "题李公谟壁",
        "author": "刘卞功",
        "content": "日转庭槐影渐移，重门复壁转呼迟。\n不如拂袖穿云去，惟有落花流水知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1c69566c0ceaae5777ab5eb044c23fc9",
        "title": "自题写真",
        "author": "刘卞功",
        "content": "韬光晦迹居环堵，闭自凝神事事疏。\n漫许丹青传道貌，难将妙笔写真如。",
        "dynasty": "宋",
        "collection_info": null
    }
]