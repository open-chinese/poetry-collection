[
    {
        "uuid": "e058f1450f24459cf18f94d8ccaefb94",
        "title": "答外兄王子常问修行之术",
        "author": "刘卞功",
        "content": "非道亦非律，又非虚空禅。\n独守一亩宅，惟耕己心田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "472b006020e4f1926dc99e5aee2e91ba",
        "title": "题李公谟壁",
        "author": "刘卞功",
        "content": "日转庭槐影渐移，重门复壁转呼迟。\n不如拂袖穿云去，惟有落花流水知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d8fe61c1cbad09aba78f7fa9942513df",
        "title": "自题写真",
        "author": "刘卞功",
        "content": "韬光晦迹居环堵，闭自凝神事事疏。\n漫许丹青传道貌，难将妙笔写真如。",
        "dynasty": "宋",
        "collection_info": null
    }
]