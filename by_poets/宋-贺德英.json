[
    {
        "uuid": "a8f31cb22cf4b0174e98a57846cae3f9",
        "title": "圣小儿诗",
        "author": "贺德英",
        "content": "英秀钟河岳，生来号圣儿。\n虽云年尚小，不待学而知。\n博古通今识，超羣迈秀姿。\n浩然食牛气，非若聚鱼嬉。\n祖莹耽书日，甘罗入相时。\n看看头角长，身到凤凰池。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1d39c94de7505416fd0df5850059527b",
        "title": "赋雪",
        "author": "贺德英",
        "content": "谁翦银河水，飞花出汉霾。\n月明人竞往，踏破水晶街。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "74516548e3241bef7a5e9fe2d10a805b",
        "title": "题县厅壁",
        "author": "贺德英",
        "content": "昆明池裏跃金鳞，怒目扬鬐不露身。\n想是龙门曾点额，至今羞见钓鳌人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "35173949dc80063f46d9254c156a2929",
        "title": "句  其一",
        "author": "贺德英",
        "content": "易描通臂状，难写断肠声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "28395d959248a5501e45558ecbf2b9fa",
        "title": "句  其二",
        "author": "贺德英",
        "content": "只因错写天文表，流落人间一纪零。",
        "dynasty": "宋",
        "collection_info": null
    }
]