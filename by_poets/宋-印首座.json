[
    {
        "uuid": "74b8022f3c93ee4b29f3ba8a817c73e4",
        "title": "偶书",
        "author": "印首座",
        "content": "折脚铛见谩自煨，饭余长是坐堆堆。\n一从近日生涯拙，百鸟衔华去不来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "26b33892d3f584a5323526fc0c1191ce",
        "title": "偈一首",
        "author": "印首座",
        "content": "不挂寸丝方免寒，何须特地袅长竿。\n而今落落零零也，七佛之名甚处安。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1a2bc6fc7bb22250d9a03ab0575f853e",
        "title": "偈",
        "author": "印首座",
        "content": "丘师伯莫晓，寂寂明皎皎。\n日午打三更，谁人打得了。",
        "dynasty": "宋",
        "collection_info": null
    }
]