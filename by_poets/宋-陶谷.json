[
    {
        "uuid": "2d0b2165327d6447f927fe222976c468",
        "title": "题玉堂壁",
        "author": "陶谷",
        "content": "官职有来须与做，才能用处不忧无。\n堪笑翰林陶学士，一生依样画葫芦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0710f4adb4633a74ea79a00d968c4d79",
        "title": "石桥",
        "author": "陶谷",
        "content": "重重翠幛耸云端，玉殿金楼缥缈间。\n圣境不容凡俗到，故将飞瀑隔尘寰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d6e42493fd091729a8745abd388e73ad",
        "title": "寄赠梦英大师",
        "author": "陶谷",
        "content": "是箇碑文念得全，聪明灵性自天然。\n离吴别楚三千里，入洛游梁二十年。\n负艺已闻喧世界，高眠长见卧云烟。\n相逢与我情何厚，问佛方知宿有缘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "af0ea3ccaff50852f5307ebfbec8c54a",
        "title": "句  其一",
        "author": "陶谷",
        "content": "尖檐帽子卑凡厮，短靿靴儿末厥兵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fc0ab0e48a54fa0e9552bfade6b8f36d",
        "title": "句  其二",
        "author": "陶谷",
        "content": "此生头已白，无路扫王门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "be0de08f3f801e829026f468acf85d46",
        "title": "句  其三",
        "author": "陶谷",
        "content": "井蛙休恃重溟险，泽马曾嘶九曲滨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "36a6d7d5435b47f88b75542fa15e518f",
        "title": "句  其四",
        "author": "陶谷",
        "content": "乞与金钟病眼明。",
        "dynasty": "宋",
        "collection_info": null
    }
]