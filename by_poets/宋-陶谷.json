[
    {
        "uuid": "91fcfefc2c835b87a9c3ebc4ece94b01",
        "title": "题玉堂壁",
        "author": "陶谷",
        "content": "官职有来须与做，才能用处不忧无。\n堪笑翰林陶学士，一生依样画葫芦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c15e6d39b68df4d84975e96e7d8fbba",
        "title": "石桥",
        "author": "陶谷",
        "content": "重重翠幛耸云端，玉殿金楼缥缈间。\n圣境不容凡俗到，故将飞瀑隔尘寰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "274bb6b26fabcbb446947726c90c5183",
        "title": "寄赠梦英大师",
        "author": "陶谷",
        "content": "是箇碑文念得全，聪明灵性自天然。\n离吴别楚三千里，入洛游梁二十年。\n负艺已闻喧世界，高眠长见卧云烟。\n相逢与我情何厚，问佛方知宿有缘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8f503ed78fdbecf1b86df2e8e31763bf",
        "title": "句  其一",
        "author": "陶谷",
        "content": "尖檐帽子卑凡厮，短靿靴儿末厥兵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cf42d89e3a49bf794b66d0fcf57167d0",
        "title": "句  其二",
        "author": "陶谷",
        "content": "此生头已白，无路扫王门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e86c61b6267f2da7762b08b4d4069156",
        "title": "句  其三",
        "author": "陶谷",
        "content": "井蛙休恃重溟险，泽马曾嘶九曲滨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "335fc402033296be96a881a62de27ca8",
        "title": "句  其四",
        "author": "陶谷",
        "content": "乞与金钟病眼明。",
        "dynasty": "宋",
        "collection_info": null
    }
]