[
    {
        "uuid": "3f0d9db2569ef9e741fe1523c5e5168d",
        "title": "偈二首  其一",
        "author": "释洪英",
        "content": "阿家尝醋三赤喙，新妇洗面摸着鼻。\n道吾答话得腰裩，玄沙开书是白纸。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8ebb9f3bb37dc414c251069bd525875a",
        "title": "偈二首  其二",
        "author": "释洪英",
        "content": "万锻鑪中铁蒺藜，直须高价莫饶伊。\n横来竖去呵呵笑，一任旁人鼓是非。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4e781aeea4bece582bc8a58c5a147a59",
        "title": "偈三首  其一",
        "author": "释洪英",
        "content": "青山重迭迭，绿水响潺潺。\n未到悬崖处，抬头子细看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "67e9cf11632e47efa23ac2b99258ecc8",
        "title": "偈三首  其二",
        "author": "释洪英",
        "content": "宝峰高士罕曾到，岩前雪压枯松倒。\n岭前岭后野猿啼，一条古路清风扫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5fc55fffc01097651341f545ada68270",
        "title": "偈三首  其三",
        "author": "释洪英",
        "content": "石门巇崄铁关牢，举目重重万仞高。\n无角铁牛衝得破，毗卢海内作波涛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "77932e297827bd0be2ce73487544c481",
        "title": "句  其一",
        "author": "释洪英",
        "content": "喜得狼烟息，弓弰壁上悬。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c1893a0686bf86d6d02929befb1685b8",
        "title": "句  其二",
        "author": "释洪英",
        "content": "不到乌江畔，知君未肯休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f2dfa4c80e223f12471f0b3bcd8f3f0c",
        "title": "句  其三",
        "author": "释洪英",
        "content": "天际雪埋千尺石，洞门冻折数株松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ee355e3ea65ec6c6aebf3137dee17ad6",
        "title": "句  其四",
        "author": "释洪英",
        "content": "一句不遑无着问，迄今犹作野盘僧。",
        "dynasty": "宋",
        "collection_info": null
    }
]