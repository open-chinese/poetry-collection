[
    {
        "uuid": "b67826fca36186247fff6d484a1174f4",
        "title": "偈二首  其一",
        "author": "释洪英",
        "content": "阿家尝醋三赤喙，新妇洗面摸着鼻。\n道吾答话得腰裩，玄沙开书是白纸。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71523fb8fd78c9c596d89951cd37ed44",
        "title": "偈二首  其二",
        "author": "释洪英",
        "content": "万锻鑪中铁蒺藜，直须高价莫饶伊。\n横来竖去呵呵笑，一任旁人鼓是非。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3277c9b3c2f62356bbec004db10f4422",
        "title": "偈三首  其一",
        "author": "释洪英",
        "content": "青山重迭迭，绿水响潺潺。\n未到悬崖处，抬头子细看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7dadff2df78e5323e9d2e7624811c601",
        "title": "偈三首  其二",
        "author": "释洪英",
        "content": "宝峰高士罕曾到，岩前雪压枯松倒。\n岭前岭后野猿啼，一条古路清风扫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "64e72dcce35e62b95c0f589b80729ea9",
        "title": "偈三首  其三",
        "author": "释洪英",
        "content": "石门巇崄铁关牢，举目重重万仞高。\n无角铁牛衝得破，毗卢海内作波涛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b15f97d8e548a05b1f9a2f565e431500",
        "title": "句  其一",
        "author": "释洪英",
        "content": "喜得狼烟息，弓弰壁上悬。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1a1eb4e87d31c96741de4024353deb53",
        "title": "句  其二",
        "author": "释洪英",
        "content": "不到乌江畔，知君未肯休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cb34c8c8d8b640491d99e37556273493",
        "title": "句  其三",
        "author": "释洪英",
        "content": "天际雪埋千尺石，洞门冻折数株松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b4457a44899f53e74e245f7201225f5f",
        "title": "句  其四",
        "author": "释洪英",
        "content": "一句不遑无着问，迄今犹作野盘僧。",
        "dynasty": "宋",
        "collection_info": null
    }
]