[
    {
        "uuid": "7ac2e3b430d9e0171aeb0daaa2df2b9e",
        "title": "留题山谷寺",
        "author": "陆宰",
        "content": "芙蓉已入双林寂，山谷今传佛祖衣。\n千里客来何所遇，夜堂人静雨霏霏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "da73752c46cf783d7d041c89007b34cc",
        "title": "留别东阳",
        "author": "陆宰",
        "content": "前身疑是此山僧，猿鹤相逢亦有情。\n珍重岭头风与月，百年常记老夫名。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bc1e37742a45bd4bd95bf7e6bcb5a2c7",
        "title": "句  其一",
        "author": "陆宰",
        "content": "奴爱才如萧颖士，婢知诗郑康成。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6a3013471cdfad2fca9835aae961e184",
        "title": "句  其二",
        "author": "陆宰",
        "content": "岁月悠悠悲往事，川原冉冉梦重游。",
        "dynasty": "宋",
        "collection_info": null
    }
]