[
    {
        "uuid": "9a86533b22c39a7709f765da3705321b",
        "title": "商角调・黄莺儿",
        "author": "庾吉甫",
        "content": "怀古，怀古，废兴两字，干弋疑虑戈几度？问当时富贵谁家？陈宫后主。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "be1a375bdc455dd530bad5f8f2f76cdd",
        "title": "踏莎行・残照底西风老树，据秦淮终是帝王都。爱山围水绕，龙蟠虎踞。",
        "author": "庾吉甫",
        "content": "依稀睹，六朝风物。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "4c0c5acc5cb0ee7a19d8bbc0ee60768f",
        "title": "盖天旗・光阴迅速，多半晴天变雨。待拣搭溪山好处，吞一杯，嚎数曲。",
        "author": "庾吉甫",
        "content": "身有欢娱，事无荣辱。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "a324af29d22f9d2a3343c5d4bd49b05a",
        "title": "应天长・引一仆，着两壶。谢老东山，黄花时好去。适意林泉游未足。烟",
        "author": "庾吉甫",
        "content": "波暮，堪凝伫，谪仙诗句。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "92767e2f84520044f488b00aeef3b224",
        "title": "尾・一线寄乌衣，二水分白鹭。台上凤凰游，井口胭脂污。想玉树后庭花，",
        "author": "庾吉甫",
        "content": "好金陵建康府。\n怀古，怀古，物换千年，星移几度。\n想当时帝子元婴，阎公都督。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "e9624e225c7cff28ec076df83a784572",
        "title": "踏莎行・彩射龙光，云埋铁柱。迷津烟暗，渡水平湖。高士词堂，旌阳殿",
        "author": "庾吉甫",
        "content": "洪恩路，藕花无数。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "5ce89c20d980b442e2f754cc530525f2",
        "title": "盖天旗・残碑淋雨，留得王郎佳句。信步携筇，登临闲伫。雁惊寒，衡阳",
        "author": "庾吉甫",
        "content": "秋水长天，落霞孤鹜。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "9524d4ac71205d7993f7bbd9237b37df",
        "title": "应天长・东接吴，南甸楚。绀坞荒村，苍烟古木。俯挹遥岑伤未足。夕阳",
        "author": "庾吉甫",
        "content": "暮，空无语，昔人何处？。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "0a35ccbac0d9746533c34dcd12222f17",
        "title": "尾・孤塔插晴空，高阁临江渚。栋飞南浦云，帘卷西山雨。观胜概，壮江",
        "author": "庾吉甫",
        "content": "山，叹鸣銮，罢歌舞。\n别况无语，无语，闷人怕到，江天日暮。\n大都来一种相思，柔肠万缕。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "16b8af975539f85f175d97edadf213bf",
        "title": "幺・嫩玉，肌肤，会调弦理管，能歌妙舞。从别后，有谁拘束？",
        "author": "庾吉甫",
        "content": "",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "ca84731b551f507abd1b8f39901bba71",
        "title": "垂丝钓・求神问卜，道须有团圆一处。奈目下佳期，未得相逢愁最苦。正",
        "author": "庾吉甫",
        "content": "值着秋光，暮天凄楚。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "c7e6fdaf5e1d97f1110adf6c2442da52",
        "title": "应长天・愁成阵，更压着宋玉。便是铁石人，也今宵耽不去。早是栖惶能",
        "author": "庾吉甫",
        "content": "对付。\n难禁处，凄凉景，窗儿外眼撮聚。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "fc1c9984dd8f03939198206e3875acc7",
        "title": "随煞・起一阵菊花风，下几点芭蕉雨。风送得菊花香，雨打得芭蕉絮。芭",
        "author": "庾吉甫",
        "content": "焦雨敌庭梧，菊花风战槛竹。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "147971f3c6031b2b9194b3dfd42024fe",
        "title": "商调・定风波思情",
        "author": "庾吉甫",
        "content": "迤秋来到，正露冷风寒，微雨初收，凉风儿透冽襟袖。\n自别来愁万感，遣离情不堪回首。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "a1e333cce1547dbe40de78f68711e761",
        "title": "金菊香・到秋来还有许多忧，一寸心怀无限愁，离情镇日如病酒。似这等",
        "author": "庾吉甫",
        "content": "恹恹，终不肯断了风流。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "150f4611c4dbedd046dd13bcd01a6874",
        "title": "凤鸾吟・题起来羞，这相思何日休，好姻缘不到头。饮几盏闷酒，醉了时",
        "author": "庾吉甫",
        "content": "罢手，则怕酒醒了时还依旧。\n我为他使尽了心，他为我添消瘦，都一般减了风流。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "bf77184471996879c9bf37d39a991617",
        "title": "醋葫芦・人病久，何日休，思情欲待罢无由。哎！你个多情你可便怎下的",
        "author": "庾吉甫",
        "content": "辜负！子我知伊主意，料应来倚仗着脸儿羞。",
        "dynasty": "元",
        "collection_info": null
    },
    {
        "uuid": "3e0bc7629cb9ef981728357639c1eadc",
        "title": "尾声・本待要弃舍了你个冤家，别寻一个玉人儿成配偶。你道是强似你那",
        "author": "庾吉甫",
        "content": "模样儿的呵，说道我也不能够。\n我道来胜似你心肠儿的呵，到处里有。",
        "dynasty": "元",
        "collection_info": null
    }
]