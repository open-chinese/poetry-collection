[
    {
        "uuid": "8fcf7d8c1130c3816755a9de2d401b8a",
        "title": "赠周孝子二首  其一",
        "author": "徐应镳",
        "content": "卜吉崇岗思怆然，双魂同閟昼如年。\n山中细草初回碧，庐墓人啼血色鲜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "432e73a5aecc32fa2e34a59a27ee778a",
        "title": "赠周孝子二首  其二",
        "author": "徐应镳",
        "content": "一自仙游去不回，徒令孝子有余哀。\n他年华表归来后，知是人间几劫灰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1eb6fc512d26864278ff9b8c98bb8472",
        "title": "绝命词",
        "author": "徐应镳",
        "content": "二男并一女，随我上梯云。\n烈士甘焚死，丹心照紫雯。",
        "dynasty": "宋",
        "collection_info": null
    }
]