[
    {
        "uuid": "0b46f13f32fc41b79047334aed9a0abd",
        "title": "赠周孝子二首  其一",
        "author": "徐应镳",
        "content": "卜吉崇岗思怆然，双魂同閟昼如年。\n山中细草初回碧，庐墓人啼血色鲜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18411696ebd71b5a4cb1803bbe420aca",
        "title": "赠周孝子二首  其二",
        "author": "徐应镳",
        "content": "一自仙游去不回，徒令孝子有余哀。\n他年华表归来后，知是人间几劫灰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eddde32183447c052f93499162fb0c69",
        "title": "绝命词",
        "author": "徐应镳",
        "content": "二男并一女，随我上梯云。\n烈士甘焚死，丹心照紫雯。",
        "dynasty": "宋",
        "collection_info": null
    }
]