[
    {
        "uuid": "099da79e947de2cf1514d18a006e05dd",
        "title": "吴 黄金车",
        "author": "孙元晏",
        "content": "分擘山河即渐开，许昌基业已倾颓。\n黄金车与斑斓耳，早个须知入谶来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9f697bd0c802f7255449fce7a0785d44",
        "title": "吴 赤壁",
        "author": "孙元晏",
        "content": "会猎书来举国惊，祗应周鲁不教迎。\n曹公一战奔波后，赤壁功传万古名。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e80d04039e119adf84170a980570ad82",
        "title": "吴 鲁肃指囷",
        "author": "孙元晏",
        "content": "破产移家事亦难，佐吴从此霸江山。\n争教不立功勋得，指出千囷如等闲。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "2dde2873cef84cd0acacd29e64e694f8",
        "title": "吴 甘宁斫营",
        "author": "孙元晏",
        "content": "夜深偷入魏军营，满寨惊忙火似星。\n百口宝刀千匹绢，也应消得与甘宁。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "aae187ca6731cb96995975bb818cd1ea",
        "title": "吴 徐盛",
        "author": "孙元晏",
        "content": "欲把江山鼎足分，邢真衔册到江南。\n当时将相谁堪重，徐盛将军最不甘。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9948d919ecbc559ab0eed7a34054bf24",
        "title": "吴 鲁肃",
        "author": "孙元晏",
        "content": "斫案兴言断众疑，鼎分从此定雄雌。\n若无子敬心相似，争得乌林破魏师。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "889f84b1dcd4783e5b0432fe11101b8f",
        "title": "吴 武昌",
        "author": "孙元晏",
        "content": "西塞山高截九垓，谶谣终日自相催。\n武昌鱼美应难恋，历数须归建业来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9380b6134de9817394479cbe094f86b8",
        "title": "吴 顾雍",
        "author": "孙元晏",
        "content": "赞国经纶更有谁，蔡公相叹亦相师。\n贵为丞相封侯了，归后家人总不知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "13d9f114535b8ca1a612c51ca046a4dd",
        "title": "吴 吕蒙",
        "author": "孙元晏",
        "content": "幼小家贫实可哀，愿征行去志难回。\n不探虎穴求身达，争得人间富贵来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "09f2da1c0ac90f6213bc028058aef68f",
        "title": "吴 介象",
        "author": "孙元晏",
        "content": "好道君王遇亦难，变通灵异几多般。\n介先生有神仙术，钓得鲈鱼在玉盘。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d0e317e53a9088d28188cf43875bbb3e",
        "title": "吴 濡须坞",
        "author": "孙元晏",
        "content": "风揭洪涛响若雷，枕波为垒险相隈。\n莫言有个濡须坞，几度曹公失志回。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "34d645ae3bd17d7a24c278dfa583e6a0",
        "title": "吴 周泰",
        "author": "孙元晏",
        "content": "名与诸公又不同，金疮痕在满身中。\n不将御盖宣恩泽，谁信将军别有功。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8acd518cf62fde4e239f673cd3701f0e",
        "title": "吴 张纮",
        "author": "孙元晏",
        "content": "东部张公与众殊，共施经畧赞全吴。\n陈琳漫自称雄伯，神气应须怯大巫。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "f678ad0db06ed7ce0bee0900e55500fe",
        "title": "吴 太史慈",
        "author": "孙元晏",
        "content": "圣德招贤远近知，曹公心计却成欺。\n陈韩昔日尝投楚，岂是当归召得伊。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e4b0a3eb2569c6706ac85ed76abd554c",
        "title": "吴 孙坚后",
        "author": "孙元晏",
        "content": "委存张公翊圣材，几将贤德赞文台。\n争教不霸江山得，日月征曾入梦来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e50b330f0fd8d139550b0d9270020ca3",
        "title": "吴 陆统",
        "author": "孙元晏",
        "content": "将军身殁有儿孤，虎子为名教读书。\n更向宫中教骑马，感君恩重合何如。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8b075e2f912b45d88ed1ead8cf418cfb",
        "title": "吴 青盖",
        "author": "孙元晏",
        "content": "历数将终势已摧，不修君德更堪哀。\n被他青盖言相误，元是须教入晋来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ac967c8afa9fef02ffe5695676158bd7",
        "title": "晋 七宝鞭",
        "author": "孙元晏",
        "content": "天命须知岂偶然，乱臣徒欲用兵权。\n圣谟庙畧还应别，浑不消他七宝鞭。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d0307e123389c6bbc1ce182a7f7fba46",
        "title": "晋 庾悦鹅炙",
        "author": "孙元晏",
        "content": "春暖江南景气新，子鹅炙美就中珍。\n庾家厨盛刘公困，浑弗相贻也恼人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e3bbafb41d45f5a7ac8cb8dfc025bb51",
        "title": "晋 谢玄",
        "author": "孙元晏",
        "content": "百万兵来逼合肥，谢玄为将统雄师。\n旌旗首尾千余里，浑不消他一局棊。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "4fc567c550794e07cd4fa915436b1e97",
        "title": "晋 谢混",
        "author": "孙元晏",
        "content": "尚主当初偶未成，此时谁合更关情。\n可怜谢混风华在，千古翻传禁脔名。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "0d5aad74f08347588efc682a8a82b762",
        "title": "晋 陆玩",
        "author": "孙元晏",
        "content": "陆公高论亦由衷，谦让还惭未有功。\n天下忠良人欲尽，始应交我作三公。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "5115f5a6d5952abdeb6f682795dedde5",
        "title": "晋 王坦之",
        "author": "孙元晏",
        "content": "晋祚安危只此行，坦之何必苦忧惊。\n谢公合定寰区在，争遣当时事得成。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "bf509f570a48a152f57bea262936eb2b",
        "title": "晋 蒲葵扇",
        "author": "孙元晏",
        "content": "抛舍东山岁月遥，几施经畧挫雄豪。\n若非名德喧寰宇，争得蒲葵价数高。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d989f668e9540a2308e6fdbd25bf22a9",
        "title": "晋 王郎",
        "author": "孙元晏",
        "content": "太尉门庭亦甚高，王郎名重礼相饶。\n自家妻父犹如此，谁更逢君得折腰。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "4e5ddc29fcfa0e40ec96d4c7efc8baff",
        "title": "晋 刘毅",
        "author": "孙元晏",
        "content": "遶床堪壮喝卢声，似铁容仪众尽惊。\n二十七人同举义，几人全得旧功名。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "58f2a7b7b43013bd67dff0f85450d786",
        "title": "晋 王恭",
        "author": "孙元晏",
        "content": "春风濯濯柳容仪，鹤氅神情举世推。\n可惜教君仗旄钺，枉将心地託牢之。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d19f3bb4cf0a931d4efb98cf481a1053",
        "title": "晋 谢公赌墅",
        "author": "孙元晏",
        "content": "发遣将军欲去时，畧无情挠只贪棊。\n自从乞与羊昙后，赌墅功成更有谁。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "de29338f3e5e1b422bb7a9a613413142",
        "title": "晋 苻坚投棰",
        "author": "孙元晏",
        "content": "投棰填江语未终，谢安乘此立殊功。\n三台星烂干坤在，且与张华死不同。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ad0a4763ee1ca6ee225c383abb4f6422",
        "title": "晋 卫玠",
        "author": "孙元晏",
        "content": "叔宝羊车海内稀，山家女壻好风姿。\n江东士女无端甚，看杀玉人浑不知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "043dc101bc0042c502301961b0920353",
        "title": "晋 郭璞脱襦",
        "author": "孙元晏",
        "content": "吟坐因思郭景纯，每言穷达似通神。\n到头分命难移改，解脱清襦与别人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "59a25a59872da72d0f602f9055eca894",
        "title": "晋 庾楼",
        "author": "孙元晏",
        "content": "江州楼上月明中，从事同登眺远空。\n玉树忽薶千载后，有谁重此继清风。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9fc58fad10a1a3c1320d6ac6824923e2",
        "title": "晋 新亭",
        "author": "孙元晏",
        "content": "容易乘虚逼帝畿，满江艛艣与旌旗。\n卢循若解新亭上，胜负还应未可知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "167304b708e4486e79e0c63e83bb11fd",
        "title": "宋 大岘",
        "author": "孙元晏",
        "content": "大岘纔过喜可知，指空言已副心期。\n公孙计策嗟无用，天与南朝作霸基。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "75d70930e37b31ba20cc1d73ca5ed8da",
        "title": "宋 放宫人",
        "author": "孙元晏",
        "content": "纳谏廷臣免犯颜，自然恩可霸江山。\n姚兴侍女方承宠，放出宫闱若等闲。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "0b6d4e1da6a7eb95f999c1c031c4123d",
        "title": "宋 借南苑",
        "author": "孙元晏",
        "content": "人主词应不偶然，几人曾说笑掀天。\n不知南苑今何在，借与张公三百年。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "aa6932035792323f5ec150da12412b97",
        "title": "宋 谢澹云霞友",
        "author": "孙元晏",
        "content": "仗气凌人岂可亲，只将范泰是知闻。\n缘何唤作云霞友，却恐云霞未似君。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "14b94655eed9e7a6a5d39243dfcf802c",
        "title": "宋 乌衣巷",
        "author": "孙元晏",
        "content": "古迹荒基好叹嗟，满川吟景只烟霞。\n乌衣巷在何人住，回首令人忆谢家。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "3c2bfe816516725aa66ea4f9d99a4fb3",
        "title": "宋 袁粲",
        "author": "孙元晏",
        "content": "负才尚气满朝知，高卧闲吟见客稀。\n独步何人识袁尹，白杨郊外醉方归。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "a0d5c1461e413b2a43ee978b4fa9ca2d",
        "title": "宋 刘伯龙",
        "author": "孙元晏",
        "content": "位重何如不厌贫，伯龙孤子只修身。\n固知生计还须有，穷鬼临时也笑人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "cfdf216b8067db4a4f14855380e3f16e",
        "title": "宋 王方平",
        "author": "孙元晏",
        "content": "拂衣耕钓已多时，江上山前乐可知。\n着却貂裘将采药，任他人唤作渔师。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8dd5cbf6fcee32e99b340c7687d35b05",
        "title": "宋 黄罗襦",
        "author": "孙元晏",
        "content": "戚属羣臣尽见猜，预忧身后又堪哀。\n到头委付何曾是，虚把罗襦与彦回。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "7f86aa8a1a03ef25342b1c52437aa0fa",
        "title": "宋 谢朏",
        "author": "孙元晏",
        "content": "谢家诸子尽兰香，各震芳名满帝乡。\n惟有千金更堪重，只将高卧向齐王。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c49f97b8abd2cd186be44a666700f28e",
        "title": "宋 羊玄保",
        "author": "孙元晏",
        "content": "运命将来各有期，好官才阙即思之。\n就中堪爱羊玄保，偏受君王分外知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d498e0fb0a847d7c35eeebda209985ac",
        "title": "齐 谢朏",
        "author": "孙元晏",
        "content": "解玺传呼诏侍中，却来高卧岂疏慵。\n此时忠节还希有，堪羡君王特地容。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "6426a7b5b56ed02d5842a64302fb6696",
        "title": "齐 小儿执烛",
        "author": "孙元晏",
        "content": "谢公情量已难量，忠宋心诚岂暂忘。\n执烛小儿浑放去，略无言语与君王。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "3392bf096a26e6de11856f28d7e1d51e",
        "title": "齐 王僧佑",
        "author": "孙元晏",
        "content": "肯与公卿作等伦，澹然名德只推君。\n任他车骑来相访，箫鼓盈庭似不闻。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "0dda992570902d53c86ed5d3dbd990ca",
        "title": "齐 王僧虔",
        "author": "孙元晏",
        "content": "位高名重不堪疑，恳让仪同帝亦知。\n不学常流争进取，却忧门有二台司。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "38130977c78201f87edb661e0bdd9312",
        "title": "齐 明帝裹蒸",
        "author": "孙元晏",
        "content": "至尊尊贵异人间，御膳天厨岂等闲。\n惜得裹蒸无用处，不如安霸取江山。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "3c0f5facea787a28be76359293c3be2b",
        "title": "齐 郁林王",
        "author": "孙元晏",
        "content": "强哀强惨亦从伊，归到私庭喜可知。\n喜字漫书三十六，到头能得几多时。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ebbbc645b93c85094abb36758d1eb93b",
        "title": "齐 何氏小山",
        "author": "孙元晏",
        "content": "显达何曾肯繫心，筑居郊外好园林。\n赚他谢朏出山去，赢得高名直至今。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "76ed8df1d931ff0099543f190aea55b3",
        "title": "齐 王伦之",
        "author": "孙元晏",
        "content": "豫章太守重词林，图画陈蕃与华歆。\n更奠子将幷孺子，为君千载作知音。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e96ad369ea94d26f1f205a5987e48bd2",
        "title": "齐 潘妃",
        "author": "孙元晏",
        "content": "曾步金莲宠绝伦，岂甘今日委埃尘。\n玉儿还有怀恩处，不肯将身嫁小臣。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9c1074c33075b3ad52eec7247e75cd98",
        "title": "齐 王亮",
        "author": "孙元晏",
        "content": "后见梁王未免哀，奈何无计拯倾颓。\n若教彼相颠扶得，争遣明公到此来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "f4568acdac2806b0278d613b34c9033d",
        "title": "梁 分宫女",
        "author": "孙元晏",
        "content": "涤荡齐宫法令新，分张宫女二千人。\n可怜无限如花貌，重见世间桃李春。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "f19315c491c033acabe577ce14b55fd8",
        "title": "梁 马仙埤",
        "author": "孙元晏",
        "content": "齐朝太守不甘降，忠节当时动四方。\n义士要教天下见，且留君住待袁昂。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8216e930a6387127ba22af393b84ef7b",
        "title": "梁 勍敌",
        "author": "孙元晏",
        "content": "传闻天子重儒才，特为皇华绮宴开。\n今日方惊遇勍敌，此人元自北朝来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9a0dc0b0df9487a50840edec9341bbc9",
        "title": "梁 蔡撙",
        "author": "孙元晏",
        "content": "紫茄白苋以为珍，守任清真转更贫。\n不饮吴兴郡中水，古今能有几多人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "50dfde26c939f02d2e98586f4c340c0c",
        "title": "梁 楚祠",
        "author": "孙元晏",
        "content": "曾与萧侯醉玉桮，此时神影尽倾颓。\n莫云千古无灵圣，也向西川助敌来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "fecdc82602882eeae896e07b5f889f36",
        "title": "梁 谢朏小舆",
        "author": "孙元晏",
        "content": "小舆升殿掌钧台，不免无憀却忆回。\n应恨被他何胤误，悔先容易出山来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "def65d50e12502d0077e5d8bc8dbd4f1",
        "title": "梁 八关斋",
        "author": "孙元晏",
        "content": "依凭金地甚虔诚，忍溺空王为圣明。\n内殿设斋申祷祝，岂无功德及台城。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "2e67efcb782ba30d380a57e15f0b0f1f",
        "title": "梁 庾信",
        "author": "孙元晏",
        "content": "苦心词赋向谁谈，沦落周朝志岂甘。\n可惜多才庾开府，一生惆怅忆江南。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d7053d78b6a69137027cca99146c3f81",
        "title": "陈 王僧辨",
        "author": "孙元晏",
        "content": "彼此英雄各有名，石头高卧拟争衡。\n当时堪笑王僧辨，待欲将心託圣明。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "5c54b0caa4efb10df407c05ab66771ad",
        "title": "陈 武帝蚌盘",
        "author": "孙元晏",
        "content": "金翠丝黄略不舒，蚌盘清宴意何如。\n岂知三阁繁华日，解为君王妙破除。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "01cf414caa3d443231fde56128e7620f",
        "title": "陈 虞居士",
        "author": "孙元晏",
        "content": "苦谏将军总不知，几随烟焰作尘飞。\n东山居士何人识，惟有君王却许归。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "10a3eff040c4fd623d199da21ee5e53b",
        "title": "陈 姚察",
        "author": "孙元晏",
        "content": "曾佐徐陵向北游，剖陈疑事动名流。\n却归掌选清何甚，一匹花綀不肯收。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "879adf6615d338d54c2a16e31bc7be2e",
        "title": "陈 宣帝伤将卒",
        "author": "孙元晏",
        "content": "前后兵师战胜回，百余城垒尽归来。\n当时将卒应知感，况得君王为举哀。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d589a062344c6688deb4714f9aa969bf",
        "title": "陈 临春阁",
        "author": "孙元晏",
        "content": "临春高阁上侵云，风起香飘数里闻。\n自是君王正沈醉，岂知消息报隋军。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "1ec9f7ac921b7bfd6e2abb1fc231c426",
        "title": "陈 [结]绮阁",
        "author": "孙元晏",
        "content": "结绮高宜眺海涯，上凌丹汉拂云霞。\n一千朱翠同居此，争奈恩多属丽华。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c3b9d8b8a5c60cba13a30639e65731bc",
        "title": "陈 望僊阁",
        "author": "孙元晏",
        "content": "多少沈檀结筑成，望仙为号倚青冥。\n不知孔氏何形状，醉得君主不解醒。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ead438ca6e85393af504bf69cb5d7dff",
        "title": "陈 三阁",
        "author": "孙元晏",
        "content": "三阁相通绮宴开，数千朱翠遶周回。\n只知断送君王醉，不道韩擒已到来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "be29360a4edc6f0c8016657600425998",
        "title": "陈 狎客",
        "author": "孙元晏",
        "content": "八宫妃尽赋篇章，风揭歌声锦绣香。\n选得十人为狎客，有谁能解谏君王。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "5721b74053054ee07ffee51ddb5bc069",
        "title": "陈 淮水",
        "author": "孙元晏",
        "content": "文物衣冠尽入秦，六朝繁盛忽埃尘。\n自从淮水干枯后，不见王家更有人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "a727dd3a816309762d18a2ba239b222c",
        "title": "陈 江令宅",
        "author": "孙元晏",
        "content": "不向南朝立谏名，旧居基在事分明。\n令人惆怅江中令，只作篇章过一生。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "99820b897c66350d29d1958a34c890cd",
        "title": "陈 后庭舞",
        "author": "孙元晏",
        "content": "嬿婉回风态若飞，丽华翘袖玉为姿。\n后庭一曲从教舞，舞破江山君未知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9ce53e10ca126536c26a7d938a423fa9",
        "title": "吴：赤壁",
        "author": "孙元晏",
        "content": "会猎书来举国惊，只应周鲁不教迎。\n曹公一战奔波后，赤壁功传万古名。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "a4aac6ae68bdb6d1a9ce7ddc0602b4cc",
        "title": "吴：鲁肃指囷",
        "author": "孙元晏",
        "content": "破产移家事亦难，佐吴从此霸江山。\n争教不立功勋得，指出千囷如等閒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "234b509f5fc20b2a3302a5537c1bcc63",
        "title": "吴：张纮",
        "author": "孙元晏",
        "content": "东部张公与众殊，共施经略赞全吴。\n陈琳漫自称雄伯，神气应须怯大巫。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "3c8da886535e992d6bbeeecb8ef2b98d",
        "title": "晋：七宝鞭",
        "author": "孙元晏",
        "content": "天命须知岂偶然，乱臣徒欲用兵权。\n圣谟庙略还应别，浑不消他七宝鞭。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "b6b9b6004198e7fe52af8c291b06695b",
        "title": "晋：谢玄",
        "author": "孙元晏",
        "content": "百万兵来逼合肥，谢玄为将统雄师。\n旌旗首尾千余裏，浑不消他一局棋。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "873e3ca277fdff474ff6e186f99ed3a5",
        "title": "晋：蒲葵扇",
        "author": "孙元晏",
        "content": "抛舍东山岁月遥，几施经略挫雄豪。\n若非名德喧寰宇，争得蒲葵价数高。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "bc35cbc8d9e2411a3ad4ff4104823e28",
        "title": "晋：刘毅",
        "author": "孙元晏",
        "content": "绕床堪壮喝卢声，似铁容仪众尽惊。\n二十七人同举义，几人全得旧功名。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "a2ed3f52ab423a80d77af183523be18d",
        "title": "晋：王恭",
        "author": "孙元晏",
        "content": "春风濯濯柳容仪，鹤氅神情举世推。\n可惜教君仗旄钺，枉将心地托牢之。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "20550272f2f538781678ff72704b7436",
        "title": "晋：谢公赌墅",
        "author": "孙元晏",
        "content": "发遣将军欲去时，略无情挠只贪棋。\n自从乞与羊昙后，赌墅功成更有谁。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "88a68c1d8b803cfff9f71c0b82b59d79",
        "title": "晋：卫玠",
        "author": "孙元晏",
        "content": "叔宝羊车海内稀，山家女婿好风姿。\n江东士女无端甚，看杀玉人浑不知。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "95a6f4e110f93eeb56d5e582dbaaa329",
        "title": "晋：郭璞脱襦",
        "author": "孙元晏",
        "content": "吟坐因思郭景纯，每言穷达似通神。\n到头分命难移改，解脱青襦与别人。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "c715644bb633fe2049bfe8f916ca5487",
        "title": "晋：新亭",
        "author": "孙元晏",
        "content": "容易乘虚逼帝畿，满江艛橹与旌旗。\n卢循若解新亭上，胜负还应未可知。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "e832464ddf88c6977c5bd4a96129b77e",
        "title": "宋：大岘",
        "author": "孙元晏",
        "content": "大岘才过喜可知，指空言已副心期。\n公孙计策嗟无用，天与南朝作霸基。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "97224743723fb09eb62cd05735f8ab50",
        "title": "宋：放宫人",
        "author": "孙元晏",
        "content": "纳谏廷臣免犯颜，自然恩可霸江山。\n姚兴侍女方承宠，放出宫闱若等閒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "3209ae55e24c1e19bec47e54aedeac15",
        "title": "宋：黄罗襦",
        "author": "孙元晏",
        "content": "戚属群臣尽见猜，预忧身后又堪哀。\n到头委付何曾是，虚把罗襦与彦回。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "f5b64e79b5a0316817abe8f448c48fcd",
        "title": "齐：谢朏",
        "author": "孙元晏",
        "content": "解玺传呼诏侍中，却来高卧岂疏慵。\n此时忠节还稀有，堪羡君王特地容。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "2e6675a8b30482c544da0944aae51739",
        "title": "齐：明帝裹蒸",
        "author": "孙元晏",
        "content": "至尊尊贵异人间，御膳天厨岂等閒。\n惜得裹蒸无用处，不如安霸取江山。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "5c0af7fa6a91a8a8c1245a60acf36710",
        "title": "齐：何氏小山",
        "author": "孙元晏",
        "content": "显达何曾肯系心，筑居郊外好园林。\n赚他谢朏出山去，赢得高名直至今。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "628ae45bdf9b1c35303eba53de12d4fb",
        "title": "齐：王伦之",
        "author": "孙元晏",
        "content": "豫章太守重词林，图画陈蕃与华歆。\n更奠子将并孺子，为君千载作知音。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "737cb5b54a538701226b86a50065f5f4",
        "title": "梁：楚祠",
        "author": "孙元晏",
        "content": "曾与萧侯醉玉杯，此时神影尽倾颓。\n莫云千古无灵圣，也向西川助敌来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "17153690248869cd8f0164898d2050da",
        "title": "陈：王僧辨",
        "author": "孙元晏",
        "content": "彼此英雄各有名，石头高卧拟争衡。\n当时堪笑王僧辨，待欲将心托圣明。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "fbb223abf3d381c69f18e3037965a430",
        "title": "陈：临春阁",
        "author": "孙元晏",
        "content": "临春高阁上侵云，风起香飘数裏闻。\n自是君王正沈醉，岂知消息报隋军。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "3f2e62250f8f17cbc24cec812ba24528",
        "title": "陈：望仙阁",
        "author": "孙元晏",
        "content": "多少沈檀结筑成，望仙为号倚青冥。\n不知孔氏何形状，醉得君王不解醒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "430fd7669f640dae624d774d87bc0ba4",
        "title": "陈：三阁",
        "author": "孙元晏",
        "content": "三阁相通绮宴开，数千朱翠绕周回。\n只知断送君王醉，不道韩擒已到来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    }
]