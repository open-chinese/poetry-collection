[
    {
        "uuid": "b79c4a49cab1db1468991b80a8f494bb",
        "title": "吴 黄金车",
        "author": "孙元晏",
        "content": "分擘山河即渐开，许昌基业已倾颓。\n黄金车与斑斓耳，早个须知入谶来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "99ee29526917314027bc0e79cd19ba74",
        "title": "吴 赤壁",
        "author": "孙元晏",
        "content": "会猎书来举国惊，祗应周鲁不教迎。\n曹公一战奔波后，赤壁功传万古名。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "87e3d19291151e9ff8c526c03dcd1007",
        "title": "吴 鲁肃指囷",
        "author": "孙元晏",
        "content": "破产移家事亦难，佐吴从此霸江山。\n争教不立功勋得，指出千囷如等闲。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c4f0c981bc520298865999dd56a58f8b",
        "title": "吴 甘宁斫营",
        "author": "孙元晏",
        "content": "夜深偷入魏军营，满寨惊忙火似星。\n百口宝刀千匹绢，也应消得与甘宁。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8db00ecbb417de716a1e18fc7ec2c517",
        "title": "吴 徐盛",
        "author": "孙元晏",
        "content": "欲把江山鼎足分，邢真衔册到江南。\n当时将相谁堪重，徐盛将军最不甘。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "5d899e2a2ccda9011fecdb0f314eda3e",
        "title": "吴 鲁肃",
        "author": "孙元晏",
        "content": "斫案兴言断众疑，鼎分从此定雄雌。\n若无子敬心相似，争得乌林破魏师。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "cf4218fdc6424f94207394dcb1396e3f",
        "title": "吴 武昌",
        "author": "孙元晏",
        "content": "西塞山高截九垓，谶谣终日自相催。\n武昌鱼美应难恋，历数须归建业来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "4508684c4e8b769c596a5d74211628fd",
        "title": "吴 顾雍",
        "author": "孙元晏",
        "content": "赞国经纶更有谁，蔡公相叹亦相师。\n贵为丞相封侯了，归后家人总不知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "6b752c5b1dc6fb8304c3881267498e22",
        "title": "吴 吕蒙",
        "author": "孙元晏",
        "content": "幼小家贫实可哀，愿征行去志难回。\n不探虎穴求身达，争得人间富贵来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "eaa516e7c4cb2a93d8270842a345ad7c",
        "title": "吴 介象",
        "author": "孙元晏",
        "content": "好道君王遇亦难，变通灵异几多般。\n介先生有神仙术，钓得鲈鱼在玉盘。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d64cc201e1d5b66e29d4b93492689ced",
        "title": "吴 濡须坞",
        "author": "孙元晏",
        "content": "风揭洪涛响若雷，枕波为垒险相隈。\n莫言有个濡须坞，几度曹公失志回。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8ac81d13db81c978ea4bf92f447d85f0",
        "title": "吴 周泰",
        "author": "孙元晏",
        "content": "名与诸公又不同，金疮痕在满身中。\n不将御盖宣恩泽，谁信将军别有功。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "2a62f121fe4be71c38f665612e4d4a2d",
        "title": "吴 张纮",
        "author": "孙元晏",
        "content": "东部张公与众殊，共施经畧赞全吴。\n陈琳漫自称雄伯，神气应须怯大巫。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "46350208d45c65c84ed61942f5dbe49a",
        "title": "吴 太史慈",
        "author": "孙元晏",
        "content": "圣德招贤远近知，曹公心计却成欺。\n陈韩昔日尝投楚，岂是当归召得伊。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "0f56246ee195e5722fc6352ab177fcf7",
        "title": "吴 孙坚后",
        "author": "孙元晏",
        "content": "委存张公翊圣材，几将贤德赞文台。\n争教不霸江山得，日月征曾入梦来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "532fe6253013b1f37811da6998984bba",
        "title": "吴 陆统",
        "author": "孙元晏",
        "content": "将军身殁有儿孤，虎子为名教读书。\n更向宫中教骑马，感君恩重合何如。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "5dfb8ef62fb518154f3d36e1da051e84",
        "title": "吴 青盖",
        "author": "孙元晏",
        "content": "历数将终势已摧，不修君德更堪哀。\n被他青盖言相误，元是须教入晋来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "3d3a7323e853fe2511afa3cf7a6894cf",
        "title": "晋 七宝鞭",
        "author": "孙元晏",
        "content": "天命须知岂偶然，乱臣徒欲用兵权。\n圣谟庙畧还应别，浑不消他七宝鞭。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ed1f3e2f33f01862f31a5131a1cd0532",
        "title": "晋 庾悦鹅炙",
        "author": "孙元晏",
        "content": "春暖江南景气新，子鹅炙美就中珍。\n庾家厨盛刘公困，浑弗相贻也恼人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "1bc45e0b3de44efc1e73bb78fcef1079",
        "title": "晋 谢玄",
        "author": "孙元晏",
        "content": "百万兵来逼合肥，谢玄为将统雄师。\n旌旗首尾千余里，浑不消他一局棊。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e0acb0baa8115579d82ac5ff83e580ff",
        "title": "晋 谢混",
        "author": "孙元晏",
        "content": "尚主当初偶未成，此时谁合更关情。\n可怜谢混风华在，千古翻传禁脔名。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c801301e14bc7d6c9ae4135af61f2340",
        "title": "晋 陆玩",
        "author": "孙元晏",
        "content": "陆公高论亦由衷，谦让还惭未有功。\n天下忠良人欲尽，始应交我作三公。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d657ceebe335027fec6d8b21e2ba3a5f",
        "title": "晋 王坦之",
        "author": "孙元晏",
        "content": "晋祚安危只此行，坦之何必苦忧惊。\n谢公合定寰区在，争遣当时事得成。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e701d2bd280be860b5cf3b3a71a3bedb",
        "title": "晋 蒲葵扇",
        "author": "孙元晏",
        "content": "抛舍东山岁月遥，几施经畧挫雄豪。\n若非名德喧寰宇，争得蒲葵价数高。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e330d6139bc96be587cc3b6a6705343d",
        "title": "晋 王郎",
        "author": "孙元晏",
        "content": "太尉门庭亦甚高，王郎名重礼相饶。\n自家妻父犹如此，谁更逢君得折腰。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c2bcf36b7736e776ff085e9451b3ea9c",
        "title": "晋 刘毅",
        "author": "孙元晏",
        "content": "遶床堪壮喝卢声，似铁容仪众尽惊。\n二十七人同举义，几人全得旧功名。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8708f8f5bd29aeb6f5717da86502bc6d",
        "title": "晋 王恭",
        "author": "孙元晏",
        "content": "春风濯濯柳容仪，鹤氅神情举世推。\n可惜教君仗旄钺，枉将心地託牢之。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9d35dba21991dc7af96c4397165eedf6",
        "title": "晋 谢公赌墅",
        "author": "孙元晏",
        "content": "发遣将军欲去时，畧无情挠只贪棊。\n自从乞与羊昙后，赌墅功成更有谁。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e3e6f1c5574d5542208748c5110d8d8f",
        "title": "晋 苻坚投棰",
        "author": "孙元晏",
        "content": "投棰填江语未终，谢安乘此立殊功。\n三台星烂干坤在，且与张华死不同。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "0cdfcdf28ec0a58d961d1612d1fb4687",
        "title": "晋 卫玠",
        "author": "孙元晏",
        "content": "叔宝羊车海内稀，山家女壻好风姿。\n江东士女无端甚，看杀玉人浑不知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b8a582d64a927ab7eb439c60c200f115",
        "title": "晋 郭璞脱襦",
        "author": "孙元晏",
        "content": "吟坐因思郭景纯，每言穷达似通神。\n到头分命难移改，解脱清襦与别人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ebcd86da688dff5e4504607c5df68084",
        "title": "晋 庾楼",
        "author": "孙元晏",
        "content": "江州楼上月明中，从事同登眺远空。\n玉树忽薶千载后，有谁重此继清风。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "18b7340104245b6ef83b5b9341613a9f",
        "title": "晋 新亭",
        "author": "孙元晏",
        "content": "容易乘虚逼帝畿，满江艛艣与旌旗。\n卢循若解新亭上，胜负还应未可知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "4177f6b175581bacf3ca116ce912b17d",
        "title": "宋 大岘",
        "author": "孙元晏",
        "content": "大岘纔过喜可知，指空言已副心期。\n公孙计策嗟无用，天与南朝作霸基。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "90a6dfc122b96d4368a3526994a82120",
        "title": "宋 放宫人",
        "author": "孙元晏",
        "content": "纳谏廷臣免犯颜，自然恩可霸江山。\n姚兴侍女方承宠，放出宫闱若等闲。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d5d053a94c5e773fcb8b13855f5d6454",
        "title": "宋 借南苑",
        "author": "孙元晏",
        "content": "人主词应不偶然，几人曾说笑掀天。\n不知南苑今何在，借与张公三百年。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9d3cf439ba87e106832ef059093f39fc",
        "title": "宋 谢澹云霞友",
        "author": "孙元晏",
        "content": "仗气凌人岂可亲，只将范泰是知闻。\n缘何唤作云霞友，却恐云霞未似君。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ddfbfc706971d37d965b725fc7cc9733",
        "title": "宋 乌衣巷",
        "author": "孙元晏",
        "content": "古迹荒基好叹嗟，满川吟景只烟霞。\n乌衣巷在何人住，回首令人忆谢家。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "604c88611ef01d1e7bd709686d96dfce",
        "title": "宋 袁粲",
        "author": "孙元晏",
        "content": "负才尚气满朝知，高卧闲吟见客稀。\n独步何人识袁尹，白杨郊外醉方归。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9116b9404beea52bdabe4ad42fd7d239",
        "title": "宋 刘伯龙",
        "author": "孙元晏",
        "content": "位重何如不厌贫，伯龙孤子只修身。\n固知生计还须有，穷鬼临时也笑人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "41a03d6b7ee2829a43d9cd76a90bdda0",
        "title": "宋 王方平",
        "author": "孙元晏",
        "content": "拂衣耕钓已多时，江上山前乐可知。\n着却貂裘将采药，任他人唤作渔师。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d181ac28e0823bb801f75edc0ee0ee3f",
        "title": "宋 黄罗襦",
        "author": "孙元晏",
        "content": "戚属羣臣尽见猜，预忧身后又堪哀。\n到头委付何曾是，虚把罗襦与彦回。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c1ee4a2eccb6f5a8284d5964b6bdc2e9",
        "title": "宋 谢朏",
        "author": "孙元晏",
        "content": "谢家诸子尽兰香，各震芳名满帝乡。\n惟有千金更堪重，只将高卧向齐王。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "2caad9b8941f92a13ee224ed5d9f809e",
        "title": "宋 羊玄保",
        "author": "孙元晏",
        "content": "运命将来各有期，好官才阙即思之。\n就中堪爱羊玄保，偏受君王分外知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "76596c42c0eea70a1d851ef0b7c593ff",
        "title": "齐 谢朏",
        "author": "孙元晏",
        "content": "解玺传呼诏侍中，却来高卧岂疏慵。\n此时忠节还希有，堪羡君王特地容。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b43dc4f5618158f9065e842d3698139e",
        "title": "齐 小儿执烛",
        "author": "孙元晏",
        "content": "谢公情量已难量，忠宋心诚岂暂忘。\n执烛小儿浑放去，略无言语与君王。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "054e0ae2a42caab635ea634e4dafa435",
        "title": "齐 王僧佑",
        "author": "孙元晏",
        "content": "肯与公卿作等伦，澹然名德只推君。\n任他车骑来相访，箫鼓盈庭似不闻。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "46531ae0cf6ff482eb6759e16a6ee303",
        "title": "齐 王僧虔",
        "author": "孙元晏",
        "content": "位高名重不堪疑，恳让仪同帝亦知。\n不学常流争进取，却忧门有二台司。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "0c4ab6297260735ca5cc10287872c3fc",
        "title": "齐 明帝裹蒸",
        "author": "孙元晏",
        "content": "至尊尊贵异人间，御膳天厨岂等闲。\n惜得裹蒸无用处，不如安霸取江山。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8dab42b579575704bb26c753bdf96c1e",
        "title": "齐 郁林王",
        "author": "孙元晏",
        "content": "强哀强惨亦从伊，归到私庭喜可知。\n喜字漫书三十六，到头能得几多时。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "059daba89555372ae1846964adcdd2dd",
        "title": "齐 何氏小山",
        "author": "孙元晏",
        "content": "显达何曾肯繫心，筑居郊外好园林。\n赚他谢朏出山去，赢得高名直至今。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "cd3cce05576cf3edd63cafa7f031569c",
        "title": "齐 王伦之",
        "author": "孙元晏",
        "content": "豫章太守重词林，图画陈蕃与华歆。\n更奠子将幷孺子，为君千载作知音。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "6d03d3076f15792f2cf742fabce82cff",
        "title": "齐 潘妃",
        "author": "孙元晏",
        "content": "曾步金莲宠绝伦，岂甘今日委埃尘。\n玉儿还有怀恩处，不肯将身嫁小臣。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "32a279417e4caa2f0f3d0733af0975bd",
        "title": "齐 王亮",
        "author": "孙元晏",
        "content": "后见梁王未免哀，奈何无计拯倾颓。\n若教彼相颠扶得，争遣明公到此来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9b1c2d7d95438eadc6525c65b04d6ee2",
        "title": "梁 分宫女",
        "author": "孙元晏",
        "content": "涤荡齐宫法令新，分张宫女二千人。\n可怜无限如花貌，重见世间桃李春。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "39fd19cd77cdae662373775dd21735fc",
        "title": "梁 马仙埤",
        "author": "孙元晏",
        "content": "齐朝太守不甘降，忠节当时动四方。\n义士要教天下见，且留君住待袁昂。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "80da633cee442fa5aa7f6f713bd35391",
        "title": "梁 勍敌",
        "author": "孙元晏",
        "content": "传闻天子重儒才，特为皇华绮宴开。\n今日方惊遇勍敌，此人元自北朝来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "fdab88c2ce1765ce1d73c4984e549599",
        "title": "梁 蔡撙",
        "author": "孙元晏",
        "content": "紫茄白苋以为珍，守任清真转更贫。\n不饮吴兴郡中水，古今能有几多人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8d76a108f053d47400d025a9807c4166",
        "title": "梁 楚祠",
        "author": "孙元晏",
        "content": "曾与萧侯醉玉桮，此时神影尽倾颓。\n莫云千古无灵圣，也向西川助敌来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c9505832066396eb3a07c49c684fb28d",
        "title": "梁 谢朏小舆",
        "author": "孙元晏",
        "content": "小舆升殿掌钧台，不免无憀却忆回。\n应恨被他何胤误，悔先容易出山来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b017d003011e7212f86a77586c6f2b65",
        "title": "梁 八关斋",
        "author": "孙元晏",
        "content": "依凭金地甚虔诚，忍溺空王为圣明。\n内殿设斋申祷祝，岂无功德及台城。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "cb8453ae18e22712f912064607045044",
        "title": "梁 庾信",
        "author": "孙元晏",
        "content": "苦心词赋向谁谈，沦落周朝志岂甘。\n可惜多才庾开府，一生惆怅忆江南。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ae3e1179612bc5d280d00620ed74f506",
        "title": "陈 王僧辨",
        "author": "孙元晏",
        "content": "彼此英雄各有名，石头高卧拟争衡。\n当时堪笑王僧辨，待欲将心託圣明。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "f878c087fbe13b914c7b96f24a236757",
        "title": "陈 武帝蚌盘",
        "author": "孙元晏",
        "content": "金翠丝黄略不舒，蚌盘清宴意何如。\n岂知三阁繁华日，解为君王妙破除。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "e01bc6136acb8e4e8ff8ce904fc99938",
        "title": "陈 虞居士",
        "author": "孙元晏",
        "content": "苦谏将军总不知，几随烟焰作尘飞。\n东山居士何人识，惟有君王却许归。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c72ede668da276abd57a097faee3e3d9",
        "title": "陈 姚察",
        "author": "孙元晏",
        "content": "曾佐徐陵向北游，剖陈疑事动名流。\n却归掌选清何甚，一匹花綀不肯收。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8adb14827106ecd25f8aea594e443b4d",
        "title": "陈 宣帝伤将卒",
        "author": "孙元晏",
        "content": "前后兵师战胜回，百余城垒尽归来。\n当时将卒应知感，况得君王为举哀。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b324c297f9b95bb750875984a983d69a",
        "title": "陈 临春阁",
        "author": "孙元晏",
        "content": "临春高阁上侵云，风起香飘数里闻。\n自是君王正沈醉，岂知消息报隋军。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8a60c32bd6ce111c30829a762e72192d",
        "title": "陈 [结]绮阁",
        "author": "孙元晏",
        "content": "结绮高宜眺海涯，上凌丹汉拂云霞。\n一千朱翠同居此，争奈恩多属丽华。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9b5fccd6d55278655009591bc21f005b",
        "title": "陈 望僊阁",
        "author": "孙元晏",
        "content": "多少沈檀结筑成，望仙为号倚青冥。\n不知孔氏何形状，醉得君主不解醒。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "987e642893b6efe907bc91b5fa3cc66f",
        "title": "陈 三阁",
        "author": "孙元晏",
        "content": "三阁相通绮宴开，数千朱翠遶周回。\n只知断送君王醉，不道韩擒已到来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "42fa44f021082cf57bcfe3d1e675b200",
        "title": "陈 狎客",
        "author": "孙元晏",
        "content": "八宫妃尽赋篇章，风揭歌声锦绣香。\n选得十人为狎客，有谁能解谏君王。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c88845d845235f4dd0259b3d46369f1d",
        "title": "陈 淮水",
        "author": "孙元晏",
        "content": "文物衣冠尽入秦，六朝繁盛忽埃尘。\n自从淮水干枯后，不见王家更有人。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b07e81a96adf4df77d5600beec159a0f",
        "title": "陈 江令宅",
        "author": "孙元晏",
        "content": "不向南朝立谏名，旧居基在事分明。\n令人惆怅江中令，只作篇章过一生。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "81192f5cd7fa35219bcdd15cf73b00ee",
        "title": "陈 后庭舞",
        "author": "孙元晏",
        "content": "嬿婉回风态若飞，丽华翘袖玉为姿。\n后庭一曲从教舞，舞破江山君未知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "fdc21d7c313f91fd0b38ff5375843aa5",
        "title": "吴：黄金车",
        "author": "孙元晏",
        "content": "分擘山河即渐开，许昌基业已倾颓。\n黄金车与斑斓耳，早个须知入谶来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "d7181df59ff5ad569be2a1c289e1357f",
        "title": "吴：赤壁",
        "author": "孙元晏",
        "content": "会猎书来举国惊，只应周鲁不教迎。\n曹公一战奔波后，赤壁功传万古名。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "64f37559114f5d3addbfeb936ade756a",
        "title": "吴：鲁肃指囷",
        "author": "孙元晏",
        "content": "破产移家事亦难，佐吴从此霸江山。\n争教不立功勋得，指出千囷如等閒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "4a54f32066cd20475dd9234bb246f15b",
        "title": "吴：甘宁斫营",
        "author": "孙元晏",
        "content": "夜深偷入魏军营，满寨惊忙火似星。\n百口宝刀千匹绢，也应消得与甘宁。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "a1a15c87f2167e94d3b25a5ea9b9c702",
        "title": "吴：徐盛",
        "author": "孙元晏",
        "content": "欲把江山鼎足分，邢真衔册到江南。\n当时将相谁堪重，徐盛将军最不甘。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "18863ee3b28eb7ca2318eabcfa4a9a52",
        "title": "吴：鲁肃",
        "author": "孙元晏",
        "content": "斫案兴言断众疑，鼎分从此定雄雌。\n若无子敬心相似，争得乌林破魏师。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "451acbc9200537c7adcf2c5b95e23582",
        "title": "吴：武昌",
        "author": "孙元晏",
        "content": "西塞山高截九垓，谶谣终日自相催。\n武昌鱼美应难恋，历数须归建业来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "3f754688ef044bf32198d57f6329d90b",
        "title": "吴：顾雍",
        "author": "孙元晏",
        "content": "赞国经纶更有谁，蔡公相叹亦相师。\n贵为丞相封侯了，归后家人总不知。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "8c9604b1daaa41d6a5d75b963f59c3b9",
        "title": "吴：吕蒙",
        "author": "孙元晏",
        "content": "幼小家贫实可哀，愿征行去志难回。\n不探虎穴求身达，争得人间富贵来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "2a0c70c23dae0ea516882ef2fe8d78be",
        "title": "吴：介象",
        "author": "孙元晏",
        "content": "好道君王遇亦难，变通灵异几多般。\n介先生有神仙术，钓得鲈鱼在玉盘。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "0668d55e8a061e23fe001d20dbea0834",
        "title": "吴：濡须坞",
        "author": "孙元晏",
        "content": "风揭洪涛响若雷，枕波为垒险相隈。\n莫言有个濡须坞，几度曹公失志回。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "a6341705890a4897d282c2bdd554ee68",
        "title": "吴：周泰",
        "author": "孙元晏",
        "content": "名与诸公又不同，金疮痕在满身中。\n不将御盖宣恩泽，谁信将军别有功。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "db587b98ddef9fac0be58df6c18dfac1",
        "title": "吴：张纮",
        "author": "孙元晏",
        "content": "东部张公与众殊，共施经略赞全吴。\n陈琳漫自称雄伯，神气应须怯大巫。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "bf874b61bf30f3e720125203f8988e9b",
        "title": "吴：太史慈",
        "author": "孙元晏",
        "content": "圣德招贤远近知，曹公心计却成欺。\n陈韩昔日尝投楚，岂是当归召得伊。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "d2968c257cd71da39dab3c2b91bec3eb",
        "title": "吴：孙坚后",
        "author": "孙元晏",
        "content": "委存张公翊圣材，几将贤德赞文台。\n争教不霸江山得，日月征曾入梦来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "efc1a4fe4da56e155ea8376ae36ef6e5",
        "title": "吴：陆统",
        "author": "孙元晏",
        "content": "将军身殁有儿孤，虎子为名教读书。\n更向宫中教骑马，感君恩重合何如。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "421ddc5db4459c40559c13f4c4f3c6f4",
        "title": "吴：青盖",
        "author": "孙元晏",
        "content": "历数将终势已摧，不修君德更堪哀。\n被他青盖言相误，元是须教入晋来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "34a12743b18c999051750b312589d71e",
        "title": "晋：七宝鞭",
        "author": "孙元晏",
        "content": "天命须知岂偶然，乱臣徒欲用兵权。\n圣谟庙略还应别，浑不消他七宝鞭。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "d210a11a5eeade1491c5fa7921e55e28",
        "title": "晋：庾悦鹅炙",
        "author": "孙元晏",
        "content": "春暖江南景气新，子鹅炙美就中珍。\n庾家厨盛刘公困，浑弗相贻也恼人。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "7e6f987226342033b9e12b836e5f6bf6",
        "title": "晋：谢玄",
        "author": "孙元晏",
        "content": "百万兵来逼合肥，谢玄为将统雄师。\n旌旗首尾千余裏，浑不消他一局棋。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "22990c04f1420b69a41e4785d26fe26f",
        "title": "晋：谢混",
        "author": "孙元晏",
        "content": "尚主当初偶未成，此时谁合更关情。\n可怜谢混风华在，千古翻传禁脔名。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "08a060a3286deb0e0d00cbe4a70e2aa5",
        "title": "晋：陆玩",
        "author": "孙元晏",
        "content": "陆公高论亦由衷，谦让还惭未有功。\n天下忠良人欲尽，始应交我作三公。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "0cac310eb247fcb44182b892cd2705a6",
        "title": "晋：王坦之",
        "author": "孙元晏",
        "content": "晋祚安危只此行，坦之何必苦忧惊。\n谢公合定寰区在，争遣当时事得成。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "81bcf0fcb3277affe8b42b7283e7b9a3",
        "title": "晋：蒲葵扇",
        "author": "孙元晏",
        "content": "抛舍东山岁月遥，几施经略挫雄豪。\n若非名德喧寰宇，争得蒲葵价数高。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "fcb4b3a5ccd6f4141808b6e7873a87e4",
        "title": "晋：王郎",
        "author": "孙元晏",
        "content": "太尉门庭亦甚高，王郎名重礼相饶。\n自家妻父犹如此，谁更逢君得折腰。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "478dfc9872d6180b1b214cbb9427460f",
        "title": "晋：刘毅",
        "author": "孙元晏",
        "content": "绕床堪壮喝卢声，似铁容仪众尽惊。\n二十七人同举义，几人全得旧功名。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "1d89cfebfb1fe5c07cb61c3721787757",
        "title": "晋：王恭",
        "author": "孙元晏",
        "content": "春风濯濯柳容仪，鹤氅神情举世推。\n可惜教君仗旄钺，枉将心地托牢之。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "63d149a433a0aab8cb9d80bcbb53ecab",
        "title": "晋：谢公赌墅",
        "author": "孙元晏",
        "content": "发遣将军欲去时，略无情挠只贪棋。\n自从乞与羊昙后，赌墅功成更有谁。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "21660c9ebd926a6d27e411577b4711e9",
        "title": "晋：苻坚投棰",
        "author": "孙元晏",
        "content": "投棰填江语未终，谢安乘此立殊功。\n三台星烂干坤在，且与张华死不同。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "d37e2eb58361246769048f5c8436f268",
        "title": "晋：卫玠",
        "author": "孙元晏",
        "content": "叔宝羊车海内稀，山家女婿好风姿。\n江东士女无端甚，看杀玉人浑不知。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "d6719c7d407ab8dc57c7a9ff54c2c124",
        "title": "晋：郭璞脱襦",
        "author": "孙元晏",
        "content": "吟坐因思郭景纯，每言穷达似通神。\n到头分命难移改，解脱青襦与别人。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "b08bd734f2605bbcd0355900a61cace8",
        "title": "晋：庾楼",
        "author": "孙元晏",
        "content": "江州楼上月明中，从事同登眺远空。\n玉树忽薶千载后，有谁重此继清风。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "cfe5d30c389351b7c06ac97ecc319e40",
        "title": "晋：新亭",
        "author": "孙元晏",
        "content": "容易乘虚逼帝畿，满江艛橹与旌旗。\n卢循若解新亭上，胜负还应未可知。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "8928e32c274598fb465a56bc1e0b6ab8",
        "title": "宋：大岘",
        "author": "孙元晏",
        "content": "大岘才过喜可知，指空言已副心期。\n公孙计策嗟无用，天与南朝作霸基。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "5eeb75f57112aa1a4f6bfc6f6ab15959",
        "title": "宋：放宫人",
        "author": "孙元晏",
        "content": "纳谏廷臣免犯颜，自然恩可霸江山。\n姚兴侍女方承宠，放出宫闱若等閒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "7820d059685457578f73d3d4ebda5609",
        "title": "宋：借南苑",
        "author": "孙元晏",
        "content": "人主词应不偶然，几人曾说笑掀天。\n不知南苑今何在，借与张公三百年。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "0817a4516f7b43f2608fd33ec0171003",
        "title": "宋：谢澹云霞友",
        "author": "孙元晏",
        "content": "仗气凌人岂可亲，只将范泰是知闻。\n缘何唤作云霞友，却恐云霞未似君。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "61763163d71af63ffb8f6b3ca94d1b7b",
        "title": "宋：乌衣巷",
        "author": "孙元晏",
        "content": "古迹荒基好叹嗟，满川吟景只烟霞。\n乌衣巷在何人住，回首令人忆谢家。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "a7c06fc560905f2e8ad6c6ee95577e50",
        "title": "宋：袁粲",
        "author": "孙元晏",
        "content": "负才尚气满朝知，高卧闲吟见客稀。\n独步何人识袁尹，白杨郊外醉方归。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "39933ec89c37e3cb7c4256d4e4073f35",
        "title": "宋：刘伯龙",
        "author": "孙元晏",
        "content": "位重何如不厌贫，伯龙孤子只修身。\n固知生计还须有，穷鬼临时也笑人。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "5eec994bff1c2283942010511f0626c7",
        "title": "宋：王方平",
        "author": "孙元晏",
        "content": "拂衣耕钓已多时，江上山前乐可知。\n着却貂裘将采药，任他人唤作渔师。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "c20f041326c3805ef6ff222ab8d0f5b4",
        "title": "宋：黄罗襦",
        "author": "孙元晏",
        "content": "戚属群臣尽见猜，预忧身后又堪哀。\n到头委付何曾是，虚把罗襦与彦回。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "24bbbf016d7b1ea1483f582d711ed6cf",
        "title": "宋：谢朏",
        "author": "孙元晏",
        "content": "谢家诸子尽兰香，各震芳名满帝乡。\n惟有千金更堪重，只将高卧向齐王。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "1b09634204d711c16b4f0ebf1e446737",
        "title": "宋：羊玄保",
        "author": "孙元晏",
        "content": "运命将来各有期，好官才阙即思之。\n就中堪爱羊玄保，偏受君王分外知。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "28be4e11c684a3497a333323425b0485",
        "title": "齐：谢朏",
        "author": "孙元晏",
        "content": "解玺传呼诏侍中，却来高卧岂疏慵。\n此时忠节还稀有，堪羡君王特地容。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "3f4f47ba10bfd03e8fd18960b2b5e696",
        "title": "齐：小儿执烛",
        "author": "孙元晏",
        "content": "谢公情量已难量，忠宋心诚岂暂忘。\n执烛小儿浑放去，略无言语与君王。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "5db316ee0cad514b7d32e34ff46e0f89",
        "title": "齐：王僧佑",
        "author": "孙元晏",
        "content": "肯与公卿作等伦，澹然名德只推君。\n任他车骑来相访，箫鼓盈庭似不闻。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "4ce591d73146b63d07e3bd3a8606a99d",
        "title": "齐：王僧虔",
        "author": "孙元晏",
        "content": "位高名重不堪疑，恳让仪同帝亦知。\n不学常流争进取，却忧门有二台司。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "1382c4c49c2b155d3441e475e7102c83",
        "title": "齐：明帝裹蒸",
        "author": "孙元晏",
        "content": "至尊尊贵异人间，御膳天厨岂等閒。\n惜得裹蒸无用处，不如安霸取江山。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "99bbefb018d9d9f382777f3d1fa17195",
        "title": "齐：郁林王",
        "author": "孙元晏",
        "content": "强哀强惨亦从伊，归到私庭喜可知。\n喜字漫书三十六，到头能得几多时。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "f67e48429f21f870820604dc348794a4",
        "title": "齐：何氏小山",
        "author": "孙元晏",
        "content": "显达何曾肯系心，筑居郊外好园林。\n赚他谢朏出山去，赢得高名直至今。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "f84ddc1eab64a2574b7521f6ad80ada4",
        "title": "齐：王伦之",
        "author": "孙元晏",
        "content": "豫章太守重词林，图画陈蕃与华歆。\n更奠子将并孺子，为君千载作知音。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "f395ad010e10f886ebb5f18c43c25b04",
        "title": "齐：潘妃",
        "author": "孙元晏",
        "content": "曾步金莲宠绝伦，岂甘今日委埃尘。\n玉儿还有怀恩处，不肯将身嫁小臣。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "40351c28f46a3c4e83f2fcfc55b76283",
        "title": "齐：王亮",
        "author": "孙元晏",
        "content": "后见梁王未免哀，奈何无计拯倾颓。\n若教彼相颠扶得，争遣明公到此来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "c77e0f4e505a6a69137d1d7df7b592cd",
        "title": "梁：分宫女",
        "author": "孙元晏",
        "content": "涤荡齐宫法令新，分张宫女二千人。\n可怜无限如花貌，重见世间桃李春。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "f9e63c6e891fba9c8e4f3c31c6917f5f",
        "title": "梁：马仙埤",
        "author": "孙元晏",
        "content": "齐朝太守不甘降，忠节当时动四方。\n义士要教天下见，且留君住待袁昂。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "b9355e150d1ad8daf88b4f51fa330591",
        "title": "梁：勍敌",
        "author": "孙元晏",
        "content": "传闻天子重儒才，特为皇华绮宴开。\n今日方惊遇勍敌，此人元自北朝来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "2c84b969be2cd7f7dc953ae1c9cd9c01",
        "title": "梁：蔡撙",
        "author": "孙元晏",
        "content": "紫茄白苋以为珍，守任清真转更贫。\n不饮吴兴郡中水，古今能有几多人。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "de7f906c57b938efed045691a22cc938",
        "title": "梁：楚祠",
        "author": "孙元晏",
        "content": "曾与萧侯醉玉杯，此时神影尽倾颓。\n莫云千古无灵圣，也向西川助敌来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "e05a821fb4d659a2bc46f851a30b29fa",
        "title": "梁：谢朏小舆",
        "author": "孙元晏",
        "content": "小舆升殿掌钧台，不免无憀却忆回。\n应恨被他何胤误，悔先容易出山来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "ebecef4c2cfa61ed2fda5e43987ff339",
        "title": "梁：八关斋",
        "author": "孙元晏",
        "content": "依凭金地甚虔诚，忍溺空王为圣明。\n内殿设斋申祷祝，岂无功德及台城。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "cc8ee411c230549f65dab259e9483ee1",
        "title": "梁：庾信",
        "author": "孙元晏",
        "content": "苦心词赋向谁谈，沦落周朝志岂甘。\n可惜多才庾开府，一生惆怅忆江南。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "76ecbf2d7a8620a76963604be6c8b57f",
        "title": "陈：王僧辨",
        "author": "孙元晏",
        "content": "彼此英雄各有名，石头高卧拟争衡。\n当时堪笑王僧辨，待欲将心托圣明。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "6943361c62e078ee620b65e7f94d0f20",
        "title": "陈：武帝蚌盘",
        "author": "孙元晏",
        "content": "金翠丝黄略不舒，蚌盘清宴意何如。\n岂知三阁繁华日，解为君王妙破除。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "927f81d2c849d610d7ad880d6af6a856",
        "title": "陈：虞居士",
        "author": "孙元晏",
        "content": "苦谏将军总不知，几随烟焰作尘飞。\n东山居士何人识，惟有君王却许归。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "8848558ececb97058b8351b62df5d24a",
        "title": "陈：姚察",
        "author": "孙元晏",
        "content": "曾佐徐陵向北游，剖陈疑事动名流。\n却归掌选清何甚，一匹花綀不肯收。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "ee4e3ad7e1a02451377c6d37ad7db451",
        "title": "陈：宣帝伤将卒",
        "author": "孙元晏",
        "content": "前后兵师战胜回，百余城垒尽归来。\n当时将卒应知感，况得君王为举哀。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "5804accd809cc2eaf7d6d882cfacc73c",
        "title": "陈：临春阁",
        "author": "孙元晏",
        "content": "临春高阁上侵云，风起香飘数裏闻。\n自是君王正沈醉，岂知消息报隋军。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "bb985dd147399e261d34f15ccb761aa5",
        "title": "陈：结绮阁",
        "author": "孙元晏",
        "content": "结绮高宜眺海涯，上凌丹汉拂云霞。\n一千朱翠同居此，争奈恩多属丽华。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "2c8eb2a012365318fcfecec0beac079e",
        "title": "陈：望仙阁",
        "author": "孙元晏",
        "content": "多少沈檀结筑成，望仙为号倚青冥。\n不知孔氏何形状，醉得君王不解醒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "744e2c9107488131811060814c92086c",
        "title": "陈：三阁",
        "author": "孙元晏",
        "content": "三阁相通绮宴开，数千朱翠绕周回。\n只知断送君王醉，不道韩擒已到来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百六十七"
        }
    },
    {
        "uuid": "9bf2110b743e36a3f2ee9f7f5c702212",
        "title": "陈：狎客",
        "author": "孙元晏",
        "content": "八宫妃尽赋篇章，风揭歌声锦绣香。\n选得十人为狎客，有谁能解谏君王。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
