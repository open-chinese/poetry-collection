[
    {
        "uuid": "b3cd67f50134c9f8809930c5c2c8f803",
        "title": "会真诗",
        "author": "云台峰女仙",
        "content": "人世徒纷扰，其生似梦华。\n谁言今昔裏，俯首视云霞。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "be47f960243fa3f1fbf36e209a137d56",
        "title": "会真诗",
        "author": "云台峰女仙",
        "content": "几劫澄烦思，今身仅小成。\n誓将云外隐，不向世间存。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "77fadc1687eb8b4542bee1e3ede5734f",
        "title": "会真诗",
        "author": "云台峰女仙",
        "content": "绰约离尘世，从容上太清。\n云衣无绽日，鹤驾没遥程。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "28d38c419cc065eb33059eb523046bdf",
        "title": "会真诗",
        "author": "云台峰女仙",
        "content": "华岳无三尺，东瀛仅一杯。\n入云骑彩凤，歌舞上蓬莱。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "2adc896691259c632f10d19c80db6b8c",
        "title": "会真诗",
        "author": "云台峰女仙",
        "content": "共作云山侣，俱辞世界尘。\n静思前日事，抛却几年身。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    }
]