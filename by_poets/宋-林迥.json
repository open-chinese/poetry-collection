[
    {
        "uuid": "219cb1acd4d4a9efd82a72b9f1f94bc1",
        "title": "同黄秘校游连江玉泉",
        "author": "林迥",
        "content": "泉山好景微，权尹讼庭希。\n晓马破云去，夜船乘月归。\n妓歌珠不断，人醉玉相依。\n薄宦自拘者，咄哉多是非。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "35257b066b8e63ebdcd3936c7a25b707",
        "title": "四时山丹",
        "author": "林迥",
        "content": "叶剪青油药渥丹，春风随众出栏干。\n碧桃黄菊凋残后，谁伴长松到岁寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a1155f6237ab4e9bc0031cb98973000d",
        "title": "长春",
        "author": "林迥",
        "content": "洞门深不放春归，客到寻芳莫问时。\n迟日暖风花草丽，照人红艷一枝枝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4ce6ea5417b2787233302cef59f6bfd6",
        "title": "题林子默隠居",
        "author": "林迥",
        "content": "先生平昔命何非，万卷诗书一布衣。\n回首长安成底事，吴山苍翠几时归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aa9dceb3e03ca53905cbe8e9cd4b3746",
        "title": "留题宿猿洞",
        "author": "林迥",
        "content": "几年形胜晦南州，一旦声名蔼湛侯。\n夜色併来三岛月，水光分破五湖秋。\n荔枝影裏安吟榻，红藕香中繫钓舟。\n金印解还天子后，诏书征起不回头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "55fb5b898536481b5eee9669a6d813c0",
        "title": "句  其一",
        "author": "林迥",
        "content": "门前红日海千里，池上白莲山四邻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b6df7773f6a79d649f65cb276f9b5fdb",
        "title": "句  其二",
        "author": "林迥",
        "content": "青韮供盘餠面圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "582dd557230566b320d5a0855160a2e3",
        "title": "句  其三",
        "author": "林迥",
        "content": "春晚花王在处稀，山中还信正芳菲。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3e6d65e69dae8d72938d46ee1843a2d9",
        "title": "句  其四",
        "author": "林迥",
        "content": "多谢东君深属意。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eeaf2c37fc8fe04954567ecb685a9ea4",
        "title": "句  其五",
        "author": "林迥",
        "content": "过门人指朝郎宅，入室浑如野老家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1efa0cfc2772dd5930cedfe6956e3d40",
        "title": "句  其六",
        "author": "林迥",
        "content": "尺书中夜至，清晓即扬鞭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4e01a29690f09879acbeb6ce23247c82",
        "title": "句  其七",
        "author": "林迥",
        "content": "汲水僧归林下寺。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "47c3d2452eff916b60c68b4cdda6b1dd",
        "title": "句  其八",
        "author": "林迥",
        "content": "当年春榜首传名，对御如君有几人。",
        "dynasty": "宋",
        "collection_info": null
    }
]