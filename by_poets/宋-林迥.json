[
    {
        "uuid": "7799853a493d3a12ff00fca98864da40",
        "title": "同黄秘校游连江玉泉",
        "author": "林迥",
        "content": "泉山好景微，权尹讼庭希。\n晓马破云去，夜船乘月归。\n妓歌珠不断，人醉玉相依。\n薄宦自拘者，咄哉多是非。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e99c7e481665d758bd5e4f4519327ee",
        "title": "四时山丹",
        "author": "林迥",
        "content": "叶剪青油药渥丹，春风随众出栏干。\n碧桃黄菊凋残后，谁伴长松到岁寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "376dbf092a96860222a364db9da155b7",
        "title": "长春",
        "author": "林迥",
        "content": "洞门深不放春归，客到寻芳莫问时。\n迟日暖风花草丽，照人红艷一枝枝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a1fa0070a44130bc2b8bb8d22a9852e2",
        "title": "题林子默隠居",
        "author": "林迥",
        "content": "先生平昔命何非，万卷诗书一布衣。\n回首长安成底事，吴山苍翠几时归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5051dabd20ada001e990f35d10fb0bb5",
        "title": "留题宿猿洞",
        "author": "林迥",
        "content": "几年形胜晦南州，一旦声名蔼湛侯。\n夜色併来三岛月，水光分破五湖秋。\n荔枝影裏安吟榻，红藕香中繫钓舟。\n金印解还天子后，诏书征起不回头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b5a51fe9ff43f480ea6204947b213e12",
        "title": "句  其一",
        "author": "林迥",
        "content": "门前红日海千里，池上白莲山四邻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a71697802a86bea64187b004db246d47",
        "title": "句  其二",
        "author": "林迥",
        "content": "青韮供盘餠面圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "257a2124baedc8afe809fec1093456da",
        "title": "句  其三",
        "author": "林迥",
        "content": "春晚花王在处稀，山中还信正芳菲。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a8c7a39c42060c862a14800ecf58421e",
        "title": "句  其四",
        "author": "林迥",
        "content": "多谢东君深属意。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "328034ea0f5493d387803c33d2791491",
        "title": "句  其五",
        "author": "林迥",
        "content": "过门人指朝郎宅，入室浑如野老家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "29d390456f60076a9bc3ff32f57567bc",
        "title": "句  其六",
        "author": "林迥",
        "content": "尺书中夜至，清晓即扬鞭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "60c29c247e9cc71a45b9c6926fcef144",
        "title": "句  其七",
        "author": "林迥",
        "content": "汲水僧归林下寺。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6ffac21008b62c59c31aa8b2c6ce2991",
        "title": "句  其八",
        "author": "林迥",
        "content": "当年春榜首传名，对御如君有几人。",
        "dynasty": "宋",
        "collection_info": null
    }
]