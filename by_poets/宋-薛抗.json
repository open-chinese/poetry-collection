[
    {
        "uuid": "0d6698c2250ff0e4c7929cc57ae0b16d",
        "title": "县圃十绝和朱待制  其一",
        "author": "薛抗",
        "content": "春花弄晴风，昼帘垂永日。\n公退卧黄紬，放衙头懒出。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b53cd25bad22c3e2f93294c655d7593e",
        "title": "县圃十绝和朱待制  其二",
        "author": "薛抗",
        "content": "斗牛不能神，枭卢无好采。\n碌碌甘抱关，谁能媚时宰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "312c95241b5e62376c5b9ac6e191aef7",
        "title": "县圃十绝和朱待制  其三",
        "author": "薛抗",
        "content": "二亭岿然立，紫薇为题颜。\n朝晖射银鈎，伟观压海山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "acf2c053723d8d10d376b9bcd0cc2eea",
        "title": "县圃十绝和朱待制  其四",
        "author": "薛抗",
        "content": "讼牒如蜂房，未易谈笑理。\n却思买扁舟，归钓荆溪水。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7c1a2327fe40313ba2bc0254bb46768f",
        "title": "县圃十绝和朱待制  其五",
        "author": "薛抗",
        "content": "我无割鸡刀，乃当万家县。\n飞蝗不入境，感谢年谷贱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a60ad525349e5dd1068c086d8eece4f3",
        "title": "县圃十绝和朱待制  其六",
        "author": "薛抗",
        "content": "五斗虽甚微，偃鼠饮满腹。\n大书归来赋，有味数过读。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dea558fbc1cb0840b91427066e9daa73",
        "title": "县圃十绝和朱待制  其七",
        "author": "薛抗",
        "content": "焚香坐清昼，此心自如如。\n我知拙催科，考甘下下书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f761959c03745db21c23c7265cd4d682",
        "title": "县圃十绝和朱待制  其八",
        "author": "薛抗",
        "content": "桐乡文章伯，我欲执鞭从。\n明堂选擎天，云壑卧老松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "72c14cf686823a09831b224c4a7296c7",
        "title": "县圃十绝和朱待制  其九",
        "author": "薛抗",
        "content": "寿亲髮斑斑，綵服老莱戏。\n欲赋招隐诗，小山念松桂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bf9965a3448b3223772bb97580a8bfce",
        "title": "县圃十绝和朱待制  其一○",
        "author": "薛抗",
        "content": "援琴鼓流水，絃绝无知音。\n为语逐臭夫，兰芷生深林。",
        "dynasty": "宋",
        "collection_info": null
    }
]