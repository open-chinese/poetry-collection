[
    {
        "uuid": "e4d91928a511fdfc0388f9990da6d26b",
        "title": "县圃十绝和朱待制  其一",
        "author": "薛抗",
        "content": "春花弄晴风，昼帘垂永日。\n公退卧黄紬，放衙头懒出。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "86cd58c31a7ecb8ff74921a655980787",
        "title": "县圃十绝和朱待制  其二",
        "author": "薛抗",
        "content": "斗牛不能神，枭卢无好采。\n碌碌甘抱关，谁能媚时宰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b61bc5fa64a76c21a626f1a62da7eab",
        "title": "县圃十绝和朱待制  其三",
        "author": "薛抗",
        "content": "二亭岿然立，紫薇为题颜。\n朝晖射银鈎，伟观压海山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f6f515666a5e7f7c5e327b031537ed01",
        "title": "县圃十绝和朱待制  其四",
        "author": "薛抗",
        "content": "讼牒如蜂房，未易谈笑理。\n却思买扁舟，归钓荆溪水。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "28bc030eaeecadf109486b99873acb12",
        "title": "县圃十绝和朱待制  其五",
        "author": "薛抗",
        "content": "我无割鸡刀，乃当万家县。\n飞蝗不入境，感谢年谷贱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "94fc031ca4d9f477a64c439e90fee06e",
        "title": "县圃十绝和朱待制  其六",
        "author": "薛抗",
        "content": "五斗虽甚微，偃鼠饮满腹。\n大书归来赋，有味数过读。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c268c25dabf4b484ad2efc84d41b53d",
        "title": "县圃十绝和朱待制  其七",
        "author": "薛抗",
        "content": "焚香坐清昼，此心自如如。\n我知拙催科，考甘下下书。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0301ef7791d8728693a8200f45692b65",
        "title": "县圃十绝和朱待制  其八",
        "author": "薛抗",
        "content": "桐乡文章伯，我欲执鞭从。\n明堂选擎天，云壑卧老松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "78fc10be7387bd7b35f4934d1eb47548",
        "title": "县圃十绝和朱待制  其九",
        "author": "薛抗",
        "content": "寿亲髮斑斑，綵服老莱戏。\n欲赋招隐诗，小山念松桂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "24580555a7a395ee914d37b8da2dee5d",
        "title": "县圃十绝和朱待制  其一○",
        "author": "薛抗",
        "content": "援琴鼓流水，絃绝无知音。\n为语逐臭夫，兰芷生深林。",
        "dynasty": "宋",
        "collection_info": null
    }
]