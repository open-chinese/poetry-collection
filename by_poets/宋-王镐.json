[
    {
        "uuid": "39dae25d12253b63776ad63b83ac2eed",
        "title": "移竹后雨",
        "author": "王镐",
        "content": "洗红窣窣乌苣雨，落紫飕飕皂角风。\n挂起北窗聊问讯，新移来竹定惺松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "771ea7c390b54376b225bc16bd91f565",
        "title": "简州",
        "author": "王镐",
        "content": "山水三池郡，平湖坞可园。\n要成真吏隠，故作小桃源。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "704b0459c77487baa82ffe5fde1eeb1d",
        "title": "小桃源",
        "author": "王镐",
        "content": "二水清浑合，三山锦绣张。\n迹沉簿书冗，身在水云乡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e0a7c2d2bd6b995c82e2a8a81f0b4927",
        "title": "句  其一",
        "author": "王镐",
        "content": "只言弱水蓬莱远，无奈仙风道骨何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "16b711638a8693a1358dd9aedc1d72ca",
        "title": "句  其二",
        "author": "王镐",
        "content": "仁心广大麟难比，寿骨清奇鹤不如。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0fedcb9c25edcf2cd512dac9716b472d",
        "title": "句  其三",
        "author": "王镐",
        "content": "髮爽带风梳，齿疎和月潄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5ab0e47f3d2157e9feea98457711842",
        "title": "句  其四",
        "author": "王镐",
        "content": "避喧那厌雨，宜睡不思茶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ee4f5575d4a6cc4dcede89780abf6302",
        "title": "句  其五",
        "author": "王镐",
        "content": "云学催诗黑，风仍作颂清。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "63dcced6edc6ebb2ebe0386ac2bc0cf9",
        "title": "句  其六",
        "author": "王镐",
        "content": "未知真是岳，祇见半为云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2cca333cbdc99654add5529ef2bebd1c",
        "title": "句  其七",
        "author": "王镐",
        "content": "冻雪寒梅双蜡屐，澄江明月一竿丝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8833e07fa2ffd57bfbf38af49152e21a",
        "title": "句  其八",
        "author": "王镐",
        "content": "催租更扰潘邠老，付麦谁怜石曼卿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ee29f53a27ee3ea362f0659ad50df376",
        "title": "句  其九",
        "author": "王镐",
        "content": "山色两间供步障，松阴半亩当邮亭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a3cb3e5122e0dbd1a68514b2791d524a",
        "title": "句  其一○",
        "author": "王镐",
        "content": "籍通上界神仙府，身现甘泉侍从臣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5c958d35443dc56cb35f1c36dcef096c",
        "title": "句  其一一",
        "author": "王镐",
        "content": "蓬山兰若入胜概，西寺岩壁饶幽奇。",
        "dynasty": "宋",
        "collection_info": null
    }
]