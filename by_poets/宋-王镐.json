[
    {
        "uuid": "33a6f7420463e7ea8550dbe152513508",
        "title": "移竹后雨",
        "author": "王镐",
        "content": "洗红窣窣乌苣雨，落紫飕飕皂角风。\n挂起北窗聊问讯，新移来竹定惺松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f358bc4d8a8139d7955a70d7ad6350bc",
        "title": "简州",
        "author": "王镐",
        "content": "山水三池郡，平湖坞可园。\n要成真吏隠，故作小桃源。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ff7761d5252c416ed07b9821127dfb55",
        "title": "小桃源",
        "author": "王镐",
        "content": "二水清浑合，三山锦绣张。\n迹沉簿书冗，身在水云乡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6f459c13b0357644be7e4834ec9eb388",
        "title": "句  其一",
        "author": "王镐",
        "content": "只言弱水蓬莱远，无奈仙风道骨何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3264a286314ba792e5698e6fa7079ac6",
        "title": "句  其二",
        "author": "王镐",
        "content": "仁心广大麟难比，寿骨清奇鹤不如。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11f844d3538d4686db532aa74ae9c6e6",
        "title": "句  其三",
        "author": "王镐",
        "content": "髮爽带风梳，齿疎和月潄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0e4149541ebcf0cf74aacc24ce0833f9",
        "title": "句  其四",
        "author": "王镐",
        "content": "避喧那厌雨，宜睡不思茶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a2277feb76a90b2d8a52ffdb0c060bb2",
        "title": "句  其五",
        "author": "王镐",
        "content": "云学催诗黑，风仍作颂清。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4e0d3315034048059810ca5ef2c30619",
        "title": "句  其六",
        "author": "王镐",
        "content": "未知真是岳，祇见半为云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "abfddf118d60d042c9c226c41c5037ec",
        "title": "句  其七",
        "author": "王镐",
        "content": "冻雪寒梅双蜡屐，澄江明月一竿丝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f74f33fbe393b5e8de38bfe63b991b91",
        "title": "句  其八",
        "author": "王镐",
        "content": "催租更扰潘邠老，付麦谁怜石曼卿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "961af3548775af4f17d439ef5b7f9571",
        "title": "句  其九",
        "author": "王镐",
        "content": "山色两间供步障，松阴半亩当邮亭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e10575505cca09da7f9f1d040894f27c",
        "title": "句  其一○",
        "author": "王镐",
        "content": "籍通上界神仙府，身现甘泉侍从臣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f910e554bf7c7ece98e397f0772d9283",
        "title": "句  其一一",
        "author": "王镐",
        "content": "蓬山兰若入胜概，西寺岩壁饶幽奇。",
        "dynasty": "宋",
        "collection_info": null
    }
]