[
    {
        "uuid": "cc1620ff2d797e06b72b214e578c0c7d",
        "title": "隠求斋  其一",
        "author": "林汝砺",
        "content": "蜗名蝇利处樊笼，世事其如转眼空。\n惟有高人林下隠，只求明月与清风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9ebd0e381e5829b2e1441a14f46f6618",
        "title": "隠求斋  其二",
        "author": "林汝砺",
        "content": "朝同猿鸟吟，暮与麋鹿游。\n吾生有良愿，舍此当安求。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3e56b7a6e64e68c8f0641f8ebd4ab885",
        "title": "题武夷仙掌岩",
        "author": "林汝砺",
        "content": "汉武昇遐几度春，金茎事迹总无闻。\n悬崖更不承朝露，空印纤纤十指纹。",
        "dynasty": "宋",
        "collection_info": null
    }
]