[
    {
        "uuid": "85fc76fae1523044e552758230b88841",
        "title": "书驿壁  其一",
        "author": "姚嗣宗",
        "content": "踏碎贺兰石，扫清西海尘。\n布衣能效死，可惜作穷鳞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "efbe381affba25eb944c77579ce46872",
        "title": "书驿壁  其二",
        "author": "姚嗣宗",
        "content": "百越干戈未息肩，九原金鼓又轰天。\n崆峒山叟笑不语，静听松风春昼眠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "328a9606d35b6d52979c36b199e20a53",
        "title": "畅岩",
        "author": "姚嗣宗",
        "content": "石作峒房势，水随人意流。\n两岩长溅雨，五月便惊秋。\n此景多深趣，吾心真乐游。\n山翁中宿酒，欲醉五更头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fab52c2175964835aaaeba6d65a9392a",
        "title": "畅岩怀古",
        "author": "姚嗣宗",
        "content": "寒谷长留□秋气，畅岩别是一壶天。\n清风不断名犹在，还继宣尼出圣贤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7b65e086df3fd3fe738d0f12164a5f36",
        "title": "句  其一",
        "author": "姚嗣宗",
        "content": "大开双白眼，只见一青天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "587583c562b1d70d15c27d9a0d796d03",
        "title": "句  其二",
        "author": "姚嗣宗",
        "content": "井邑倦炎酷，西岩境独清。",
        "dynasty": "宋",
        "collection_info": null
    }
]