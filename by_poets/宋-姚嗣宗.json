[
    {
        "uuid": "d3f508b1f0113f6f219f9554d4712ade",
        "title": "书驿壁  其一",
        "author": "姚嗣宗",
        "content": "踏碎贺兰石，扫清西海尘。\n布衣能效死，可惜作穷鳞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "74f1f67d33bb1418609e47f84c4b2be6",
        "title": "书驿壁  其二",
        "author": "姚嗣宗",
        "content": "百越干戈未息肩，九原金鼓又轰天。\n崆峒山叟笑不语，静听松风春昼眠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ea7a7e7b3483bb0f34673ae212d69797",
        "title": "题闽中驿舍",
        "author": "姚嗣宗",
        "content": "欲挂衣冠神武门，先寻水竹渭南村。\n却将旧斩楼兰剑，买得黄牛教子孙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8f63e054468e992c2fa0530f80574be3",
        "title": "畅岩",
        "author": "姚嗣宗",
        "content": "石作峒房势，水随人意流。\n两岩长溅雨，五月便惊秋。\n此景多深趣，吾心真乐游。\n山翁中宿酒，欲醉五更头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "37e1223d86f0847199c6175e95976108",
        "title": "畅岩怀古",
        "author": "姚嗣宗",
        "content": "寒谷长留□秋气，畅岩别是一壶天。\n清风不断名犹在，还继宣尼出圣贤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3a8f99ded2b93acb377f969b002c22d7",
        "title": "句  其一",
        "author": "姚嗣宗",
        "content": "大开双白眼，只见一青天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bed2fc20eec9133058e5c3bb31bd8d64",
        "title": "句  其二",
        "author": "姚嗣宗",
        "content": "井邑倦炎酷，西岩境独清。",
        "dynasty": "宋",
        "collection_info": null
    }
]