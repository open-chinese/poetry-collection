[
    {
        "uuid": "0d8d9e10dbb3d031306f651b44a06c4f",
        "title": "徐真君祠堂东西二室次许将韵",
        "author": "陈旸",
        "content": "六题曾彻九重门，暂阻尧阶奏万言。\n今日又蒙师许我，汉廷当沛异常恩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "65db5b908495f66073e339619f4024f0",
        "title": "赵真君岩",
        "author": "陈旸",
        "content": "岩顶风云常不散，洞门日日吐烟霞。\n济时偏解为霖雨，信是神仙第一家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7d52b598dd54b675171fcd16b04214e3",
        "title": "南台沙合亭",
        "author": "陈旸",
        "content": "闽州胜概是南台，沙合相传有自来。\n一道三桥横渡处，分明平步揖天台。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "26c0a680f2d39bb74136b1c42c4fe038",
        "title": "缺题",
        "author": "陈旸",
        "content": "行艺当年帝所闻，声名久矣动簪绅。\n文场秉笔渊源厚，诗席谈经业履醇。\n白髮忽惊蒿里暮，青衫难问箬溪春。\n两京模范垂芳远，多少生徒泪满巾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "08e06fef53b4c731903bb019669ce684",
        "title": "句  其一",
        "author": "陈旸",
        "content": "地喷飞泉成雨露，根盘乔木列旌麾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "afd7a06c33c771624dd5001a01c55ca6",
        "title": "句  其二",
        "author": "陈旸",
        "content": "南台沙合半成洲，千尺虹桥橄榄舟。",
        "dynasty": "宋",
        "collection_info": null
    }
]