[
    {
        "uuid": "946ef1e628f35bedee1746f5c10fa086",
        "title": "宫词",
        "author": "翁宏",
        "content": "又是春残也，如何出翠帷。\n落花人独立，微雨燕双飞。\n寓目魂将断，经年梦亦非。\n那堪嚮秋夕，萧飒暮蟾辉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "435358dddf2b66ac9b3372a7bedd082f",
        "title": "句  其一",
        "author": "翁宏",
        "content": "风高弓力大，霜重角声干。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13c211a9a25aeec4d3b9c3888e93fef7",
        "title": "句  其三",
        "author": "翁宏",
        "content": "寒清万国土，冷斗四维根。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ef7b8b7844f60d3e382acf8d63999188",
        "title": "句  其四",
        "author": "翁宏",
        "content": "漏光残井甃，缺影背山椒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "52abf57c55e3725ea3ccd2c15536946e",
        "title": "句  其五",
        "author": "翁宏",
        "content": "万木残秋裏，孤舟半夜猿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b0494a713a12824d627af862a07c2b76",
        "title": "句  其六",
        "author": "翁宏",
        "content": "因寻买珠客，误入射猿家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4f6dfa46424921b05144d2ad50bb1153",
        "title": "句  其七",
        "author": "翁宏",
        "content": "何处残春夜，和花落古宫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd5e4855fd3d37500bb802c919d2990b",
        "title": "句  其八",
        "author": "翁宏",
        "content": "孤舟半夜雨，上国十年心。",
        "dynasty": "宋",
        "collection_info": null
    }
]