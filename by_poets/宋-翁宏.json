[
    {
        "uuid": "ee694b32eef67152a17107cee6359e9d",
        "title": "宫词",
        "author": "翁宏",
        "content": "又是春残也，如何出翠帷。\n落花人独立，微雨燕双飞。\n寓目魂将断，经年梦亦非。\n那堪嚮秋夕，萧飒暮蟾辉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2e17ae8d8a4a608890df97214ba42704",
        "title": "秋风曲",
        "author": "翁宏",
        "content": "又是秋残也，无聊意若何。\n客程江外远，归思夜深多。\n岘首飞黄叶，湘湄走白波。\n仍闻汉都护，今岁合休戈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d5a41606cf96bd25d2d4a57163f2e93d",
        "title": "赠衡山廖融南游",
        "author": "翁宏",
        "content": "病卧瘴云间，莓苔渍竹关。\n孤吟牛渚月，老忆洞庭山。\n壮志潜消尽，淳风竟未还。\n今朝忽相遇，执手一开颜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5a93f55f51a07ab9ac5f9f00e48f9816",
        "title": "句  其一",
        "author": "翁宏",
        "content": "风高弓力大，霜重角声干。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ecd9d5bb81870668e411ea78cb17a9ac",
        "title": "句  其二",
        "author": "翁宏",
        "content": "客帆来异域，别岛落蟠桃。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e99ffc70c29469687b101f1073f584d4",
        "title": "句  其三",
        "author": "翁宏",
        "content": "寒清万国土，冷斗四维根。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2caf1d0d36c2ed71d8ddb34b82bb4a88",
        "title": "句  其四",
        "author": "翁宏",
        "content": "漏光残井甃，缺影背山椒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8c095f62019155228c9051349f5249b7",
        "title": "句  其五",
        "author": "翁宏",
        "content": "万木残秋裏，孤舟半夜猿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "751d2fbdcf094608cb15cddf2e043c1a",
        "title": "句  其六",
        "author": "翁宏",
        "content": "因寻买珠客，误入射猿家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "91677831c33be06e1f710aff4546d039",
        "title": "句  其七",
        "author": "翁宏",
        "content": "何处残春夜，和花落古宫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "651b6b1ddab221486e8f87815b083ad6",
        "title": "句  其八",
        "author": "翁宏",
        "content": "孤舟半夜雨，上国十年心。",
        "dynasty": "宋",
        "collection_info": null
    }
]