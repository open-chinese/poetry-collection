[
    {
        "uuid": "5ea81e969c167dabd4cb2c05cd474960",
        "title": "春日田园杂兴",
        "author": "赵必范",
        "content": "一岁农功只在春，夫夫妇妇几艰辛。\n青门旧有种瓜地，绿野新添躬稼人。\n早把牛衣教诸子，欲修蚕具问良辰。\n夜来谷口东风过，只恐逢人问子真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3cf9869a40ecaa46293fcae7aad170cb",
        "title": "句  其一",
        "author": "赵必范",
        "content": "卜岁聊凭六壬课，有生未识五辛盘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bd79cf73fe7fa848d181274563d675bc",
        "title": "句  其二",
        "author": "赵必范",
        "content": "幸有渔樵同此乐，苦无车马自相諠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "deefb8e3d424b12733063d031d35b719",
        "title": "句  其三",
        "author": "赵必范",
        "content": "几点飞花芳径雨，一双新燕荻帘风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a147255fb5a6fcfe6cc7885282cec887",
        "title": "句  其四",
        "author": "赵必范",
        "content": "茭白生苗藏蛤地，桑黄含葚浴蚕天。",
        "dynasty": "宋",
        "collection_info": null
    }
]