[
    {
        "uuid": "4770103e30ae9d539e1fa51a84787b48",
        "title": "春日田园杂兴",
        "author": "赵必范",
        "content": "一岁农功只在春，夫夫妇妇几艰辛。\n青门旧有种瓜地，绿野新添躬稼人。\n早把牛衣教诸子，欲修蚕具问良辰。\n夜来谷口东风过，只恐逢人问子真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5942ff720dd36a8e16e593ba95cbc806",
        "title": "句  其一",
        "author": "赵必范",
        "content": "卜岁聊凭六壬课，有生未识五辛盘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "748dfb6ae22ba430e53e9a677e74fc44",
        "title": "句  其二",
        "author": "赵必范",
        "content": "幸有渔樵同此乐，苦无车马自相諠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b7cee943a739104127b576a53edf3aa7",
        "title": "句  其三",
        "author": "赵必范",
        "content": "几点飞花芳径雨，一双新燕荻帘风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b12b073c39775dd2fa921b573b6ad230",
        "title": "句  其四",
        "author": "赵必范",
        "content": "茭白生苗藏蛤地，桑黄含葚浴蚕天。",
        "dynasty": "宋",
        "collection_info": null
    }
]