[
    {
        "uuid": "80134c92ea53e1d94468102056d253b6",
        "title": "大涤洞",
        "author": "何炳然",
        "content": "杳默閟虚旷，森爽离人寰。\n玉室非所诣，回首烟中山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e2d4bcbaef368d9920893a5abea89d2c",
        "title": "天柱峰",
        "author": "何炳然",
        "content": "终古鬰岧峣，东南此撑柱。\n当时若更高，恐碍日月路。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "061a944858c208df1263d41ccfc3b28e",
        "title": "云根峰",
        "author": "何炳然",
        "content": "十洲仙子骑龙鹤，路阔云深贪舞跃。\n不知缥缈已无心，一朵飞来坠山脚。\n几度天风吹不开，数回山雨误飘来。\n直恐老龙招得去，千顷万顷游蓬莱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1fa8eebafa79880dbd7b748afbf9fbdb",
        "title": "千年古石",
        "author": "何炳然",
        "content": "怪底黄石公，乃是赤松子。\n蒲柳不自强，一见颡应泚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "14e9c874861ba24eb754f5032c6981e1",
        "title": "龙洞",
        "author": "何炳然",
        "content": "胡髯抱嵌岩，白云拥万里。\n若弄华阳珠，我欲割左耳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a44d53f479153ee2002d3dae0c283b0f",
        "title": "右凤洞",
        "author": "何炳然",
        "content": "上作千仞翔，下作千岩栖。\n箫韶在何处，苍梧云气迷。",
        "dynasty": "宋",
        "collection_info": null
    }
]