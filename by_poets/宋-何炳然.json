[
    {
        "uuid": "55545677bd57b2c9083870a782d5aaae",
        "title": "大涤洞",
        "author": "何炳然",
        "content": "杳默閟虚旷，森爽离人寰。\n玉室非所诣，回首烟中山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6671cc809f3df2679f9931d573f299d5",
        "title": "天柱峰",
        "author": "何炳然",
        "content": "终古鬰岧峣，东南此撑柱。\n当时若更高，恐碍日月路。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8019ae82b220a3e59a1721c2abb39dc2",
        "title": "云根峰",
        "author": "何炳然",
        "content": "十洲仙子骑龙鹤，路阔云深贪舞跃。\n不知缥缈已无心，一朵飞来坠山脚。\n几度天风吹不开，数回山雨误飘来。\n直恐老龙招得去，千顷万顷游蓬莱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cd3fa320e53ae3f1e261121b0076c461",
        "title": "千年古石",
        "author": "何炳然",
        "content": "怪底黄石公，乃是赤松子。\n蒲柳不自强，一见颡应泚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "15996bcf329a6917ff10eae966e2e9b4",
        "title": "龙洞",
        "author": "何炳然",
        "content": "胡髯抱嵌岩，白云拥万里。\n若弄华阳珠，我欲割左耳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dd8bfb89ea83dab5dc92412fc4e4be7b",
        "title": "右凤洞",
        "author": "何炳然",
        "content": "上作千仞翔，下作千岩栖。\n箫韶在何处，苍梧云气迷。",
        "dynasty": "宋",
        "collection_info": null
    }
]