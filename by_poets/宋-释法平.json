[
    {
        "uuid": "c2f58b636d27c6d13682d876a7c4ff2f",
        "title": "登北固楼",
        "author": "释法平",
        "content": "不负南徐眼，来看北固云。\n金焦两山小，吴楚一江分。\n雨气生苍壁，秋声起夕曛。\n半生流落恨，此日重殷勤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11c2e3898f9ab3fb8ef86cae03db3ea8",
        "title": "绝句",
        "author": "释法平",
        "content": "春风吹枕小屏寒，客舍无聊梦不安。\n花叶暗时人已去，夕阳光满旧栏干。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d18408d343eae94e8121fc72dd976b55",
        "title": "晓上人还浙东",
        "author": "释法平",
        "content": "鸟宿柴门静，更深别意倾。\n孤灯寒有晕，细雨夜无声。\n渡晓人争立，潮平橹乱鸣。\n君今成独往，老罢苦为情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2c04ea6df68e2327385e0b9a748da038",
        "title": "句",
        "author": "释法平",
        "content": "梦魂不入人閒世。",
        "dynasty": "宋",
        "collection_info": null
    }
]