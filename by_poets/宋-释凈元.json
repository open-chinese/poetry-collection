[
    {
        "uuid": "ad2341e7c191b7f56482d951b552c1e4",
        "title": "投海偈二首  其一",
        "author": "释凈元",
        "content": "我舍世间如梦，众人须我作颂。\n颂即语言边事，了取自家真梦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e878644f42e71fee5ee8f2a9728e654d",
        "title": "投海偈二首  其二",
        "author": "释凈元",
        "content": "世间人心易了，只为人多不晓。\n了即皎在目前，未了千般学道。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1a3387489d27f2a6adaf967f3a1fdb82",
        "title": "辞众偈",
        "author": "释凈元",
        "content": "会得祖师真妙诀，无得无物又无说。\n喝散乌云千万重，一点灵心明皎洁。",
        "dynasty": "宋",
        "collection_info": null
    }
]