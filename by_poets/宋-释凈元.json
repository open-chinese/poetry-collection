[
    {
        "uuid": "ff467ffd8693493b44b6f0b7c18b4c40",
        "title": "投海偈二首  其一",
        "author": "释凈元",
        "content": "我舍世间如梦，众人须我作颂。\n颂即语言边事，了取自家真梦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8c1bc3dcd4d1e5b152a358588d8caeb1",
        "title": "投海偈二首  其二",
        "author": "释凈元",
        "content": "世间人心易了，只为人多不晓。\n了即皎在目前，未了千般学道。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "723afb9a62e3fee9af2606f4bb398bb6",
        "title": "辞众偈",
        "author": "释凈元",
        "content": "会得祖师真妙诀，无得无物又无说。\n喝散乌云千万重，一点灵心明皎洁。",
        "dynasty": "宋",
        "collection_info": null
    }
]