[
    {
        "uuid": "07ef95a03c788bba9db6bdc2caf69964",
        "title": "赠王山人",
        "author": "熊知至",
        "content": "闲拄瘦筇枝，衣冠半异时。\n入城人不识，卖药俗长疑。\n饮酒难逢醉，餐松自止饥。\n华峰千嶂裏，常与白云期。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9bf1bdfde83ac88eb05018fcf88809de",
        "title": "句  其一",
        "author": "熊知至",
        "content": "岸上犹哀惧，舟中争得安。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e10ef44bafc070d425eed73e9b931d91",
        "title": "句  其二",
        "author": "熊知至",
        "content": "楼台上下火照火，车马往来人看人。",
        "dynasty": "宋",
        "collection_info": null
    }
]