[
    {
        "uuid": "b21444cb0081f6f358f78bb729e38f29",
        "title": "次韵张晋彦秋日",
        "author": "王从",
        "content": "地迥高楼目，天寒故国心。\n江鸥无万里，云木自千寻。\n门掩斜阳下，人归落叶深。\n风前怅何许，入耳越乡吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "14a84c0ca42b035511e8ce7acc4683d6",
        "title": "句  其一",
        "author": "王从",
        "content": "落木森犹力，寒山淡欲无。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "143bd9bcdf715afc5df8b4e8cb18a13f",
        "title": "句  其二",
        "author": "王从",
        "content": "凉风回远笛，暝色带归舟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8bf21abd8d211658db900438c9161218",
        "title": "句  其三",
        "author": "王从",
        "content": "尘心依水凈，归鬓与山青。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fecca5a6779f7ad022e5b31cc9211372",
        "title": "句  其四",
        "author": "王从",
        "content": "堕蕊尽教输燕子，嫩寒犹及占梨花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5178412200731d8e25691c8eeb6621c1",
        "title": "句  其五",
        "author": "王从",
        "content": "一番风雨催寒食，千里鶑花想故园。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "707f5070576d7ccc34bf07850ef93caf",
        "title": "句  其六",
        "author": "王从",
        "content": "身闲更得凭陵酒，花早殊非爱惜春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4d5f625f0f4ce9e5931878955ab03bd0",
        "title": "句  其七",
        "author": "王从",
        "content": "秋生列岫云尤薄，泉漱悬崖路更悭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2edca8d86a461c198ed3746ce7f56b80",
        "title": "句  其八",
        "author": "王从",
        "content": "临春惨不舒，盖国空自香。",
        "dynasty": "宋",
        "collection_info": null
    }
]