[
    {
        "uuid": "ad58b9d1665aa0fd749e0e356926d67e",
        "title": "次韵张晋彦秋日",
        "author": "王从",
        "content": "地迥高楼目，天寒故国心。\n江鸥无万里，云木自千寻。\n门掩斜阳下，人归落叶深。\n风前怅何许，入耳越乡吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "825fa8647ec14bf9db76ea4b26bc5df1",
        "title": "句  其一",
        "author": "王从",
        "content": "落木森犹力，寒山淡欲无。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2d49453ce0fd8ceb0504d974a552ddb5",
        "title": "句  其二",
        "author": "王从",
        "content": "凉风回远笛，暝色带归舟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4335c2bdf3033603d81dd8ab2acf9f81",
        "title": "句  其三",
        "author": "王从",
        "content": "尘心依水凈，归鬓与山青。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bcd2687f6adfeafd8d7937b2ef09bef1",
        "title": "句  其四",
        "author": "王从",
        "content": "堕蕊尽教输燕子，嫩寒犹及占梨花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a3f1de157572da016c17eccac17f2c9b",
        "title": "句  其五",
        "author": "王从",
        "content": "一番风雨催寒食，千里鶑花想故园。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fb8dcbdb6e487085f855c9a5062a1eb0",
        "title": "句  其六",
        "author": "王从",
        "content": "身闲更得凭陵酒，花早殊非爱惜春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7e11acab64d0224b883f552817755405",
        "title": "句  其七",
        "author": "王从",
        "content": "秋生列岫云尤薄，泉漱悬崖路更悭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b49436b7d59737df282a436fd9247d7f",
        "title": "句  其八",
        "author": "王从",
        "content": "临春惨不舒，盖国空自香。",
        "dynasty": "宋",
        "collection_info": null
    }
]