[
    {
        "uuid": "b377c88c3dcb7f3324e6d625df6f6148",
        "title": "后土夫人",
        "author": "蜀宫群仙",
        "content": "偶引群仙到世间，熏风殿裏醉华筵。\n等閒贪赏不归去，愁杀韦郎一觉眠。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "ecaff2e63b3ac64f3b19c4a5ed772fb3",
        "title": "王母",
        "author": "蜀宫群仙",
        "content": "沧海成尘几万秋，碧桃花发长春愁。\n不来便是数千载，周穆汉皇何处游。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "27ca6913e31f017d750b2922c82ab7e7",
        "title": "麻姑",
        "author": "蜀宫群仙",
        "content": "世间何事不潸然，得失人情命不延。\n适向蔡家厅上饮，回头已见一千年。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "40e7742e42076da9c707f536e7fba4c3",
        "title": "上元夫人",
        "author": "蜀宫群仙",
        "content": "思量往事一愁容，阿母曾邀到汉宫。\n城阙不存人不见，茂陵荒草恨无穷。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "b2a0cde92ed3b49ae2db4224766e7856",
        "title": "弄玉",
        "author": "蜀宫群仙",
        "content": "采凤飞来到禁闱，便随王母驻瑶池。\n如今记得秦楼上，偷见萧郎恼妾时。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    },
    {
        "uuid": "33b84ffd59eb27edfd18b2b4d49b7972",
        "title": "太真",
        "author": "蜀宫群仙",
        "content": "春梦悠扬生下界，一堪成笑一堪悲。\n马嵬不是无情地，自遇蓬莱睡觉时。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷八百六十三"
        }
    }
]