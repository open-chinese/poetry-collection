[
    {
        "uuid": "ef7a0ebdb82267e0bab428aa4122c487",
        "title": "渔父",
        "author": "方丰之",
        "content": "已携巨鲤换新秔，尚有鲦鯈得自烹。\n闻道烹鲜易烦碎，呼儿勿用苦为羹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c4693022ca5226578c263b8b40f9564a",
        "title": "历崎道中",
        "author": "方丰之",
        "content": "漠漠春阳接海低，蒙蒙晚雨傍山飞。\n半敧古埭无人过，时有村童护鸭归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13c4e29d6f207a9f8b85e8136ee1f178",
        "title": "绝句",
        "author": "方丰之",
        "content": "舍人早定江西派，句法须将活处参。\n参取陵阳正法眼，寒花承露落毵毵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cf28ec7efe7dde645e7a067c3bb40b9b",
        "title": "句  其一",
        "author": "方丰之",
        "content": "天女终降大居士，登伽惟挠小乘禅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3682a6ebed76423c4b769bed4e4826e8",
        "title": "句  其二",
        "author": "方丰之",
        "content": "老夫六贼销磨尽，时为幽香一败禅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "76d1aedc5b4fd0f36266568293127115",
        "title": "句  其三",
        "author": "方丰之",
        "content": "白是盐埕青是麦，谁云斥卤不堪田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "392ebd76d9bf77339e9a7012a99998a4",
        "title": "句  其四",
        "author": "方丰之",
        "content": "魂梦无金印，生涯有瓦盆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ec3843e3d825ff4ce79d5930ad43bbe9",
        "title": "句  其五",
        "author": "方丰之",
        "content": "汉室虽然和冒顿，贾生终愿繫单于。",
        "dynasty": "宋",
        "collection_info": null
    }
]