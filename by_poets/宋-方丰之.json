[
    {
        "uuid": "196362fd6d82cb7e0529fb0540149f51",
        "title": "渔父",
        "author": "方丰之",
        "content": "已携巨鲤换新秔，尚有鲦鯈得自烹。\n闻道烹鲜易烦碎，呼儿勿用苦为羹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18436c7ae757e67f0c271fd998bf367d",
        "title": "历崎道中",
        "author": "方丰之",
        "content": "漠漠春阳接海低，蒙蒙晚雨傍山飞。\n半敧古埭无人过，时有村童护鸭归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "84bd0c4a662234f9a459123b6c8db3d1",
        "title": "绝句",
        "author": "方丰之",
        "content": "舍人早定江西派，句法须将活处参。\n参取陵阳正法眼，寒花承露落毵毵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4093831fc9fc817f5c78f857cc21055e",
        "title": "句  其一",
        "author": "方丰之",
        "content": "天女终降大居士，登伽惟挠小乘禅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cc0d743095898520a28c5f69ce813cd9",
        "title": "句  其二",
        "author": "方丰之",
        "content": "老夫六贼销磨尽，时为幽香一败禅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "afa7cd0d9944ccf0442facaf681db62d",
        "title": "句  其三",
        "author": "方丰之",
        "content": "白是盐埕青是麦，谁云斥卤不堪田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "97386f1190d13b86f3e0e67127a5c5cd",
        "title": "句  其四",
        "author": "方丰之",
        "content": "魂梦无金印，生涯有瓦盆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0736c260859fc2d0fa2a178549f43f3b",
        "title": "句  其五",
        "author": "方丰之",
        "content": "汉室虽然和冒顿，贾生终愿繫单于。",
        "dynasty": "宋",
        "collection_info": null
    }
]