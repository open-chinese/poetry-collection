[
    {
        "uuid": "b6466c76016098ceccf5c41469e6dded",
        "title": "三句颂 函盖干坤",
        "author": "道禅师",
        "content": "干坤并万象，地狱及天堂。\n物物皆真见，头头用不伤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8837794d81459e04bc7e86411825c22e",
        "title": "三句颂 截断众流",
        "author": "道禅师",
        "content": "堆山积岳来，一一尽尘埃。\n更拟论玄妙，冰消解瓦摧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d3caae5b741c7f89ff8e1e8d4c48b300",
        "title": "三句颂 随波逐浪",
        "author": "道禅师",
        "content": "辩口利舌问，高低总不亏。\n还知应病药，诊候在临时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "540285bf8f74e6a76869741a2b8b8474",
        "title": "三句外",
        "author": "道禅师",
        "content": "当人如举唱，三句岂能该。\n有问如何事，南岳与天台。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6f9dd33bb57661105f1359c0ddd04a2f",
        "title": "抬荐商量",
        "author": "道禅师",
        "content": "相见不扬眉，君东我亦西。\n红霞穿碧落，白日绕须弥。",
        "dynasty": "宋",
        "collection_info": null
    }
]