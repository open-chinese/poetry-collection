[
    {
        "uuid": "24e9a62bfc2eb3f4dbfefeb13398f2d2",
        "title": "三句颂 函盖干坤",
        "author": "道禅师",
        "content": "干坤并万象，地狱及天堂。\n物物皆真见，头头用不伤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "be65c12252104fbab08cae55f811c15d",
        "title": "三句颂 截断众流",
        "author": "道禅师",
        "content": "堆山积岳来，一一尽尘埃。\n更拟论玄妙，冰消解瓦摧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d569552ba1b17ac45d7014c8881ed00f",
        "title": "三句颂 随波逐浪",
        "author": "道禅师",
        "content": "辩口利舌问，高低总不亏。\n还知应病药，诊候在临时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f1382790ac8c21f000079936b1b82a94",
        "title": "三句外",
        "author": "道禅师",
        "content": "当人如举唱，三句岂能该。\n有问如何事，南岳与天台。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2cecca6be476bb816d6fc4da76c435da",
        "title": "抬荐商量",
        "author": "道禅师",
        "content": "相见不扬眉，君东我亦西。\n红霞穿碧落，白日绕须弥。",
        "dynasty": "宋",
        "collection_info": null
    }
]