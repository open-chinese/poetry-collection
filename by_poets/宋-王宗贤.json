[
    {
        "uuid": "1e72a335a29e7437314eb9fae61913b0",
        "title": "舫斋",
        "author": "王宗贤",
        "content": "满船载月归时趣，泛宅浮家避世狂。\n趺坐舫斋能静嘿，神驰游宴无何乡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fc44bba372de267608537c94ad5cb13c",
        "title": "题漱玉馆",
        "author": "王宗贤",
        "content": "寓馆欣传漱玉名，泉流古窦响琮琤。\n幽窗偃息惊回梦，误听虚檐雨滴声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "022fb4a442af9ad10cab704a94fa65ff",
        "title": "茶岭",
        "author": "王宗贤",
        "content": "秀钟天柱产灵芝，造就仙芽发嫩旗。\n采向先春烟露裏，喊山凤尾岂为奇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1bc103114e914be1d4b65ab1814ab6a1",
        "title": "九锁山道中",
        "author": "王宗贤",
        "content": "山萦九锁路幽深，古木高森积翠阴。\n自是仙家风景异，欣欣杖策步云林。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1e9ae71fff9dd94adcb212ac52dc66b7",
        "title": "九锁山路口",
        "author": "王宗贤",
        "content": "九锁山深隠洞天，一峰柱耸现云巅。\n从兹可达栖真境，透入青霄会众仙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5be9e198e8b3499e57a447c77dc2965a",
        "title": "大涤山",
        "author": "王宗贤",
        "content": "穹崇千尺压群峦，终日登临兴未阑。\n佳趣清幽尘自涤，昇天犹有许君坛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5340535575729508561911763de844f4",
        "title": "大涤洞",
        "author": "王宗贤",
        "content": "符札深投进玉函，琅琅震响穴难探。\n隔凡不许游人到，一窦中藏万丈潭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3466a5aac0ed26f19228b3d57f3c0068",
        "title": "天柱山",
        "author": "王宗贤",
        "content": "擎天一柱出群峰，时见真仙隠化中。\n瑞应中都成福地，伯元主宰有神功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "529b9e704dba6c897b27ef588c1ab613",
        "title": "龙蜕洞",
        "author": "王宗贤",
        "content": "羽人游洞获龙骸，知是鳞潜蜕骨来。\n岩穴嵌巉深数丈，惊人蛇虺隔凡才。",
        "dynasty": "宋",
        "collection_info": null
    }
]