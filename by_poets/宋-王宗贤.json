[
    {
        "uuid": "fb3243fd333079b5d4bab0b752e1467f",
        "title": "舫斋",
        "author": "王宗贤",
        "content": "满船载月归时趣，泛宅浮家避世狂。\n趺坐舫斋能静嘿，神驰游宴无何乡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fc2de4cb3705b3e9a39a2104be14458d",
        "title": "题漱玉馆",
        "author": "王宗贤",
        "content": "寓馆欣传漱玉名，泉流古窦响琮琤。\n幽窗偃息惊回梦，误听虚檐雨滴声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "51c9d281dcac535dad2af4bc34bebd2e",
        "title": "茶岭",
        "author": "王宗贤",
        "content": "秀钟天柱产灵芝，造就仙芽发嫩旗。\n采向先春烟露裏，喊山凤尾岂为奇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "61865c89cddf9c20596d9477d363dee8",
        "title": "九锁山道中",
        "author": "王宗贤",
        "content": "山萦九锁路幽深，古木高森积翠阴。\n自是仙家风景异，欣欣杖策步云林。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b0cf311b99e866da1d22e079189ff502",
        "title": "九锁山路口",
        "author": "王宗贤",
        "content": "九锁山深隠洞天，一峰柱耸现云巅。\n从兹可达栖真境，透入青霄会众仙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9a16d5d4a0d7975079d94049473adbe1",
        "title": "大涤山",
        "author": "王宗贤",
        "content": "穹崇千尺压群峦，终日登临兴未阑。\n佳趣清幽尘自涤，昇天犹有许君坛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d3b20dff3386df0b9eab3ac59354c875",
        "title": "大涤洞",
        "author": "王宗贤",
        "content": "符札深投进玉函，琅琅震响穴难探。\n隔凡不许游人到，一窦中藏万丈潭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cb49182e41ea96babc359524dc223b80",
        "title": "天柱山",
        "author": "王宗贤",
        "content": "擎天一柱出群峰，时见真仙隠化中。\n瑞应中都成福地，伯元主宰有神功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "65a4db1336f641da6e0a75120a403a0e",
        "title": "龙蜕洞",
        "author": "王宗贤",
        "content": "羽人游洞获龙骸，知是鳞潜蜕骨来。\n岩穴嵌巉深数丈，惊人蛇虺隔凡才。",
        "dynasty": "宋",
        "collection_info": null
    }
]