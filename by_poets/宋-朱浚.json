[
    {
        "uuid": "fcbc6a717ffca0f235ead13ea35215a3",
        "title": "寄友",
        "author": "朱浚",
        "content": "溪月知相忆，江云约共吟。\n夜船书一纸，飞过赤枫林。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5a93032bfd1163c7864f3bb393fab94c",
        "title": "秋日西湖",
        "author": "朱浚",
        "content": "几州陵变谷，经月雨为霖。\n世事今如此，西风动客心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "364335ab53eddc8eac5addc760908a31",
        "title": "赠卖墨吕云叔访陈烟山  其一",
        "author": "朱浚",
        "content": "仙宗传诀自分明，未数当年老树精。\n一片紫云光滴滴，丹墀成就几公卿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "418e7e7a605597e7c8bf466f96e85a3f",
        "title": "赠卖墨吕云叔访陈烟山  其二",
        "author": "朱浚",
        "content": "蓬莱仙子世偏珍，琮璧多携问楚津。\n若向积烟峰下过，殷勤传语卧楼人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5e18fc74fe08397f9ef3bae745359159",
        "title": "都城初秋和赵京倅  其一",
        "author": "朱浚",
        "content": "从来美玉价连城，结佩乘风十二京。\n携上峥嵘今有约，好留彩笔调清平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d9154ac7e117d601bfc3d2184ead39d5",
        "title": "都城初秋和赵京倅  其二",
        "author": "朱浚",
        "content": "暑梦虫虫似醉中，片云挟雨过长空。\n不知秋色来多少，分数暖凉卜晚风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a4c6f00c2c2d497e37698f7d2740f1a",
        "title": "都城初秋和赵京倅  其三",
        "author": "朱浚",
        "content": "宇宙清愁望眼中，谁将凉雨借龙公。\n秋风夜入关河迥，破贼红旗𥏌晓空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2e58a670b3669cb2a34fce33d0777086",
        "title": "句  其一",
        "author": "朱浚",
        "content": "寺钟城鼓两相和，共醉烟关感慨多。\n堤柳垂金春几许，檐花鸣玉夜如何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4d08cca96860b2a66506e0c9c675b287",
        "title": "句  其二",
        "author": "朱浚",
        "content": "西风落叶长安梦，晚节寒华老圃心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0b8241390c125ca36e39a626cdf3345b",
        "title": "句  其三",
        "author": "朱浚",
        "content": "玉帐分兵赋采薇，梅花吹雪上戎衣。\n将军意气中江楫，丞相功名采石矶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2ff41ecff179f7a3b633a3e292f07462",
        "title": "句  其四",
        "author": "朱浚",
        "content": "红云近处催班急，只待春前奏凯归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fa1ce0a1e2f7add9f5cf0e8b7cca48ad",
        "title": "句  其五",
        "author": "朱浚",
        "content": "蓬莱春思重云上，江汉秋情十载前。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b9cfa25a1d9bf96fec61e2e1d8458baf",
        "title": "句  其六",
        "author": "朱浚",
        "content": "金寒霜气山无虎，剑动星光水有龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8a2d7fcdfdb3d0eb2acf8cc5200328e4",
        "title": "句  其七",
        "author": "朱浚",
        "content": "皇帝当年第九春，宫符仍诏刻麒麟。\n军咨祭酒金台客，国子先生铁壁人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c908c78bf6626935eb5c09f18e258d04",
        "title": "句  其八",
        "author": "朱浚",
        "content": "明明采石兵前样，参赞归来即近臣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1e4918815e6d422667c7752cb93a5530",
        "title": "句  其九",
        "author": "朱浚",
        "content": "落花水暖千船聚，芳草春深一鹤归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed4514fc18a5302021bb3c41f77c7278",
        "title": "句  其一○",
        "author": "朱浚",
        "content": "米车已过尘迷路，渔网纔收月满船。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "99ab674488251f085ca11d880e081c5f",
        "title": "句  其一一",
        "author": "朱浚",
        "content": "海燕故人意，杜鹃游子心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "865614f2ea6bf52bb77854f36cbe8d3a",
        "title": "句  其一二",
        "author": "朱浚",
        "content": "故里千山隔，终年一字无。",
        "dynasty": "宋",
        "collection_info": null
    }
]