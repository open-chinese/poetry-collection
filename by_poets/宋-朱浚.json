[
    {
        "uuid": "93f7f5aec6720726079a517ed44b52b9",
        "title": "寄友",
        "author": "朱浚",
        "content": "溪月知相忆，江云约共吟。\n夜船书一纸，飞过赤枫林。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8380c463c29e1b2ff281295cf2508a38",
        "title": "秋日西湖",
        "author": "朱浚",
        "content": "几州陵变谷，经月雨为霖。\n世事今如此，西风动客心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "181c904715506a8f498bcc3cc5555b07",
        "title": "赠卖墨吕云叔访陈烟山  其一",
        "author": "朱浚",
        "content": "仙宗传诀自分明，未数当年老树精。\n一片紫云光滴滴，丹墀成就几公卿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3fb725968978467cc7fe7c6276346787",
        "title": "赠卖墨吕云叔访陈烟山  其二",
        "author": "朱浚",
        "content": "蓬莱仙子世偏珍，琮璧多携问楚津。\n若向积烟峰下过，殷勤传语卧楼人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cecb12f91feefac25776970321558c1e",
        "title": "都城初秋和赵京倅  其一",
        "author": "朱浚",
        "content": "从来美玉价连城，结佩乘风十二京。\n携上峥嵘今有约，好留彩笔调清平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "99be6bd7fa07eda0c406d2b7077b0454",
        "title": "都城初秋和赵京倅  其二",
        "author": "朱浚",
        "content": "暑梦虫虫似醉中，片云挟雨过长空。\n不知秋色来多少，分数暖凉卜晚风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "96b9499961a8f21346dd5fbfb8070ec4",
        "title": "都城初秋和赵京倅  其三",
        "author": "朱浚",
        "content": "宇宙清愁望眼中，谁将凉雨借龙公。\n秋风夜入关河迥，破贼红旗𥏌晓空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "47fd27e8e4773726c108b4503ddf83c3",
        "title": "句  其一",
        "author": "朱浚",
        "content": "寺钟城鼓两相和，共醉烟关感慨多。\n堤柳垂金春几许，檐花鸣玉夜如何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a67f047f2d8f72ae11e04f84efaf28ef",
        "title": "句  其二",
        "author": "朱浚",
        "content": "西风落叶长安梦，晚节寒华老圃心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d698cac6a16920eead3aaae54be62c04",
        "title": "句  其三",
        "author": "朱浚",
        "content": "玉帐分兵赋采薇，梅花吹雪上戎衣。\n将军意气中江楫，丞相功名采石矶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d3ac93f7655062772d60772c09391b17",
        "title": "句  其四",
        "author": "朱浚",
        "content": "红云近处催班急，只待春前奏凯归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f4e548cd1b9ac830768d361769672cf9",
        "title": "句  其五",
        "author": "朱浚",
        "content": "蓬莱春思重云上，江汉秋情十载前。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1f0fa0e21383936b6973562b810b4e14",
        "title": "句  其六",
        "author": "朱浚",
        "content": "金寒霜气山无虎，剑动星光水有龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "506b2fef69f8d2ab408b978809cfbd26",
        "title": "句  其七",
        "author": "朱浚",
        "content": "皇帝当年第九春，宫符仍诏刻麒麟。\n军咨祭酒金台客，国子先生铁壁人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9f48d56784ec18551c270419dca07330",
        "title": "句  其八",
        "author": "朱浚",
        "content": "明明采石兵前样，参赞归来即近臣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8e27401792d3f172bd947f8b4639ad5f",
        "title": "句  其九",
        "author": "朱浚",
        "content": "落花水暖千船聚，芳草春深一鹤归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c9ec88a4de4bb38ff157e91a9416d7e2",
        "title": "句  其一○",
        "author": "朱浚",
        "content": "米车已过尘迷路，渔网纔收月满船。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cc4202b9dcbf30ba789f403eda1ba65e",
        "title": "句  其一一",
        "author": "朱浚",
        "content": "海燕故人意，杜鹃游子心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "482b9c45786c6fcd1f4966fd48281319",
        "title": "句  其一二",
        "author": "朱浚",
        "content": "故里千山隔，终年一字无。",
        "dynasty": "宋",
        "collection_info": null
    }
]