[
    {
        "uuid": "d2505e2d184e3c4dc99f4afb080704f6",
        "title": "望江南・忆江南 百尺长藤垂到地",
        "author": "邵伯温",
        "content": "百尺长藤垂到地，千株乔木密参天。\n只在郡城边。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "49c94bc814929c15ebb1455d904d626f",
        "title": "调笑・调笑令 翻翻绣袖上红茵",
        "author": "邵伯温",
        "content": "翻翻绣袖上红茵。\n舞姬犹是旧精神。\n坐中莫怪无欢意，我与将军是故人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0bdb47d110fc6a4b98240ee6b9f720fd",
        "title": "李氏席上有感",
        "author": "邵伯温",
        "content": "翻翻绣袖上红裀，舞姬犹是旧精神。\n坐中莫怪无欢意，我与将军是故人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0a5b2a534707bd62c8c10d3827dfbf13",
        "title": "元夕",
        "author": "邵伯温",
        "content": "从昔遨游盛两川，充城人物自骈阗。\n万家灯火春风陌，十里绮罗明月天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f8b17ecae162728c12552a1a39f99b54",
        "title": "郡治五绝",
        "author": "邵伯温",
        "content": "蜀地春争一月天，杂花新岁已争妍。\n却愁取次都开尽，辜负清明谷雨天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "03aef623beb749ddf358e1b56ce18bec",
        "title": "充城口号",
        "author": "邵伯温",
        "content": "山围翠合水连云，万室楼台照眼明。\n胜地风淳真乐国，四川惟说好充城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c407c9b1cdd154efb38b6c34b9e9bd7f",
        "title": "观进黄柑",
        "author": "邵伯温",
        "content": "果州多黄柑，初比橘柚贱。\n一朝贡神州，妙极天下选。\n使者谨共时，顿首乃敢遣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9de610d303499fc02824e88c049fb6a1",
        "title": "黄柑",
        "author": "邵伯温",
        "content": "果山仙果秀天香，处处圆金树树黄。\n书后欲题三百颗，满林犹待洞庭霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "368913d644784e2f174c4e41db515673",
        "title": "柑园",
        "author": "邵伯温",
        "content": "守臣方效献芹心，直拟移根到上林。\n充贡先时犹茂绿，置邮须更待黄深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c3da03ed0187beb395ac1951c9adaec5",
        "title": "过渭城馆",
        "author": "邵伯温",
        "content": "又将鞍马送残春，吹尽征衣染尽尘。\n还是前年旧时候，渭城花柳一番新。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8246189c7e615b07c15011d208c8bd39",
        "title": "句  其一",
        "author": "邵伯温",
        "content": "屈原宅畔蕙兰怨，神女祠边云雨愁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "296d4601f2a56213506b72ac3289ee28",
        "title": "句  其二",
        "author": "邵伯温",
        "content": "阆苑南边第一州，江山胜绝对城楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9719a0e97d7105aee7c633259dd1444",
        "title": "句  其三",
        "author": "邵伯温",
        "content": "老守自惭无善政，衰公不称作遨头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f38b5e226b32fe80b9b99ce0de585c96",
        "title": "句  其四",
        "author": "邵伯温",
        "content": "白头老守无风味，不解寻春只杜门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "017eef3397bbf57f35800f812f5820c6",
        "title": "句  其五",
        "author": "邵伯温",
        "content": "楼外江山无限好，二年能得几回看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6f2f839fcbfae0a2971aa19271934b1e",
        "title": "句  其六",
        "author": "邵伯温",
        "content": "自昔充城号奥区，蜀人唤作小成都。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "12c29daac6009db7b1234f64f25f8aa5",
        "title": "句  其七",
        "author": "邵伯温",
        "content": "今日行人去果州，倾城出送旧遨头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18aedb1f0123f991ff501ab1c5386a81",
        "title": "句  其八",
        "author": "邵伯温",
        "content": "充城繁盛冠东川。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8334f3308bba45b669e57205475986f6",
        "title": "句  其九",
        "author": "邵伯温",
        "content": "春去秋来好风月，鹤楼端胜庾公楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f7921ff21421fd9fdd3093bc1ed711fc",
        "title": "句  其一○",
        "author": "邵伯温",
        "content": "秀近亭边回首处，郡城高插水云间。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fbe2c6f6b62939330acbe3e45944df86",
        "title": "句  其一一",
        "author": "邵伯温",
        "content": "江上危亭名四水，山头禅剎占三嵎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ef902518124fc5aa3a7a6d2c8021f014",
        "title": "句  其一二",
        "author": "邵伯温",
        "content": "千载登真谢自然，半山遗迹号金泉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e0b6c375648dd8a64f808ccacd3af11e",
        "title": "句  其一三",
        "author": "邵伯温",
        "content": "南充到日黄柑熟，待摘霜包寄远情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e23a6288d084f890155d2862c395a2e8",
        "title": "句  其一四",
        "author": "邵伯温",
        "content": "霜后秋香千树橘，雨余春色一川花。",
        "dynasty": "宋",
        "collection_info": null
    }
]