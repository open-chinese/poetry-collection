[
    {
        "uuid": "991001eff5203967a6ed5ed3cb017255",
        "title": "望江南・忆江南 百尺长藤垂到地",
        "author": "邵伯温",
        "content": "百尺长藤垂到地，千株乔木密参天。\n只在郡城边。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "123d89dc7ed6d181c7b69d7c4d2dfe73",
        "title": "调笑・调笑令 翻翻绣袖上红茵",
        "author": "邵伯温",
        "content": "翻翻绣袖上红茵。\n舞姬犹是旧精神。\n坐中莫怪无欢意，我与将军是故人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0de9f874ab81e6059b82ae7fe10b2d7d",
        "title": "李氏席上有感",
        "author": "邵伯温",
        "content": "翻翻绣袖上红裀，舞姬犹是旧精神。\n坐中莫怪无欢意，我与将军是故人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9d8bee525efb398b8d0a9fa77ffc981b",
        "title": "元夕",
        "author": "邵伯温",
        "content": "从昔遨游盛两川，充城人物自骈阗。\n万家灯火春风陌，十里绮罗明月天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fdbe93555844e21df9659aedee1840f6",
        "title": "郡治五绝",
        "author": "邵伯温",
        "content": "蜀地春争一月天，杂花新岁已争妍。\n却愁取次都开尽，辜负清明谷雨天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cef683e236c4031e8c3d65425df6f552",
        "title": "充城口号",
        "author": "邵伯温",
        "content": "山围翠合水连云，万室楼台照眼明。\n胜地风淳真乐国，四川惟说好充城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8a6c99e6fc3f8b501f42353c8595224e",
        "title": "观进黄柑",
        "author": "邵伯温",
        "content": "果州多黄柑，初比橘柚贱。\n一朝贡神州，妙极天下选。\n使者谨共时，顿首乃敢遣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b6effac7f6c6175a3c19053aaee584b2",
        "title": "黄柑",
        "author": "邵伯温",
        "content": "果山仙果秀天香，处处圆金树树黄。\n书后欲题三百颗，满林犹待洞庭霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13695766237ef067d167b99d57c6cc3b",
        "title": "柑园",
        "author": "邵伯温",
        "content": "守臣方效献芹心，直拟移根到上林。\n充贡先时犹茂绿，置邮须更待黄深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd46bc2e95b8bbd1ba4f1945f257900d",
        "title": "过渭城馆",
        "author": "邵伯温",
        "content": "又将鞍马送残春，吹尽征衣染尽尘。\n还是前年旧时候，渭城花柳一番新。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "27ef73098fdd4de7334590bcbb6df460",
        "title": "句  其一",
        "author": "邵伯温",
        "content": "屈原宅畔蕙兰怨，神女祠边云雨愁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "665a4b2cc79883e91c40fdcad501fe78",
        "title": "句  其二",
        "author": "邵伯温",
        "content": "阆苑南边第一州，江山胜绝对城楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dcad4b86f932374a73763c2548de5f29",
        "title": "句  其三",
        "author": "邵伯温",
        "content": "老守自惭无善政，衰公不称作遨头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "79b377c6860b7041be28ea2f89a2c58c",
        "title": "句  其四",
        "author": "邵伯温",
        "content": "白头老守无风味，不解寻春只杜门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "adc0f638df1863761480c72580f0bd14",
        "title": "句  其五",
        "author": "邵伯温",
        "content": "楼外江山无限好，二年能得几回看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "04482f4960ccbaf08d1c53b87855d5d2",
        "title": "句  其六",
        "author": "邵伯温",
        "content": "自昔充城号奥区，蜀人唤作小成都。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "34d6ce5eba50edb3d370bf59de212497",
        "title": "句  其七",
        "author": "邵伯温",
        "content": "今日行人去果州，倾城出送旧遨头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "db456c7ca39456805b70d3383777df5e",
        "title": "句  其八",
        "author": "邵伯温",
        "content": "充城繁盛冠东川。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d2475ccc24497d9a4c009b7a84f242fc",
        "title": "句  其九",
        "author": "邵伯温",
        "content": "春去秋来好风月，鹤楼端胜庾公楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e23448aba29870ab9baab1436ce8510f",
        "title": "句  其一○",
        "author": "邵伯温",
        "content": "秀近亭边回首处，郡城高插水云间。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f2d8bf50f5146e1f87ba1ac21efaf502",
        "title": "句  其一一",
        "author": "邵伯温",
        "content": "江上危亭名四水，山头禅剎占三嵎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d76d044f898603dc39fc624f2fc7e5b3",
        "title": "句  其一二",
        "author": "邵伯温",
        "content": "千载登真谢自然，半山遗迹号金泉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "554105b2bcfb3c2ea98bbc05f5a3a148",
        "title": "句  其一三",
        "author": "邵伯温",
        "content": "南充到日黄柑熟，待摘霜包寄远情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a1786fe824ba8d810b807fd6284c3c0c",
        "title": "句  其一四",
        "author": "邵伯温",
        "content": "霜后秋香千树橘，雨余春色一川花。",
        "dynasty": "宋",
        "collection_info": null
    }
]