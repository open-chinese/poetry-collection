[
    {
        "uuid": "ede33b7b7986ac720be2d2eba0dd5b5f",
        "title": "福唐元夕三首  其一",
        "author": "方孝能",
        "content": "街头如昼火山红，酒面生鳞锦障风。\n佳客醉醒春色裏，新妆歌舞月明中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f9bb52cc546760310eb4a63b21048498",
        "title": "福唐元夕三首  其二",
        "author": "方孝能",
        "content": "薄薄春衫新缕金，樽前风细怯轻阴。\n酒香隠约生红粉，正与桃花共浅深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d7b8e23aafd4dbe1c676b00077886dae",
        "title": "福唐元夕三首  其三",
        "author": "方孝能",
        "content": "灯外风摇沽酒斾，月中人数买花钱。\n少年心绪如飞絮，争逐遗香拾坠钿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c0c516baae80e5f0b9a9cb19b09ed182",
        "title": "句",
        "author": "方孝能",
        "content": "壶公簪绂数如沙，三世青云在一家。",
        "dynasty": "宋",
        "collection_info": null
    }
]