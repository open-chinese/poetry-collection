[
    {
        "uuid": "63a226713a4aefd31a24a4e47f109d33",
        "title": "诗三首  其一",
        "author": "严中和",
        "content": "山中道人不裹巾，一片石床生绿鳞。\n昨夜瓦瓶冰冻破，梅花无水自精神。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6ec345b884197799a1f16350bb6adcd5",
        "title": "诗三首  其二",
        "author": "严中和",
        "content": "梅天雨气入帘栊，衣润频添柏火烘。\n四月江南无矮树，人家都在绿阴中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "af48fd2f632f2bf304cfa5b05520b138",
        "title": "诗三首  其三",
        "author": "严中和",
        "content": "一声杜宇三更后，向晓绿阴连碧苔。\n却恨楼高春数尺，柳绵飞不上窗来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1cf85fb660836c705d58250f1e2a7d1f",
        "title": "月夜看桂",
        "author": "严中和",
        "content": "玉杵声中舞素娥，西风叶叶自婆娑。\n一天清气无人管，惟有山楼树影多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b0527592be698f5e774204899338d2da",
        "title": "句  其一",
        "author": "严中和",
        "content": "树影踏不碎，花香嗅却无。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d993dd494e49e1cf9ceef29d3d6248d1",
        "title": "句  其二",
        "author": "严中和",
        "content": "寺老柳遮塔，雨多春碍船。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0582045981185bac8ce2b0fac4813c5b",
        "title": "句  其三",
        "author": "严中和",
        "content": "涧水活不冻，山枫老自灵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ad17c0d2310c2267652560a910d5ee71",
        "title": "句  其四",
        "author": "严中和",
        "content": "画扇题诗鸾背湿，铜彝煎脑鸭心香。",
        "dynasty": "宋",
        "collection_info": null
    }
]