[
    {
        "uuid": "bae7cd20426906e3b155109e54f1faa0",
        "title": "九江琵琶亭",
        "author": "宋敏求",
        "content": "夜泊浔阳宿酒楼，琵琶亭畔荻花秋。\n云沈鸟没事已往，月白风清江自流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "85f54489f1e7dd8c475dfeafc89ac040",
        "title": "送客西陵",
        "author": "宋敏求",
        "content": "若耶溪畔醉秋风，猎猎船旗照水红。\n后夜钱塘酒楼上，梦魂应绕浙江东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "03e16707d9cb5bf69433671e52ad5dae",
        "title": "送裴司门倅越",
        "author": "宋敏求",
        "content": "剡中新佐郡，去路触炎威。\n民富杼机利，地含山水晖。\n樵风送舟綄，梅雨润朝衣。\n政在宜谣俗，当期奏课归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1aa0ff79d641d307f9485d27b9bf71aa",
        "title": "送程给事知越州",
        "author": "宋敏求",
        "content": "浙东歌舞望旌麾，远泛樵风羽綄飞。\n论譔内通仙殿籍，拜兴初别夕郎闱。\n老人日俟刘公至，狂客今无贺监归。\n闻有新诗频寄我，莫嗟梅雨裛朝衣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "40ef0006b81e60dbf4897ed56cef5214",
        "title": "史院席上奉和首相吴公元韵",
        "author": "宋敏求",
        "content": "二圣垂鸿烈，天临四纪余。\n元台地率属，赐会宠刊书。\n世业叨荣甚，君恩可报欤。\n衮衣相照烂，归拥鹿鸣车。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a83dc181e6fa4b457a927f3ac3de5e79",
        "title": "题招提院静照堂",
        "author": "宋敏求",
        "content": "丛林起新构，燕坐水云乡。\n本自禅心静，能令世累忘。\n幡花围昼永，钟梵度宵长。\n归裓新篇富，笼纱映宝坊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6274d5bb7bd137089653692c898e1431",
        "title": "句  其一",
        "author": "宋敏求",
        "content": "十年情味与君同。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "df2a392258014ff2cb0f43d2f164c354",
        "title": "句  其二",
        "author": "宋敏求",
        "content": "左川风俗美，资中亭上人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a65804069f8836acc35b18577663f10a",
        "title": "句  其三",
        "author": "宋敏求",
        "content": "圣时建官追茂制，唐家压角失前规。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fc836fd998f62e04f36cbd1c8885d519",
        "title": "句  其四",
        "author": "宋敏求",
        "content": "千岩相望蔽松筠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2b089d183f66f6222543e2f77447617d",
        "title": "句  其五",
        "author": "宋敏求",
        "content": "林腰似泻瑰，水面如融汞。",
        "dynasty": "宋",
        "collection_info": null
    }
]