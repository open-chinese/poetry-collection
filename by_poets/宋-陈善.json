[
    {
        "uuid": "1aee6baa9b2ed06e3f32bbe26d9edda0",
        "title": "满江红 三月风前花薄命",
        "author": "陈善",
        "content": "三月风前花薄命，五更枕上春无力。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9531c624bee321f35611fd61ed9a86d",
        "title": "和人",
        "author": "陈善",
        "content": "鲛绡巧织在深泉，不与人间机杼联。\n要知妙在笔墨外，第一莫为醒者传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4f2b61561534116c8c44e2445b8ef3df",
        "title": "姑苏灵岩寺",
        "author": "陈善",
        "content": "山僧不好古，改作任所欲。\n洞荒径已迷，廊空响谁续。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "adf96642df57eab0bb06ad95b03080f8",
        "title": "句",
        "author": "陈善",
        "content": "不到沧浪亭上望，那知此句是天成。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e9a167e3a62037d5e2e013f74be6c19a",
        "title": "句  其一",
        "author": "陈善",
        "content": "春风一日归深院，巫峡千山锁暮云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b3c6e72880aea4d35cfb99a21c0d1bb7",
        "title": "句  其二",
        "author": "陈善",
        "content": "闻说平生辅汉卿，武夷山下啜残羹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c7173e5ce52f6a2456451357ccf76d9c",
        "title": "句  其三",
        "author": "陈善",
        "content": "不知筋力衰多少，但觉新来懒上楼。",
        "dynasty": "宋",
        "collection_info": null
    }
]