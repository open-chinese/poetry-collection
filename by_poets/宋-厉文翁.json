[
    {
        "uuid": "f027351f078adf7fc57de3a2d3738f71",
        "title": "杏花",
        "author": "厉文翁",
        "content": "杏花菖叶渐烂斑，率属劭农入祖关。\n昔日郎君方彩袖，今朝太守已苍颜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a1174ac1b495324311763ef818bfd2b5",
        "title": "庭竹",
        "author": "厉文翁",
        "content": "渭川未暇栽千亩，庭槛聊须种数根。\n敢爱深秋群木脱，独欺风雨战黄昏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fe7aca78a210941454596c7eb712d868",
        "title": "茶",
        "author": "厉文翁",
        "content": "海滨鱼盐胜耕稼，尝呼父老采丁宁。\n江乡鱼茗尤逐末，为分利害别重轻。\n拔茶种桑何苦口，崇阳因得良吏名。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bb70cd4f9bd4fde358400a5278cef9f8",
        "title": "无题",
        "author": "厉文翁",
        "content": "滟滪拓瞿唐，二孤障澜蠡。\n大哉神禹功，天地相终始。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a1c2e75e17f8c8cc12638077c899f8a",
        "title": "句  其一",
        "author": "厉文翁",
        "content": "暮云春雪江南北，回首人生嘆路歧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cf03669df353df41cd00ddc19ca2613c",
        "title": "句  其二",
        "author": "厉文翁",
        "content": "翠草庭前生意足，甘棠堤上去思多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7ebe59c18e794838a3e685e9aa634ade",
        "title": "句  其三",
        "author": "厉文翁",
        "content": "苍芜万顷渺无边，鞭石飞梁接市廛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5f9142614a69b5a24182bdda3a5d0fdb",
        "title": "句  其四",
        "author": "厉文翁",
        "content": "且喜卧龙常自在，新诗特地报平安。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6ab754e34e612d5a0d50c997b8d1b725",
        "title": "句  其五",
        "author": "厉文翁",
        "content": "若见集英门外柳，为言曾惹御炉香。",
        "dynasty": "宋",
        "collection_info": null
    }
]