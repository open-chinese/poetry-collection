[
    {
        "uuid": "0c99512422e6a02962d5ed6a9ac34c3e",
        "title": "华亭百咏 顾亭林",
        "author": "许尚",
        "content": "旦暮潮流急，东西径路斜。\n无从质疑字，共属野王家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "307b63a164ea7d7a7fc1ebc913ff528a",
        "title": "华亭百咏 顾府君宅",
        "author": "许尚",
        "content": "萧萧兰若地，知是府君居。\n祠事今谁奉，吾来一嘆吁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ace25514d25257aa50e4dc1326b43937",
        "title": "华亭百咏 东庵异迹",
        "author": "许尚",
        "content": "地胜风雷吼，人疑涌海潮。\n将军英爽在，何物敢兴妖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ef1a95cc87b11e490d1a215c61375a44",
        "title": "华亭百咏 金山",
        "author": "许尚",
        "content": "激浪闻澎湃，山神阴力多。\n风涛归指顾，海若敢谁何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f93b5717e96bf6bd5d5dc26f1ea9f871",
        "title": "华亭百咏 安公像",
        "author": "许尚",
        "content": "浮钵来沧海，幡然厌世尘。\n补陀崖畔月，长照定中身。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e8f21b3ae7b8c0f99705916187e42439",
        "title": "华亭百咏 寒穴泉",
        "author": "许尚",
        "content": "濆涌悬崖下，泠泠注不穷。\n将期挹甘冷，弱水渐难通。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a42d26a76957479de145124df86ab2e9",
        "title": "华亭百咏 苏州洋",
        "author": "许尚",
        "content": "已出天池外，狂澜尚尔高。\n蛮商识吴路，岁入几千艘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f28fc40dbccb4cf3ac651fbc3f1a3e03",
        "title": "华亭百咏 华亭谷",
        "author": "许尚",
        "content": "瀰漫达迥野，潮汐往来频。\n惯识松陵路，重来不问津。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c0d5ec575c879fdf107fb4a2c10439b5",
        "title": "华亭百咏 八角井",
        "author": "许尚",
        "content": "浚极疑无底，丰棱定有因。\n沈沈中夜月，照影属何人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "16b6f9466501b2c07648a2144b937fcd",
        "title": "华亭百咏 陆机茸",
        "author": "许尚",
        "content": "二陆为童日，驱驰屡忘归。\n至今桑柘响，禽鸟尚惊飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "84fb53860f9a7fbbb16201dfa5d42ad1",
        "title": "华亭百咏 柘湖",
        "author": "许尚",
        "content": "展武沈沦后，波澄一鑑明。\n桑田復更变，触目总柴荆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bb6a33ebb268fd5ed5703741592ac4c0",
        "title": "华亭百咏 秦女祠",
        "author": "许尚",
        "content": "狼秦崇苛政，有女亦蒙冤。\n欲弔兴亡事，神应耻重论。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "753b12e4a8d9386eb936a7a64479b0ae",
        "title": "华亭百咏 唳鹤湖",
        "author": "许尚",
        "content": "洸荡云閒路，奫沦一水深。\n月光涵露重，遥听九皋音。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "48c983d18358a16b2f43c5b862ccd55a",
        "title": "华亭百咏 三女冈",
        "author": "许尚",
        "content": "玉殓知无有，香魂计已销。\n频来吟楚些，岁久若为招。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f88c48a1cd566f41daa622ab537ad190",
        "title": "华亭百咏 金山城",
        "author": "许尚",
        "content": "治盛周康世，东游岂信然。\n城闉亦隳废，门堞漫相传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3502b724ae008fb34a2b6c3c2b845307",
        "title": "华亭百咏 前京城",
        "author": "许尚",
        "content": "庐落皆无有，依稀古堞存。\n登临认遗迹，林莽暮烟昏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5e1d293b02c241be523f4508ab5c418e",
        "title": "华亭百咏 昆山",
        "author": "许尚",
        "content": "岁久阡原变，平时草木深。\n英才没黄壤，旧事不堪寻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c124ad947b56bb6038fca06934c61373",
        "title": "华亭百咏 陆机宅",
        "author": "许尚",
        "content": "华亭今何在，千年谩有名。\n披榛问遗裔，寂寞一伤情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7d2619d7c66bf2d94c87008fe484635f",
        "title": "华亭百咏 谷水",
        "author": "许尚",
        "content": "短棹经由处，风披藻荇香。\n中宵孤鹤唳，片月映沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b0ee38b5f36e31320a835308bf916029",
        "title": "华亭百咏 秦皇驰道",
        "author": "许尚",
        "content": "叹昔秦皇帝，何年此逸游。\n迢迢大堽路，千古为嗟羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7b47b5a349c268889a9801ab7c06fceb",
        "title": "华亭百咏 征北将军墓碑",
        "author": "许尚",
        "content": "畸岖寻古隧，衰草隠寒原。\n欲读将军事，丰碑祗半存。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fb5d7a5d2c01534c96a45b312f55edf9",
        "title": "华亭百咏 凤皇山",
        "author": "许尚",
        "content": "览德来何代，慵归丹穴藏。\n甘心化为石，万岁瑞吾皇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d1d8a03a3f2a78488beeaee097a1cac1",
        "title": "华亭百咏 陆宝山",
        "author": "许尚",
        "content": "秘宝封坚石，山灵谨护藏。\n欲为中帑助，时復露辉光。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a82f8865c62257856992160352f2614f",
        "title": "华亭百咏 畲山",
        "author": "许尚",
        "content": "人与室俱化，阴森松竹寒。\n时时见孤鹤，疑此守神丹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5b518c18af84fed58fccab5a74bdf642",
        "title": "华亭百咏 集贤里",
        "author": "许尚",
        "content": "冠盖游从日，欣然萃一乡。\n田原总如昨，谁復嗣余芳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "56ce079f2e8d710719d3521259c37c2a",
        "title": "华亭百咏 白龙洞",
        "author": "许尚",
        "content": "呼吸湖中水，山椒寄此身。\n洞门风雨夜，电火逐霜鳞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0bec65960a995a4993f8495b76708743",
        "title": "华亭百咏 石鱼",
        "author": "许尚",
        "content": "双鲤何年刻，悠然鬐鬛全。\n登临忽怀远，尺素若为传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f71535c933ebf423cdbe0a1643bae44e",
        "title": "华亭百咏 秀道者塔",
        "author": "许尚",
        "content": "辛勤成鴈塔，俄赴积薪焚。\n静夜眈眈影，疑来护刻文。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0423edcc7684c80928e33c6f8c4387eb",
        "title": "华亭百咏 灵峰庵",
        "author": "许尚",
        "content": "路涌金沙地，高人此筑庵。\n为询林下客，妙语与谁参。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8eeb53235a780943c91044f23afac5d6",
        "title": "华亭百咏 芥子庵",
        "author": "许尚",
        "content": "林外三椽建，常思愒息时。\n莫教嫌迫窄，中有五须弥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c9bf59bdc0fad0cbe37ba854160b15b",
        "title": "华亭百咏 淀山",
        "author": "许尚",
        "content": "殿阁辉金碧，遐观足画图。\n维舟一登览，误涉小方壶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c8f0067fab01001cba5175745143efdc",
        "title": "华亭百咏 三姑庙",
        "author": "许尚",
        "content": "神居阴物护，臬阈捍洪波。\n莫虑蛟龙怒，年来畏叱诃。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2168404b9e90d343c9a528ebe31ab5dc",
        "title": "华亭百咏 莲巢",
        "author": "许尚",
        "content": "记得荷香裏，蛾眉唱采莲。\n千年龟欲总，不见叶田田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "75b3481792ddb6353b48cc39faad74f8",
        "title": "华亭百咏 鬼书",
        "author": "许尚",
        "content": "壮士为儒耻，捐躯志所甘。\n无名豫青史，幽愤亦何堪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9ef8e1dc88273ceafec797463bb96c49",
        "title": "华亭百咏 孔宅",
        "author": "许尚",
        "content": "庙貌逾千祀，泥封古碣昏。\n传云素王宅，真贋与谁论。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c2d9f20c60f6ac71b0a0459e5370bd5e",
        "title": "华亭百咏 袁崧宅",
        "author": "许尚",
        "content": "疆场功成后，归来卜一邱。\n当时应种德，名字播千秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2109925398ef28db4262521a4f375e11",
        "title": "华亭百咏 俞塘",
        "author": "许尚",
        "content": "延袤三乡外，东流与海通。\n河神屡加惠，帆借往来风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5df5dec9bc3a42ee752d68f753fb38ae",
        "title": "华亭百咏 鹤坡",
        "author": "许尚",
        "content": "索寞东郊远，仙禽尽此藏。\n梦回明月夜，林杪响圆吭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4fab01027ee32063226e32fdacfde151",
        "title": "华亭百咏 沙冈",
        "author": "许尚",
        "content": "千里平沙地，联通江海湄。\n谩传因激浪，疑是蚌成基。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "501b2e269e918588160552ef7199d182",
        "title": "华亭百咏 御史泾",
        "author": "许尚",
        "content": "疏凿人何在，流功几世余。\n农氓诵名氏，不数郑公渠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "84d19387f2189fbe373b14f6e3048187",
        "title": "华亭百咏 沪渎",
        "author": "许尚",
        "content": "泛泛松江水，迢迢笠泽通。\n万年知禹力，灌溉有余功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a53c15b0d2d11c8690e1c7c1e76296d4",
        "title": "华亭百咏 东堂",
        "author": "许尚",
        "content": "厅左清閒处，金猊罩宝香。\n令君今学道，来此吸朝阳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18b08c3a8ce127cecf901b96916c0b90",
        "title": "华亭百咏 思齐堂",
        "author": "许尚",
        "content": "伊昔絃歌治，能声着海隅。\n升堂想遗迹，励志欲齐驱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bdbfc915eb69794f35fca1bcdf31aaa3",
        "title": "华亭百咏 月榭",
        "author": "许尚",
        "content": "檐角衔墙固，明疏护翠罗。\n公余秉谈麈，竟夕对金波。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71ca904aaaf488678b900d561a10785e",
        "title": "华亭百咏 濯缨亭",
        "author": "许尚",
        "content": "舆梁斜倚处，檐影坠清流。\n往往抽簪者，常为孺子羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c415120c6589e4e532b28731b9a3ae5a",
        "title": "华亭百咏 石兽",
        "author": "许尚",
        "content": "刻凿知何谓，弥年亦有神。\n宵行无足怪，一吼已惊人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3d19ecf0c948bcd1ea69c4a7a02be25e",
        "title": "华亭百咏 震桥",
        "author": "许尚",
        "content": "瓦木明丹垩，初期镇此方。\n石梁今欲圮，尤益富蛮商。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1ba05839711625cccae4a3448e83d2bb",
        "title": "华亭百咏 小隠园",
        "author": "许尚",
        "content": "妆点林泉趣，销除市井心。\n平生跧伏计，到此意偏深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9ef8f8375c52e8facf22cac81aa26425",
        "title": "华亭百咏 姚将军庙",
        "author": "许尚",
        "content": "阴功周邑境，南面固宜然。\n毋復论民事，神今惮改迁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1f10da2879323e310c01020300504d76",
        "title": "华亭百咏 赵店",
        "author": "许尚",
        "content": "日日黄尘路，喧豗涨市声。\n神灵自安此，不欲变名称。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2d16ed89473dc65a71437a7b7b961c04",
        "title": "华亭百咏 望云桥",
        "author": "许尚",
        "content": "忆昨登临日，卿云正四翔。\n重来穷目力，天末起祥光。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "48c2715d549c6f58d71ef9c144b9b112",
        "title": "华亭百咏 石幢",
        "author": "许尚",
        "content": "矻立应千载，传因海眼成。\n蓬莱水清浅，曾不见敧倾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4995c0dfd5bc55ee7c7eaa95dc56adec",
        "title": "华亭百咏 普照寺",
        "author": "许尚",
        "content": "高门收画戟，即此建精蓝。\n施予恩虽在，缁流绝不谈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5598171263c895090112bc9a925ffd2b",
        "title": "华亭百咏 弥陀阁",
        "author": "许尚",
        "content": "拄策徐徐上，回环佛顶游。\n九山俱在望，目断暮烟浮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "685601607381db3a398babf43bbfefe6",
        "title": "华亭百咏 罗汉院",
        "author": "许尚",
        "content": "邃宇中何有，台山五百尊。\n云何困人力，金像总尘昏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b844437d3162eefd302c8c4501bd60a4",
        "title": "华亭百咏 丁公桥",
        "author": "许尚",
        "content": "令威仙去后，遗迹叹成非。\n华表成乌有，何年见鹤归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1b2c45a5f4d617a443d8fe50cce0a245",
        "title": "华亭百咏 云閒馆",
        "author": "许尚",
        "content": "邮亭临爽垲，杗廇架修虹。\n我亦轮蹄客，心期访士龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1fcc3aab4a24409d22a44a94ee071eca",
        "title": "华亭百咏 福顺庙",
        "author": "许尚",
        "content": "门垣虽蠹坏，古屋尚深沈。\n鸟雀犹知避，斯民盍致钦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2b8fa2d6250667af8115c1bf6bd3f040",
        "title": "华亭百咏 东岳行祠",
        "author": "许尚",
        "content": "明主东封后，龟蒙望益尊。\n巍然此行宇，时许荐苹蘩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5ce6b47ad343c94935a8fcc811597655",
        "title": "华亭百咏 西庵",
        "author": "许尚",
        "content": "蚤为儒冠误，齐心学竺干。\n而今林下塔，夜夜戒光圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bedabecf7336efb9662f7890e5fb7ed1",
        "title": "华亭百咏 白龙潭",
        "author": "许尚",
        "content": "神物幽潜地，沧沧水接空。\n不缘尝应祷，谁识有殊功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "acdd0b48252a8578eae31a57b8edb32d",
        "title": "华亭百咏 毗卢庵",
        "author": "许尚",
        "content": "扪萝通一径，修竹蔽中篱。\n只有奎章在，龙神暗护持。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "caa76917c2f29386ab4e76311d4eae0a",
        "title": "华亭百咏 西林",
        "author": "许尚",
        "content": "林外初晴后，斜阳满芰荷。\n频来指灵鹫，此去路无多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ff8a3fc1d64bb2d10628ddcd4bb842fa",
        "title": "华亭百咏 陆瑁养鱼池",
        "author": "许尚",
        "content": "谁得陶朱术，修池一水宽。\n皇恩浃鱼鼈，不復敢垂竿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "555c8f8284590d960a85f5cff8d59c21",
        "title": "华亭百咏 小湖",
        "author": "许尚",
        "content": "猎猎风蒲外，渊渊明月秋。\n诗翁浮短艇，弭棹看浮鯈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "875950e823f86230256be9911fa96338",
        "title": "华亭百咏 湖桥",
        "author": "许尚",
        "content": "潋滟湖光好，荷风六月凉。\n倚栏吟不倦，鱼鸟亦相忘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "80b2864c5ce8adca1a6f1602c4e9c469",
        "title": "华亭百咏 唳鹤滩",
        "author": "许尚",
        "content": "养鹤人何在，湖边水尚清。\n唤回中夜梦，滩上戛然声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bce5d8c6a9376ec0f3b81a3794361d17",
        "title": "华亭百咏 泳波亭",
        "author": "许尚",
        "content": "吕梁观已远，沂水浴应难。\n却倚亭前柳，时时照影寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "85f3bcde6c47b3785458c818ca0a136f",
        "title": "华亭百咏 风月堂",
        "author": "许尚",
        "content": "夜静蒲萄皱，云舒玉鑑凉。\n筑堂人不见，樽酒暗相望。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "72693a269daf76a1bb5c80f0023e7715",
        "title": "华亭百咏 湖光亭",
        "author": "许尚",
        "content": "日暮虾鬚卷，亭中雅燕开。\n微风起苹末，波影动樽罍。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "da1bc2f5810a2bd3425299a05117b7b2",
        "title": "华亭百咏 道院",
        "author": "许尚",
        "content": "浄观今何在，唯余柏色新。\n栖栖抱瓮者，不是炼丹人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd606d14fd124cbc122bdf54b88a2d67",
        "title": "华亭百咏 异木",
        "author": "许尚",
        "content": "蟠根知永远，曲干耸蛟虬。\n闽贾蒙私庇，年年此荐羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fed48b6fe74c3047b39a5ba67a0e0db8",
        "title": "华亭百咏 显忠庙",
        "author": "许尚",
        "content": "俯拜祠堂下，遥思弼汉功。\n吁嗟孝宣帝，忘德太匆匆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b5c8d66edc3f29fe0c41ea96b1de43b3",
        "title": "华亭百咏 东寺",
        "author": "许尚",
        "content": "爱此楞伽士，勤修戒行成。\n文龟亦知善，来听讲经声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ca7fbc86c4291d72f250f0bb6952c86c",
        "title": "华亭百咏 证觉院",
        "author": "许尚",
        "content": "有人明七浄，蠠没缔良因。\n叹惜流风替，谁为无垢人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "76bad90ab8d2775727c4ad796f3df301",
        "title": "华亭百咏 栖真庵",
        "author": "许尚",
        "content": "仙路多官府，真人未上升。\n松窗霜月白，相对诵黄庭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3d66ce70c011f0ca7231397796407a2d",
        "title": "华亭百咏 柳园",
        "author": "许尚",
        "content": "芳草池塘暖，东风桃杏繁。\n春归人亦散，寂寞锁园门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "afcc9f769c46346c51585c6d53ed2c34",
        "title": "华亭百咏 陆四公庙",
        "author": "许尚",
        "content": "晋杰云亡久，嘉名未遽湮。\n严祠坐遗像，犹解福吴民。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5fe41f1cf2df4acb7141178fcfa824b",
        "title": "华亭百咏 南庵",
        "author": "许尚",
        "content": "施水功成后，眈眈梵宇深。\n升堂瞻貌象，默会祖师心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "92afc55d64b842a471feec28414d8abb",
        "title": "华亭百咏 姜庵",
        "author": "许尚",
        "content": "阴曀楸梧裏，鸟鸣落日中。\n九原人不作，明赞亦何功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "188b980f7c4703500457c48b64fcc9c9",
        "title": "华亭百咏 望仙桥",
        "author": "许尚",
        "content": "鹤驾乘风去，千年竟不归。\n石梁凭望处，空復白云飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1d49f9f0c1dc7dd4df7daad72b334a93",
        "title": "华亭百咏 三洞庵",
        "author": "许尚",
        "content": "勾曲山头客，遨游到市尘。\n星冠顶霜月，夜半正朝真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e8192114d48af0082a2c553185998aff",
        "title": "华亭百咏 浄居院",
        "author": "许尚",
        "content": "明眼知何在，山门久寂然。\n宗风未沦替，还喜一镫传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "61aba3400c49c9f6e2f21166b68792d2",
        "title": "华亭百咏 冰檗庵",
        "author": "许尚",
        "content": "鳬舄栖迟地，池流一径斜。\n先生久清苦，不肯竞纷华。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0747bf18f85ad03ae0f6d934ef246604",
        "title": "华亭百咏 圆珠庵",
        "author": "许尚",
        "content": "定回人寂静，绕鼻碧莲香。\n却认循檐月，摩尼发夜光。",
        "dynasty": "宋",
        "collection_info": null
    }
]