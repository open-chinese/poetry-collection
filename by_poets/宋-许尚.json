[
    {
        "uuid": "7206aabc76b7f19e146428b4a02b6864",
        "title": "华亭百咏 顾亭林",
        "author": "许尚",
        "content": "旦暮潮流急，东西径路斜。\n无从质疑字，共属野王家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "95cc8e8db3f35ad61e99985a9fa7b83d",
        "title": "华亭百咏 顾府君宅",
        "author": "许尚",
        "content": "萧萧兰若地，知是府君居。\n祠事今谁奉，吾来一嘆吁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d5cddc055b7ae60df828d21b04bd62e3",
        "title": "华亭百咏 东庵异迹",
        "author": "许尚",
        "content": "地胜风雷吼，人疑涌海潮。\n将军英爽在，何物敢兴妖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e7535b2134e68454ba946c8ee1a433ab",
        "title": "华亭百咏 金山",
        "author": "许尚",
        "content": "激浪闻澎湃，山神阴力多。\n风涛归指顾，海若敢谁何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5e6cdef4c42dc6a7ebcfdba3795be9d4",
        "title": "华亭百咏 安公像",
        "author": "许尚",
        "content": "浮钵来沧海，幡然厌世尘。\n补陀崖畔月，长照定中身。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6bf1264087e30cc81933fb1cc7f62dd5",
        "title": "华亭百咏 寒穴泉",
        "author": "许尚",
        "content": "濆涌悬崖下，泠泠注不穷。\n将期挹甘冷，弱水渐难通。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "debb67ff75e3489f3ec21260dac857f1",
        "title": "华亭百咏 苏州洋",
        "author": "许尚",
        "content": "已出天池外，狂澜尚尔高。\n蛮商识吴路，岁入几千艘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3008dfaa5751ac2166a80770ca882ea4",
        "title": "华亭百咏 华亭谷",
        "author": "许尚",
        "content": "瀰漫达迥野，潮汐往来频。\n惯识松陵路，重来不问津。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4f226f2683a8d1ee5c523d180d4808de",
        "title": "华亭百咏 八角井",
        "author": "许尚",
        "content": "浚极疑无底，丰棱定有因。\n沈沈中夜月，照影属何人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b17d4fbf9fa17c777418908a2fe49928",
        "title": "华亭百咏 陆机茸",
        "author": "许尚",
        "content": "二陆为童日，驱驰屡忘归。\n至今桑柘响，禽鸟尚惊飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "efb12c9a8fbc0188ad9ee6cf3d0db415",
        "title": "华亭百咏 柘湖",
        "author": "许尚",
        "content": "展武沈沦后，波澄一鑑明。\n桑田復更变，触目总柴荆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "232500d9c0ddae1ff7cfb850d9a078e7",
        "title": "华亭百咏 秦女祠",
        "author": "许尚",
        "content": "狼秦崇苛政，有女亦蒙冤。\n欲弔兴亡事，神应耻重论。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "32f48e909c0f9e0769133304d3f1c732",
        "title": "华亭百咏 唳鹤湖",
        "author": "许尚",
        "content": "洸荡云閒路，奫沦一水深。\n月光涵露重，遥听九皋音。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "647d993e36a7ca1d2e6c987037212072",
        "title": "华亭百咏 三女冈",
        "author": "许尚",
        "content": "玉殓知无有，香魂计已销。\n频来吟楚些，岁久若为招。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f7ca2a47b5fc850b68e138c5e12a57a2",
        "title": "华亭百咏 金山城",
        "author": "许尚",
        "content": "治盛周康世，东游岂信然。\n城闉亦隳废，门堞漫相传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6113f698456a967c27a709a2d2d57ea8",
        "title": "华亭百咏 前京城",
        "author": "许尚",
        "content": "庐落皆无有，依稀古堞存。\n登临认遗迹，林莽暮烟昏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "96ae6420ce83a36a7a7519406a84b83d",
        "title": "华亭百咏 昆山",
        "author": "许尚",
        "content": "岁久阡原变，平时草木深。\n英才没黄壤，旧事不堪寻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fbfe1fd4afe39be2634793ca94cdfeb6",
        "title": "华亭百咏 陆机宅",
        "author": "许尚",
        "content": "华亭今何在，千年谩有名。\n披榛问遗裔，寂寞一伤情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "47ffb7d1adc17d113a7c1f51030f1978",
        "title": "华亭百咏 谷水",
        "author": "许尚",
        "content": "短棹经由处，风披藻荇香。\n中宵孤鹤唳，片月映沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d2e7d23014b44e6bfe66fc9c2cf3ab81",
        "title": "华亭百咏 秦皇驰道",
        "author": "许尚",
        "content": "叹昔秦皇帝，何年此逸游。\n迢迢大堽路，千古为嗟羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5667920276e023a7d0b5fadf4ff1d6a",
        "title": "华亭百咏 征北将军墓碑",
        "author": "许尚",
        "content": "畸岖寻古隧，衰草隠寒原。\n欲读将军事，丰碑祗半存。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f408af490b16760952378839f20d788b",
        "title": "华亭百咏 凤皇山",
        "author": "许尚",
        "content": "览德来何代，慵归丹穴藏。\n甘心化为石，万岁瑞吾皇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2d8c1203553b4a23ef348e69162c2bc1",
        "title": "华亭百咏 陆宝山",
        "author": "许尚",
        "content": "秘宝封坚石，山灵谨护藏。\n欲为中帑助，时復露辉光。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6625bffb87bfbd8a11c6bbbaa666daf5",
        "title": "华亭百咏 畲山",
        "author": "许尚",
        "content": "人与室俱化，阴森松竹寒。\n时时见孤鹤，疑此守神丹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "82779804eeadb0e8d6131f02457b8bf1",
        "title": "华亭百咏 集贤里",
        "author": "许尚",
        "content": "冠盖游从日，欣然萃一乡。\n田原总如昨，谁復嗣余芳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "15ddea1ff283b142785dd58170541ce5",
        "title": "华亭百咏 白龙洞",
        "author": "许尚",
        "content": "呼吸湖中水，山椒寄此身。\n洞门风雨夜，电火逐霜鳞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "41971bee2d0a0d506496e4fd6fab6522",
        "title": "华亭百咏 石鱼",
        "author": "许尚",
        "content": "双鲤何年刻，悠然鬐鬛全。\n登临忽怀远，尺素若为传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ea0376fc3bcef43484169c7682e2bd2c",
        "title": "华亭百咏 秀道者塔",
        "author": "许尚",
        "content": "辛勤成鴈塔，俄赴积薪焚。\n静夜眈眈影，疑来护刻文。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b22e18d9131baf493e48b65a23c8d74c",
        "title": "华亭百咏 灵峰庵",
        "author": "许尚",
        "content": "路涌金沙地，高人此筑庵。\n为询林下客，妙语与谁参。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "804619292b37ea295b6c6a5ff22014a2",
        "title": "华亭百咏 芥子庵",
        "author": "许尚",
        "content": "林外三椽建，常思愒息时。\n莫教嫌迫窄，中有五须弥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b31c62704ead159ebe3c4911bd852957",
        "title": "华亭百咏 淀山",
        "author": "许尚",
        "content": "殿阁辉金碧，遐观足画图。\n维舟一登览，误涉小方壶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "786c0e9a26beab424908201850e35c35",
        "title": "华亭百咏 三姑庙",
        "author": "许尚",
        "content": "神居阴物护，臬阈捍洪波。\n莫虑蛟龙怒，年来畏叱诃。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "05baa1a72cc47d01570bc42e82bf381b",
        "title": "华亭百咏 莲巢",
        "author": "许尚",
        "content": "记得荷香裏，蛾眉唱采莲。\n千年龟欲总，不见叶田田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b69408c0db93d8478a7f50f3d9f8ab0a",
        "title": "华亭百咏 鬼书",
        "author": "许尚",
        "content": "壮士为儒耻，捐躯志所甘。\n无名豫青史，幽愤亦何堪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "01b6ad82ee6e618d073c997dd2306c55",
        "title": "华亭百咏 孔宅",
        "author": "许尚",
        "content": "庙貌逾千祀，泥封古碣昏。\n传云素王宅，真贋与谁论。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e6ff40bd719d4ecd896f13ba762c8c93",
        "title": "华亭百咏 袁崧宅",
        "author": "许尚",
        "content": "疆场功成后，归来卜一邱。\n当时应种德，名字播千秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a7f180f2ce98506e8cf415ede44e7a39",
        "title": "华亭百咏 俞塘",
        "author": "许尚",
        "content": "延袤三乡外，东流与海通。\n河神屡加惠，帆借往来风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "44cd07cc0021b5c9ee951774a32a807a",
        "title": "华亭百咏 鹤坡",
        "author": "许尚",
        "content": "索寞东郊远，仙禽尽此藏。\n梦回明月夜，林杪响圆吭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b11c825796ac1c4863c5fe684d747b5f",
        "title": "华亭百咏 沙冈",
        "author": "许尚",
        "content": "千里平沙地，联通江海湄。\n谩传因激浪，疑是蚌成基。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "76588182c686d577f6890c67f765324e",
        "title": "华亭百咏 御史泾",
        "author": "许尚",
        "content": "疏凿人何在，流功几世余。\n农氓诵名氏，不数郑公渠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6f05d1f7beff21d3b7209eb55a068d08",
        "title": "华亭百咏 沪渎",
        "author": "许尚",
        "content": "泛泛松江水，迢迢笠泽通。\n万年知禹力，灌溉有余功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fcd1857e045427ba5f85e3dac3621bfc",
        "title": "华亭百咏 东堂",
        "author": "许尚",
        "content": "厅左清閒处，金猊罩宝香。\n令君今学道，来此吸朝阳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f811a3c5a1341ce22ced8e09d85c5b95",
        "title": "华亭百咏 思齐堂",
        "author": "许尚",
        "content": "伊昔絃歌治，能声着海隅。\n升堂想遗迹，励志欲齐驱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6fc4e3d89c91535c0e03c2d1c7d936d6",
        "title": "华亭百咏 月榭",
        "author": "许尚",
        "content": "檐角衔墙固，明疏护翠罗。\n公余秉谈麈，竟夕对金波。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e838b78fbe7ab04989c2904f06417289",
        "title": "华亭百咏 濯缨亭",
        "author": "许尚",
        "content": "舆梁斜倚处，檐影坠清流。\n往往抽簪者，常为孺子羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7a125881923592043dc678838cf2a0d8",
        "title": "华亭百咏 石兽",
        "author": "许尚",
        "content": "刻凿知何谓，弥年亦有神。\n宵行无足怪，一吼已惊人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b31405d79ff2700f41633e4bb6e81ef5",
        "title": "华亭百咏 震桥",
        "author": "许尚",
        "content": "瓦木明丹垩，初期镇此方。\n石梁今欲圮，尤益富蛮商。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f7168566b17ea62d2cc92a395c3af165",
        "title": "华亭百咏 小隠园",
        "author": "许尚",
        "content": "妆点林泉趣，销除市井心。\n平生跧伏计，到此意偏深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "39eeaae6778bf01a96d48cc0fa55d5d5",
        "title": "华亭百咏 姚将军庙",
        "author": "许尚",
        "content": "阴功周邑境，南面固宜然。\n毋復论民事，神今惮改迁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8858182af3c6f8f2221f1dd2f2a4834f",
        "title": "华亭百咏 赵店",
        "author": "许尚",
        "content": "日日黄尘路，喧豗涨市声。\n神灵自安此，不欲变名称。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e398c09068dc0ebe0f3ec462a6099b05",
        "title": "华亭百咏 望云桥",
        "author": "许尚",
        "content": "忆昨登临日，卿云正四翔。\n重来穷目力，天末起祥光。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "67b091632a8a00019b3eb5ef942aa854",
        "title": "华亭百咏 石幢",
        "author": "许尚",
        "content": "矻立应千载，传因海眼成。\n蓬莱水清浅，曾不见敧倾。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bbf0e8cd892ada3c21a8cecdbd79404c",
        "title": "华亭百咏 普照寺",
        "author": "许尚",
        "content": "高门收画戟，即此建精蓝。\n施予恩虽在，缁流绝不谈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "54492fd02968ef22f4873180d40657da",
        "title": "华亭百咏 弥陀阁",
        "author": "许尚",
        "content": "拄策徐徐上，回环佛顶游。\n九山俱在望，目断暮烟浮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "de57a36dd02047a76929763a9627402b",
        "title": "华亭百咏 罗汉院",
        "author": "许尚",
        "content": "邃宇中何有，台山五百尊。\n云何困人力，金像总尘昏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "56c28a28bf5549ee2ffaa814a81a73eb",
        "title": "华亭百咏 丁公桥",
        "author": "许尚",
        "content": "令威仙去后，遗迹叹成非。\n华表成乌有，何年见鹤归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "da1dd49418818dfc71336ac8a2e9f5e3",
        "title": "华亭百咏 云閒馆",
        "author": "许尚",
        "content": "邮亭临爽垲，杗廇架修虹。\n我亦轮蹄客，心期访士龙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "da2a40e298f0e6a7ee3c97715fc8ae4b",
        "title": "华亭百咏 福顺庙",
        "author": "许尚",
        "content": "门垣虽蠹坏，古屋尚深沈。\n鸟雀犹知避，斯民盍致钦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7bf6a4e5133bb81b4b7a59828cb97a1b",
        "title": "华亭百咏 东岳行祠",
        "author": "许尚",
        "content": "明主东封后，龟蒙望益尊。\n巍然此行宇，时许荐苹蘩。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9d5247e054386af30ab34001b25a02c9",
        "title": "华亭百咏 西庵",
        "author": "许尚",
        "content": "蚤为儒冠误，齐心学竺干。\n而今林下塔，夜夜戒光圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cc160e5204e82beef4534e19ac6cda6a",
        "title": "华亭百咏 白龙潭",
        "author": "许尚",
        "content": "神物幽潜地，沧沧水接空。\n不缘尝应祷，谁识有殊功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c6a7c72081fe5b35cf80f0f5a7ac5c2a",
        "title": "华亭百咏 毗卢庵",
        "author": "许尚",
        "content": "扪萝通一径，修竹蔽中篱。\n只有奎章在，龙神暗护持。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "126907902d9f9fe0ea503411a78a57f8",
        "title": "华亭百咏 西林",
        "author": "许尚",
        "content": "林外初晴后，斜阳满芰荷。\n频来指灵鹫，此去路无多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eb0c6e7a6af909e01e4edb3b363541d4",
        "title": "华亭百咏 陆瑁养鱼池",
        "author": "许尚",
        "content": "谁得陶朱术，修池一水宽。\n皇恩浃鱼鼈，不復敢垂竿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3f6fa1074da18ef76df1365cefdcc1a9",
        "title": "华亭百咏 小湖",
        "author": "许尚",
        "content": "猎猎风蒲外，渊渊明月秋。\n诗翁浮短艇，弭棹看浮鯈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b5bdff57ed4000e2bea77edc2bdcdb7e",
        "title": "华亭百咏 湖桥",
        "author": "许尚",
        "content": "潋滟湖光好，荷风六月凉。\n倚栏吟不倦，鱼鸟亦相忘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fe68c9aca35a2faeb37a1fc1c062eae7",
        "title": "华亭百咏 唳鹤滩",
        "author": "许尚",
        "content": "养鹤人何在，湖边水尚清。\n唤回中夜梦，滩上戛然声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "28de1466fefa7c0e572404f5ab5cf3f5",
        "title": "华亭百咏 泳波亭",
        "author": "许尚",
        "content": "吕梁观已远，沂水浴应难。\n却倚亭前柳，时时照影寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5501b3171abb5a52cf0aa98be3f1677f",
        "title": "华亭百咏 风月堂",
        "author": "许尚",
        "content": "夜静蒲萄皱，云舒玉鑑凉。\n筑堂人不见，樽酒暗相望。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e229251c017f716727473cedb1986e8e",
        "title": "华亭百咏 湖光亭",
        "author": "许尚",
        "content": "日暮虾鬚卷，亭中雅燕开。\n微风起苹末，波影动樽罍。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1459bbc2960d0f9469cb1e70b6d37c6d",
        "title": "华亭百咏 道院",
        "author": "许尚",
        "content": "浄观今何在，唯余柏色新。\n栖栖抱瓮者，不是炼丹人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e81b653c519a9e77a67c1c35711801b1",
        "title": "华亭百咏 异木",
        "author": "许尚",
        "content": "蟠根知永远，曲干耸蛟虬。\n闽贾蒙私庇，年年此荐羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9262deceb4a0cc87d3ea669201d9cf06",
        "title": "华亭百咏 显忠庙",
        "author": "许尚",
        "content": "俯拜祠堂下，遥思弼汉功。\n吁嗟孝宣帝，忘德太匆匆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd04622139323aeccf3db2b5aa711ba7",
        "title": "华亭百咏 东寺",
        "author": "许尚",
        "content": "爱此楞伽士，勤修戒行成。\n文龟亦知善，来听讲经声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cacbc1743b51f44f1a34b823580ccd86",
        "title": "华亭百咏 证觉院",
        "author": "许尚",
        "content": "有人明七浄，蠠没缔良因。\n叹惜流风替，谁为无垢人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "072c88dad177b5c0b6980c3ea5eaab5f",
        "title": "华亭百咏 栖真庵",
        "author": "许尚",
        "content": "仙路多官府，真人未上升。\n松窗霜月白，相对诵黄庭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "37eeaca4ae4404e419545703f64438a2",
        "title": "华亭百咏 柳园",
        "author": "许尚",
        "content": "芳草池塘暖，东风桃杏繁。\n春归人亦散，寂寞锁园门。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dfe3def2a6190cda61acb5023390014d",
        "title": "华亭百咏 陆四公庙",
        "author": "许尚",
        "content": "晋杰云亡久，嘉名未遽湮。\n严祠坐遗像，犹解福吴民。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1d9d62873a04ba9cc3e3fa6990804128",
        "title": "华亭百咏 南庵",
        "author": "许尚",
        "content": "施水功成后，眈眈梵宇深。\n升堂瞻貌象，默会祖师心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9df91dc452fce4fc14ba95a5cfe64fc5",
        "title": "华亭百咏 姜庵",
        "author": "许尚",
        "content": "阴曀楸梧裏，鸟鸣落日中。\n九原人不作，明赞亦何功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0ab3a77862de5cf8e5199b8eef1a3914",
        "title": "华亭百咏 望仙桥",
        "author": "许尚",
        "content": "鹤驾乘风去，千年竟不归。\n石梁凭望处，空復白云飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "93cdb21e8d1ff525301503acebf3b6ac",
        "title": "华亭百咏 三洞庵",
        "author": "许尚",
        "content": "勾曲山头客，遨游到市尘。\n星冠顶霜月，夜半正朝真。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9d2832e66ad602eddc188e6d79333e7e",
        "title": "华亭百咏 浄居院",
        "author": "许尚",
        "content": "明眼知何在，山门久寂然。\n宗风未沦替，还喜一镫传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e60a7d78afc9bed121cec5877d8f4436",
        "title": "华亭百咏 冰檗庵",
        "author": "许尚",
        "content": "鳬舄栖迟地，池流一径斜。\n先生久清苦，不肯竞纷华。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a6597f698f71a646266235180ed5aba8",
        "title": "华亭百咏 圆珠庵",
        "author": "许尚",
        "content": "定回人寂静，绕鼻碧莲香。\n却认循檐月，摩尼发夜光。",
        "dynasty": "宋",
        "collection_info": null
    }
]