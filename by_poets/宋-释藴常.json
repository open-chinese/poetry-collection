[
    {
        "uuid": "be34ad342f476b1115658059db085417",
        "title": "咏石菖蒲",
        "author": "释藴常",
        "content": "细水围棋石，纤纤手自移。\n几年离雁荡，万里到天池。\n浮玉春风后，小姑烟雨时。\n它年怀胜绝，魂梦亦清奇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8c193864b637cde186f5b9320938f28f",
        "title": "咏兰",
        "author": "释藴常",
        "content": "目断山河恨莫裁，折芳犹记小徘徊。\n细看叶底春风面，疑自幽篁影下来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed338563088c912358cb407ae75e4946",
        "title": "送空上人",
        "author": "释藴常",
        "content": "过了梨花春亦归，小窗新绿正相宜。\n白头更作西州梦，细雨青灯话别离。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b0712c97e96e61871e0bc4cef86fb17e",
        "title": "知止亭晚望寄清大师",
        "author": "释藴常",
        "content": "柳绕平湖绿映堤，栏干人静夕阳迟。\n归鸦欲尽千林暝，白鸟飞来一段奇。\n短髮缘愁分壮志，断云含雨入新诗。\n不堪更作怀人恨，歌罢竹枝成别离。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b21c47e6abfc056217e62b9fc7dc7d2c",
        "title": "别苏养直",
        "author": "释藴常",
        "content": "老去难为别，愁边更着秋。\n碧芦围野水，落日满行舟。\n雁断西风急，天寒古寺幽。\n两乡无百里，能寄短书否。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ca5cf54a70845d2ebdccad3c3c1bf2e2",
        "title": "登虎丘即事",
        "author": "释藴常",
        "content": "九月凭栏已怯风，鬓霜留我客愁中。\n夕阳看尽烟生树，野水无情天四空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "071c98033e23a687365144c5b455f0ca",
        "title": "春日",
        "author": "释藴常",
        "content": "烧灯过了客思家，独立衡门数晚鸦。\n燕子未归梅落尽，小窗明月属梨花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eba6f4dc246c58c4afa7b68e3aacb93c",
        "title": "江村",
        "author": "释藴常",
        "content": "江村风急苇花飞，漠漠吹烟树影稀。\n亦有人家沙际住，夕阳鸡犬傍人归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5fb294c7926a8307b03f7dcafe953893",
        "title": "游天台赤城山",
        "author": "释藴常",
        "content": "拄藜扶我过桥东，便觉晴岚翠扫空。\n紫府百年藏玉简，丹崖千丈挹仙风。\n释籤书到新罗国，卧佛岩连旧梵宫。\n稽首导师悲愿在，我宁辛苦守诗穷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b8bf07821a301766b2f98c0b4254f838",
        "title": "句  其一",
        "author": "释藴常",
        "content": "断桥人寂寂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ee192cc7194e31ed45ba78a5fa4716b1",
        "title": "句  其二",
        "author": "释藴常",
        "content": "尘埃自衮衮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6edc86b15a4a40b6d9da074961a739aa",
        "title": "句  其三",
        "author": "释藴常",
        "content": "归舟藏别浦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "027cc86f892b09d2c7ac44eff7d011ea",
        "title": "句  其四",
        "author": "释藴常",
        "content": "碧涧流红叶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4230e93d08959454ce1dfc851a2d89bc",
        "title": "句  其五",
        "author": "释藴常",
        "content": "云寒湖贴贴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c6864bcb89e0ea983bf52e68d40f4f89",
        "title": "句  其六",
        "author": "释藴常",
        "content": "端为故人留。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "be340cfa869114a59e3eae658a4d5494",
        "title": "句  其七",
        "author": "释藴常",
        "content": "愁来独赋诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f0ffb6c04f722225eabccd6b39e64df0",
        "title": "句  其八",
        "author": "释藴常",
        "content": "柳色葱葱官渡平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b930aace62a3085a201e5fce169863c4",
        "title": "句  其九",
        "author": "释藴常",
        "content": "十年归梦满青山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6a478eca1dbc3aa52afb79955a06f99e",
        "title": "句  其一○",
        "author": "释藴常",
        "content": "月生更爱栏干影。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b2931d6c35f482ee8db9c21e24d339c3",
        "title": "句  其一一",
        "author": "释藴常",
        "content": "白髮青灯两无语。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b4641d963a53907d1f129cc96abc0d86",
        "title": "句  其一二",
        "author": "释藴常",
        "content": "寒涕垂颐独掩关。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "43885a3180eef7916512f3324563e05a",
        "title": "句  其一三",
        "author": "释藴常",
        "content": "吸江亭下雨初晴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8708ac75844b40636a52fd70492e131f",
        "title": "句  其一四",
        "author": "释藴常",
        "content": "正是周王二月时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b037c48ab11970bec5a2729a5d9adc57",
        "title": "句  其一五",
        "author": "释藴常",
        "content": "细看横枝閒绿筠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f830c61ab382d43b408782a3c2088c87",
        "title": "句  其一六",
        "author": "释藴常",
        "content": "眼明端为小迟留。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eac0b762c3a455e58588dc9bca5ea02f",
        "title": "句  其一七",
        "author": "释藴常",
        "content": "栏干倚徧斜阳晚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1a42c5901e37c825479c8e32f2194d37",
        "title": "句  其一八",
        "author": "释藴常",
        "content": "愁裏有诗人不会。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "46f6e758842f3466edefbe1825d36419",
        "title": "句  其一九",
        "author": "释藴常",
        "content": "小舟夜宿越州城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1b64beefd2ca056effcf82c19414d1f2",
        "title": "句  其二○",
        "author": "释藴常",
        "content": "全是蓬莱十月天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bcecd73ee520729b944355640a08536d",
        "title": "句  其二一",
        "author": "释藴常",
        "content": "拄杖寻诗独自行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "66a4dfbe9d257c096efbed6788ab9529",
        "title": "句  其二二",
        "author": "释藴常",
        "content": "竹裏梅花映白头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f84266e19ac790fd26cc4e1869fa9c52",
        "title": "句  其二三",
        "author": "释藴常",
        "content": "梦迴彷佛三更后。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a0fd9b7839c1dfeb22dbebd9dde2f3df",
        "title": "句  其二四",
        "author": "释藴常",
        "content": "一枝斜映小窗明。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18d6dbe2323963a4706ad2e5146a9a68",
        "title": "句  其二五",
        "author": "释藴常",
        "content": "不须更觅吴州画。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3c523894c18e90c6bfc8a3c08a562e6d",
        "title": "句  其二六",
        "author": "释藴常",
        "content": "诗到扬州定清绝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dbff97d65d312028518c699ae2001f2d",
        "title": "句  其二七",
        "author": "释藴常",
        "content": "夕阳无语淡烟初。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bc4c9c5ea08f56f0917f152cada02b55",
        "title": "句  其二八",
        "author": "释藴常",
        "content": "断肠重看白鸥盟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b77170bd1dd96e560fdff89ad2474cb8",
        "title": "句  其二九",
        "author": "释藴常",
        "content": "青山断处夕阳垂。",
        "dynasty": "宋",
        "collection_info": null
    }
]