[
    {
        "uuid": "0bc80c545d17052239950ed242615718",
        "title": "咏石菖蒲",
        "author": "释藴常",
        "content": "细水围棋石，纤纤手自移。\n几年离雁荡，万里到天池。\n浮玉春风后，小姑烟雨时。\n它年怀胜绝，魂梦亦清奇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "25e9785f647d1c7f67c919ab5dcb06a2",
        "title": "咏兰",
        "author": "释藴常",
        "content": "目断山河恨莫裁，折芳犹记小徘徊。\n细看叶底春风面，疑自幽篁影下来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "44733b9f5246dc1d2ca38dc0ae572e93",
        "title": "送空上人",
        "author": "释藴常",
        "content": "过了梨花春亦归，小窗新绿正相宜。\n白头更作西州梦，细雨青灯话别离。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "50ae6059bf9b5bf2d7441ebfeb49cd7a",
        "title": "天竺道中",
        "author": "释藴常",
        "content": "紫兰含春风，日暮香更远。\n涧道水平分，曲折渡清浅。\n飞花当面堕，颠倒落苔藓。\n念此芳意阑，归思纷莫遣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2f416d70ae9d2b7efbfc9ef4f7bca0b8",
        "title": "知止亭晚望寄清大师",
        "author": "释藴常",
        "content": "柳绕平湖绿映堤，栏干人静夕阳迟。\n归鸦欲尽千林暝，白鸟飞来一段奇。\n短髮缘愁分壮志，断云含雨入新诗。\n不堪更作怀人恨，歌罢竹枝成别离。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b36fbfc2d8c4cbf45ac5756e4d193fef",
        "title": "别苏养直",
        "author": "释藴常",
        "content": "老去难为别，愁边更着秋。\n碧芦围野水，落日满行舟。\n雁断西风急，天寒古寺幽。\n两乡无百里，能寄短书否。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e2609c44504ffc57a57d6538f51d50a4",
        "title": "登虎丘即事",
        "author": "释藴常",
        "content": "九月凭栏已怯风，鬓霜留我客愁中。\n夕阳看尽烟生树，野水无情天四空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c93c0bd41298b84d7d5de3eb332dce8",
        "title": "春日",
        "author": "释藴常",
        "content": "烧灯过了客思家，独立衡门数晚鸦。\n燕子未归梅落尽，小窗明月属梨花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a92d1beb80f5e40e9ad5f260d351f7a5",
        "title": "江村",
        "author": "释藴常",
        "content": "江村风急苇花飞，漠漠吹烟树影稀。\n亦有人家沙际住，夕阳鸡犬傍人归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8547e11b323c57024b9e656c7a8e705a",
        "title": "游天台赤城山",
        "author": "释藴常",
        "content": "拄藜扶我过桥东，便觉晴岚翠扫空。\n紫府百年藏玉简，丹崖千丈挹仙风。\n释籤书到新罗国，卧佛岩连旧梵宫。\n稽首导师悲愿在，我宁辛苦守诗穷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "efb6a18b1d6659857caf30ce7e9ecd63",
        "title": "句  其一",
        "author": "释藴常",
        "content": "断桥人寂寂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "65b58e92a6e8ca39c6c9e5748a78e249",
        "title": "句  其二",
        "author": "释藴常",
        "content": "尘埃自衮衮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2041d6b2fdb61e937d0758c41740582f",
        "title": "句  其三",
        "author": "释藴常",
        "content": "归舟藏别浦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c7cfb99a44ad224b97675460f7d4d48b",
        "title": "句  其四",
        "author": "释藴常",
        "content": "碧涧流红叶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5d79cb23fd5e2a1a5b10280c13368fbd",
        "title": "句  其五",
        "author": "释藴常",
        "content": "云寒湖贴贴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "831cb92e4e65211d4bb855cd9ccf15e8",
        "title": "句  其六",
        "author": "释藴常",
        "content": "端为故人留。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ee8b5ab91412e78064858b36da7b865e",
        "title": "句  其七",
        "author": "释藴常",
        "content": "愁来独赋诗。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b7634eabc8a7b8e189ff5ab29f4aa810",
        "title": "句  其八",
        "author": "释藴常",
        "content": "柳色葱葱官渡平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4dee5c2319569f7094d67041ef219349",
        "title": "句  其九",
        "author": "释藴常",
        "content": "十年归梦满青山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8b4c8c41e63cbedb07524272ab856611",
        "title": "句  其一○",
        "author": "释藴常",
        "content": "月生更爱栏干影。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e7e3ec8686f8c6239f3bdac688d6028a",
        "title": "句  其一一",
        "author": "释藴常",
        "content": "白髮青灯两无语。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e2e0492fc897725d6c6dc271c7cebb9b",
        "title": "句  其一二",
        "author": "释藴常",
        "content": "寒涕垂颐独掩关。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e05f660f8c4a83b0dfc82c831d2b6513",
        "title": "句  其一三",
        "author": "释藴常",
        "content": "吸江亭下雨初晴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0f41f5e5196fc2910b61f3d7bb5ffe3f",
        "title": "句  其一四",
        "author": "释藴常",
        "content": "正是周王二月时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b36487364eeea8f7ec91131ea6330b1a",
        "title": "句  其一五",
        "author": "释藴常",
        "content": "细看横枝閒绿筠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1edc4f83ea13bd9db5fe1043ced073bf",
        "title": "句  其一六",
        "author": "释藴常",
        "content": "眼明端为小迟留。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cdca73e15f6ee321f636231238ad810b",
        "title": "句  其一七",
        "author": "释藴常",
        "content": "栏干倚徧斜阳晚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "431f34ecfea24927ce2631a8985be604",
        "title": "句  其一八",
        "author": "释藴常",
        "content": "愁裏有诗人不会。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e832716dfc94d538cd2775efd514285",
        "title": "句  其一九",
        "author": "释藴常",
        "content": "小舟夜宿越州城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "53c22a01007d08a88a88a90a70c3ecfa",
        "title": "句  其二○",
        "author": "释藴常",
        "content": "全是蓬莱十月天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "30e1d13ba9681d8aeacd1205e57f4309",
        "title": "句  其二一",
        "author": "释藴常",
        "content": "拄杖寻诗独自行。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c49f9c9024ed3a404e6d8c292b845e50",
        "title": "句  其二二",
        "author": "释藴常",
        "content": "竹裏梅花映白头。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3fc68f9f09d4b3125414118921bf7b0f",
        "title": "句  其二三",
        "author": "释藴常",
        "content": "梦迴彷佛三更后。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4a591bd1b545e2afcbcbeeab035503e2",
        "title": "句  其二四",
        "author": "释藴常",
        "content": "一枝斜映小窗明。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "976abf239d5bc07987471447856486b4",
        "title": "句  其二五",
        "author": "释藴常",
        "content": "不须更觅吴州画。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8f46e4d34047b63a02f53a398c02ff49",
        "title": "句  其二六",
        "author": "释藴常",
        "content": "诗到扬州定清绝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "715ff915b5962f39e9e2685c0b245803",
        "title": "句  其二七",
        "author": "释藴常",
        "content": "夕阳无语淡烟初。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6bb91cb5e3cdac798f4d7c62c38c7f9f",
        "title": "句  其二八",
        "author": "释藴常",
        "content": "断肠重看白鸥盟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a244e56e04479a5a7968696249c9f3dd",
        "title": "句  其二九",
        "author": "释藴常",
        "content": "青山断处夕阳垂。",
        "dynasty": "宋",
        "collection_info": null
    }
]