[
    {
        "uuid": "e135976c14b64438003d8f6b4fe2ba51",
        "title": "寄弟子慧山偈",
        "author": "释道琼",
        "content": "口觜不中祥老子，爱向丛林鼓是非。\n分付雪峰山首座，为吾痛骂莫饶伊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e7335a9af97d82ae6de77e7847c3a3ba",
        "title": "颂古二首  其一",
        "author": "释道琼",
        "content": "掩耳不及忽雷奔，天低尺五雨倾盆。\n叶随流水归何处，牛带寒鸦过别村。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d92f5400d830ab67d7b8697cec33be67",
        "title": "颂古二首  其二",
        "author": "释道琼",
        "content": "舍筏怀兼济，逢耕更问津。\n却将未归意，说与欲行人。",
        "dynasty": "宋",
        "collection_info": null
    }
]