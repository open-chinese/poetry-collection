[
    {
        "uuid": "143f340a1c180945fba605e8864c2dbc",
        "title": "寄弟子慧山偈",
        "author": "释道琼",
        "content": "口觜不中祥老子，爱向丛林鼓是非。\n分付雪峰山首座，为吾痛骂莫饶伊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "08d35ba63d2c14da8c4958cfdc88756b",
        "title": "颂古二首  其一",
        "author": "释道琼",
        "content": "掩耳不及忽雷奔，天低尺五雨倾盆。\n叶随流水归何处，牛带寒鸦过别村。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0f63d878eb5899d804a35be1874b29da",
        "title": "颂古二首  其二",
        "author": "释道琼",
        "content": "舍筏怀兼济，逢耕更问津。\n却将未归意，说与欲行人。",
        "dynasty": "宋",
        "collection_info": null
    }
]