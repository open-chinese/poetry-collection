[
    {
        "uuid": "8bdd51c74ed8f7eb97920de4cb450da8",
        "title": "武夷山昇真洞",
        "author": "陈觉民",
        "content": "昇真洞口接天门，灵草丹桃日日春。\n听说列仙来瑞世，三朝德业在斯民。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c3aa4072a71148718d59f212cc623099",
        "title": "五仙观",
        "author": "陈觉民",
        "content": "古木修篁绝世间，越王台殿作仙坛。\n白羊常在玉虚远，玄鹤不来金井寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "abe8dec5c2d1fd59f76a66746047d9af",
        "title": "寄米元章二首  其一",
        "author": "陈觉民",
        "content": "汩汩尘埃阅岁华，青山相见认空花。\n清淮风月元无价，凭仗诗翁为我赊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5a175fee4764f9cd82de0e56791f7884",
        "title": "寄米元章二首  其二",
        "author": "陈觉民",
        "content": "长淮千古自流东，六月城头日日风。\n天际玉潢无少处，夜山图在明月中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "22216999bba9d1ca5f1dcbc6c3349c5f",
        "title": "句  其一",
        "author": "陈觉民",
        "content": "却忆当年江处士，辞言秋色蔼人家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7b11e757f4942508b9fcfd5a5aeae734",
        "title": "句  其二",
        "author": "陈觉民",
        "content": "弔江为处士。",
        "dynasty": "宋",
        "collection_info": null
    }
]