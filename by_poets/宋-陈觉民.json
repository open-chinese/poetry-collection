[
    {
        "uuid": "a6fa503535970213a2d8be04229f80a8",
        "title": "武夷山昇真洞",
        "author": "陈觉民",
        "content": "昇真洞口接天门，灵草丹桃日日春。\n听说列仙来瑞世，三朝德业在斯民。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "725614996d3a7a898389655d5cdc9bf4",
        "title": "五仙观",
        "author": "陈觉民",
        "content": "古木修篁绝世间，越王台殿作仙坛。\n白羊常在玉虚远，玄鹤不来金井寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7bb6e4ff2e2786f9f7654286343009c2",
        "title": "寄米元章二首  其一",
        "author": "陈觉民",
        "content": "汩汩尘埃阅岁华，青山相见认空花。\n清淮风月元无价，凭仗诗翁为我赊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2c4be92049d7b3b5d93e4e842ee3773b",
        "title": "寄米元章二首  其二",
        "author": "陈觉民",
        "content": "长淮千古自流东，六月城头日日风。\n天际玉潢无少处，夜山图在明月中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "76c39b8f5d1a93a33ab3e3807912d46c",
        "title": "句  其一",
        "author": "陈觉民",
        "content": "却忆当年江处士，辞言秋色蔼人家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "94bfee3fa460dbe73fd6c7398406f623",
        "title": "句  其二",
        "author": "陈觉民",
        "content": "弔江为处士。",
        "dynasty": "宋",
        "collection_info": null
    }
]