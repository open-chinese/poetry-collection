[
    {
        "uuid": "7db5d2ba9dad0633627d59f48adc646b",
        "title": "诣李侍郎",
        "author": "王鲁復",
        "content": "文字元无底，功夫转到难。\n苦心三百首，暂请侍郎看。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "9ee655332eff58e6ba0d169952fab650",
        "title": "弔灵均",
        "author": "王鲁復",
        "content": "万古汨罗深，骚人道不沈。\n明明唐日月，应见楚臣心。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "afe294343007b71297ecc5f49251d6b1",
        "title": "弔韩侍郎",
        "author": "王鲁復",
        "content": "星落少微宫，高人入古风。\n几年才子泪，併写五言中。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "00b4e520675073951f758e316d5d8dad",
        "title": "故白岩禅师院",
        "author": "王鲁復",
        "content": "能师还世名还在，空闭禅堂满院苔。\n花树不随人寂寞，数枝犹自出墙来。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "98ee924cba5a6aca680c6b57fd63ea4b",
        "title": "水楼",
        "author": "王鲁復",
        "content": "山衔落日溪光动，岸转回风槛影浮。\n座内数声来远鹤，烟中一派辨孤舟。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "87c2d342729f80c475fda108e5d4a618",
        "title": "句",
        "author": "王鲁復",
        "content": "清泉遶屋澄心远，曙月衔山出定迟。",
        "dynasty": "唐",
        "collection_info": null
    }
]