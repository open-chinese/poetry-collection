[
    {
        "uuid": "b511290e00b648be7c1180961750da6d",
        "title": "苦阴",
        "author": "吴光",
        "content": "荒山干没更节序，浊气拥地愁无边。\n非烟非雾不见日，欲雨欲雪难为天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "855ba55ddcf1cf5606b3d1a5d884de00",
        "title": "回文暮春",
        "author": "吴光",
        "content": "娇声啭处藏鹦小，美睡浓时落日斜。\n桥拂柳溪深涨水，眼惊春雨过飞花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a0c69de56fa4598cf67966b192664c58",
        "title": "句  其一",
        "author": "吴光",
        "content": "檐头清响银匙动，阶下寒光玉碗翻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6b6ee83e345f7446f174f609de93cbd5",
        "title": "句  其二",
        "author": "吴光",
        "content": "也应只是寻常夜，未必如今分外圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5dcda7192d1331daaad0fac6f213d808",
        "title": "句  其三",
        "author": "吴光",
        "content": "独鹤下随鸡饮啄，众星高共月徘徊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "35179d4ec6d8786431216a249cf6f5bf",
        "title": "句  其四",
        "author": "吴光",
        "content": "半塘微涨聚红皱，几亩清阴锁碧鲜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "895ec13ea74a19ecadcbf5481269abca",
        "title": "句  其五",
        "author": "吴光",
        "content": "花蕊有鬚浑带蜜，桑枝无叶已成衣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "827df80a65c03e803106f3cddad0f8f4",
        "title": "句  其六",
        "author": "吴光",
        "content": "轻雷入树惊花魄，白浪浮空涨水脂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eda7651aab7839ad06ffd32cc53a5676",
        "title": "句  其七",
        "author": "吴光",
        "content": "日长人静闻风佩，雨久堂空生水衣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f9774b87c3653b5ea1e631ff057cb3d1",
        "title": "句  其八",
        "author": "吴光",
        "content": "二天开佛日，一道羃卿云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71d6909f5acc72e8cbc8d37d9dd1e795",
        "title": "句  其九",
        "author": "吴光",
        "content": "无苗何处寻黄犊，露地谁家觅白牛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "751ff6138153da60d41b001f6452f0cd",
        "title": "句  其一○",
        "author": "吴光",
        "content": "檐花谢女雪，径扫沈郎钱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "80a13f590282f289f67d5bd36c8d2b43",
        "title": "句  其一一",
        "author": "吴光",
        "content": "笛弄一声横钓艇，月明千里上层楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "88d0fd07f9132e6d932c7ff1003f9bff",
        "title": "句  其一二",
        "author": "吴光",
        "content": "写处似移墙上影，捲来如寄陇头春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ffcd22248683835dcce08ccb904dcc56",
        "title": "句  其一三",
        "author": "吴光",
        "content": "朱唇不注晓妆薄，玉颊频啼夜雨翻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7304d4326b6b0928975957326e2b8b96",
        "title": "句  其一四",
        "author": "吴光",
        "content": "农祥待白未为雪，春榜争魁欲放梅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3c55888d0adcd317bd3d07b37758792d",
        "title": "句  其一五",
        "author": "吴光",
        "content": "泉声如有语，山色自忘形。\n花觉青春半，山将白昼阴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c9b1a726f053efeb300394cfedf9d05b",
        "title": "句  其一六",
        "author": "吴光",
        "content": "冰看时俗薄，雨弄客愁多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cc39aa21262b33fd8afee27771d642fd",
        "title": "句  其一七",
        "author": "吴光",
        "content": "月借窗移疏影转，风翻雪放一枝高。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "56fce968dcac28063e0149aa54422745",
        "title": "句  其一八",
        "author": "吴光",
        "content": "蒹葭露滴思乡恨，芦荻风萦羁旅愁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eef2c832886d911c267d835c7a5ab74d",
        "title": "句  其一九",
        "author": "吴光",
        "content": "人情不似溪流水，不改当时枕上声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0864b4332737dd1d0faa62d71e66f1e1",
        "title": "句  其二○",
        "author": "吴光",
        "content": "一轴尘埃古涩体，十年灯火短长檠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cc7419beb78d0a63058104cafa06f8b6",
        "title": "句  其二一",
        "author": "吴光",
        "content": "梦过一年还是魇，心更万事竟成灰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "06f5f042e49dc80cc29ded924e768eb7",
        "title": "句  其二二",
        "author": "吴光",
        "content": "道上飞来燕，帘间不避人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "01a4b03f17e996d08361bc0443102152",
        "title": "句  其二三",
        "author": "吴光",
        "content": "弹压京畿贤大尹，藩宣上国古贤侯。",
        "dynasty": "宋",
        "collection_info": null
    }
]