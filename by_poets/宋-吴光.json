[
    {
        "uuid": "dc3033415f8c684690c1575ffd8a1997",
        "title": "苦阴",
        "author": "吴光",
        "content": "荒山干没更节序，浊气拥地愁无边。\n非烟非雾不见日，欲雨欲雪难为天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f78db3b4a255410f979a8094dcb877f0",
        "title": "回文暮春",
        "author": "吴光",
        "content": "娇声啭处藏鹦小，美睡浓时落日斜。\n桥拂柳溪深涨水，眼惊春雨过飞花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b51140427fcfb1c5c76cc96134bbf497",
        "title": "句  其一",
        "author": "吴光",
        "content": "檐头清响银匙动，阶下寒光玉碗翻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cab9802a981cc730f0af3da2ee0e76cf",
        "title": "句  其二",
        "author": "吴光",
        "content": "也应只是寻常夜，未必如今分外圆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7c047bceb6656c7728f09a545825811b",
        "title": "句  其三",
        "author": "吴光",
        "content": "独鹤下随鸡饮啄，众星高共月徘徊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e895c724c310b093ac83e1fe11f39058",
        "title": "句  其四",
        "author": "吴光",
        "content": "半塘微涨聚红皱，几亩清阴锁碧鲜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "873c1def94ce3bf0185cd5ed322b7c48",
        "title": "句  其五",
        "author": "吴光",
        "content": "花蕊有鬚浑带蜜，桑枝无叶已成衣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "490613e0274a9678cc7c591ed75657fd",
        "title": "句  其六",
        "author": "吴光",
        "content": "轻雷入树惊花魄，白浪浮空涨水脂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6cb7cac333dfa557b32d10ea2b123da4",
        "title": "句  其七",
        "author": "吴光",
        "content": "日长人静闻风佩，雨久堂空生水衣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "21eb4b20291f9336d510fc14392ebfaa",
        "title": "句  其八",
        "author": "吴光",
        "content": "二天开佛日，一道羃卿云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c190a1414ec401588d350f7d63b29b9e",
        "title": "句  其九",
        "author": "吴光",
        "content": "无苗何处寻黄犊，露地谁家觅白牛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "26ef6a0738c4ae8d63597d168d9feb4e",
        "title": "句  其一○",
        "author": "吴光",
        "content": "檐花谢女雪，径扫沈郎钱。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7e4ce8561815bc16a50328fe2b70e8f8",
        "title": "句  其一一",
        "author": "吴光",
        "content": "笛弄一声横钓艇，月明千里上层楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c5b0bb46a3cbee3e3f6dd7572558e8bf",
        "title": "句  其一二",
        "author": "吴光",
        "content": "写处似移墙上影，捲来如寄陇头春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e39f3a7b270df8f6cef90dcb8988a431",
        "title": "句  其一三",
        "author": "吴光",
        "content": "朱唇不注晓妆薄，玉颊频啼夜雨翻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c9dda76e78a5e86f45c567f8299fa870",
        "title": "句  其一四",
        "author": "吴光",
        "content": "农祥待白未为雪，春榜争魁欲放梅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "90584bd6e667873cd6d7c75af29ac5b3",
        "title": "句  其一五",
        "author": "吴光",
        "content": "泉声如有语，山色自忘形。\n花觉青春半，山将白昼阴。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d44e2c96377b0b1e0635dd08409328ce",
        "title": "句  其一六",
        "author": "吴光",
        "content": "冰看时俗薄，雨弄客愁多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ceb774bcfeb553535996dc3313d03c36",
        "title": "句  其一七",
        "author": "吴光",
        "content": "月借窗移疏影转，风翻雪放一枝高。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e3058845a9438f00b33e36b4b541dce3",
        "title": "句  其一八",
        "author": "吴光",
        "content": "蒹葭露滴思乡恨，芦荻风萦羁旅愁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "47443b0989574c203b729b53f5401ef0",
        "title": "句  其一九",
        "author": "吴光",
        "content": "人情不似溪流水，不改当时枕上声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fa13359473db9d82c8c66d620ea210cb",
        "title": "句  其二○",
        "author": "吴光",
        "content": "一轴尘埃古涩体，十年灯火短长檠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dc3df3e15b4ab2b313ac551658813a11",
        "title": "句  其二一",
        "author": "吴光",
        "content": "梦过一年还是魇，心更万事竟成灰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cd4477c46e430b244d1cd6579bc8874b",
        "title": "句  其二二",
        "author": "吴光",
        "content": "道上飞来燕，帘间不避人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d495d42e252212f0b4b89381ea39cb0e",
        "title": "句  其二三",
        "author": "吴光",
        "content": "弹压京畿贤大尹，藩宣上国古贤侯。",
        "dynasty": "宋",
        "collection_info": null
    }
]