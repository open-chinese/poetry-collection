[
    {
        "uuid": "ac7474824af7b15c1559a21093d4031d",
        "title": "蛱蝶行",
        "author": "卢频",
        "content": "东园宫草绿，上下飞相逐。\n君恩不禁春，昨夜花中宿。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ab7379da1c13a865e43593986de51839",
        "title": "东西行",
        "author": "卢频",
        "content": "种荷玉盆裏，不及沟中水。\n养雉黄金笼，见草心先喜。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8d59972270f57f4066d13eac767668b7",
        "title": "失题 一",
        "author": "卢频",
        "content": "春泪烂罗绮，泣声抽恨多。\n莫滴芙蓉池，愁伤连蔕荷。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "c9d3c8bd34bbf3fbe17c1325a71333f6",
        "title": "失题 二",
        "author": "卢频",
        "content": "一朵花叶飞，一枝花光彩。\n美人惜花心，但愿春长在。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b034ff250898a0d8c98e2c609d5a8372",
        "title": "失题",
        "author": "卢频",
        "content": "春泪烂罗绮，泣声抽恨多。\n莫滴芙蓉池，愁伤连蒂荷。\n一朵花叶飞，一枝花光彩。\n美人惜花心，但愿春长在。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷七百一十九"
        }
    }
]