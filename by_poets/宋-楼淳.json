[
    {
        "uuid": "3cae53a25d61c938d3e31aef31ebd2df",
        "title": "隠居凤山清明寺  其一",
        "author": "楼淳",
        "content": "凤鸟久不至，哲人叹已矣。\n空留千载名，寂寞残阳裏。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ce06c906f725157a8ad6e663bef91320",
        "title": "隠居凤山清明寺  其二",
        "author": "楼淳",
        "content": "日上钟未鸣，老僧睡犹熟。\n谁是山中人，长享清明福。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0121b3f903935459c193142efdf2ae75",
        "title": "清明寺",
        "author": "楼淳",
        "content": "风尘满天下，此地独清明。\n神州何处是，有泪落新亭。",
        "dynasty": "宋",
        "collection_info": null
    }
]