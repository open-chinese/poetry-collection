[
    {
        "uuid": "fa2f953445ed8c4aaf6dcfae6ea2c8aa",
        "title": "烟雨楼",
        "author": "李丹",
        "content": "干坤交泰气氤氲，烟雨标名望意云。\n自是圣时多瑞霭，莫疑蛮徼有妖氛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2e6496d32cb016cf7876c9c7ab3b2484",
        "title": "天堂地狱偈",
        "author": "李丹",
        "content": "释迦生中国，设教如周孔。\n周孔生西方，设教如释迦。\n天堂无则已，有则君子登。\n地狱无则已，有则小人入。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "68aee9aaf56009029bb7fd0834e5ee65",
        "title": "留题真阳南山",
        "author": "李丹",
        "content": "南山地久与天长，盘石安基势永昌。\n亿世皇图齐不拔，万年圣寿等无疆。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6aface053870ef9fc1295e858b1cf5bc",
        "title": "句",
        "author": "李丹",
        "content": "二顷田园祇旧时。",
        "dynasty": "宋",
        "collection_info": null
    }
]