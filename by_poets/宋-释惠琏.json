[
    {
        "uuid": "857b015b0d4d2a3b5d20a7afd29d2203",
        "title": "客愁",
        "author": "释惠琏",
        "content": "向来春兴自不浅，老去客愁还復深。\n半夜蜀禽呼梦破，柳风桃雨总关心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "863ba0ed28ce955f9f73c49706eb1367",
        "title": "古意",
        "author": "释惠琏",
        "content": "杏花枝头蛱蝶飞，似郎游冶忘归时。\n杏花落尽蛱蝶去，郎不归来向底嬉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6500135a42a1272b5189ad9d68e63dbc",
        "title": "别赵莘老",
        "author": "释惠琏",
        "content": "十里青山照眼，一篷疏雨催诗。\n记取江边作别，烟村梅子黄时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "97dec33ce25a511662634419155eede0",
        "title": "书楶上人庵",
        "author": "释惠琏",
        "content": "雨余野禽声断续，清阴碧流遶茅屋。\n道人乞食晚未归，独向残阳淡云木。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7b557fd4bfd35bbc1030f72306edca14",
        "title": "野月",
        "author": "释惠琏",
        "content": "野月今宵满，小庭人共瞻。\n却愁人去后，残魄独窥檐。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d9b51df243f1f627fade20adfcb9c15b",
        "title": "多雨",
        "author": "释惠琏",
        "content": "尽道春多雨，伤摧花易空。\n不知春态度，尤在雨阴中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9eaf47dadfe7c35cb624411b5cb717fa",
        "title": "舟行寒江曲港",
        "author": "释惠琏",
        "content": "扬帆出浦又入浦，转盼顺风还逆风。\n芦叶萧萧两岸合，蓼花细细一川红。\n鸥兼野鹜衝行棹，浪挟汀沙打卧篷。\n行李向来吾自决，漫将晴雨问渔翁。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "88a3430db2584298dbcbb666850de354",
        "title": "梅花",
        "author": "释惠琏",
        "content": "梦断笛悲风渚，吹阑月淡烟村。\n空想暗香靓色，难招□□□魂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9a9dafb7a0c46c3bb8088ca1a91c6892",
        "title": "句",
        "author": "释惠琏",
        "content": "瘦倚疏篁半出墙。",
        "dynasty": "宋",
        "collection_info": null
    }
]