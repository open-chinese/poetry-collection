[
    {
        "uuid": "e5267e74f97e3732f11eb5eaf479450e",
        "title": "问田夫",
        "author": "刘光",
        "content": "清晨出柴扉，偶遇耕田夫。\n为问尔田间，近曾得雨无。\n田夫欣欣言，旱势早已苏。\n昨午云四合，一雨半日余。\n高下粗沾溉，芃然蔼菑畬。\n老夫闻此言，欣怿当何如。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d73d3bb8dcef6a88e464d37a2fad89cb",
        "title": "句  其一",
        "author": "刘光",
        "content": "满地残红惊不见，晓风吹上石榴枝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dafd490cc445e2cb38e239028c11374c",
        "title": "句  其二",
        "author": "刘光",
        "content": "仙馆无人春寂寂，一林红日炼樱丹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "44376b57fffcc3dea42946ba81226f73",
        "title": "句  其三",
        "author": "刘光",
        "content": "晓来多谢梁间燕，尽把春愁举似人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "37354eb6bd4556af2cfbeacd5195d1be",
        "title": "句  其四",
        "author": "刘光",
        "content": "山翁何事衝寒往，知有梅花在隔溪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9bd32d80ca48a92c6fa0cc67ada60cf2",
        "title": "句  其五",
        "author": "刘光",
        "content": "归路夕阳收，山翠染襟袖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "19bc2783d31877ba9bc6244e4742aa1d",
        "title": "题栝苍洞宫",
        "author": "刘光",
        "content": "古洞藏真不记年，翠崖苍壁故依然。\n怪来一夜清无梦，身在仙家第十天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7eb040b002f9f775fd1c3ae7ec1af3a2",
        "title": "题南峰蓝光轩",
        "author": "刘光",
        "content": "百尺擎空窣堵波，羣峰奈此独高何。\n鼓钟鸣处是非外，天地望中苍翠多。\n閒看野云披玉柱，醉眠秋浪捲银河。\n问师乞取窗前月，一榻清风许我么。",
        "dynasty": "宋",
        "collection_info": null
    }
]