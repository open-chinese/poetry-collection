[
    {
        "uuid": "419348fa738a4e6fa848750e4675c75c",
        "title": "骆谷",
        "author": "崔觐",
        "content": "高峰偃蹇云崔巍，层崖巨壑长峡开。\n龙蛇纵横虎豹乱，古栈朽裂埋深谷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4adc6138210af782ac8b453bf833fc2b",
        "title": "兴元  其一",
        "author": "崔觐",
        "content": "地形连楚阔，山势入秦豪。\n平外斜通骆，深中远认褒。\n图经何壮观，故事有萧曹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "62c88c78011b2f3f70293ddc92324fda",
        "title": "兴元  其二",
        "author": "崔觐",
        "content": "寥寥官舍静于僧，虽有园亭亦倦登。\n桃李花阴流似水，图书滋味冷如冰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5534a2a9fb9f32f8e9f9f354ec0fe10c",
        "title": "句  其一",
        "author": "崔觐",
        "content": "断烟横沔水，孤鹜入洋州。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7e6eb8292974161f8039cd7cde743344",
        "title": "句  其二",
        "author": "崔觐",
        "content": "晚霭昏斜谷，晴阳灵斗山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "69a20df66e2e8c6ad51fda7d4baf703d",
        "title": "句  其三",
        "author": "崔觐",
        "content": "江水不流廉节去，清名长解胜贪泉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "68557c2fc2ce9ae51bf4a08c4cb0b132",
        "title": "句  其四",
        "author": "崔觐",
        "content": "岂知王子山前月，曾照曹刘夜战来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e9660b93f89ce34935db952af5a27fd4",
        "title": "句  其五",
        "author": "崔觐",
        "content": "直望汉江三百里，一条如綫下洋州。",
        "dynasty": "宋",
        "collection_info": null
    }
]