[
    {
        "uuid": "5715b2f19be89ab60db1be38e1075b4e",
        "title": "骆谷",
        "author": "崔觐",
        "content": "高峰偃蹇云崔巍，层崖巨壑长峡开。\n龙蛇纵横虎豹乱，古栈朽裂埋深谷。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4cf9bb8c7efab7461c89c241d7bdb129",
        "title": "兴元  其二",
        "author": "崔觐",
        "content": "寥寥官舍静于僧，虽有园亭亦倦登。\n桃李花阴流似水，图书滋味冷如冰。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "09b98c9dd4462c256c50c9e1d9ae4360",
        "title": "句  其一",
        "author": "崔觐",
        "content": "断烟横沔水，孤鹜入洋州。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "45edea0b890108a22b8044aa327a17f5",
        "title": "句  其二",
        "author": "崔觐",
        "content": "晚霭昏斜谷，晴阳灵斗山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c78753b4c4c797980d2f20fbfe400c6c",
        "title": "句  其五",
        "author": "崔觐",
        "content": "直望汉江三百里，一条如綫下洋州。",
        "dynasty": "宋",
        "collection_info": null
    }
]