[
    {
        "uuid": "392cb6fc6c1eed094836573a49129f2e",
        "title": "沙冈",
        "author": "赵方",
        "content": "问予何酷爱沙冈，奇秀山光接水光。\n槐树绿阴刚似盖，天风吹动蛰龙翔。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bfbae61c8d959b4db7278e7c77eb640f",
        "title": "岳亭",
        "author": "赵方",
        "content": "南岳高峰七十二，祝融峰直老人星。\n平生最有看山兴，一上斯亭双眼青。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dc58310bfda09f9f79bc79181d03adab",
        "title": "月池",
        "author": "赵方",
        "content": "道人院外一池横，源水流来静且清。\n明月自能夜夜到，镜光上下鑑虚盈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "65fff88a6be8550648193c941adad0c5",
        "title": "荷沼",
        "author": "赵方",
        "content": "露珠颗颗荡青荷，云锦重重铺碧波。\n外直中通心独苦，拟诸君子待如何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5110cf7fa5df99446a6439809b603b8a",
        "title": "水阁",
        "author": "赵方",
        "content": "四围绿水绕重重，孤阁高撑渺渺中。\n人在阁中忧思远，应随水到海涯东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dd15c321f8768c9e71fdf00e78d7ad72",
        "title": "溪鱼",
        "author": "赵方",
        "content": "溪流渺渺浄涟漪，鱼跃鱼潜乐自知。\n若逐桃花浪裏去，风雷相送入天池。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6a2cfc97842aeebc8d1e75d44800388e",
        "title": "龙潭",
        "author": "赵方",
        "content": "静深空洞碧沉沉，鑑影分明照我心。\n照我心于何处是，潜龙时见普甘霖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e009e1997c0a37d18ba2a2b0da172f2",
        "title": "石泉",
        "author": "赵方",
        "content": "石上泉流泉上石，泉之寒冽石坚贞。\n补天到海何须论，且慰往来行路人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "58436a9983c50d7f8d01d25dec56649d",
        "title": "幽谷",
        "author": "赵方",
        "content": "静虚空阔深盘旋，松挂薜萝笼紫烟。\n尝引我来寻诗料，和残皎皎白驹篇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "81ac3dff0980641e5d7ae7b2fad99d2c",
        "title": "东岑",
        "author": "赵方",
        "content": "读书茅屋白云岑，日月如流老欲侵。\n岑傍松杉栽万树，于兹十载已成林。",
        "dynasty": "宋",
        "collection_info": null
    }
]