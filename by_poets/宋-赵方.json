[
    {
        "uuid": "940cb6db736dfa6876fb1a5772f1a76d",
        "title": "沙冈",
        "author": "赵方",
        "content": "问予何酷爱沙冈，奇秀山光接水光。\n槐树绿阴刚似盖，天风吹动蛰龙翔。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8a16e5dda7c1cae11f5ea3928073f51e",
        "title": "岳亭",
        "author": "赵方",
        "content": "南岳高峰七十二，祝融峰直老人星。\n平生最有看山兴，一上斯亭双眼青。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b4f81e4cc54b2635befc0889b52499ac",
        "title": "月池",
        "author": "赵方",
        "content": "道人院外一池横，源水流来静且清。\n明月自能夜夜到，镜光上下鑑虚盈。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8f0f9f8f2dc940ee8e17205af546d4a0",
        "title": "荷沼",
        "author": "赵方",
        "content": "露珠颗颗荡青荷，云锦重重铺碧波。\n外直中通心独苦，拟诸君子待如何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aa706ea4303e0ec85948982f566c9214",
        "title": "水阁",
        "author": "赵方",
        "content": "四围绿水绕重重，孤阁高撑渺渺中。\n人在阁中忧思远，应随水到海涯东。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "013d97a3352592d56542d98fe2fcdaf1",
        "title": "溪鱼",
        "author": "赵方",
        "content": "溪流渺渺浄涟漪，鱼跃鱼潜乐自知。\n若逐桃花浪裏去，风雷相送入天池。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8d360f73c4ab9a998e49d05c909d1c47",
        "title": "龙潭",
        "author": "赵方",
        "content": "静深空洞碧沉沉，鑑影分明照我心。\n照我心于何处是，潜龙时见普甘霖。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eab91a413c75d9e1674cd783ab734b26",
        "title": "石泉",
        "author": "赵方",
        "content": "石上泉流泉上石，泉之寒冽石坚贞。\n补天到海何须论，且慰往来行路人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fd725f1b5233fa399332f9926c86d357",
        "title": "幽谷",
        "author": "赵方",
        "content": "静虚空阔深盘旋，松挂薜萝笼紫烟。\n尝引我来寻诗料，和残皎皎白驹篇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c9281129c566d90d1c6b3d6f37573462",
        "title": "东岑",
        "author": "赵方",
        "content": "读书茅屋白云岑，日月如流老欲侵。\n岑傍松杉栽万树，于兹十载已成林。",
        "dynasty": "宋",
        "collection_info": null
    }
]