[
    {
        "title": "句  其一",
        "author": "李缜",
        "content": "疑与月相吞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二",
        "author": "李缜",
        "content": "借发明年最早花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其三",
        "author": "李缜",
        "content": "客愁春恨两茫茫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其四",
        "author": "李缜",
        "content": "南园北园花满风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其五",
        "author": "李缜",
        "content": "岂待世间桃李风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其六",
        "author": "李缜",
        "content": "莫嫌春浅花房小。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其七",
        "author": "李缜",
        "content": "酥滴寒英露滴香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其八",
        "author": "李缜",
        "content": "兰替天香蜜替黄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其九",
        "author": "李缜",
        "content": "疑是西湖处士魂。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一○",
        "author": "李缜",
        "content": "又遣相思梦到家。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一一",
        "author": "李缜",
        "content": "直似山阴雪下时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一二",
        "author": "李缜",
        "content": "可怜几许怀春意。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一三",
        "author": "李缜",
        "content": "眼明初识道家装。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一四",
        "author": "李缜",
        "content": "仙衣襞积素中单。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一五",
        "author": "李缜",
        "content": "鶗鴂一声春事空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一六",
        "author": "李缜",
        "content": "几回梦作花间蝶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一七",
        "author": "李缜",
        "content": "似嫌时世妆梳浅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一八",
        "author": "李缜",
        "content": "女鬟夸巧幻寒芳。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一九",
        "author": "李缜",
        "content": "惆怅晚来风定后。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二○",
        "author": "李缜",
        "content": "从教革带减诗镮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二一",
        "author": "李缜",
        "content": "几回醉嚼春风骨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二二",
        "author": "李缜",
        "content": "天与风流春与娇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二三",
        "author": "李缜",
        "content": "暗香都自着人来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二四",
        "author": "李缜",
        "content": "童初宫裏雨斑斑。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二五",
        "author": "李缜",
        "content": "浅笑微颦尽断肠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二六",
        "author": "李缜",
        "content": "行人将底酬春色。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二七",
        "author": "李缜",
        "content": "蜂撩蝶嬲不相饶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二八",
        "author": "李缜",
        "content": "芒鞵远觅香来处。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二九",
        "author": "李缜",
        "content": "驼裘几折犯寒花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其三○",
        "author": "李缜",
        "content": "何人月下此幽寻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其三一",
        "author": "李缜",
        "content": "江头千树玉模糊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其三二",
        "author": "李缜",
        "content": "可怜风味故依然。",
        "dynasty": "宋",
        "collection_info": null
    }
]