[
    {
        "uuid": "f55760243c4bf7cbe1da4363ed3e1d51",
        "title": "石姥山",
        "author": "周兑",
        "content": "石貌苍颜迥不殊，天工偶尔赋形躯。\n顽然本是无情物，应误行人指望夫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1f242c16b9d50faed7ab3f9c5944c8a4",
        "title": "雨涌泉",
        "author": "周兑",
        "content": "杨岐山下出灵泉，与海相通亘古传。\n汹涌便知天欲雨，为言阴石不须鞭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e253ffb349fec81178d4c375d5f23835",
        "title": "题毛仙山",
        "author": "周兑",
        "content": "仙翁辟谷绿毛生，假向人间作姓称。\n应与桐君为伴侣，丹成白日共飞昇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cf7c5d121f2249382887bebf7020a2f3",
        "title": "罗汉影",
        "author": "周兑",
        "content": "法身无着本来空，应供随缘处处同。\n偶向石间留箇影，一轮秋日碧潭中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f57fc2ad55485a81547afb8dd2d73aa6",
        "title": "葛仙坛",
        "author": "周兑",
        "content": "稚川不恋晋衣冠，勾漏仙成九转丹。\n剩得数行风雨竹，人间珍重扫空坛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4636ddebb0a43b6d93fb3ffa31300bb7",
        "title": "九房石",
        "author": "周兑",
        "content": "巍巍古殿峙中央，匼匝屏风列九房。\n莫惜冥搜访仙迹，其间恐有紫金林。",
        "dynasty": "宋",
        "collection_info": null
    }
]