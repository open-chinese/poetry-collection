[
    {
        "uuid": "b77a9a83381e3cfb4e9a3ba0526e3f5a",
        "title": "石姥山",
        "author": "周兑",
        "content": "石貌苍颜迥不殊，天工偶尔赋形躯。\n顽然本是无情物，应误行人指望夫。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9f74561553d54c688ca4afea1af8ac68",
        "title": "雨涌泉",
        "author": "周兑",
        "content": "杨岐山下出灵泉，与海相通亘古传。\n汹涌便知天欲雨，为言阴石不须鞭。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "618922aece5c720fdefddbac93435c3e",
        "title": "题毛仙山",
        "author": "周兑",
        "content": "仙翁辟谷绿毛生，假向人间作姓称。\n应与桐君为伴侣，丹成白日共飞昇。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "28d8a06dac88aecba7d7d93eaec7fb6f",
        "title": "罗汉影",
        "author": "周兑",
        "content": "法身无着本来空，应供随缘处处同。\n偶向石间留箇影，一轮秋日碧潭中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5ce6829fb00d34a96230360bde72fa88",
        "title": "葛仙坛",
        "author": "周兑",
        "content": "稚川不恋晋衣冠，勾漏仙成九转丹。\n剩得数行风雨竹，人间珍重扫空坛。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f886c19e817e41b966adaca0e1445ce9",
        "title": "九房石",
        "author": "周兑",
        "content": "巍巍古殿峙中央，匼匝屏风列九房。\n莫惜冥搜访仙迹，其间恐有紫金林。",
        "dynasty": "宋",
        "collection_info": null
    }
]