[
    {
        "uuid": "9b6533ea8a1c5c83db257b3ef6973006",
        "title": "绝句  其一",
        "author": "释景淳",
        "content": "夜色中旬后，虚堂坐几更。\n临溪猿不叫，当槛月初生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6bcb44c6be600e324732ed4ed66cac96",
        "title": "绝句  其二",
        "author": "释景淳",
        "content": "后夜客来稀，幽斋独掩扉。\n月中无旁立，草际一萤飞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dac8df28d1ebe9795fb9826afe5c681c",
        "title": "句  其一",
        "author": "释景淳",
        "content": "捲箔西风起，乘时上古台。\n秋声随叶下，夜色带烟来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fca6bbb738d8198ed024efdc2de0740a",
        "title": "句  其二",
        "author": "释景淳",
        "content": "渔翁睡重春潭阔，白鸟不飞舟自横。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "22c377b2765a3cb689152c867db7b923",
        "title": "偈",
        "author": "释景淳",
        "content": "怕寒懒剃髼松髮，爱暖频添榾柮柴。\n破衲伽黎撩乱搭，谁能劳力强安排。",
        "dynasty": "宋",
        "collection_info": null
    }
]