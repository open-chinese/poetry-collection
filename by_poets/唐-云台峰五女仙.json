[
    {
        "uuid": "5c7566d86c43b9686f9c6bc9ab9363f7",
        "title": "会真诗 一",
        "author": "云台峰五女仙",
        "content": "人世徒纷扰，其生似梦华。\n谁言今昔裏，俯首视云霞。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "5bda8e1a117b6b035e550485446f53db",
        "title": "会真诗 二",
        "author": "云台峰五女仙",
        "content": "几劫澄烦思，今身仅小成。\n誓将云外隐，不向世间存。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ebe9d197f40b641f11a48a6ab8ee0030",
        "title": "会真诗 三",
        "author": "云台峰五女仙",
        "content": "绰约离尘世，从容上太清。\n云衣无绽日，鹤驾没遥程。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "3d1bf8515958475f142e05a398cf0002",
        "title": "会真诗 四",
        "author": "云台峰五女仙",
        "content": "华岳无三尺，东瀛仅一杯。\n入云骑彩凤，歌舞上蓬莱。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "28bf1970c66a16e754e95f99a1a1b43c",
        "title": "会真诗 五",
        "author": "云台峰五女仙",
        "content": "共作云山侣，俱辞世界尘。\n静思前日事，抛却几年身。",
        "dynasty": "唐",
        "collection_info": null
    }
]