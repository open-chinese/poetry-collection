[
    {
        "uuid": "b1e938c5fbfd6ba31aa05d0b02af1146",
        "title": "会真诗 一",
        "author": "云台峰五女仙",
        "content": "人世徒纷扰，其生似梦华。\n谁言今昔裏，俯首视云霞。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8fdaef4be238de16681ebf9dda26c4c7",
        "title": "会真诗 二",
        "author": "云台峰五女仙",
        "content": "几劫澄烦思，今身仅小成。\n誓将云外隐，不向世间存。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "808e9f56f2c7e6aa9b0548f6246bb255",
        "title": "会真诗 三",
        "author": "云台峰五女仙",
        "content": "绰约离尘世，从容上太清。\n云衣无绽日，鹤驾没遥程。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "ed9942a10e7ee79ab58c2ec6bfb927ac",
        "title": "会真诗 四",
        "author": "云台峰五女仙",
        "content": "华岳无三尺，东瀛仅一杯。\n入云骑彩凤，歌舞上蓬莱。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8334f4c1d07fb50113a9e60a1e4eba9c",
        "title": "会真诗 五",
        "author": "云台峰五女仙",
        "content": "共作云山侣，俱辞世界尘。\n静思前日事，抛却几年身。",
        "dynasty": "唐",
        "collection_info": null
    }
]