[
    {
        "uuid": "a02c9c4acf5a7d2af1bbfd86fc563398",
        "title": "南阳酇诗",
        "author": "王祜",
        "content": "置酒向南宫，分明将将雄。\n指踪为第一，大国赏元戎。\n似续惟人杰，危忘尽狗功。\n贤哉垂俭德，千古仰清风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "941f533874587ceb1506c2c8db8e1e62",
        "title": "赠率子连  其一",
        "author": "王祜",
        "content": "下瞰虚空临绝涧，上排烟雾倚山颠。\n四边险绝无猿鸟，独卧深云三十年。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "56634a786d56c30ed6ba3a8013a9da69",
        "title": "赠率子连  其二",
        "author": "王祜",
        "content": "古星当崖映日间，年年常伴白云闲。\n糇粮丹火何从出，四面无人见下山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "af333fb5c2bd68e4c673fb44628871e4",
        "title": "赠率子连  其三",
        "author": "王祜",
        "content": "心意逍遥物莫知，山中山下识人稀。\n想君绝虑离尘土，不是王乔即令威。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "08e2a9b3c802c5c930a4f9aebd7521e7",
        "title": "句  其一",
        "author": "王祜",
        "content": "花片已随山下水，云层虚锁河中春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "380ba06883d08ffd67d6d79959081530",
        "title": "句  其二",
        "author": "王祜",
        "content": "诗狂直欲吞云雾，酒渴何妨吸洞庭。",
        "dynasty": "宋",
        "collection_info": null
    }
]