[
    {
        "uuid": "cd61efffc4dca85337a1e330c7789f33",
        "title": "南阳酇诗",
        "author": "王祜",
        "content": "置酒向南宫，分明将将雄。\n指踪为第一，大国赏元戎。\n似续惟人杰，危忘尽狗功。\n贤哉垂俭德，千古仰清风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b4d806d37cfe4c87e85c4752a5870370",
        "title": "赠率子连  其一",
        "author": "王祜",
        "content": "下瞰虚空临绝涧，上排烟雾倚山颠。\n四边险绝无猿鸟，独卧深云三十年。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dd91224eba68e8f81c85b0fd5b2c9ea6",
        "title": "赠率子连  其二",
        "author": "王祜",
        "content": "古星当崖映日间，年年常伴白云闲。\n糇粮丹火何从出，四面无人见下山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f3b0d3a93e43a62ab02530624e0cc587",
        "title": "赠率子连  其三",
        "author": "王祜",
        "content": "心意逍遥物莫知，山中山下识人稀。\n想君绝虑离尘土，不是王乔即令威。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "402983fd4e7d2c01eb4facefa05cad3e",
        "title": "句  其一",
        "author": "王祜",
        "content": "花片已随山下水，云层虚锁河中春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed295ce5f3e670c105e759fde35f1656",
        "title": "句  其二",
        "author": "王祜",
        "content": "诗狂直欲吞云雾，酒渴何妨吸洞庭。",
        "dynasty": "宋",
        "collection_info": null
    }
]