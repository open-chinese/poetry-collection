[
    {
        "uuid": "4cd593809bf8a4e0343d9cc160cf7275",
        "title": "环溪夜坐",
        "author": "吴璋",
        "content": "江天閒晚已斜阳，静掩柴门对草堂。\n叶落转枝翻鹘影，星飞横水带萤光。\n微风得隽驱残暑，新月出音生嫩凉。\n坐觉秋容转清爽，一声渔笛在沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2c043d013c187b8e6ef6bb5c736de212",
        "title": "上人陈情十首之一",
        "author": "吴璋",
        "content": "下笔非能不自休，清谈未足第三流。\n梦中读易疑神动，醉裏吟诗想鬼愁。\n飞鹘岂徒惊燕雀，骑馿直欲度骅骝。\n此君须异无人识，惭愧当年遇褚裒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cdece4ec89742f238ff4331105e97cc8",
        "title": "句  其一",
        "author": "吴璋",
        "content": "各自芬芳花默默，两相呼唤鸟津津。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b90952d0cedd0db40cec253d25efde95",
        "title": "句  其二",
        "author": "吴璋",
        "content": "风急城闉砧杵动，雨余园圃辘轳收。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1462e1adb5fd1de2bc688e86ce1c4558",
        "title": "句  其三",
        "author": "吴璋",
        "content": "始讶长郊从此断，忽惊行客近前来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8491cd89fc0c4cacd344853315f84081",
        "title": "句  其四",
        "author": "吴璋",
        "content": "倒悬欲堕花如缀，背立将飞鸟似惊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b3c6630268a113b5b0a7fd9eb2371481",
        "title": "句  其五",
        "author": "吴璋",
        "content": "池塘不用梦春草，风雪何须在灞桥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c37d0b4cbcf0813dbedf8ff8571c9d50",
        "title": "句  其六",
        "author": "吴璋",
        "content": "惜别有情空执手，赠行无绪不成章。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e7974bcc2071d418e43f1eee487b1053",
        "title": "句  其七",
        "author": "吴璋",
        "content": "雁飞云抹欹斜字，鱼跃波摇出没星。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0dc952f896684d44852f9cc6faa32207",
        "title": "句  其八",
        "author": "吴璋",
        "content": "水流天际惊春动，山入云间斗碧攒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7648cd2c8b6b190a850ffbf79eeb6edf",
        "title": "句  其九",
        "author": "吴璋",
        "content": "地暖尚疑消玉马，云开早欲放金雅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6df1831611b3a5b5326befa9bf891ce5",
        "title": "句  其一○",
        "author": "吴璋",
        "content": "衣无锦绣劳心织，味乏膏粱困舌耕。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c3e79393429adfa7327296005681fb6f",
        "title": "句  其一一",
        "author": "吴璋",
        "content": "閒中坐作蜘蛛隠，梦裏逃成燕雀禅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d7d06c3afb71f7bb527d34f2becf0173",
        "title": "句  其一二",
        "author": "吴璋",
        "content": "弈棋但以忘忧耳，纵酒无如作病何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5408117173a356feb4d4efb6b9b8162e",
        "title": "句  其一三",
        "author": "吴璋",
        "content": "龙肝雉尾瓜莼好，玉核金槌枣柰香。\n只有牛心犹未割，愿如逸少得先尝。",
        "dynasty": "宋",
        "collection_info": null
    }
]