[
    {
        "uuid": "95e05c8e95e1fe2013e23552b02e077d",
        "title": "环溪夜坐",
        "author": "吴璋",
        "content": "江天閒晚已斜阳，静掩柴门对草堂。\n叶落转枝翻鹘影，星飞横水带萤光。\n微风得隽驱残暑，新月出音生嫩凉。\n坐觉秋容转清爽，一声渔笛在沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c5b515d9fb68f1ade46b0a2360bdf64f",
        "title": "上人陈情十首之一",
        "author": "吴璋",
        "content": "下笔非能不自休，清谈未足第三流。\n梦中读易疑神动，醉裏吟诗想鬼愁。\n飞鹘岂徒惊燕雀，骑馿直欲度骅骝。\n此君须异无人识，惭愧当年遇褚裒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "732c243a38ac74cbdc547ab3f2c46018",
        "title": "句  其一",
        "author": "吴璋",
        "content": "各自芬芳花默默，两相呼唤鸟津津。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4a914ff0715c457e60eb876b65d05815",
        "title": "句  其二",
        "author": "吴璋",
        "content": "风急城闉砧杵动，雨余园圃辘轳收。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7c5b10adad946165ec73ae1fe52d51b7",
        "title": "句  其三",
        "author": "吴璋",
        "content": "始讶长郊从此断，忽惊行客近前来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "703c514ca18460d61bca27966281a954",
        "title": "句  其四",
        "author": "吴璋",
        "content": "倒悬欲堕花如缀，背立将飞鸟似惊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "721871002c70b8ef435102f1e9134fa9",
        "title": "句  其五",
        "author": "吴璋",
        "content": "池塘不用梦春草，风雪何须在灞桥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed47dbaafbeafaa6755d42b1540e6777",
        "title": "句  其六",
        "author": "吴璋",
        "content": "惜别有情空执手，赠行无绪不成章。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2d23fa6e8db86841ab03bea554915ed0",
        "title": "句  其七",
        "author": "吴璋",
        "content": "雁飞云抹欹斜字，鱼跃波摇出没星。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fab19ae8ad27a3a86ea8bcea7202b979",
        "title": "句  其八",
        "author": "吴璋",
        "content": "水流天际惊春动，山入云间斗碧攒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13ebb8aeb8b76c1e20ea65fb5ffc7125",
        "title": "句  其九",
        "author": "吴璋",
        "content": "地暖尚疑消玉马，云开早欲放金雅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "369dd43b5f584fac469dcbca698c37a2",
        "title": "句  其一○",
        "author": "吴璋",
        "content": "衣无锦绣劳心织，味乏膏粱困舌耕。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "508c387d048fa4839f3528061f3354cc",
        "title": "句  其一一",
        "author": "吴璋",
        "content": "閒中坐作蜘蛛隠，梦裏逃成燕雀禅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "633f7abd31721617d89983a53e2b0bf8",
        "title": "句  其一二",
        "author": "吴璋",
        "content": "弈棋但以忘忧耳，纵酒无如作病何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e4f56ab59809cfceeb36a4d64b96cd07",
        "title": "句  其一三",
        "author": "吴璋",
        "content": "龙肝雉尾瓜莼好，玉核金槌枣柰香。\n只有牛心犹未割，愿如逸少得先尝。",
        "dynasty": "宋",
        "collection_info": null
    }
]