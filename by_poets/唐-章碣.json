[
    {
        "uuid": "7d2aa90747bedb3b39e1f660badb46e0",
        "title": "句",
        "author": "章碣",
        "content": "事向无心得。\n（《老学庵笔记》卷四）。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "435de219c050e2db5ee141d450e8dcd4",
        "title": "城南偶题",
        "author": "章碣",
        "content": "谁家朱阁道边开，竹拂栏干满壁苔。\n野水不知何处去，可惜登临好光景，五门须听鼓声回。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "f82991940bac67b6d75f974e3c000cd9",
        "title": "赠边将",
        "author": "章碣",
        "content": "千千铁骑拥尘红，去去平吞万里空。\n宛转龙蟠金剑雪，只有河源与辽海，如今全属指麾中。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "e91278d5e1b993c99a84e6a4f724ef71",
        "title": "桃源",
        "author": "章碣",
        "content": "绝壁相欹是洞门，昔人从此入仙源。\n数株花下逢珠翠，山前空有无情水，犹绕当时碧树村。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "760082d41c6344192ba289890266eeae",
        "title": "曲江",
        "author": "章碣",
        "content": "日照香尘逐马蹄，风吹浪溅几回堤。\n无穷罗绮填花径，只缘频燕蓬洲客，引得游人去似迷。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "d4bef3c88ccda226bebd0878556ad8da",
        "title": "送韦岫郎中典泗州",
        "author": "章碣",
        "content": "玉皇恩诏别星班，去压徐方分野间。\n有鸟尽巢垂汴柳，想忆朝天独吟坐，旋飞新作过秦关。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "de778c30021f93b2d4e5d49949243e08",
        "title": "赠婺州苏员外",
        "author": "章碣",
        "content": "帝念琼枝欲并芳，星分婺女寄仙郎。\n鸾从阙下虽辞侣，即看龙虎西归去，便佐羲轩活万方。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "a6263990953894437fa5516b9728e696",
        "title": "寄友人",
        "author": "章碣",
        "content": "谢家山水属君家，曾共持钩掷岁华。\n竹裏竹鸡眠藓石，昨日西风动归思，满船凉叶在天涯。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "2254a9e080ac52b05f95b319ca0b64ee",
        "title": "雨",
        "author": "章碣",
        "content": "低着烟花漠漠轻，正堪吟坐掩柴扃。\n乱沾细网垂穷巷，霁来还有风流事，重染南山一遍青。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "1cedaf27b8b69bcbeceaf349988f0bdf",
        "title": "观锡宴",
        "author": "章碣",
        "content": "倾朝朱紫正骈阗，红杏青莎映广筵。\n不道楼台无锦绣，日暮骅骝相拥去，几人沉醉失金鞭。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "c2017f10225fbf716a473c61212ad414",
        "title": "城东即事",
        "author": "章碣",
        "content": "闲寻香陌凤城东，时暂开襟向远风。\n玉笛一声芳草外，回首汉宫烟霭裏，天河金阁未央宫。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "1b3aeba99d3a6d211e9b7290fdbb5568",
        "title": "夏日湖上即事寄晋陵萧明府",
        "author": "章碣",
        "content": "亭午羲和驻火轮，开门嘉树庇湖濆。\n行来宾客奇茶味，陶家岂是无诗酒，公退堪惊日已曛。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "211e0e980d3ec67b4a0ba44d507ed3df",
        "title": "对月",
        "author": "章碣",
        "content": "残霞卷尽出东溟，万古难消一片冰。\n公子踏开香径藓，别有洞天三十六，水晶台殿冷层层。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "5d15fd15aed1f4578f2b7ed67e382456",
        "title": "浙西送杜晦侍御入关",
        "author": "章碣",
        "content": "紫诏征贤发帝聪，绣衣行处扑香风。\n鹗归秦树幽禽散，鞭鞘所拂三千里，多少诸侯合避骢。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "c654f27c35bf3fd02f1846e5cc3a3766",
        "title": "寄江东道友",
        "author": "章碣",
        "content": "野亭歌罢指西秦，避俗争名兴各新。\n碧带黄麻呈缥缈，可惜人间好声势，片帆羸马不相亲。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "eafb65643b2832d39db8e3b53d5b1788",
        "title": "下第有怀",
        "author": "章碣",
        "content": "故乡朝夕有人还，欲作家书下笔难。\n灭烛何曾妨夜坐，但使他年遇公道，月轮长在桂珊珊。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "e68e76db35211600b598fb27f8f4e92b",
        "title": "春日经湖上友人别业",
        "author": "章碣",
        "content": "何处狂歌破积愁，携觞共下木兰舟。\n绿泉溅石银屏湿，一年一电逡巡事，不合花前不醉游。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "9da3593e8169ed9b7e296f97b536af51",
        "title": "长安春日",
        "author": "章碣",
        "content": "春日皇家瑞景迟，东风无力雨微微。\n六宫罗绮同时泊，输他得路蓬洲客，红绿山头烂醉归。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "72510b62994fcfdf85934e8cb6728a6d",
        "title": "陪浙西王侍郎夜宴",
        "author": "章碣",
        "content": "深锁雷门宴上才，旋看歌舞旋传杯。\n黄金鸂鶒当筵睡，小儒末座频倾耳，只怕城头画角催。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "99a07e14da5187bcd80e255a7e5005f3",
        "title": "春别",
        "author": "章碣",
        "content": "掷下离觞指乱山，趋程不待凤笙残。\n花边马嚼金衔去，殷勤莫厌貂裘重，恐犯三边五月寒。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "000bb266f20a31b62649c50b177499c9",
        "title": "送谢进士还闽",
        "author": "章碣",
        "content": "百越风烟接巨鳌，还乡心壮不知劳。\n雷霆入地建溪险，名场声利喧喧在，莫向林泉改鬓毛。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "3a674438acd681d6bfebc7658f653102",
        "title": "焚书坑",
        "author": "章碣",
        "content": "竹帛烟销帝业虚，关河空锁祖龙居。\n坑灰未冷山东乱，刘项元来不读书。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "84bbcbd6676a2a7ecbcbf8753410c974",
        "title": "东都望幸",
        "author": "章碣",
        "content": "懒修珠翠上高台，眉月连娟恨不开。\n纵使东巡也无益，君王自领美人来。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "5ac630467e37f755a2d89ebf476c194e",
        "title": "旅舍早起",
        "author": "章碣",
        "content": "迹暗心多感，神疲梦不游。\n惊舟同厌夜，独树对悲秋。\n晚角和人战，残星入汉流。\n门前早行子，敲镫唱离忧。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "691fee2de5fb3a0df4e76bfca10bb70d",
        "title": "癸卯岁毗陵登高会中贻同志",
        "author": "章碣",
        "content": "流落常嗟胜会稀，故人相遇菊花时。\n凤笙龙笛数巡酒，长杨羽猎须留本，开济重为阙下期。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "4b4211ac966eaffb5ee2baf4367d2663",
        "title": "上元夜建元寺观灯呈智通上人",
        "author": "章碣",
        "content": "建元看别上元灯，处处回廊斗火层。\n珠玉乱抛高殿佛，无限喧阗留不得，月华西下露华凝。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    },
    {
        "uuid": "97acb592caa4b475fd8f048fd71ef085",
        "title": "《变体诗",
        "author": "章碣",
        "content": "东南路尽吴江畔，正是穷愁暮雨天。\n鸥鹭不嫌斜两岸，今古若论英达算，鸱夷高兴固无边。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷六百六十九"
        }
    }
]