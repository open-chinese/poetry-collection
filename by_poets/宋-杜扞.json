[
    {
        "uuid": "9039d3bf9bbd6a20f378c503f0c9d3fb",
        "title": "引烟坞",
        "author": "杜扞",
        "content": "行爱种桃处，蒙蒙罩轻素。\n花深人不辨，似入迷仙路。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0f463cc8e6efddd0f975e0ddf70686ba",
        "title": "天泽源",
        "author": "杜扞",
        "content": "湛碧一泓深，潺湲泻不断。\n高穹沛四时，派别环周半。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d61b9f3259c27a2632c5805f890cf9ee",
        "title": "句  其一",
        "author": "杜扞",
        "content": "铁乌老龙挺，谁为辨雌雄。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eb2fd9cd94e47c7c2a4c2ba700926d0b",
        "title": "句  其二",
        "author": "杜扞",
        "content": "倘使在南阳，应为孔明石。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "051b06a0c7abce5074f0dad6d1b6abd5",
        "title": "句  其三",
        "author": "杜扞",
        "content": "应为览德归，来仪化灵迹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "70098fc596cfd7b29e059c9e110129c7",
        "title": "句  其四",
        "author": "杜扞",
        "content": "波心石船并，中载白莲馨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4512198292dd794ed73128a777e93665",
        "title": "句  其五",
        "author": "杜扞",
        "content": "上就巾子头，轩昂成皂盖。",
        "dynasty": "宋",
        "collection_info": null
    }
]