[
    {
        "uuid": "41fde827a26f1acc61926a3ae8feed22",
        "title": "天游观",
        "author": "林锡翁",
        "content": "览遍溪山最上头，琳宫贝阙更清幽。\n虽然未得长生诀，也道曾从天上游。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f61f7b73044009041abb2c7af5f43444",
        "title": "天柱峰",
        "author": "林锡翁",
        "content": "共工发怒亦徒然，炼石忙忙女补天。\n既用断鰲并立极，此峰何不立当先。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "764f581ced91ba8fe1be1b7e38f1e152",
        "title": "接笋岩",
        "author": "林锡翁",
        "content": "儿童日报竹平安，新笋如何折一痕。\n接住喜凭仙掌力，春风胡不长琅玕。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1021826265c7cb1917068fac612d3d0e",
        "title": "猫儿峰",
        "author": "林锡翁",
        "content": "猫儿峰小眼精光，来护仙家石廪粮。\n回顾威风何凛凛，山中安得有拖肠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f7a234213d96e9956761eaefa776c552",
        "title": "金鷄岩",
        "author": "林锡翁",
        "content": "几载修真洞裏人，只凭啁哳报天明。\n丹成不逐刘安去，只在岩头唱五更。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "43fbe387e497b4c730b3770c9143fde1",
        "title": "隠求斋",
        "author": "林锡翁",
        "content": "蜗名蝇利处樊笼，世事□其转眼空。\n惟有高人林下隠，只求明月与清风。",
        "dynasty": "宋",
        "collection_info": null
    }
]