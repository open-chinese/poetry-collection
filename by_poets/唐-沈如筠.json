[
    {
        "uuid": "03bc702185586d9aafb7c9e32d6d4e24",
        "title": "寄张征古",
        "author": "沈如筠",
        "content": "寂历远山意，微冥半空碧。\n绿萝无冬春，彩云竟朝夕。\n张子海内奇，久为岩中客。\n圣君当梦想，安得老松石。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "2c44411be22c1eae3a125c3626dee061",
        "title": "闺怨二首 一",
        "author": "沈如筠",
        "content": "鴈尽书难寄，愁多梦不成。\n愿随孤月影，流照伏波营。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "2dea6d656d8f91bcc67dd9cea80a4031",
        "title": "闺怨二首 二",
        "author": "沈如筠",
        "content": "陇底嗟长别，流襟一动君。\n何言幽咽所，更作死生分。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "d3fd700c99c92e7b36ee24e902e0301f",
        "title": "寄天台司马道士",
        "author": "沈如筠",
        "content": "河洲花艳爚，庭树光彩蒨。\n白云天台山，可思不可见。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "643a9bc151f06ade4f9b405f2462e4ed",
        "title": "句",
        "author": "沈如筠",
        "content": "思酸寒鴈断，淅沥秋树空。\n渔阳燕旧都，美女花不如。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "cc3bde0df841818f9d2c2d6b117fac73",
        "title": "闺怨二首",
        "author": "沈如筠",
        "content": "雁尽书难寄，愁多梦不成。\n愿随孤月影，流照伏波营。\n陇底嗟长别，流襟一动君。\n何言幽咽所，更作死生分。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷一百一十四"
        }
    },
    {
        "uuid": "443607262943e0d62d7fc17e687d781f",
        "title": "句",
        "author": "沈如筠",
        "content": "思酸寒雁断，淅沥秋树空。\n渔阳燕旧都，美女花不如。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷一百一十四"
        }
    }
]