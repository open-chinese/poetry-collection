[
    {
        "uuid": "40a4b0d547d6f7c6b01ee9468a114194",
        "title": "寄张征古",
        "author": "沈如筠",
        "content": "寂历远山意，微冥半空碧。\n绿萝无冬春，彩云竟朝夕。\n张子海内奇，久为岩中客。\n圣君当梦想，安得老松石。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "f4a9a724e2a0f3f3e6eabdc108a33776",
        "title": "闺怨二首 一",
        "author": "沈如筠",
        "content": "鴈尽书难寄，愁多梦不成。\n愿随孤月影，流照伏波营。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "8685a5c115620a55d901c011f3e825ad",
        "title": "闺怨二首 二",
        "author": "沈如筠",
        "content": "陇底嗟长别，流襟一动君。\n何言幽咽所，更作死生分。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "fc559f0c9844e3ef4d54c6bc2264d125",
        "title": "寄天台司马道士",
        "author": "沈如筠",
        "content": "河洲花艳爚，庭树光彩蒨。\n白云天台山，可思不可见。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "7d3fa169f24dcc647b20400ec2e89d1e",
        "title": "句",
        "author": "沈如筠",
        "content": "思酸寒鴈断，淅沥秋树空。\n渔阳燕旧都，美女花不如。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "3303e26ab99a7a52cbe6dd8e29cc3bf1",
        "title": "闺怨二首",
        "author": "沈如筠",
        "content": "雁尽书难寄，愁多梦不成。\n愿随孤月影，流照伏波营。\n陇底嗟长别，流襟一动君。\n何言幽咽所，更作死生分。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷一百一十四"
        }
    },
    {
        "uuid": "ff1f186e4188825f0e5cf8b07abaadd1",
        "title": "句",
        "author": "沈如筠",
        "content": "思酸寒雁断，淅沥秋树空。\n渔阳燕旧都，美女花不如。",
        "dynasty": "唐",
        "collection_info": {
            "collection": "御定全唐诗",
            "volume": "卷一百一十四"
        }
    }
