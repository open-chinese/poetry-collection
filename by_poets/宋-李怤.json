[
    {
        "uuid": "1a019af2dec938dd8cda23f3c75dd332",
        "title": "春晚",
        "author": "李怤",
        "content": "花瘦烟羸可奈何，不关渠事鸟声和。\n无人扫地惊残梦，分付轻红上碧莎。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b52bb903babb04b5096f16778829eb4",
        "title": "句  其一",
        "author": "李怤",
        "content": "去国春城桃李花，枫林叶病尚天涯。\n今年九日风前帽，北客南舟雨后沙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f21209302fd6bdc90b3d15b3ab9aa70f",
        "title": "句  其二",
        "author": "李怤",
        "content": "苍黄避地小儿女，漂泊连床老弟兄。",
        "dynasty": "宋",
        "collection_info": null
    }
]