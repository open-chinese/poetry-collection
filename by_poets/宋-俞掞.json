[
    {
        "uuid": "4a1f4b6ff8e7b721de19e669cfd89493",
        "title": "白华岩",
        "author": "俞掞",
        "content": "风剪飞丝雨脚残，半山虚旷九秋寒。\n洗开万里干坤眼，更上苍岩顶上看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "13af55e47910867214d3637156d10e33",
        "title": "三岩",
        "author": "俞掞",
        "content": "三岩之下有贤良，暮史朝经万卷长。\n树老山空人已尽，烱然千古姓名香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3ac8d7308358db5ad0d9b8579dcc6db5",
        "title": "六石峰",
        "author": "俞掞",
        "content": "劚石云边着着高，几经吟笔写风骚。\n山翁莫袖经纶手，好趁春风钓六鳌。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "76e786310317ffb138c938a663889724",
        "title": "浯溪  其一",
        "author": "俞掞",
        "content": "大唐有颂到浯溪，翠藓苍崖古画垂。\n西望函关今万里，淡烟斜日几荒碑。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f431562ff56ad43567a76f060849863f",
        "title": "浯溪  其二",
        "author": "俞掞",
        "content": "宋朝一统旧江山，南北中分已百年。\n壮士不须夸此颂，健提椽笔上燕然。",
        "dynasty": "宋",
        "collection_info": null
    }
]