[
    {
        "uuid": "a1f70d6686f7f69a11342ee6d3a61cd0",
        "title": "题清辉堂",
        "author": "刘宗孟",
        "content": "曙光伦紫翠，秋气莫争青。\n山近岚同润，堂虚日共生。\n断霞云去接，夕照月来迎。\n翻想鲛人室，珠盘亦让明。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "65ee00cbc3fd0f3e304a0eeaacfe0d81",
        "title": "句  其一",
        "author": "刘宗孟",
        "content": "万山攒地窄，四气得春偏。\n分野侵秦壤，星河共楚天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4becc38513831c997b7e0491e0c0ea6c",
        "title": "句  其二",
        "author": "刘宗孟",
        "content": "气润梅蒸雨，阴浓柳鬰烟。\n夏深卢橘熟，花尽海棠妍。",
        "dynasty": "宋",
        "collection_info": null
    }
]