[
    {
        "uuid": "166730afecba3c943399987c18dc6a68",
        "title": "怀良人",
        "author": "葛鵶儿",
        "content": "蓬鬓荆钗世所稀，布裙犹是嫁时衣。\n胡麻好种无人种，正是归时不见归。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "13f161396cbc268dfd1c10496e3baafd",
        "title": "会仙诗 一",
        "author": "葛鵶儿",
        "content": "彩凤摇摇下翠微，烟光漠漠徧芳枝。\n玉窗仙会何人见，唯有春风仔细知。",
        "dynasty": "唐",
        "collection_info": null
    },
    {
        "uuid": "b7d6a8eba24dacb345bddd72433cb80d",
        "title": "会仙诗 二",
        "author": "葛鵶儿",
        "content": "烟霞迤逦接蓬莱，宫殿参差晓日开。\n羣玉山前人别处，紫鸾飞起望仙台。",
        "dynasty": "唐",
        "collection_info": null
    }
]