[
    {
        "uuid": "46c3e4dec23559b1550ee415ac9a565d",
        "title": "吕子进知睦州予追送累日别后寄之",
        "author": "张举",
        "content": "篱鷃云鹏各有程，匆匆相别未忘情。\n恨君不在篷笼底，共听萧萧夜雨声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cdb3a8bed8b9160e393b223efbc13d25",
        "title": "梦中作",
        "author": "张举",
        "content": "楚峡云娇宋玉愁，月明溪浄印银鈎。\n襄王定是思前梦，又抱霞衾上玉楼。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ebb8262f135f25f94e56840ef457ddd5",
        "title": "游山寺",
        "author": "张举",
        "content": "冻仆堆堆依竈燎，山僧草草具盘飧。\n井丹已厌尝葱叶，庾亮何劳借薤根。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9f049f1f7ede3d7c46c48dec5f92a143",
        "title": "句  其一",
        "author": "张举",
        "content": "无限寒鸦冒雨飞，红树高高出粉墙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9dfb36046c92981982c3e8622592222",
        "title": "句  其二",
        "author": "张举",
        "content": "苦厌尘沙随马足，却思风浪拍船头。",
        "dynasty": "宋",
        "collection_info": null
    }
]