[
    {
        "uuid": "967c71a033848f224ec3593d9062b6ad",
        "title": "山中有所思",
        "author": "王嵒",
        "content": "零零夜雨渍愁根，触物伤离好断魂。\n莫怪杜鹃飞去尽，紫微花裏有啼猿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e21f51458204766d59baa23e0c35f327",
        "title": "回旧山",
        "author": "王嵒",
        "content": "庾家楼上谢家池，处处风烟少旧知。\n明日落花谁共醉，野溪猿鸟恨归迟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c8e5e794131014d283370175ac17676",
        "title": "残冬客次资阳江",
        "author": "王嵒",
        "content": "淡云残雪簇江天，策蹇迟回客兴阑。\n持钵老僧来咒水，倚船商女待搬滩。\n沙翘白鹭非真静，竹映繁梅奈苦寒。\n阮籍莫嗟歧路异，旧山溪畔有渔竿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "183f26a62734a1586a94a35cf9bed2a6",
        "title": "句  其一",
        "author": "王嵒",
        "content": "何人轻大厦，放尔偃深云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4ef12eb10f79029cd97817fdc6fe8ae9",
        "title": "句  其二",
        "author": "王嵒",
        "content": "高僧惯来看，踏破绿苔纹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c534867a1423a4f81cf74981d8374b20",
        "title": "句  其三",
        "author": "王嵒",
        "content": "暗穿地脉龙先觉，密赞天工雨不知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a231d35153fee8e61e6d092bd9068ed5",
        "title": "句  其四",
        "author": "王嵒",
        "content": "溪分涧夺朝宗晚，残月残云信所之。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e44e883a613089aa2c24e086e96810c8",
        "title": "句  其五",
        "author": "王嵒",
        "content": "朝宗任迭千重浪，捧日能消几片云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9eac0f87b2e13cb27376eb25fdae9d8a",
        "title": "句  其六",
        "author": "王嵒",
        "content": "多惭亦偶休明代，击壤空随野老羣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c21f5202414b080f93fd2fdbfc96502",
        "title": "句  其七",
        "author": "王嵒",
        "content": "飞来疑是鹤，下处却寻鱼。",
        "dynasty": "宋",
        "collection_info": null
    }
]