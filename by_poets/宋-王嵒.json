[
    {
        "uuid": "bfc9819fed229677a9daad8a9a3a2365",
        "title": "残冬客次资阳江",
        "author": "王嵒",
        "content": "淡云残雪簇江天，策蹇迟回客兴阑。\n持钵老僧来咒水，倚船商女待搬滩。\n沙翘白鹭非真静，竹映繁梅奈苦寒。\n阮籍莫嗟歧路异，旧山溪畔有渔竿。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c58a0b83178e7c42f2e0c493b27b277",
        "title": "句  其一",
        "author": "王嵒",
        "content": "何人轻大厦，放尔偃深云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6c1a8b69e6528cd7f4d6c59e71b6eebd",
        "title": "句  其二",
        "author": "王嵒",
        "content": "高僧惯来看，踏破绿苔纹。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "86804393c629885c93e45119d30ca1cd",
        "title": "句  其三",
        "author": "王嵒",
        "content": "暗穿地脉龙先觉，密赞天工雨不知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed7c5743b3cdfad40c7673eb7f54aa07",
        "title": "句  其四",
        "author": "王嵒",
        "content": "溪分涧夺朝宗晚，残月残云信所之。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9479c4110369fc49542270f161739c05",
        "title": "句  其五",
        "author": "王嵒",
        "content": "朝宗任迭千重浪，捧日能消几片云。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0ebfd70d33d81ea1e0c6d205a00ce4b0",
        "title": "句  其六",
        "author": "王嵒",
        "content": "多惭亦偶休明代，击壤空随野老羣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ebb04dff01e9641d0d69ad41fb159338",
        "title": "句  其七",
        "author": "王嵒",
        "content": "飞来疑是鹤，下处却寻鱼。",
        "dynasty": "宋",
        "collection_info": null
    }
]