[
    {
        "uuid": "d27e9dde6ee86e707894764fd17bf37a",
        "title": "演雅  其一",
        "author": "汪韶",
        "content": "布谷不耕不穑，巧妇无褐无衣。\n提壶不可挹酒，络纬宁来贸丝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7c0aec832b55a5e6b94f7f856df07251",
        "title": "演雅  其二",
        "author": "汪韶",
        "content": "蜾蠃尧舜父子，鸿雁鲁卫弟兄。\n斗蚁滕薛争长，狎鸥晋郑寻盟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "96fbba0a1906c64ab53c951ac0cb29c6",
        "title": "读史",
        "author": "汪韶",
        "content": "南北几离合，江山一古今。\n秋风驼卧棘，春雨燕巢林。\n家国兴亡梦，英雄胜败心。\n九疑呼不起，愁绝暮云深。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "aab483f9b33c028fbeee71021e85e7e6",
        "title": "孙武",
        "author": "汪韶",
        "content": "为忧玩色堕军实，故假陈兵去二姬。\n却恨此机深莫悟，后人不谏受西施。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "da568085f9f2992b00772dec783b01e9",
        "title": "句",
        "author": "汪韶",
        "content": "十八九常如意少，百千亿任化身多。",
        "dynasty": "宋",
        "collection_info": null
    }
]