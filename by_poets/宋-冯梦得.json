[
    {
        "uuid": "d418ff8e03a0aab8c8d3a6506dc1bf59",
        "title": "忆乡歌四首  其一",
        "author": "冯梦得",
        "content": "侬是剑南人，惯识故乡好。\n绿叶接红花，辉映绥安道。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e936758142540ce1993046d4f62dedb",
        "title": "忆乡歌四首  其二",
        "author": "冯梦得",
        "content": "炎夏天颇热，侬门对溪住。\n摇荡采莲舟，深处疑风露。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "30b1c01d7cd6cfdebfa76377a8db0856",
        "title": "忆乡歌四首  其三",
        "author": "冯梦得",
        "content": "塞上秋始肃，野草叶叶黄。\n侬家有菊花，灼灼不忧霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "82705c39b57e68cda72778bed45242d6",
        "title": "忆乡歌四首  其四",
        "author": "冯梦得",
        "content": "塞北寒气逼，对火拥氍毹。\n犹忆侬家裏，蓬头两夹襦。",
        "dynasty": "宋",
        "collection_info": null
    }
]