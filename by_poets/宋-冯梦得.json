[
    {
        "uuid": "9c5026ab7b12c4992a111edc1b13310c",
        "title": "忆乡歌四首  其一",
        "author": "冯梦得",
        "content": "侬是剑南人，惯识故乡好。\n绿叶接红花，辉映绥安道。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "938413f801c76671add3686f1149e832",
        "title": "忆乡歌四首  其二",
        "author": "冯梦得",
        "content": "炎夏天颇热，侬门对溪住。\n摇荡采莲舟，深处疑风露。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c6f20c46b3624a0db392fa08ec9090c8",
        "title": "忆乡歌四首  其三",
        "author": "冯梦得",
        "content": "塞上秋始肃，野草叶叶黄。\n侬家有菊花，灼灼不忧霜。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "18331f8b5f22599c4eb3c389716deb38",
        "title": "忆乡歌四首  其四",
        "author": "冯梦得",
        "content": "塞北寒气逼，对火拥氍毹。\n犹忆侬家裏，蓬头两夹襦。",
        "dynasty": "宋",
        "collection_info": null
    }
]