[
    {
        "uuid": "e837a5122aceefc9d785fcb4e4cdcd4b",
        "title": "常州问东坡疾",
        "author": "释维琳",
        "content": "扁舟驾兰陵，自愞旧风物。\n君家有天人，雄雄维摩诘。\n我口吞文殊，千里来问疾。\n若以默相酬，露柱皆笑出。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c4e4cbbed57b2b6b201f5ff9f21a36a7",
        "title": "白云堂",
        "author": "释维琳",
        "content": "堂与碧山对，白云长作邻。\n日暮白云合，谁见白云心。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9065cd1353e2d9defd43dae9ae6f766f",
        "title": "有司欲取寺松供朝用感赋",
        "author": "释维琳",
        "content": "大夫去作栋梁材，无復清阴覆绿苔。\n今夜月明风露冷，误他千里鹤飞来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "da790af56032377e9e10f6bcedf44c7e",
        "title": "句",
        "author": "释维琳",
        "content": "瓯面乳花谁共看。",
        "dynasty": "宋",
        "collection_info": null
    }
]