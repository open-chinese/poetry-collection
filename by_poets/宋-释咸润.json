[
    {
        "uuid": "0584ef278839b4fdbd953369f8339de7",
        "title": "五泄山三学院十题 五泄",
        "author": "释咸润",
        "content": "飞泉成五级，一级一龙湫。\n削出万仞秀，讵让蓬莱丘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "933f1f4aa571bd49cee504ded101cdd1",
        "title": "五泄山三学院十题 西源",
        "author": "释咸润",
        "content": "造化凿绅维，千岩斗空碧。\n有时云雨开，残阳落幽石。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d1e50836eab57cdcdce97d396e3f0dc7",
        "title": "五泄山三学院十题 夹岩",
        "author": "释咸润",
        "content": "一径数里间，巉岩镇幽谷。\n中夹清溪流，白云长断续。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "57b2b28cd618626e448aad0fd4e863de",
        "title": "五泄山三学院十题 龙井",
        "author": "释咸润",
        "content": "寒井深无极，人谓藏虬龙。\n尝闻救炎旱，丝雨洒长空。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2ec6c5451d02c2d920d4fe1ec48f5451",
        "title": "五泄山三学院十题 石鼓",
        "author": "释咸润",
        "content": "巨石平如掌，天然状鼙鼓。\n击之还有声，分明含太古。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "126af579c3ee15d006493e480d1584a4",
        "title": "五泄山三学院十题 石门",
        "author": "释咸润",
        "content": "双峰起云际，髣髴五侯门。\n烟霞锁不开，曾无车马奔。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5e8d39699346569d6e3b2050874751af",
        "title": "五泄山三学院十题 石屏",
        "author": "释咸润",
        "content": "峭石状危屏，截断诸峰青。\n白云触不飞，仙掌空遗名。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "83977521746c44a4ae5d05dbfc86e1c5",
        "title": "五泄山三学院十题 俱胝岩",
        "author": "释咸润",
        "content": "昔人持此咒，岩坐岁寒深。\n梵言不復闻，雅名流至今。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f19a4f44e9d4c9d7a7e2d0bdb7b958a5",
        "title": "五泄山三学院十题 祷雨潭",
        "author": "释咸润",
        "content": "尝闻越邑民，祷雨碧岩畔。\n神龙如有期，为霖通旱暵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4fa656be88379ec20f89ae638b5a6aa0",
        "title": "五泄山三学院十题 摘星岩",
        "author": "释咸润",
        "content": "扪萝梯峭石，直上青云端。\n咫尺星斗光，冷射人心寒。",
        "dynasty": "宋",
        "collection_info": null
    }
]