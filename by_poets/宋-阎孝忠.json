[
    {
        "uuid": "d9ddf46254adb0369ff929c904ee8c62",
        "title": "寄郴守阮户部",
        "author": "阎孝忠",
        "content": "郴阳古名邦，四座拱山色。\n秀气钟异人，阛阓俨仙宅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5c79dc780278d6fc6f7efe05d95a80f",
        "title": "俯春亭",
        "author": "阎孝忠",
        "content": "城上危亭可摘云，四边山色翠为邻。\n下窥城郭无余藴，草色花光尽是春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c8b74737324ab680b1b0d956daee1865",
        "title": "黄相山",
        "author": "阎孝忠",
        "content": "东带连山接五羊，西分郴水下三湘。\n路人到此休南去，岭外千峰尽瘴乡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e5eb8616b213460b2f7f3564239810e1",
        "title": "句  其一",
        "author": "阎孝忠",
        "content": "不辞远作潇湘客，且入三仙洞裏来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "63a411f903935c0fe9fcb3e7daa455df",
        "title": "句  其二",
        "author": "阎孝忠",
        "content": "橘柚人家黄压帽，松篁僧寺翠为邻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c4d1a3e2fc24674eb45af50eab6f0cf",
        "title": "句  其三",
        "author": "阎孝忠",
        "content": "贤如赠扇扬风日，贫似开门卧雪时。",
        "dynasty": "宋",
        "collection_info": null
    }
]