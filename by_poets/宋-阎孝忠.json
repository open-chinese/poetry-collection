[
    {
        "uuid": "d5bfb0b374b547820c6b460a220e2a2e",
        "title": "寄郴守阮户部",
        "author": "阎孝忠",
        "content": "郴阳古名邦，四座拱山色。\n秀气钟异人，阛阓俨仙宅。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "20343b0a06b9e657663f3944d15cb17c",
        "title": "俯春亭",
        "author": "阎孝忠",
        "content": "城上危亭可摘云，四边山色翠为邻。\n下窥城郭无余藴，草色花光尽是春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6bea9aa5de0bd0d48bd0a41008394af7",
        "title": "黄相山",
        "author": "阎孝忠",
        "content": "东带连山接五羊，西分郴水下三湘。\n路人到此休南去，岭外千峰尽瘴乡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9556e493ae31cc07d5bf70711a3615ba",
        "title": "句  其一",
        "author": "阎孝忠",
        "content": "不辞远作潇湘客，且入三仙洞裏来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c3419b5a7b0a665feff1c5c5573fc661",
        "title": "句  其二",
        "author": "阎孝忠",
        "content": "橘柚人家黄压帽，松篁僧寺翠为邻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7eabd068c97f584347f853b5c2a59409",
        "title": "句  其三",
        "author": "阎孝忠",
        "content": "贤如赠扇扬风日，贫似开门卧雪时。",
        "dynasty": "宋",
        "collection_info": null
    }
]