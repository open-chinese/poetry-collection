[
    {
        "title": "茉莉花",
        "author": "叶廷珪",
        "content": "露花洗出通身白，沈水熏成换骨香。\n近说根苗移上苑，休惭系本出南荒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其一",
        "author": "叶廷珪",
        "content": "不是妙音生妙指，只缘流水似流泉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其二",
        "author": "叶廷珪",
        "content": "人休清樾摩挲认，鸟泊高枝睥睨看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其三",
        "author": "叶廷珪",
        "content": "几多雁鹜行间吏，衙退频来礼释迦。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其四",
        "author": "叶廷珪",
        "content": "戍兵数有流星递，元白诗筒幸寄将。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其五",
        "author": "叶廷珪",
        "content": "道旁曾种万株松。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "title": "句  其六",
        "author": "叶廷珪",
        "content": "名字惟因佛书见，根苗应逐贾胡来。",
        "dynasty": "宋",
        "collection_info": null
    }
]