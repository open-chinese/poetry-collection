[
    {
        "uuid": "9fd63c57dcd6007bca7d4d91ef501fd3",
        "title": "黄山杂咏 桃花峰",
        "author": "邓宗度",
        "content": "刘阮辞世尘，桃花谁復主。\n无计殢春风，一夜零红雨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c13ae81977e1f2ed08df3cb909362487",
        "title": "黄山杂咏 石床峰",
        "author": "邓宗度",
        "content": "原非人力建，造化琢磨成。\n一枕游仙梦，蟾波白昼生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "39cfd15d93bdaa61d6e309b4f581a9db",
        "title": "黄山杂咏 祥符寺",
        "author": "邓宗度",
        "content": "云水最饶处，轮蹄无到时。\n夜蟾生佛面，春雪印僧眉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a54b3e2e23f48d75c598d0227850dac5",
        "title": "黄山杂咏 石门峰",
        "author": "邓宗度",
        "content": "暮云无意锁，春草岂能关。\n何事栖霞客，从兹去不还。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b3fc0b4d7758a22ef5befe25915d5a79",
        "title": "黄山杂咏 丹霞峰",
        "author": "邓宗度",
        "content": "绮段堆青壁，点缀秋颜色。\n几使晓来风，纵横裁不得。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0f4c8b63b6c8a1c52194acfdc4765c1f",
        "title": "黄山杂咏 滴水岩",
        "author": "邓宗度",
        "content": "石壁碎寒流，行人暮已愁。\n更倾风月耳，无水不生秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2df6eb04a992a1dd5dc0d648fc4cc410",
        "title": "黄山杂咏 苍豹",
        "author": "邓宗度",
        "content": "却隠他山雾，来眠此洞云。\n区区麛鹿辈，战栗敢子群。",
        "dynasty": "宋",
        "collection_info": null
    }
]