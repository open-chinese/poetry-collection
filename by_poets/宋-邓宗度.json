[
    {
        "uuid": "09102fe978fc7a6ce87719b8e5eae983",
        "title": "黄山杂咏 桃花峰",
        "author": "邓宗度",
        "content": "刘阮辞世尘，桃花谁復主。\n无计殢春风，一夜零红雨。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f0a11995fda5fff19a6582b6f40b16d3",
        "title": "黄山杂咏 石床峰",
        "author": "邓宗度",
        "content": "原非人力建，造化琢磨成。\n一枕游仙梦，蟾波白昼生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a9bd3b7089ddc5c8d93586bf11b2e830",
        "title": "黄山杂咏 祥符寺",
        "author": "邓宗度",
        "content": "云水最饶处，轮蹄无到时。\n夜蟾生佛面，春雪印僧眉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a341b87f40f01bd37f6ce43970d068d6",
        "title": "黄山杂咏 石门峰",
        "author": "邓宗度",
        "content": "暮云无意锁，春草岂能关。\n何事栖霞客，从兹去不还。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b894e94436bf4906323dd16c62ee73f4",
        "title": "黄山杂咏 丹霞峰",
        "author": "邓宗度",
        "content": "绮段堆青壁，点缀秋颜色。\n几使晓来风，纵横裁不得。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9e6ae2f2cd496f3716424cbb89a091a9",
        "title": "黄山杂咏 滴水岩",
        "author": "邓宗度",
        "content": "石壁碎寒流，行人暮已愁。\n更倾风月耳，无水不生秋。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d1d9cffd9bc20346bf1257b743a5afda",
        "title": "黄山杂咏 苍豹",
        "author": "邓宗度",
        "content": "却隠他山雾，来眠此洞云。\n区区麛鹿辈，战栗敢子群。",
        "dynasty": "宋",
        "collection_info": null
    }
]