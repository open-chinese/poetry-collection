[
    {
        "uuid": "cdb0295a1b4fe63df065231e3c29affd",
        "title": "武陵别文莹上人  其一",
        "author": "张师正",
        "content": "忆昔荆州屡过从，当时心已慕冥鸿。\n渚宫禅伯唐齐己，淮甸诗豪宋惠崇。\n老格疏閒松倚涧，清谈潇洒坐生风。\n史官若觅高僧事，莫把名参伎术中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e7cbd8b89cff8858358a766d5a9ebfcd",
        "title": "武陵别文莹上人  其二",
        "author": "张师正",
        "content": "碧嶂孤云冉冉归，解携情绪异常时。\n余生岁月能多少，此别应难约后期。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ed8b67ab9b0a7145ae4a0d4113325ebd",
        "title": "句  其一",
        "author": "张师正",
        "content": "旧将封侯尽，降王赐姓归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "439bdd089af3fab28cb8b6341feacf2f",
        "title": "句  其二",
        "author": "张师正",
        "content": "蜗角功名时不与，涧松材干老甘休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a0de50b39737a370f9adc518c7447d45",
        "title": "句  其三",
        "author": "张师正",
        "content": "分鹿是非皆委梦，落花贵贱不由人。",
        "dynasty": "宋",
        "collection_info": null
    }
]