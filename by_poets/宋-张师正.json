[
    {
        "uuid": "4479e042b02b64fe93cb11c0a93618af",
        "title": "武陵别文莹上人  其一",
        "author": "张师正",
        "content": "忆昔荆州屡过从，当时心已慕冥鸿。\n渚宫禅伯唐齐己，淮甸诗豪宋惠崇。\n老格疏閒松倚涧，清谈潇洒坐生风。\n史官若觅高僧事，莫把名参伎术中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b4dab4efa13e73bcabb858a9c726bc8",
        "title": "武陵别文莹上人  其二",
        "author": "张师正",
        "content": "碧嶂孤云冉冉归，解携情绪异常时。\n余生岁月能多少，此别应难约后期。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3f84272f256e5e99d148cd7e8d74c611",
        "title": "句  其一",
        "author": "张师正",
        "content": "旧将封侯尽，降王赐姓归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "73774f675a8afcc1300e00ffbbc6f8c0",
        "title": "句  其二",
        "author": "张师正",
        "content": "蜗角功名时不与，涧松材干老甘休。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "49de50722f2d6f000980df943c7cef8c",
        "title": "句  其三",
        "author": "张师正",
        "content": "分鹿是非皆委梦，落花贵贱不由人。",
        "dynasty": "宋",
        "collection_info": null
    }
]