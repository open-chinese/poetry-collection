[
    {
        "uuid": "67ad9ed1cd6c7ff169dacbeea9141722",
        "title": "嘉禾百咏 胥山",
        "author": "张尧同",
        "content": "马革浮尸去，君王太忍人。\n此山空庙貌，何以劝忠臣。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2bc692a461e97036da52e03aec60556a",
        "title": "嘉禾百咏 殳山",
        "author": "张尧同",
        "content": "丹竈功成后，寥寥掩竹扉。\n但闻蝉已蜕，不见鹤来归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1887e9c1d5a0fdabb10ca0d9605e43e3",
        "title": "嘉禾百咏 滮湖",
        "author": "张尧同",
        "content": "四境田相接，烟澜自渺瀰。\n客来吟此景，无或比滮池。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f667526ad78a04c7946750cdf80e0b10",
        "title": "嘉禾百咏 鸳鸯湖",
        "author": "张尧同",
        "content": "东西两湖水，相并比鸳鸯。\n湖裏鸳鸯鸟，双双锦翼长。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7e46a32380c20b8cd0733f9f2a84e861",
        "title": "嘉禾百咏 长水乡",
        "author": "张尧同",
        "content": "古来名未灭，人好水还清。\n一读高僧疏，尘劳悟此生。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0228e496ce7ace727c0d014e4b378e78",
        "title": "嘉禾百咏 谷水",
        "author": "张尧同",
        "content": "短棹经行处，风披藻荇香。\n中宵孤鹤唳，片月在沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "98fd978ab6a4fb62a3936768dcbadf7a",
        "title": "嘉禾百咏 天星河",
        "author": "张尧同",
        "content": "拍岸浮春渌，菰蒲四远生。\n网罗人不到，鱼鸟亦忘情。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0dda309915580e332b7d12beac9a6a8b",
        "title": "嘉禾百咏 穆溪",
        "author": "张尧同",
        "content": "静练明田外，源流笠泽通。\n不因逢岁旱，谁识济时功。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b432e2f79c5084409deec4e2e22e8399",
        "title": "嘉禾百咏 韭溪",
        "author": "张尧同",
        "content": "终与双溪接，分流入郭来。\n市桥人影合，不解洗尘埃。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c9ea03c42bb384ab893b7b8c0e25aa80",
        "title": "嘉禾百咏 双溪",
        "author": "张尧同",
        "content": "柔橹声相应，名齐汉魏收。\n可怜一溪水，分作两塘流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d4820236e5f38bad67862cc9ae2beace",
        "title": "嘉禾百咏 秀水",
        "author": "张尧同",
        "content": "好景明于昼，长浮五色波。\n一竿吾欲钓，来此听渔歌。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f143f84f9554efcad9fad7adafe7289b",
        "title": "嘉禾百咏 白龙潭",
        "author": "张尧同",
        "content": "呼吸风云合，飞潜自有时。\n只愁潭底浅，头角被人窥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d268d9acd68de7e3b44a80efd3ab4ef2",
        "title": "嘉禾百咏 御儿",
        "author": "张尧同",
        "content": "用此临吴战，何人为越谋。\n夫差曾不寤，亡国始知羞。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "29e79ccc97c6258390423698a255f488",
        "title": "嘉禾百咏 范蠡湖",
        "author": "张尧同",
        "content": "少伯曾居此，螺纹吐綵丝。\n一奁秋镜好，犹可照西施。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3dbaba58d176f8067004d5aa7c5d7b75",
        "title": "嘉禾百咏 梧桐泾",
        "author": "张尧同",
        "content": "落落梧桐树，何年作凤鸣。\n试看千古翠，流尽一溪声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6af62da121a5912cc300acddfc76bc1d",
        "title": "嘉禾百咏 汾湖",
        "author": "张尧同",
        "content": "我是沧浪叟，閒来繫钓艭。\n如何一湖水，丰秀半吴江。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "71db7b4b29123d8f20c059246cbc013e",
        "title": "嘉禾百咏 柘湖",
        "author": "张尧同",
        "content": "神居阴阳护，寻国捍洪波。\n莫虑蛟龙怒，年来畏叱呵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b9fae4e33e3f36661d51f87b576e82ad",
        "title": "嘉禾百咏 马塘泾",
        "author": "张尧同",
        "content": "疑着风波恶，民谣亦畏人。\n筑塘成堰后，斩马祭河神。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "eafa25b9ef1bd24134a6ec614009e93a",
        "title": "嘉禾百咏 杉青堰",
        "author": "张尧同",
        "content": "旧吏家何在，空寻古柳堤。\n世情今亦尔，莫笑买臣妻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4f0d4dcebd1eb35946e872a871ed74f0",
        "title": "嘉禾百咏 凤凰洲",
        "author": "张尧同",
        "content": "竹秀水色浄，忽来双凤凰。\n岐周昔鸣瑞，今復见文王。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0bbe9091b1be2b19b7e85cdcd62a1c7d",
        "title": "嘉禾百咏 龙舌头",
        "author": "张尧同",
        "content": "一吸长川静，烟云忽有无。\n谁看颔底月，误欲探骊珠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "c82dddd0851f80717f4b867413c8cef0",
        "title": "嘉禾百咏 月壕",
        "author": "张尧同",
        "content": "入径深于坞，沧波近马蹄。\n每寻莲社友，咫尺走东西。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3bd298d317977416a5af3e6b072d3ccc",
        "title": "嘉禾百咏 灵光井",
        "author": "张尧同",
        "content": "玉甃祥光发，难藏世上名。\n定多慈护力，一饮百疴轻。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2c1651c0678c012999a552051a19be41",
        "title": "嘉禾百咏 幽澜泉",
        "author": "张尧同",
        "content": "神女钟灵处，真堪疗渴羌。\n满罂秋玉色，一酌洒清凉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6f8cc3aa9a71b5f7f942209561c6e2bc",
        "title": "嘉禾百咏 槜李城",
        "author": "张尧同",
        "content": "螳螂方捕楚，黄雀遽乘吴。\n交怨终亡国，君王到死愚。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bbacb002efc5fa2e9069d3c07b374781",
        "title": "嘉禾百咏 射襄城",
        "author": "张尧同",
        "content": "此地连江海，曾经古战争。\n干戈今不见，空有射襄名。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fede7be27a8a30c9dd5900f54d3f6732",
        "title": "嘉禾百咏 四城",
        "author": "张尧同",
        "content": "吴越争雄日，区区在用兵。\n空余争战地，无处不高城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5b8c7a01ecf8bc82e0b288d7f709be3b",
        "title": "嘉禾百咏 何城",
        "author": "张尧同",
        "content": "报越心何壮，贪齐计已轻。\n忠言殊不听，何用筑斯城。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a20359eee9a203a7435ee64de1185c1f",
        "title": "嘉禾百咏 由拳废县",
        "author": "张尧同",
        "content": "故城人不见，击剑泪空霑。\n一绝悲声远，今犹说孝廉。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "637ec2b2f840b3bbdf6ca775172e7a10",
        "title": "嘉禾百咏 嘉禾墩",
        "author": "张尧同",
        "content": "寂寞黄龙后，纷然壠上碑。\n废兴无处问，空咏草离离。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "25adf0f60c7dc90bcbfb8429e5b96502",
        "title": "嘉禾百咏 古战场",
        "author": "张尧同",
        "content": "自昔干戈地，城空草自荒。\n渔樵怀旧事，何敢议兴亡。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6b1f0bc5a3570fad0981cef9406be244",
        "title": "嘉禾百咏 村门口",
        "author": "张尧同",
        "content": "昔日荒閒地，今闻市井喧。\n人情看促迫，无復似当年。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "02078eb1da8ede9495b286be7a3554f0",
        "title": "嘉禾百咏 宣公桥",
        "author": "张尧同",
        "content": "祗因裴少保，功业败垂成。\n往往桥边客，今犹恨未平。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "15958c356ce215541202e9bf002ca8a3",
        "title": "嘉禾百咏 五柳桥",
        "author": "张尧同",
        "content": "为怀陶靖节，无復见其人。\n谁种桥边柳，犹含旧日春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "dd7b18bb80b1e6a6aa343170a0630c04",
        "title": "嘉禾百咏 百步桥",
        "author": "张尧同",
        "content": "旧址依然在，荒墟未百年。\n还分一桥水，流入几家田。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0c5d7f995f352c81476882eada277fda",
        "title": "嘉禾百咏 跨塘桥",
        "author": "张尧同",
        "content": "路接张泾近，塘连谷水长。\n一声清鹤唳，片月在沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7093635efc4e24d2895613730f65ad91",
        "title": "嘉禾百咏 菩萨桥",
        "author": "张尧同",
        "content": "片云初出岫，南北自飞扬。\n旧日桥边路，犹闻舄履香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1e3d8853780cb8f64da268ad9cc3f451",
        "title": "嘉禾百咏 竹桥",
        "author": "张尧同",
        "content": "腰枕政亭北，横斜古渡头。\n风吹双泪落，恨逐水东流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0bfb402b800c2cd88c5736ca2908ab1d",
        "title": "嘉禾百咏 河内亭",
        "author": "张尧同",
        "content": "掾死缘非命，儿来哭未休。\n旧时亭下泪，疑与水长流。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a47f828bd0be3e3ae50dbacdda8e61ab",
        "title": "嘉禾百咏 嘉禾亭",
        "author": "张尧同",
        "content": "五马来何暮，经营指一弹。\n石碑书近事，留与后人看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "436ff4fd5625ca1fee413876b3c2dbdb",
        "title": "嘉禾百咏 列岫亭",
        "author": "张尧同",
        "content": "吾州风物好，惟是欠青山。\n忽有洞庭色，来从一笑间。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a20706234cb6462444c86ec0ef9d8fcd",
        "title": "嘉禾百咏 水心亭",
        "author": "张尧同",
        "content": "波光磨不尽，镜面绝纤尘。\n照得红亭子，亭亭似美人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "997e7021cc71725d29c4da42cc9ca14d",
        "title": "嘉禾百咏 乐郊亭",
        "author": "张尧同",
        "content": "竹下清风好，时来挂幅巾。\n待逢田叟问，知是乐郊人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8b41f73e1ddee68d3a0f9e150d85e25d",
        "title": "嘉禾百咏 会景亭",
        "author": "张尧同",
        "content": "有景道不出，须来此水滨。\n十分花鸟色，团作一亭春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ecd52afe22d0a5a748c35eff1f767270",
        "title": "嘉禾百咏 南坞",
        "author": "张尧同",
        "content": "岸回波曲曲，城转草层层。\n忽见桃花落，还知到武陵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ff84885efe2062ff865e9d1191a43396",
        "title": "嘉禾百咏 海棠亭",
        "author": "张尧同",
        "content": "藓砌迎烟渚，花开忽满株。\n春风吹烂漫，如展蜀川图。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "248a48d87b78559ef011546bd448500d",
        "title": "嘉禾百咏 白莲沼",
        "author": "张尧同",
        "content": "沧波围四面，艷艷玉开花。\n自可除帘幕，清香不用遮。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2e58200d68e80dba701eb95402e354f9",
        "title": "嘉禾百咏 桃花亭",
        "author": "张尧同",
        "content": "不待东风拆，池塘已自春。\n试看源上客，犹恐是秦人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "516d2a9368cbbc1c4dcfcefee2f7e900",
        "title": "嘉禾百咏 红薇径",
        "author": "张尧同",
        "content": "渐放秋风晚，花无烂漫香。\n浅深红似锦，散影在沧浪。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e886e1c2356bd42aeb1c3e4da9bb8970",
        "title": "嘉禾百咏 茶溪",
        "author": "张尧同",
        "content": "茶林那復有，零落付樵人。\n旧日溪边叟，空悲二月春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a5e3527e9ffc006876f8cee06ea719ed",
        "title": "嘉禾百咏 仙鹤亭",
        "author": "张尧同",
        "content": "杳杳云间路，重重隔翠林。\n数声清夜后，来伴月边吟。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "346393fef844fdb87abf1c37d5d984a7",
        "title": "嘉禾百咏 芙蓉塘",
        "author": "张尧同",
        "content": "一发连千丈，红云出素波。\n秋风吹欲老，无奈落花何。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a112bc5005d7ff9def224f4f70e8249",
        "title": "嘉禾百咏 白苎桥",
        "author": "张尧同",
        "content": "遍陇敷青绿，桑麻共几家。\n满城罗绮子，应笑不栽花。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4f964e3eb5afb5470b84a6712518f89a",
        "title": "嘉禾百咏 渔溆",
        "author": "张尧同",
        "content": "野水连天阔，沧波引钓缗。\n寄言溪上叟，莫学直鈎人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ae73b2f8ca9037bd891fb2ccd30c1e25",
        "title": "嘉禾百咏 怀苏亭",
        "author": "张尧同",
        "content": "犹使樽前客，常怀没后名。\n好风吹远籁，如有笑歌声。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0d7cacec2fb15e01ee9397de3c38751b",
        "title": "嘉禾百咏 披云阁",
        "author": "张尧同",
        "content": "兹楼当胜地，高望极崚嶒。\n欲覩青天色，还须上一层。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0c0db95c506d3b14e17a6e9514afecb9",
        "title": "嘉禾百咏 景龙楼",
        "author": "张尧同",
        "content": "千秋传异綵，百尺护雕阑。\n欲识金龙瑞，丹霄试仰看。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "26cfc4040f43f42fd5049d7dab599a59",
        "title": "嘉禾百咏 烟雨楼",
        "author": "张尧同",
        "content": "妙手谁烘染，梳烟沐雨姿。\n一声长笛晚，人在倚楼时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4c21adca15806e683cc8573eac92dc5f",
        "title": "嘉禾百咏 月波楼",
        "author": "张尧同",
        "content": "天阔冰壶浄，溪平玉鑑寒。\n卷帘看晚色，鸥鸟并阑干。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cab06893b7f40101819ef799a0fb30c8",
        "title": "嘉禾百咏 真如院",
        "author": "张尧同",
        "content": "田家轻脱屣，聊结浄人缘。\n影外香灯寂，清风百世传。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "79e0568839cf09b00e380d466652c280",
        "title": "嘉禾百咏 雪峰庵",
        "author": "张尧同",
        "content": "一语合领悟，师承岂在身。\n戏言庐墓客，未是报恩人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "46a9f9d2b02c6721952d6b1a95de07cf",
        "title": "嘉禾百咏 景德禅院",
        "author": "张尧同",
        "content": "白毫光未灭，胜事百年中。\n好是传灯地，林泉有古风。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "f401d14c4591b3f4aa0c9c7e5169b1a7",
        "title": "嘉禾百咏 三塔",
        "author": "张尧同",
        "content": "石匮埋神物，灵光照夜多。\n龙归天上后，无復更风波。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8e5a63964a289c16fdec70de81b8da0c",
        "title": "嘉禾百咏 学绣塔",
        "author": "张尧同",
        "content": "孤塔岧峣立，犹传学绣人。\n五湖如好在，针线不胜春。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "a3e544fef61cf54859148963a3027160",
        "title": "嘉禾百咏 水陆院",
        "author": "张尧同",
        "content": "白昼重门寂，苍苔古殿深。\n老僧香火罢，松柏夜森森。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "07ef290a132f3ecd4fa1bae2dd618088",
        "title": "嘉禾百咏 泗洲庵",
        "author": "张尧同",
        "content": "旧寺丘墟后，重兴岁月赊。\n莫将閒祸福，容易託僧伽。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d46895f7eecf5aa29b08c95f97e3dfc8",
        "title": "嘉禾百咏 云门家舍",
        "author": "张尧同",
        "content": "满箧藏锋句，家传世几何。\n远孙无悟解，却笑葛藤多。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "5c27fa90b93dc3a27a2d551a90a153f0",
        "title": "嘉禾百咏 白莲上方",
        "author": "张尧同",
        "content": "古屋深于洞，门前种白莲。\n好寻当日社，重结此生缘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e170ac66816a55025c1f89726c6f2e1e",
        "title": "嘉禾百咏 普济院",
        "author": "张尧同",
        "content": "祗应曾布地，往往化为鱼。\n一遇丁千骑，禅人便结庐。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8a7becc3a5141dbf9750b2f4a506f087",
        "title": "嘉禾百咏 兜率寺",
        "author": "张尧同",
        "content": "昔年池满地，今见屋耽耽。\n不结莲花界，龟鱼死未甘。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "bc7ca9a13e115d44121b9f089abf660c",
        "title": "嘉禾百咏 姜庵",
        "author": "张尧同",
        "content": "不入红尘市，安居三十年。\n至今庵下路，芳草碧芊芊。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "d8062738e0e7d7500f2629a2892d4557",
        "title": "嘉禾百咏 惠明堂",
        "author": "张尧同",
        "content": "地僻无人到，松风鹤梦清。\n一灯禅榻畔，自向暗中明。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "fe7b563e4ef44d7c419ea48d122706e7",
        "title": "嘉禾百咏 三过堂",
        "author": "张尧同",
        "content": "题诗三过此，坡老昔时曾。\n悟得成禅指，来为打睡僧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "63b3f902f757b5f5551741b19ed0469b",
        "title": "嘉禾百咏 寂照庵",
        "author": "张尧同",
        "content": "寂灭跏趺地，灯光照四时。\n高僧穿地脉，此意少人知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "b19a1afc41adbc0b999454c193f6d60a",
        "title": "嘉禾百咏 天庆观",
        "author": "张尧同",
        "content": "寥阳龙已化，华表鹤空悲。\n欲问梁朝事，惟应老桧知。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6ff586a0d052f682f78cbb964888cd8c",
        "title": "嘉禾百咏 福顺庙",
        "author": "张尧同",
        "content": "老屋无丹雘，空阶只翠苔。\n遶檐看箭镞，谁记赤眉来。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0e8f9b182845d5ecffefae3969ebd4a4",
        "title": "嘉禾百咏 昌武庙",
        "author": "张尧同",
        "content": "祗因吴士季，千载仰公神。\n欲借驱痁术，来临刻木人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7fa170d289e234d8cf64e49c800bb97a",
        "title": "嘉禾百咏 杨姑庙",
        "author": "张尧同",
        "content": "孝德襄陵久，天然与众殊。\n生男不能养，应合愧杨姑。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "7437d67d3b00124a8c7fd99e0b1405e5",
        "title": "嘉禾百咏 徐偃王庙",
        "author": "张尧同",
        "content": "王有岐夷德，千年貌亦存。\n我怀三刺史，谁是后来孙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0f54e3ef659ec39ba4b48b8751dbb6ff",
        "title": "嘉禾百咏 曹使君祠",
        "author": "张尧同",
        "content": "一笑淮兵却，功成未易论。\n岂惭颜刺史，诗酒在平原。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3da298802edc1c9840f7c9419ece8bab",
        "title": "嘉禾百咏 高使君祠",
        "author": "张尧同",
        "content": "一瞑移藩日，邦人昔被恩。\n步兵碑尚在，功业敢经纶。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "ae8596f82a02c0f3de056df73dc15ee1",
        "title": "嘉禾百咏 寥阳殿",
        "author": "张尧同",
        "content": "人归天上去，空识旧衣冠。\n秘录藏金匮，清风满殿寒。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0bcc78fbd53993d595bb6309a925a91a",
        "title": "嘉禾百咏 霍将军庙",
        "author": "张尧同",
        "content": "弼汉功非浅，谗夫可奈何。\n海神今亦畏，安敢布风波。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "11b5b7aca44e42da19a30c03a7fabf49",
        "title": "嘉禾百咏 读书堆",
        "author": "张尧同",
        "content": "平林标大道，曾是野王居。\n往事将谁语，凄凉六代余。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "4229c161f3f8617180731532d4cac3ef",
        "title": "嘉禾百咏 简斋",
        "author": "张尧同",
        "content": "诗老追唐句，閒居筑简斋。\n芙蓉寒浦外，红落感秋怀。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "0ec4a30fb3164b32ac1344236e540a11",
        "title": "嘉禾百咏 陈贤良隠居",
        "author": "张尧同",
        "content": "发策名犹在，回头事已非。\n池塘春草绿，空忆谢公归。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "9d8e8c34d1efb1e4d9899130717e5714",
        "title": "嘉禾百咏 徐长者园",
        "author": "张尧同",
        "content": "丹砂犹未就，白髮忽成翁。\n却有怜春意，栽花向此中。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "cddc3c4905b72cef3b13979e70b47e0a",
        "title": "嘉禾百咏 赵老园",
        "author": "张尧同",
        "content": "藏书几万卷，归老此林泉。\n不为寻莼鲙，于公亦有贤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "1d0307838213345fe478649ba2094fc5",
        "title": "嘉禾百咏 施墓",
        "author": "张尧同",
        "content": "苍苔侵坏道，晚雀散空林。\n莫问人贤否，浮生一古今。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8595c6b59f92e7ee23da1fb1008cad3a",
        "title": "嘉禾百咏 卫墓",
        "author": "张尧同",
        "content": "惆怅黎阳客，飘流浙水滨。\n定忘空腹恨，知是赤心人。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "629fe5291bc03a7438e3e058478c4652",
        "title": "嘉禾百咏 刘伶墓",
        "author": "张尧同",
        "content": "荷锸从吾后，叮咛死便埋。\n人皆讥嗜酒，我独爱忘怀。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e6a61ee84c111fe762dfcf0a670fd938",
        "title": "嘉禾百咏 严忌墓",
        "author": "张尧同",
        "content": "有子如君少，终缘叛国亡。\n冢前无后嗣，切勿怨张汤。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6010260d4f83ac3d6ceee9660fb53a4c",
        "title": "嘉禾百咏 朱买臣墓",
        "author": "张尧同",
        "content": "世事春风转，荣枯一梦间。\n绣衣人不见，孤冢旧家山。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "043e21696adbad4739f487348c74631d",
        "title": "嘉禾百咏 周都官墓",
        "author": "张尧同",
        "content": "不识君名字，应难问子孙。\n一丘浮水际，神怪欲谁论。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "95eb484159736adc06e2a1d6a0fd15c9",
        "title": "嘉禾百咏 苏小小墓",
        "author": "张尧同",
        "content": "泉下骨应朽，幽魂独未消。\n几番清夜月，孤影度南桥。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3b6d85fa6abb68e0dc2afd9f3b7fe519",
        "title": "嘉禾百咏 羞墓",
        "author": "张尧同",
        "content": "伤哉负薪子，五十始登朝。\n空使埋羞处，山花復采樵。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "8f5350577f5fee70c7c27b700a24d186",
        "title": "嘉禾百咏 木纹观音",
        "author": "张尧同",
        "content": "双瞳分日月，眉际一星悬。\n照见人间苦，三辰不在天。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "6271045c13531dd6c63664834b7c9df2",
        "title": "嘉禾百咏 邦药王龛",
        "author": "张尧同",
        "content": "父老怀遗爱，谁传肘后方。\n药囊同日葬，空忆紫元香。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "28a9ccc73652aa91abd42fe9a83b6e52",
        "title": "嘉禾百咏 浄相佳李",
        "author": "张尧同",
        "content": "地重因名果，如分沆瀣浆。\n伤心吴越战，未敢尽情尝。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2a80f7cd1da755b28391bd2f3fc4d774",
        "title": "嘉禾百咏 符姥瑞竹",
        "author": "张尧同",
        "content": "一幅霜缣外，青青出自然。\n天机何敢议，珍重白衣仙。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "2b92d5eed8c0644b9ff68c517188bb52",
        "title": "徐家庄",
        "author": "张尧同",
        "content": "节义穷方见，高名荐玉京。\n一封天子敕，千载故乡荣。",
        "dynasty": "宋",
        "collection_info": null
    }
]