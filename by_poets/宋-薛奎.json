[
    {
        "uuid": "6e2b32e91bcd4eacbb1a794c004bd190",
        "title": "忆越州  其一",
        "author": "薛奎",
        "content": "照湖方干吟格高，搜清研奇含烂毫。\n天中无云水中月，虚明晶曜忘迴舠。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3c6f941275d55a2212e32bc168c7da64",
        "title": "忆越州  其二",
        "author": "薛奎",
        "content": "千岩万壑争清奇，天地灵秀应无遗。\n吟舟恐在碧霄上，湖心澄波涵月时。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "41f81ce4c60f191b4957949a92925eda",
        "title": "寄金绳院正因大师",
        "author": "薛奎",
        "content": "僧中忆艺本超羣，释氏蒙求见讨论。\n儒行合为文畅侣，诗名雅作贯休孙。\n心灯久已传宗意，命服仍尝锡帝恩。\n宴坐栴檀消篆字，眼前阛阓任嚣喧。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "3f8c2d64a2a8f2d24630aab66d2793d6",
        "title": "句  其一",
        "author": "薛奎",
        "content": "囊书空自负，早晚达明君。",
        "dynasty": "宋",
        "collection_info": null
    },
    {
        "uuid": "e8106faec8f826431905f6bdd680f23a",
        "title": "句  其二",
        "author": "薛奎",
        "content": "千林如有喜，一气自无私。",
        "dynasty": "宋",
        "collection_info": null
    }
]